#DIM LCOUNT
#DIM LTEMP

IF ARG == 1
	;ACT 장기 보정치에 가산
	FOR LCOUNT, 0, 200
		RESULT:LCOUNT += ABSQSI(LOCAL:LCOUNT)
	NEXT
	;LOCAL 초기화
	VARSET LOCAL
	;RESULT 보유
	RETURN RESULT
ENDIF

