SELECTCASE RESULT
	CASE 0
		PRINTFORML 「입다물어지면 곤란해 버립니다만…」
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("호기심", -10)
		CALLF CM_PROF("수치", 10)
		CALLF CM_PROF("신경질", 10)
	CASE 1
		PRINTFORML 「좋았다… 기쁩니다」
		CALLF CM_PROF("공격", -10)
		CALLF CM_PROF("솔직함", 20)
		CALLF CM_PROF("프라이드", -20)
		CALLF CM_PROF("충동", -30)
		CALLF CM_PROF("수치", -10)
