@CHARAMAKE_K37(ARG)
PRINTFORMW 「아, 일어났군요」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:로마자)
PRINTL 
PRINTFORML 「나부터는 당연, 가을에 대한 질문입니다」
PRINTFORMW 「당신이 가을이라고 들어 떠올리는 것은 어떤 것일까?」
PRINTL 
PRINTFORMDL [0] 대답하지 않는다
PRINTFORMDL [1] 식욕의 가을
PRINTFORMDL [2] 독서의 가을
PRINTFORMDL [3] 스포츠의 가을
PRINTFORMDL [4] 그 외의 가을
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「혹시, 가을, 싫었다거나 해……?」
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("호기심", -10)
		CALLF CM_PROF("수치", 10)
		CALLF CM_PROF("신경질", 10)
	CASE 1
		PRINTFORML 「무엇 먹어도 맛있는 계절이지요」
		PRINTFORML 「말은 높아져도 좋지만, 당신은 조심해」
		CALLF CM_PROF("솔직함",  +10)
		CALLF CM_PROF("충동",    +10)
		CALLF CM_PROF("낙관",    +10)
		CALLF CM_PROF("지성",    -20)
		CALLF CM_PROF("신경질",  -20)
		CALLF CM_PROF("Ｂ를 좋아하는 사람",  +10)
	CASE 2
		PRINTFORML 「가을의 긴 밤에 멋대로 독서, 풍류지요」
		CALLF CM_PROF("공격",    -20)
		CALLF CM_PROF("충동",    -20)
		CALLF CM_PROF("지성",    +30)
		CALLF CM_PROF("도착",    -10)
		CALLF CM_PROF("모랄",  +10)
		CALLF CM_PROF("신경질",  +10)
	CASE 3
		PRINTFORML 「(듣)묻는 곳에 의하면, 밖의 세계에는 운동회는 축제가 있는 것 같다」
		PRINTFORML 「나도 보고 싶구나……」
		CALLF CM_PROF("공격",    +20)
		CALLF CM_PROF("피학",    -10)
		CALLF CM_PROF("충동",    +10)
		CALLF CM_PROF("수치",    -10)
		CALLF CM_PROF("지성",    -10)
		CALLF CM_PROF("신경질",  -10)
		CALLF CM_PROF("Ｖ를 좋아하는 사람",  +10)
	CASE 4
		PRINTFORML 「예술, 하이킹, 온천…… 가을은 무엇을 해도 훌륭한 계절입니다」
		PRINTFORML 「엣치한 일……?   예!?」
		CALLF CM_PROF("솔직함",  -10)
		CALLF CM_PROF("호기심",  +20)
		CALLF CM_PROF("수치",    -20)
		CALLF CM_PROF("도착",    +20)
		CALLF CM_PROF("동성애",  +10)
		CALLF CM_PROF("결벽",    -10)
		CALLF CM_PROF("모랄",  -10)
ENDSELECT
PRINTFORMW 
RETURN 1
