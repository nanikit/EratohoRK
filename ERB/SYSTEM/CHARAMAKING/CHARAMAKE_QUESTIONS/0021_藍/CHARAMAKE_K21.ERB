;藍@Enter
@CHARAMAKE_K21(ARG)
PRINTFORMW 「おはよう。ぐっすり眠れたか？　寝起きで悪いが少し質問に答えてもらうぞ」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「お前は私の式だとする」
PRINTFORML 「何か一つ、家事を手伝えと私が言った」
PRINTFORMW 「さぁ、お前なら何を手伝う？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 掃除
PRINTFORMDL [2] 皿洗い
PRINTFORMDL [3] 蔵の整理
PRINTFORMDL [4] 子作り
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「む、手伝いたくないのか？」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「まぁ、無難な選択だな」
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("好奇心", 10)
		CALLF CM_PROF("楽観", 10)
		CALLF CM_PROF("潔癖", 20)
	CASE 2
		PRINTFORML 「一緒に皿洗いか。それもいいな」
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("同性愛", 10)
		CALLF CM_PROF("潔癖", 10)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("Ｂ好き", 10)
		CALLF CM_PROF("攻撃", -30)
	CASE 3
		PRINTFORML 「ほう、なかなか粋なこともやろうとするんだな。いいことだ」
		CALLF CM_PROF("好奇心", 30)
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("神経質", -10)
		CALLF CM_PROF("Ａ好き", 10)
		CALLF CM_PROF("羞恥", 10)
	CASE 4
		PRINTFORML 「は？　ん？　まぁ、家の事と書いて『家事』だしな……それもありか……」
		CALLF CM_PROF("Ｖ好き", 20)
		CALLF CM_PROF("倒錯", 10)
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("Ｃ好き", 20)
		CALLF CM_PROF("羞恥", -20)
ENDSELECT
PRINTFORMW 「ふむ、お前はそう考えているのか。分かった。後は休んでいいぞ」
RETURN 1
