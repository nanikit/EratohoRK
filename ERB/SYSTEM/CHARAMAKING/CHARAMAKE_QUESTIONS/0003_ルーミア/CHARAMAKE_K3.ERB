PRINTFORMDL [1] 서리가 내림의 제일 맛있을 것 같은 곳
PRINTFORMDL [2] 곁들이고의 당근 소테(서양 요리)
PRINTFORMDL [3] 우선 스프를 한입
PRINTFORMDL [4] 주위에 맞춘다
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「고민하고 있으면 고기가 도망쳐 버려?」
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("호기심", -10)
		CALLF CM_PROF("수치", 10)
		CALLF CM_PROF("신경질", 10)
	CASE 1
		PRINTFORML 「응, 역시 그렇네요─」
		CALLF CM_PROF("공격", 10)
		CALLF CM_PROF("솔직함", 20)
		CALLF CM_PROF("프라이드", -20)
		CALLF CM_PROF("충동", 30)
		CALLF CM_PROF("수치", -10)
		CALLF CM_PROF("지성", -20)
		CALLF CM_PROF("결벽", -10)
		CALLF CM_PROF("낙관", 10)
		CALLF CM_PROF("모랄", -10)
		CALLF CM_PROF("Ｃ를 좋아하는 사람", 20)
		CALLF CM_PROF("Ｖ를 좋아하는 사람", 30)
		CALLF CM_PROF("Ｂ를 좋아하는 사람", -10)
	CASE 2
		PRINTFORML 「에에에… 뭐야 그것」
		CALLF CM_PROF("피학", 10)
		CALLF CM_PROF("솔직함", -10)
		CALLF CM_PROF("프라이드", 10)
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("결벽", 10)
		CALLF CM_PROF("도착", 10)
		CALLF CM_PROF("Ｖ를 좋아하는 사람", -10)
	CASE 3
		PRINTFORML 「웃와, 딱딱하다」
		CALLF CM_PROF("공격", -10)
		CALLF CM_PROF("피학", 10)
		CALLF CM_PROF("충동", -20)
		CALLF CM_PROF("프라이드", 20)
		CALLF CM_PROF("신경질", 20)
		CALLF CM_PROF("결벽", 20)
		CALLF CM_PROF("Ｂ를 좋아하는 사람", 30)
	CASE 4
		PRINTFORML 「… 무엇인가, 외모악」
		CALLF CM_PROF("낙관", -10)
		CALLF CM_PROF("피학", 20)
		CALLF CM_PROF("프라이드", -20)
		CALLF CM_PROF("공격", -10)
		CALLF CM_PROF("수치", 30)
		CALLF CM_PROF("충동", -20)
		CALLF CM_PROF("신경질", 10)
