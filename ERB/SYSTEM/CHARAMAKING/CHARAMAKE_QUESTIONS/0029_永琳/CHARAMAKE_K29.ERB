		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「まぁ、本当に嫌いなら今すぐ死んじゃえ、って感じよね」
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("好奇心", 10)
		CALLF CM_PROF("攻撃", 10)
		CALLF CM_PROF("被虐", 10)
	CASE 2
		PRINTFORML 「苦しむのを楽しむのかしら？」
