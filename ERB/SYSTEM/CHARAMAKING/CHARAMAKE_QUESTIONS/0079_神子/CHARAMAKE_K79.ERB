;神子＠ぱ。
@CHARAMAKE_K79(ARG)
PRINTFORMW 「来ましたね。おはようございます」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「君がくるのを待っていたのです」
PRINTFORMW 「さあ、君のしたいこと――欲を教えなさい。あ、話さなくてもいいです」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] いいから股開けよ
PRINTFORMDL [2] 腹へった
PRINTFORMDL [3] 寝かせて
PRINTFORMDL [4] いじめてください
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「黙っていても私は君の欲を聞くことができるのです」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「なんと…ふむ。悪い気はしませんが、困るわね」
		CALLF CM_PROF("攻撃", 20)
		CALLF CM_PROF("被虐", -20)
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("衝動", 20)
		CALLF CM_PROF("倒錯", 10)
		CALLF CM_PROF("潔癖", -10)
		CALLF CM_PROF("モラル", -30)
		CALLF CM_PROF("Ｃ好き", 10)
		CALLF CM_PROF("Ｖ好き", 10)
	CASE 2
		PRINTFORML 「…呆れるといいますか。良いと思います、健康的で」
		CALLF CM_PROF("素直さ", 30)
		CALLF CM_PROF("被虐", 10)
		CALLF CM_PROF("プライド", -20)
		CALLF CM_PROF("楽観", 30)
		CALLF CM_PROF("知性", -20)
		CALLF CM_PROF("羞恥", -20)
		CALLF CM_PROF("神経質", -20)
		CALLF CM_PROF("Ａ好き", 10)
	CASE 3
		PRINTFORML 「疲れているの？　起こしてごめんなさい」
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("被虐", 10)
		CALLF CM_PROF("楽観", 20)
		CALLF CM_PROF("好奇心", -20)
		CALLF CM_PROF("知性", 10)
		CALLF CM_PROF("倒錯", -10)
	CASE 4
		PRINTFORML 「な、なんですって？　まったく…変態というやつですか？」
		CALLF CM_PROF("攻撃", -20)
		CALLF CM_PROF("被虐", 30)
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("知性", -10)
		CALLF CM_PROF("倒錯", 30)
		CALLF CM_PROF("モラル", -10)
		CALLF CM_PROF("潔癖", -20)
		CALLF CM_PROF("Ａ好き", 20)
ENDSELECT
PRINTFORMW 「それでは欲深き君。今しばし、おやすみなさい――」
RETURN 1
