;チルノ＠ぱ。
@CHARAMAKE_K5(ARG)
PRINTFORMW 「さあ起きろ！　起きるか冬眠するか選んでもらうよ！」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「あたいからの質問だよ！」
PRINTFORMW 「氷といえば何！？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] アイスたべたいなぁ
PRINTFORMDL [2] 冷たく悲しい、そして儚い
PRINTFORMDL [3] 別に何とも…
PRINTFORMDL [9] バカ
CALL INPUTINTD(0, 1, 2, 3, 9)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「…もしかして聞かれてる意味わかってない？　バカ？」
		PRINTFORMW 「まったく…もう知らないよ！　おやすみ！」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「食べ物のことしか頭に無いの？」
		PRINTFORMW 「そんなに好きなら夢でもたらふく食べればいいよ！」
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("プライド", -10)
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("知性", -20)
		CALLF CM_PROF("潔癖", -10)
		CALLF CM_PROF("楽観", 20)
	CASE 2
		PRINTFORML 「ふふん、詩的なこと言うのね！」
		PRINTFORMW 「ゆっくり休んでいいわ、おやすみ――」
		CALLF CM_PROF("素直さ", -20)
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("楽観", -20)
		CALLF CM_PROF("知性", 10)
		CALLF CM_PROF("モラル", -10)
		CALLF CM_PROF("倒錯", 10)
	CASE 3
		PRINTFORML 「な、なにぃー！」
		PRINTFORMW 「あんたなんか冬眠してしまえー！」
		CALLF CM_PROF("好奇心", -20)
		CALLF CM_PROF("被虐", -10)
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("潔癖", 20)
		CALLF CM_PROF("神経質", 20)
	CASE 9
		PRINTFORML 「ばっバカ？　なんだそれーーー！？」
		PRINTFORMW 「永眠させてやるー！　この、この、このっ！」
		CALLF CM_PROF("攻撃", 10)
		CALLF CM_PROF("被虐", 20)
		CALLF CM_PROF("衝動", 30)
		CALLF CM_PROF("好奇心", 30)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("知性", -20)
		CALLF CM_PROF("モラル", -30)
		CALLF CM_PROF("Ｃ好き", 10)
ENDSELECT
RETURN 1
