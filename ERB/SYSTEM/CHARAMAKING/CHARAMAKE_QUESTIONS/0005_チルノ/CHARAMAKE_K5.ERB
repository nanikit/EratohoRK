@CHARAMAKE_K5(ARG)
PRINTFORMW 「자 일어나라!   일어나는지 동면할까 선택해 받는다!」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:로마자)
PRINTL 
PRINTFORML 「값으로부터의 질문이야!」
PRINTFORMW 「얼음이라고 하면 무엇!?」
PRINTL 
PRINTFORMDL [0] 대답하지 않는다
PRINTFORMDL [1] 아이스 먹고 싶구나
PRINTFORMDL [2] 차갑고 슬픈, 그리고 덧없다
PRINTFORMDL [3] 별로 뭐라고도…
PRINTFORMDL [9] 바보
CALL INPUTINTD(0, 1, 2, 3, 9)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「… 혹시 (듣)묻고 있는 의미 몰라?   바보?」
		PRINTFORMW 「전혀… 이제 몰라!   잘 자요!」
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("호기심", -10)
		CALLF CM_PROF("수치", 10)
		CALLF CM_PROF("신경질", 10)
	CASE 1
		PRINTFORML 「음식의 일 밖에 머릿속에 없는거야?」
		PRINTFORMW 「그렇게 좋아하면 꿈에서도 배불리 먹으면 좋아!」
		CALLF CM_PROF("솔직함", 10)
		CALLF CM_PROF("프라이드", -10)
		CALLF CM_PROF("충동", 10)
		CALLF CM_PROF("수치", -10)
		CALLF CM_PROF("지성", -20)
		CALLF CM_PROF("결벽", -10)
		CALLF CM_PROF("낙관", 20)
	CASE 2
		PRINTFORML 「후흥, 시적인 일 하는 거네!」
		PRINTFORMW 「천천히 쉬어도 좋아요, 잘 자요―」
		CALLF CM_PROF("솔직함", -20)
		CALLF CM_PROF("프라이드", 20)
		CALLF CM_PROF("낙관", -20)
		CALLF CM_PROF("지성", 10)
		CALLF CM_PROF("모랄", -10)
		CALLF CM_PROF("도착", 10)
	CASE 3
		PRINTFORML 「뭐, 뭐어─!」
		PRINTFORMW 「너 같은 것 동면해 버려라―!」
		CALLF CM_PROF("호기심", -20)
		CALLF CM_PROF("피학", -10)
		CALLF CM_PROF("충동", -10)
		CALLF CM_PROF("결벽", 20)
		CALLF CM_PROF("신경질", 20)
	CASE 9
		PRINTFORML 「바보?   야 그거─!?」
		PRINTFORMW 「영면시켜 준다―!  이, 이, 이!」
		CALLF CM_PROF("공격", 10)
		CALLF CM_PROF("피학", 20)
		CALLF CM_PROF("충동", 30)
		CALLF CM_PROF("호기심", 30)
		CALLF CM_PROF("도착", 20)
		CALLF CM_PROF("지성", -20)
		CALLF CM_PROF("모랄", -30)
		CALLF CM_PROF("C를 좋아하는 사람", 10)
ENDSELECT
RETURN 1
