;依姫＠ぱ。
@CHARAMAKE_K54(ARG)
SELECTCASE ARG
	CASE 0
		PRINTFORMW 「お目覚め？　早くに起こして悪いとは思うけど」
	CASE 9
		PRINTFORMW 「その顔、相当たらい回されてきたのね。大丈夫、私で最後よ」
	CASEELSE
		PRINTFORMW 「さて、手短に参りましょうか」
ENDSELECT
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「人間は神を頼り社へ参る。屋代とは神の依り代であるから」
PRINTFORMW 「問います。貴方の依りたい、力を借りたい神とは？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 菅原道真
PRINTFORMDL [2] 大国主命
PRINTFORMDL [3] 伊邪那美命
PRINTFORMDL [4] 素盞鳴尊
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「祀らぬ者を救う神はありません」
		PRINTFORMW 「祈りの声を上げ信仰すること、これが始まりなのです」
		;無回答の補正値は固定
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「道真といえば学問の神。貴方は文官か、科挙に臨む者ですか？」
		PRINTFORMW 「知恵までも神頼みとはいかにも強欲。天神様も大変よね」
		CALLF CM_PROF("被虐", -20)
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("プライド", -20)
		CALLF CM_PROF("好奇心", -20)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("楽観", 10)
		CALLF CM_PROF("知性", 30)
		CALLF CM_PROF("潔癖", 10)
		CALLF CM_PROF("Ａ好き", 10)
	CASE 2
		PRINTFORML 「ダイコク様は様々に言い伝えられる姿多き神。大変な美男と言うわね」
		;ちんこある
		IF PENIS(MASTER)
			PRINTFORMW 「彼の力ならどんな女も心乱れます。もしかするとこの私でさえ」
			CALLF CM_PROF("攻撃", 10)
			CALLF CM_PROF("被虐", -20)
			CALLF CM_PROF("素直さ", 20)
			CALLF CM_PROF("プライド", 20)
			CALLF CM_PROF("衝動", 20)
			CALLF CM_PROF("楽観", 10)
			CALLF CM_PROF("知性", -20)
			SIF TALENT:MASTER:ふたなり
				CALLF CM_PROF("同性愛", 30)
			CALLF CM_PROF("潔癖", -10)
			CALLF CM_PROF("モラル", -20)
			CALLF CM_PROF("神経質", -10)
			CALLF CM_PROF("Ｃ好き", 30)
			CALLF CM_PROF("Ｖ好き", -10)
			CALLF CM_PROF("Ｂ好き", -10)
			CALLF CM_PROF("Ａ好き", -10)
		;ちんこない
		ELSE
			PRINTFORMW 「…何その顔。貴方、抱かれてみたいのね？」
			CALLF CM_PROF("攻撃", -20)
			CALLF CM_PROF("被虐", 30)
			CALLF CM_PROF("素直さ", 10)
			CALLF CM_PROF("プライド", -30)
			CALLF CM_PROF("衝動", 20)
			CALLF CM_PROF("好奇心", 20)
			CALLF CM_PROF("羞恥", -20)
			CALLF CM_PROF("楽観", 20)
			CALLF CM_PROF("知性", -10)
			CALLF CM_PROF("同性愛", -20)
			CALLF CM_PROF("潔癖", -20)
			CALLF CM_PROF("モラル", -20)
			CALLF CM_PROF("Ｖ好き", 30)
		ENDIF
	CASE 3
		PRINTFORML 「イザナミ様は国産みの神。豊穣と多産を司る、穢れなき御方」
		PRINTFORMW 「…穢れてしまった彼女は、黄泉に棲む死と嫉妬の神。女とは悲しい生き物ね」
		CALLF CM_PROF("攻撃", -30)
		CALLF CM_PROF("被虐", 20)
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("羞恥", 30)
		CALLF CM_PROF("楽観", -20)
		CALLF CM_PROF("倒錯", 30)
		CALLF CM_PROF("同性愛", -10)
		CALLF CM_PROF("潔癖", 20)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("神経質", 10)
		CALLF CM_PROF("Ｂ好き", 20)
	CASE 4
		PRINTFORML 「不浄から産まれし荒ぶる神。貴方はそんなに力を欲しいの？」
		PRINTFORMW 「彼の妻は美しく豊かな稲田姫様。貴方は賢妻に巡り合えるのかしら」
		CALLF CM_PROF("攻撃", 30)
		CALLF CM_PROF("被虐", -30)
		CALLF CM_PROF("素直さ", -20)
		CALLF CM_PROF("プライド", 30)
		CALLF CM_PROF("衝動", 30)
		CALLF CM_PROF("好奇心", 20)
		CALLF CM_PROF("羞恥", -30)
		CALLF CM_PROF("楽観", 10)
		CALLF CM_PROF("知性", -20)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("潔癖", -30)
		CALLF CM_PROF("モラル", -30)
		CALLF CM_PROF("Ｃ好き", 20)
ENDSELECT
RETURN 1
