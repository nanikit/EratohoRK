;小悪魔＠こぁの人
;ちょっと夢魔っぽい感じで
@CHARAMAKE_K7(ARG)
PRINTFORMW 「おやすみのところ、失礼しますね」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「ふふ、いい夢を見られていたようですね……」
PRINTFORML 「少し夢に手を加えさせて頂いて、淫らな夢を見させてあげましたが……」
PRINTFORMW 「どのような夢だったか聞かせていただけますか？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] お豆さん
PRINTFORMDL [2] おっぱい！おっぱい！
PRINTFORMDL [3] ぬっぷぬっぷ
PRINTFORMDL [4] おしりしゅごいのー
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「……答えられないほどいい夢だった、ということですね」
		;無回答の補正値は固定
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「顔面騎乗？なかなかいい夢じゃないですか」
		PRINTFORMDL 小悪魔はくすりと笑った
		CALLF CM_PROF("被虐", -10)
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("プライド", -20)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("Ｃ好き", 60)
		CALLF CM_PROF("Ｖ好き", -20)
		CALLF CM_PROF("Ｂ好き", -20)
		CALLF CM_PROF("Ａ好き", -20)
	CASE 2
		PRINTFORML 「あらあら、ぱふぱふしていただいてたんですか？」
		PRINTFORML 「それは悪いタイミングで起こしてしまったようですね」
		PRINTFORMDL 小悪魔はにやにやとあなたを見ている
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("Ｃ好き", -20)
		CALLF CM_PROF("Ｖ好き", -20)
		CALLF CM_PROF("Ｂ好き", 60)
		CALLF CM_PROF("Ａ好き", -20)
	CASE 3
		PRINTFORML 「うふふ、楽しまれていたようですね」
		PRINTFORMDL 小悪魔はにっこりと笑った
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("好奇心", 10)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("同性愛", -20)
		IF TALENT:MASTER:オトコ
			CALLF CM_PROF("Ｃ好き", 60)
			CALLF CM_PROF("Ｖ好き", -20)
		ELSEIF TALENT:MASTER:ふたなり
			CALLF CM_PROF("Ｃ好き", 30)
			CALLF CM_PROF("Ｖ好き", 30)
		ELSE
			CALLF CM_PROF("Ｃ好き", -20)
			CALLF CM_PROF("Ｖ好き", 60)
		ENDIF
		CALLF CM_PROF("Ｂ好き", -20)
		CALLF CM_PROF("Ａ好き", -20)
	CASE 4
		PRINTFORML 「お尻……なかなかマニアックですねぇ」
		PRINTFORMDL 小悪魔は少し驚いたようだ
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("好奇心", 20)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("潔癖", -20)
		CALLF CM_PROF("Ｃ好き", -20)
		CALLF CM_PROF("Ｖ好き", -20)
		CALLF CM_PROF("Ｂ好き", -20)
		CALLF CM_PROF("Ａ好き", 60)
ENDSELECT
PRINTFORMW 「それではごゆっくり、お休みください」
RETURN 1
