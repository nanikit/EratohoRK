;雛＠/L
@CHARAMAKE_K39(ARG)
PRINTFORMW 「厄神の部屋へようこそ。少しの間だけどゆっくりしていって」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「そうそう、厄祓いについてあなたの考えを聞かせてもらえないかしら？」
PRINTFORMW 「ねぇ、正直に答えて」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] お守り等で神仏に頼る
PRINTFORMDL [2] 香を焚く等して、厄を追い出す
PRINTFORMDL [3] 流し雛等の身代わりを立てる
PRINTFORMDL [4] 抜いてください厄神様！
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「あっ、そう！ 答えたくないなら別にいいけど」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「無難なところね。それなりの効果もあると思うわよ」
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("好奇心", -20)
		CALLF CM_PROF("モラル", 20)
		CALLF CM_PROF("衝動", -30)
		CALLF CM_PROF("知性", 20)
		CALLF CM_PROF("攻撃", 10)
	CASE 2
		PRINTFORML 「香を焚くねぇ。私はちょっと疑問だけど」
		CALLF CM_PROF("好奇心", 40)
		CALLF CM_PROF("楽観", 30)
		CALLF CM_PROF("Ａ好き", 20)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("素直さ", -40)
		CALLF CM_PROF("衝動", 20)
		CALLF CM_PROF("神経質", 40)
	CASE 3
		PRINTFORML 「いい判断ね。その効果は私が保証するわ」
		CALLF CM_PROF("知性", 30)
		CALLF CM_PROF("潔癖", 20)
		CALLF CM_PROF("神経質", 20)
		CALLF CM_PROF("羞恥", 20)
		CALLF CM_PROF("好奇心", 30)
		CALLF CM_PROF("被虐", 10)
	CASE 4
		PRINTFORML 「……何の冗談？」
		PRINTFORML 「厄神を怒らせると酷い事になるけど、あなたそれでもいいのかしら？」
		CALLF CM_PROF("同性愛", 10)
		CALLF CM_PROF("倒錯", 30)
		CALLF CM_PROF("モラル", -40)
		CALLF CM_PROF("羞恥", -30)
		CALLF CM_PROF("知性", -40)
		CALLF CM_PROF("衝動", 50)
		CALLF CM_PROF("素直さ", 40)
		CALLF CM_PROF("攻撃", 20)
ENDSELECT
PRINTFORML 「厄祓いが必要になったら、私のところにきなさい」
PRINTFORMW 「では気をつけていってらっしゃい――」
RETURN 1
