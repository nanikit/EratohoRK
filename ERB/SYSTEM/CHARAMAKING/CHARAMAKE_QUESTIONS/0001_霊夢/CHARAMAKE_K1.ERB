;霊夢＠ぱ。
@CHARAMAKE_K1(ARG)
SELECTCASE ARG
	CASE 0
		PRINTFORMW 「おはよう。良かったわね。最初の部屋は、私よ」
	CASE 9
		PRINTFORMW 「疲れたかしら？　最後は私よ」
	CASEELSE
		PRINTFORMW 「おはよう。次は私ね」
ENDSELECT
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「質問させてもらうわね。これからあなたは幻想郷に来るそうだけど」
PRINTFORMW 「あなたにとっての幻想って、どういうもの？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 春の朝のように温かいもの
PRINTFORMDL [2] 夏の昼のように熱いもの
PRINTFORMDL [3] 秋の夕のように儚いもの
PRINTFORMDL [4] 冬の夜のように冷たいもの
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「答えられない。それも仕方ないかもね」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「優しい人なのかしら？　わかったわ」
		CALLF CM_PROF("攻撃", -20)
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("プライド", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("知性", -10)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("楽観", 10)
	CASE 2
		PRINTFORML 「…暑苦しいわね」
		CALLF CM_PROF("攻撃", 20)
		CALLF CM_PROF("素直さ", -10)
		CALLF CM_PROF("プライド", 10)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("知性", -10)
		CALLF CM_PROF("衝動", 10)
	CASE 3
		PRINTFORML 「…幻想っぽいかもね。どうだか知らないけど」
		CALLF CM_PROF("楽観", -20)
		CALLF CM_PROF("被虐", 20)
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("知性", 10)
		CALLF CM_PROF("潔癖", 20)
		CALLF CM_PROF("神経質", 10)
		CALLF CM_PROF("素直さ", 10)
	CASE 4
		PRINTFORML 「…ずいぶん悲しいこと言うのね？」
		CALLF CM_PROF("楽観", -30)
		CALLF CM_PROF("被虐", 20)
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("素直さ", -20)
		CALLF CM_PROF("モラル", -10)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("潔癖", 10)
ENDSELECT
PRINTFORMW 「それじゃ、おやすみなさい――」
RETURN 1
