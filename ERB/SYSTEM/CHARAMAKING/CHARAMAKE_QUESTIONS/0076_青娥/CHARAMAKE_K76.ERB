PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 諦める
PRINTFORMDL [2] 代わりを探す
PRINTFORMDL [3] 奪う
PRINTFORMDL [4] 交渉する
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「黙っていても、欲が無くなったりはしないわよ」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「あらあら…ふふふ、可愛い人なんですね」
		CALLF CM_PROF("攻撃", -30)
		CALLF CM_PROF("プライド", -20)
		CALLF CM_PROF("素直さ", 10)
		CALLF CM_PROF("モラル", 30)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("倒錯", -10)
	CASE 2
		PRINTFORML 「そうして誤魔化し続けて……ふふ、人間って面白いのね」
		CALLF CM_PROF("攻撃", -10)
		CALLF CM_PROF("素直さ", -20)
		CALLF CM_PROF("好奇心", 10)
		CALLF CM_PROF("楽観", 30)
		CALLF CM_PROF("知性", 20)
		CALLF CM_PROF("モラル", 10)
		CALLF CM_PROF("Ａ好き", 20)
		CALLF CM_PROF("Ｂ好き", 10)
	CASE 3
		PRINTFORML 「そう。貴方も、私の側なのね……一緒にタオを学ぶ？」
		CALLF CM_PROF("攻撃", 30)
		CALLF CM_PROF("被虐", -30)
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("衝動", 20)
		CALLF CM_PROF("好奇心", 10)
		CALLF CM_PROF("モラル", -30)
		CALLF CM_PROF("倒錯", 10)
		CALLF CM_PROF("Ｃ好き", 20)
		CALLF CM_PROF("Ｖ好き", 10)
	CASE 4
		PRINTFORML 「お金？　それとも人情ってやつ？　そんなもので解決するかしら…」
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("被虐", 10)
		CALLF CM_PROF("楽観", 20)
		CALLF CM_PROF("プライド", -20)
