;령오로공 @Ies
@CHARAMAKE_K63(ARG)
SELECTCASE ARG
	CASE 0 TO 1
		PRINTFORMW 「에!? 벌써 왔어? 아직 질문 생각하지 않아…」
	CASE 7 TO 9
		PRINTFORML 「조금 오는 것 늦은 것이 아니야? 기다려 녹초가 되었어」
		PRINTFORMW 「뭐 좋아요! 질문이야! 엣또…」
	CASEELSE
		PRINTFORMW 「자, 다음은 나의 질문에 답해 받아요! 각오 하세요!」
ENDSELECT
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:로마자)
PRINTL 

SELECTCASE ARG
	CASE 7 TO 9
		PRINTFORML 「조금 기다려! 질문 잊었다!」
		PRINTFORMW 「응, 어쩔 수 없기 때문에 당신의 질문에 답해 주어요!」
		PRINTL 
		PRINTFORMDL [0] 특히 없다
		PRINTFORMDL [1] 오늘이나 사랑스럽다
		PRINTFORMDL [2] 퓨전은 기분이 좋은거야?
		PRINTFORMDL [3] 쓰리 사이즈 가르쳐 줘!
		PRINTFORMDL [4] 오늘의 팬티 무슨 색?
		CALL INPUTINTD(0, 1, 2, 3, 4)
		SELECTCASE RESULT
			CASE 0
				PRINTFORML 「응, 무엇을 (들)물으려고 한 것이던가?」
				CALLF CM_PROF("충동", -10)
				CALLF CM_PROF("호기심", -10)
				CALLF CM_PROF("수치", 10)
				CALLF CM_PROF("신경질", 10)
			CASE 1
				PRINTFORML 「에, 엣또, 질문이 아니지만 고마워요!」
				CALLF CM_PROF("솔직함",   -20)
				CALLF CM_PROF("호기심",   +10)
				CALLF CM_PROF("수치",     +20)
				CALLF CM_PROF("도착",     -20)
				CALLF CM_PROF("결벽",     +20)
				CALLF CM_PROF("모랄",   +30)
				CALLF CM_PROF("신경질",   +10)
			CASE 2
				PRINTFORML 「굉장히 기분이 좋아! 그렇지만 시간이 없기 때문에 또 이번이군요!」
				CALLF CM_PROF("공격",     +10)
				CALLF CM_PROF("피학",     +10)
				CALLF CM_PROF("충동",     +20)
				CALLF CM_PROF("호기심",   +20)
				CALLF CM_PROF("낙관",     -20)
				CALLF CM_PROF("Ｖ를 좋아하는 사람",   +30)
			CASE 3
				PRINTFORML 「스리마일?   따위 (들)물은 적 있어!」
				CALLF CM_PROF("공격",     +10)
				CALLF CM_PROF("프라이드", -20)
				CALLF CM_PROF("충동",     +20)
				CALLF CM_PROF("호기심",   +20)
				CALLF CM_PROF("수치",     -20)
				CALLF CM_PROF("도착",     +10)
				CALLF CM_PROF("결벽",     -20)
				CALLF CM_PROF("모랄",   -20)
				CALLF CM_PROF("Ｂ를 좋아하는 사람",   +30)
			CASE 4
				PRINTFORML 「오늘은 확실히… 라고 대답할 이유 없지요!」
				CALLF CM_PROF("공격",     +20)
				CALLF CM_PROF("프라이드", -20)
				CALLF CM_PROF("충동",     +30)
				CALLF CM_PROF("호기심",   +20)
				CALLF CM_PROF("수치",     -20)
				CALLF CM_PROF("도착",     +10)
				CALLF CM_PROF("결벽",     -20)
				CALLF CM_PROF("모랄",   -30)
				CALLF CM_PROF("Ｃ를 좋아하는 사람",   +30)
				CALLF CM_PROF("Ａ를 좋아하는 사람",   +20)
		ENDSELECT
	CASEELSE
		SELECTCASE ARG
			CASE 0 TO 1
				PRINTFORMW 「엣또, 태, 태양은 어떻게 생각해?」
			CASEELSE
				PRINTFORML 「오늘도 눈부시게 빛나는 태양!」
				PRINTFORMW 「당신에 있어 태양은 도대체 뭐일까?」
		ENDSELECT
		PRINTL 
		PRINTFORMDL [0] 대답하지 않는다
		PRINTFORMDL [1] 하늘의 은혜
		PRINTFORMDL [2] 수소와 헬륨의 덩어리
		PRINTFORMDL [3] 우주 에너지
		PRINTFORMDL [4] 그것은 너의 웃는 얼굴함!
		CALL INPUTINTD(0, 1, 2, 3, 4)
		SELECTCASE RESULT
			CASE 0
				PRINTFORML 「에? 모르는거야? 혹시 바보?」
				CALLF CM_PROF("충동", -10)
				CALLF CM_PROF("호기심", -10)
				CALLF CM_PROF("수치", 10)
				CALLF CM_PROF("신경질", 10)
			CASE 1
				PRINTFORML 「태양은 은혜의 신님의 힘이야! 그러니까 너무 우쭐해지고 있으면 천벌이 내려요.」
				CALLF CM_PROF("모랄",   +20)
				CALLF CM_PROF("결벽",     +20)
				CALLF CM_PROF("솔직함",   +20)
				CALLF CM_PROF("호기심",   -10)
				CALLF CM_PROF("피학",     -20)

			CASE 2
				PRINTFORML 「수소들이 융합해 대폭발해. 잘 알고 있잖아!」
				CALLF CM_PROF("호기심",   +20)
				CALLF CM_PROF("지성",     +20)
				CALLF CM_PROF("프라이드", +10)
				CALLF CM_PROF("신경질",   +10)
				CALLF CM_PROF("충동",     +10)
			CASE 3
				PRINTFORML 「우후후, 그래요! 태양은 궁극의 에너지야!」
				CALLF CM_PROF("충동",     +30)
				CALLF CM_PROF("신경질",   -20)
				CALLF CM_PROF("도착",     +20)
				CALLF CM_PROF("호기심",   +10)
			CASE 4
				PRINTFORML 「후후후, 기쁜 일 말해 주지 않아!」
				CALLF CM_PROF("충동",     +10)
				CALLF CM_PROF("수치",     -20)
				CALLF CM_PROF("솔직함",   +10)
				CALLF CM_PROF("호기심",   +30)
				CALLF CM_PROF("Ｂ를 좋아하는 사람",   +10)
				
		ENDSELECT
ENDSELECT

SELECTCASE ARG
	CASE 9
		PRINTFORMW 「아, 이제(벌써) 다음 가도 좋아! 엣? 이것으로 끝나?」
	CASEELSE
		PRINTFORMW 「아, 이제(벌써) 다음 가도 좋아.」
ENDSELECT
RETURN 1
