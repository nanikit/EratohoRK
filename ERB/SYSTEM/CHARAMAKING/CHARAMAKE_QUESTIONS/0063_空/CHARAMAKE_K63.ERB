;霊烏路空 @Ies
@CHARAMAKE_K63(ARG)
SELECTCASE ARG
	CASE 0 TO 1
		PRINTFORMW 「え！？もう来たの？まだ質問考えてないよ･･･」
	CASE 7 TO 9
		PRINTFORML 「ちょっと来るの遅いんじゃない？待ちくたびれたよ」
		PRINTFORMW 「まぁいいわ！質問よ！えーと･･･」
	CASEELSE
		PRINTFORMW 「さぁ、次は私の質問に答えてもらうわ！覚悟しなさい！」
ENDSELECT
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 

SELECTCASE ARG
	CASE 7 TO 9
		PRINTFORML 「ちょっとまって！質問忘れた！」
		PRINTFORMW 「うーん、しょうがないからあなたの質問に答えてあげるわ！」
		PRINTL 
		PRINTFORMDL [0] 特にない
		PRINTFORMDL [1] 今日も可愛いね
		PRINTFORMDL [2] フュージョンって気持ちいいの？
		PRINTFORMDL [3] スリーサイズ教えて！
		PRINTFORMDL [4] 今日のぱんつ何色？
		CALL INPUTINTD(0, 1, 2, 3, 4)
		SELECTCASE RESULT
			CASE 0
				PRINTFORML 「うーん、何を聞こうとしたんだっけ？」
				CALLF CM_PROF("衝動", -10)
				CALLF CM_PROF("好奇心", -10)
				CALLF CM_PROF("羞恥", 10)
				CALLF CM_PROF("神経質", 10)
			CASE 1
				PRINTFORML 「え、えーっと、質問じゃないけどありがと！」
				CALLF CM_PROF("素直さ",   -20)
				CALLF CM_PROF("好奇心",   +10)
				CALLF CM_PROF("羞恥",     +20)
				CALLF CM_PROF("倒錯",     -20)
				CALLF CM_PROF("潔癖",     +20)
				CALLF CM_PROF("モラル",   +30)
				CALLF CM_PROF("神経質",   +10)
			CASE 2
				PRINTFORML 「すごく気持ちいいよ！でも時間がないからまた今度ね！」
				CALLF CM_PROF("攻撃",     +10)
				CALLF CM_PROF("被虐",     +10)
				CALLF CM_PROF("衝動",     +20)
				CALLF CM_PROF("好奇心",   +20)
				CALLF CM_PROF("楽観",     -20)
				CALLF CM_PROF("Ｖ好き",   +30)
			CASE 3
				PRINTFORML 「スリーマイル？　なんか聞いたことあるよ！」
				CALLF CM_PROF("攻撃",     +10)
				CALLF CM_PROF("プライド", -20)
				CALLF CM_PROF("衝動",     +20)
				CALLF CM_PROF("好奇心",   +20)
				CALLF CM_PROF("羞恥",     -20)
				CALLF CM_PROF("倒錯",     +10)
				CALLF CM_PROF("潔癖",     -20)
				CALLF CM_PROF("モラル",   -20)
				CALLF CM_PROF("Ｂ好き",   +30)
			CASE 4
				PRINTFORML 「今日は確か･･･って答えるワケないでしょ！」
				CALLF CM_PROF("攻撃",     +20)
				CALLF CM_PROF("プライド", -20)
				CALLF CM_PROF("衝動",     +30)
				CALLF CM_PROF("好奇心",   +20)
				CALLF CM_PROF("羞恥",     -20)
				CALLF CM_PROF("倒錯",     +10)
				CALLF CM_PROF("潔癖",     -20)
				CALLF CM_PROF("モラル",   -30)
				CALLF CM_PROF("Ｃ好き",   +30)
				CALLF CM_PROF("Ａ好き",   +20)
		ENDSELECT
	CASEELSE
		SELECTCASE ARG
			CASE 0 TO 1
				PRINTFORMW 「えーと、た、太陽ってどう思う？」
			CASEELSE
				PRINTFORML 「今日も眩しく輝く太陽！」
				PRINTFORMW 「あなたにとって太陽って一体なにかしら？」
		ENDSELECT
		PRINTL 
		PRINTFORMDL [0] 答えない
		PRINTFORMDL [1] 天の恵み
		PRINTFORMDL [2] 水素とヘリウムの塊
		PRINTFORMDL [3] 宇宙エナジー
		PRINTFORMDL [4] それは君の笑顔さ！
		CALL INPUTINTD(0, 1, 2, 3, 4)
		SELECTCASE RESULT
			CASE 0
				PRINTFORML 「え？わからないの？もしかして馬鹿？」
				CALLF CM_PROF("衝動", -10)
				CALLF CM_PROF("好奇心", -10)
				CALLF CM_PROF("羞恥", 10)
				CALLF CM_PROF("神経質", 10)
			CASE 1
				PRINTFORML 「太陽は恵みの神様の力よ！だからあまり調子にのってると天罰が下るわ。」
				CALLF CM_PROF("モラル",   +20)
				CALLF CM_PROF("潔癖",     +20)
				CALLF CM_PROF("素直さ",   +20)
				CALLF CM_PROF("好奇心",   -10)
				CALLF CM_PROF("被虐",     -20)

			CASE 2
				PRINTFORML 「水素たちが融合して大爆発するの。よく知ってるじゃん！」
				CALLF CM_PROF("好奇心",   +20)
				CALLF CM_PROF("知性",     +20)
				CALLF CM_PROF("プライド", +10)
				CALLF CM_PROF("神経質",   +10)
				CALLF CM_PROF("衝動",     +10)
			CASE 3
				PRINTFORML 「うふふ、そうよ！太陽は究極のエネルギーなの！」
				CALLF CM_PROF("衝動",     +30)
				CALLF CM_PROF("神経質",   -20)
				CALLF CM_PROF("倒錯",     +20)
				CALLF CM_PROF("好奇心",   +10)
			CASE 4
				PRINTFORML 「ふふふ、うれしい事言ってくれるじゃない！」
				CALLF CM_PROF("衝動",     +10)
				CALLF CM_PROF("羞恥",     -20)
				CALLF CM_PROF("素直さ",   +10)
				CALLF CM_PROF("好奇心",   +30)
				CALLF CM_PROF("Ｂ好き",   +10)
				
		ENDSELECT
ENDSELECT

SELECTCASE ARG
	CASE 9
		PRINTFORMW 「あ、もう次行っていいよ！えっ？これで終わり？」
	CASEELSE
		PRINTFORMW 「あ、もう次行っていいよ。」
ENDSELECT
RETURN 1
