;幽々子@咲の人
@CHARAMAKE_K20(ARG)
PRINTFORMW 「ふふ、起きたみたいね」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「気構えなくていいわ、質問を一つするだけだから」
PRINTFORMW 「あなたはどんな桜が好きかしら？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 八分～満開の綺麗な桜
PRINTFORMDL [2] 咲き始めの桜
PRINTFORMDL [3] 死体が埋まってるような、ミステリー桜
PRINTFORMDL [4] 花より団子。桜よりさくらんぼが好き
CALL INPUTINTD(0, 1, 2, 3, 4)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「答えてくれないのね…それとも、桜が嫌い？」
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「やっぱり桜は満開に咲いてるのが一番よね」
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("同性愛", 10)
		CALLF CM_PROF("楽観", 10)
		CALLF CM_PROF("攻撃", -20)
	CASE 2
		PRINTFORML 「咲き始めが好きだなんて変わってるわね」
		PRINTFORML 「でも桜は桜、好きなことには変わりないわ」
		CALLF CM_PROF("被虐", 10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("潔癖", 10)
		CALLF CM_PROF("素直さ", -20)
	CASE 3
		PRINTFORML 「不謹慎なことだけど、そこもまた桜の魅力なのかもしれないわね…」
		CALLF CM_PROF("攻撃", 30)
		CALLF CM_PROF("好奇心", 20)
		CALLF CM_PROF("倒錯", 10)
		CALLF CM_PROF("楽観", -10)
		CALLF CM_PROF("モラル", -20)
	CASE 4
		PRINTFORML 「あらあら、食い意地が張ってるのね」
		CALLF CM_PROF("素直さ", 20)
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("知性", -20)
		CALLF CM_PROF("神経質", -10)
		CALLF CM_PROF("Ｃ好き", 20)
ENDSELECT
PRINTFORMW 「それじゃ質問は終わり、また会えたら会いましょう…？」
RETURN 1
