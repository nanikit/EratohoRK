;フランドール２＠ふらさな
@CHARAMAKE_K11_2(ARG)
PRINTFORMW 「あ、起きた？　なかなか起きないから、もう　きゅっ　としちゃおうかと思っちゃった」
PRINTL 
CALL CHARA_CUTIN(CSTR:10, CSTR:11, CSTR:ローマ字)
PRINTL 
PRINTFORML 「驚かせちゃったかしら。別にとって食べたりなんかしないわ　私の質問に答えて欲しいだけ」
PRINTFORMW 「私とお遊びをするとしたらどんな遊びがいいかしら？」
PRINTL 
PRINTFORMDL [0] 答えない
PRINTFORMDL [1] 鬼ごっこ
PRINTFORMDL [2] かくれんぼ
PRINTFORMDL [3] おままごと
PRINTFORMDL [4] 弾幕ごっこ
PRINTFORMDL [5] お医者さんごっこ
PRINTFORMDL [9] イケナイお遊び
CALL INPUTINTD(0, 1, 2, 3, 4, 5, 9)
SELECTCASE RESULT
	CASE 0
		PRINTFORML 「どうして答えないの？　私とはお遊びしたくないのかしら？」
		;無回答の補正値は固定
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("好奇心", -10)
		CALLF CM_PROF("羞恥", 10)
		CALLF CM_PROF("神経質", 10)
	CASE 1
		PRINTFORML 「定番ね。…吸血”鬼”が鬼ごっこというのもおかしな感じだわ」
		CALLF CM_PROF("攻撃", 20)
		CALLF CM_PROF("被虐", 20)
		CALLF CM_PROF("衝動", 10)
		CALLF CM_PROF("楽観", -10)
	CASE 2
		PRINTFORML 「かくれんぼ？　私スペカで姿隠せるけど…　え、それはダメ？ 残念」
		CALLF CM_PROF("攻撃", -10)
		CALLF CM_PROF("衝動", -10)
		CALLF CM_PROF("知性", 10)
		CALLF CM_PROF("神経質", 20)
	CASE 3
		PRINTFORML 「楽しそう。配役は…あなたがパパで私がママ、咲夜が…（中略）残ったペット役はお姉様ね！」
		CALLF CM_PROF("攻撃", -20)
		CALLF CM_PROF("好奇心", 20)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("楽観",  20)
		CALLF CM_PROF("潔癖", 10)
		CALLF CM_PROF("モラル", 20)
	CASE 4
		PRINTFORML 「へえ、あなた弾幕ごっこに自信があるのね。　私得意なのよ？」
		CALLF CM_PROF("攻撃", 20)
		CALLF CM_PROF("プライド", 10)
		CALLF CM_PROF("楽観", -10)
		CALLF CM_PROF("知性", 10)
	CASE 5
		PRINTFORML 「えー？　あなた変な事考えてない？」
		CALLF CM_PROF("被虐", 10)
		CALLF CM_PROF("素直さ", -10)
		CALLF CM_PROF("プライド", -20)
		CALLF CM_PROF("好奇心", 30)
		CALLF CM_PROF("羞恥", -10)
		CALLF CM_PROF("知性", -10)
		CALLF CM_PROF("倒錯", 20)
		CALLF CM_PROF("Ｂ好き", 20)
		CALLF CM_PROF("モラル", -20)
	CASE 9
		PRINTFORML 「えっ？　…正直なのね。でもそういうの嫌いじゃないわ」
		CALLF CM_PROF("素直さ", 30)
		CALLF CM_PROF("プライド", 20)
		CALLF CM_PROF("衝動",  30)
		CALLF CM_PROF("羞恥", -20)
		CALLF CM_PROF("モラル", -30)
		CALLF CM_PROF("Ｃ好き", 30)
		CALLF CM_PROF("Ｖ好き", 20)
ENDSELECT
PRINTFORMW 「それじゃあね　幻想郷に来たら本当に遊びましょ」
RETURN 1
