;ABL 생성

;순종
@CM_SET_ABL_0
;기본 판정치 10
LOCAL = 10
;소질 판정치를 이용
LOCAL += CM_TALENT_MAKE("프라이드역", "피학", "솔직함", "솔직함", "모랄", "수치역", "공격역", "지성역", "신경질역")
;솔직하다면 판정치+50
SIF TALENT:MASTER:순수
	LOCAL += 50
;반항적이라면 판정치-50
SIF TALENT:MASTER:반항적
	LOCAL -= 50
;겁쟁이이다면 판정치+30
SIF TALENT:MASTER:겁쟁이
	LOCAL += 30
;다부지다면 판정치-30
SIF TALENT:MASTER:기장
	LOCAL -= 30
;몸집이 작다면 판정치+30
SIF TALENT:MASTER:작은체형
	LOCAL += 30
;얌전하면 판정치+20
SIF TALENT:MASTER:얌전함
	LOCAL += 20
;건방지다면 판정치-40
SIF TALENT:MASTER:거만함
	LOCAL -= 40
;프라이드높음과 판정치-50
SIF TALENT:MASTER:프라이드높음
	LOCAL -= 50
;프라이드낮음이라면 판정치+25
SIF TALENT:MASTER:프라이드낮음
	LOCAL += 25
;유치하다면 판정치+25
SIF TALENT:MASTER:유치
	LOCAL += 25
;3회 돌린다
FOR LOCAL:1, 0, 3
	;가산 실패하면 종료
	SIF !PERCENT(LOCAL)
		BREAK
	ABL:MASTER:순종++
	LOCAL /= 2
NEXT

