;백합 경험
@CM_SET_EXP_40
;후타나리의 경우
IF TALENT:MASTER:후타나리
	;성교 경험으로부터 정액 경험을 당긴 것인가, 사정 경험으로부터 자위 경험을 당긴 것 가운데, 많은 (분)편
	EXP:MASTER:백합경험 = MAX(EXP:MASTER:성교경험-EXP:MASTER:정액경험, EXP:MASTER:사정경험-EXP:MASTER:자위경험, 0)
;여자 아이로, 남성혐오가 있는 경우
ELSEIF TALENT:MASTER:남성혐오 && !TALENT:MASTER:남자
	;성교 경험과 정액 경험을 백합 경험으로 이동한다
	EXP:MASTER:백합경험 = (EXP:MASTER:성교경험+EXP:MASTER:정액경험)/2
	;처녀라면 함께 0에. 처녀가 아니면 RAND:3으로 보충한다
	EXP:MASTER:성교경험 = TALENT:MASTER:처녀 ? 0 # RAND:3
	EXP:MASTER:정액경험 = EXP:MASTER:성교경험
;여자 아이로, 양도가 있는 경우
ELSEIF TALENT:MASTER:양도 && !TALENT:MASTER:남자
	LOCAL = -20
	;소질 판정치를 이용
	LOCAL += CM_TALENT_MAKE("동성애", "동성애", "B를 좋아하는 사람", "도착", "호기심", "결벽역")
	SIF PERCENT(LOCAL)
		EXP:MASTER:백합경험 = (RAND:LOCAL)/5
ENDIF

