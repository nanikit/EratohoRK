;구상용으로 대상 캐릭터의 번호를 넣는다
DAILY_TARGET = ARG
;10회시행한다
FOR LCOUNT, 0, 10
	LOCAL = RAND:10
	RESULT = 0
	;봉인되어 있지 않은 경우
	SIF STRFIND(CSTR:ARG:봉인일상, @"/1_{LOCAL}/") == -1
		TRYCALLFORM DAILY_LIFE_1_{LOCAL}(ARG, ARG:1, ARG:2, ARG:3)
	SIF RESULT
		RETURN RESULT
NEXT
RETURN 0

