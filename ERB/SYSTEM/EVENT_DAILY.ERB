;반항 제거(반항각인을 가진 조교자의 아라이먼트가 높아지면 발생)
;─
@DAILY_LIFE_4(ARG)
;봉인되고 있으면 돌아오지만… 이것을 봉인해?
SIF STRFIND(CSTR:ARG:봉인일상, @"/4_0/") != -1
	RETURN 0
;구상용으로 대상 캐릭터의 번호를 넣는다
DAILY_TARGET = ARG
LOCAL = MARK:ARG:반항각인 > 10 - MARK:ARG:5 ? 10 - MARK:ARG:5 # MARK:ARG:반항각인 - 1
MARK:ARG:반항각인 = LOCAL
CFLAG:ARG:반발제거 = 0
;214, 반항 제거(회유 된 것 같습니다)
CALL KOJO_EVENT(214, 0, ARG)
PRINTL
PRINTFORML %CALLNAME:ARG%에 회유 되었기 때문에%CALLNAME:MASTER%의 반항심이 희미해졌습니다
PRINTFORML 반항각인이{LOCAL}가 되었습니다
WAIT
RETURN 1

;─
;능력 상승
;─
@DAILY_LIFE_A(ARG)
#LOCALSIZE 10
#LOCALSSIZE 10
#DIM LCOUNT
#DIM LNUM
#DIM LRES
#DIM LTEMP

;현재의 이벤트수
LNUM = 1

;LOCAL등 초기화
VARSET LOCAL
VARSET LOCALS

;보정치 읽기
SPLIT CSTR:일상능력상승, "/", LOCALS
FOR LCOUNT, 0, LNUM
	LOCAL:LCOUNT = TOINT(LOCALS:LCOUNT)
NEXT

;보정치 계산 처리

;일괄 가산(10을 가산)
FOR LCOUNT, 0, LNUM
	LOCAL:LCOUNT += 10
NEXT

;개별 가산
LOCAL:0 += (TALENT:ARG:회복느림 - TALENT:ARG:회복빠름) * 5

;기준치 설정(이것을 넘고 처음으로 실행된다)
LTEMP = LNUM * 20 + (TALENT:ARG:수동 - TALENT:ARG:헌신적) * 5

;보정치 기입
CSTR:일상능력상승 = 
FOR LCOUNT, 0, LNUM
	CSTR:일상능력상승 += @"/{LOCAL:LCOUNT}"
NEXT
CSTR:일상능력상승 = %SUBSTRING(CSTR:일상능력상승, 1)%

;보정치에 의해 처리를 선택
CALL DAILY_LIFE_A_0(ARG, LRES)

;RESULT를 돌려준다
RETURN RESULT

;능력 상승 처리
;※ARG를 바꾸는 것으로 대상자를 변경할 수 있다고 생각한다
@DAILY_LIFE_A_0(ARG, ARG:1)
;조교 대상 독심술에 의해 얻을 수 있는 정보량이 바뀐다

SELECTCASE ARG:1
	;아무것도 하지 않았다……
	CASE -1
		RETURN 0
	;성지식 향상
	CASE 0
		SIF TALENT:MASTER:독심술 == 2
			PRINTFORML %CALLNAME:ARG%는 책을 읽어 성지식의 향상에 노력한 것 같다……
		LOCAL = RAND:MAX(3 + TALENT:ARG:습득빠름 - TALENT:ARG:습득느림, 2)
		EXP:ARG:성지식 += LOCAL
		SIF TALENT:MASTER:독심술 == 2
			PRINTFORML (%CALLNAME:ARG%) 성지식+{LOCAL}
ENDSELECT
IF TALENT:MASTER:독심술 == 1
	PRINTFORML %CALLNAME:ARG%는 보지 않은 동안에 뭔가를 한 것 같다……
ENDIF
SIF ARG:1 >= 0 && TALENT:MASTER:독심술 > 0
	WAIT
SIF ARG:1 >= 0
	RETURN 1
RETURN 0

