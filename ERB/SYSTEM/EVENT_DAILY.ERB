		ENDIF
		;MASTER의 경험 증가와 내용의 표시
		EXP:MASTER:C경험 += 3
		PRINTL C경험 +3
		EXP:MASTER:B경험 += 3
		PRINTL B경험 +3
		EXP:MASTER:절정경험 += 3
		PRINTL 절정경험 +3
		IF PENIS(MASTER)
			EXP:MASTER:사정경험 += 3
			PRINTL 사정경험 +3
		ENDIF
		IF PENIS(ARG) || PENIS(ARG:1) || PENIS(MASTER)
			EXP:MASTER:정액경험 += 3
			PRINTL 정액경험 +3
		ENDIF
		PRINTL
		;ARG의 경험 증가와 내용의 표시
		EXP:ARG:C경험 += 3
		PRINTFORML (%CALLNAME:ARG%) C경험 +3
