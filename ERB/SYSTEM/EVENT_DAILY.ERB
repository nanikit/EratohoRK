		;ARG:1의 경험 증가와 내용의 표시
		EXP:(ARG:1):C경험 += 3
		PRINTFORML (%CALLNAME:(ARG:1)%) C경험 +3
		EXP:(ARG:1):B경험 += 3
		PRINTFORML (%CALLNAME:(ARG:1)%) B경험 +3
		EXP:(ARG:1):절정경험 += 3
		PRINTFORML (%CALLNAME:(ARG:1)%) 절정경험 +3
		IF TALENT:(ARG:1):후타나리
			EXP:(ARG:1):사정경험 += 3
			PRINTFORML (%CALLNAME:(ARG:1)%) 사정경험 +3
		ENDIF
		IF TALENT:ARG:후타나리 || TALENT:(ARG:1):후타나리
			EXP:(ARG:1):정액경험 += 3
			PRINTFORML (%CALLNAME:(ARG:1)%) 정액경험 +3
		ENDIF
