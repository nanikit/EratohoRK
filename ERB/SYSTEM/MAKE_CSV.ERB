
@MAKE_CSV

DELALLCHARA

FOR LOCAL, 0, 82
	ADDCHARA LOCAL
NEXT

$INPUT

INPUT

TARGET = RESULT

CLEARLINE LINECOUNT

PRINTL 

PRINTL ;캐릭터 번호(NO)
PRINTFORML 번호,{NO}

PRINTL 

PRINTL ;이름(NAME), 통칭(CALLNAME)
PRINTFORML 이름,%NAME%
PRINTFORML 통칭,%CALLNAME%

PRINTL 

PRINTL ;기초(BASE)
FOR LOCAL, 0, VARSIZE("BASE")
	IF BASE:LOCAL != 0
		PRINTFORML 기초,%BASENAME:LOCAL%,{BASE:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;소질(TALENT)
FOR LOCAL, 0, VARSIZE("TALENT")
	IF TALENT:LOCAL == 1
		PRINTFORML 소질,%TALENTNAME:LOCAL%,
	ELSEIF TALENT:LOCAL != 0
		PRINTFORML 소질,%TALENTNAME:LOCAL%,{TALENT:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;능력(ABL)
FOR LOCAL, 0, VARSIZE("ABL")
	IF ABLNAME:LOCAL != ""
		PRINTFORML 능력,%ABLNAME:LOCAL%,{ABL:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;경험(EXP)
FOR LOCAL, 0, VARSIZE("EXP")
	IF EXP:LOCAL != 0
		PRINTFORML 경험,%EXPNAME:LOCAL%,{EXP:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;궁합(RELATION)
FOR LOCAL, 0, CHARANUM
	IF NAME:LOCAL != "" && RELATION:LOCAL != 0
		PRINTFORML %"궁합," + TOSTR(LOCAL) + "," + TOSTR(RELATION:LOCAL) + ",", 15, LEFT%;%NAME:LOCAL%
	ENDIF
NEXT

PRINTL 

PRINTL ;장착물(EQUIP)
PRINTFORML %"장착물," + EQUIPNAME:0 + "," + TOSTR(EQUIP:0) + ",", 25, LEFT%;1, 장갑, 2, 소매, 3, 깃옷
PRINTFORML %"장착물," + EQUIPNAME:1 + "," + TOSTR(EQUIP:1) + ",", 25, LEFT%;1, 삭스, 2, 니삭스, 3, 가터 스타킹, 4, 버선
PRINTFORML %"장착물," + EQUIPNAME:2 + "," + TOSTR(EQUIP:2) + ",", 25, LEFT%;1, 드로워즈, 2, 팬티, 3, 트렁크스, 4, 팬티 스타킹, 5, 곤
PRINTFORML %"장착물," + EQUIPNAME:3 + "," + TOSTR(EQUIP:3) + ",", 25, LEFT%;1, 브래지어, 2, 캐미숄, 3, 쬐어, 4, Ｔ셔츠
PRINTFORML %"장착물," + EQUIPNAME:4 + "," + TOSTR(EQUIP:4) + ",", 25, LEFT%;1, 스커트, 2, 퀼로트, 3, 바지, 4, 하카마
PRINTFORML %"장착물," + EQUIPNAME:5 + "," + TOSTR(EQUIP:5) + ",", 25, LEFT%;1, 윗도리
PRINTFORML %"장착물," + EQUIPNAME:6 + "," + TOSTR(EQUIP:6) + ",", 25, LEFT%;1, 기모노, 2, 도사옷, 3, 원피스

PRINTL 

PRINTL ;캐릭터 정보(CSTR)
FOR LOCAL, 10, 15
	IF CSTRNAME:LOCAL != ""
		PRINTFORML CSTR,%CSTRNAME:LOCAL%,%CSTR:LOCAL%
	ENDIF
NEXT

PRINTL 

PRINTL ;호칭목록(CSTR)
FOR LOCAL, 20, 22
	IF CSTRNAME:LOCAL != ""
		PRINTFORML CSTR,%CSTRNAME:LOCAL%,%CSTR:LOCAL%
	ENDIF
NEXT

PRINTL 

OUTPUTLOG

GOTO INPUT

