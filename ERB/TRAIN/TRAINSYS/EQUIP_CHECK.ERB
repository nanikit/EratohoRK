			SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "C감각") * (GET_ABL(MASTER, "C감각") - 20) * SOURCE:MASTER:사랑 / 100 + B / 5
		SOURCE:MASTER:사랑 += SOURCE:MASTER:중독충족 / 5
	ELSEIF TEQUIP:성교봉사중 == 5
		A = 100 +ACT_FROM(TARGET, 7)
		B = 50 +ACT_TO(TARGET, 1)
		;윤활 부족
		IF PALAM:MASTER:윤활 < 100 && TEQUIP:로션 < 3
			TIMES A , 0.50
			TIMES B , 0.55
			C += 500
		ELSEIF PALAM:MASTER:윤활 < 250 && TEQUIP:로션 < 2
			TIMES A , 0.70
			TIMES B , 0.75
			C += 200
		ELSEIF PALAM:MASTER:윤활 < 500 && TEQUIP:로션 < 1
			TIMES A , 0.85
			TIMES B , 0.90
			C += 100
		ENDIF
		SOURCE:MASTER:쾌C += B
		SOURCE:쾌V += A
		SOURCE:MASTER:접촉 += 200 + GET_TRAINLV(TARGET) * 10
		SOURCE:MASTER:성행동 += 40 + GET_TRAINLV(TARGET) * 2
		;혼란 모란의 처리
		SIF TEQUIP:대거울
			SOURCE:MASTER:노출 += 200 + A / 5
		SOURCE:MASTER:사랑 += B / 7 - SOURCE:MASTER:트라우마 - C
		SIF SOURCE:MASTER:사랑 > 0 && ABL:MASTER:C감각 > 2
			SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "C감각") * (GET_ABL(MASTER, "C감각") - 20) * SOURCE:MASTER:사랑 / 100 + B / 5
		SOURCE:MASTER:사랑 += SOURCE:MASTER:중독충족 / 7
	ELSEIF TEQUIP:성교봉사중 == 6
		A = 500 +ACT_FROM(TARGET, 8) * 2
		B = 300 +ACT_TO(TARGET, 8) * 2
		;윤활 부족
		IF PALAM:MASTER:윤활 < 100 && TEQUIP:로션 < 3
			TIMES A , 0.30
			TIMES B , 0.40
			C += 1500
		ELSEIF PALAM:MASTER:윤활 < 250 && TEQUIP:로션 < 2
			TIMES A , 0.55
			TIMES B , 0.65
			C += 900
		ELSEIF PALAM:MASTER:윤활 < 500 && TEQUIP:로션 < 1
			TIMES A , 0.75
			TIMES B , 0.80
			C += 250
		ENDIF
		SOURCE:쾌A += A
		SOURCE:MASTER:쾌C += B
		SOURCE:MASTER:접촉 += 200 + GET_TRAINLV(TARGET) * 10
		SOURCE:MASTER:성행동 += 40 + GET_TRAINLV(TARGET) * 2
		SOURCE:MASTER:사랑 += A / 5 - C / 5 - SOURCE:MASTER:트라우마 / 5
		SIF SOURCE:MASTER:사랑 > 0 && ABL:MASTER:A감각 > 4
			SOURCE:MASTER:중독충족 += (GET_ABL(MASTER, "A감각") + 10) * (GET_ABL(MASTER, "A감각") - 30) * SOURCE:MASTER:사랑 / 100 + A / 5
		SOURCE:MASTER:사랑 += SOURCE:MASTER:중독충족 / 5
	ENDIF
ENDIF
