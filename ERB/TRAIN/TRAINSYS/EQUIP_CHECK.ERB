;─
;바이브 따위 붙이고 있을 뿐 아이템의 체크
;여기에서는 COM 선택전의 가산 처리를 실시한다
;─
@EQUIP_CHECK
;인플레 사양
;ABL와 GET_ABL()와의 대응, 대체로 원가의 10배 정도, 100을 넘지 않는 값
;	ABL	A
;	0	0
;	1	16.67
;	2	28.57
;	3	37.5
;	4	44.44
;	5	50
;	6	54.55
;	7	58.33
;	8	61.54
;	9	64.29
;	10	66.67

;─
;로션(윤활 추가)
;─
IF TEQUIP:로션
	IF PALAM:MASTER:윤활 < 500
		CUP:MASTER:윤활 += 200
	ELSEIF PALAM:MASTER:윤활 < 1200
		CUP:MASTER:윤활 += 100
	ELSEIF PALAM:MASTER:윤활 < 2500
		CUP:MASTER:윤활 += 50
	ELSE
		CUP:MASTER:윤활 += 10
	ENDIF
ENDIF

;─
;미약(욕정 추가, 이성)
;─
IF TEQUIP:미약
	IF PALAM:MASTER:욕정 < 1000
		CUP:MASTER:욕정 += 500
		DOWNBASE:MASTER:이성 += 25
	ELSEIF PALAM:MASTER:욕정 < 2000
		CUP:MASTER:욕정 += 250
		DOWNBASE:MASTER:이성 += 50
	ELSEIF PALAM:MASTER:욕정 < 3000
		CUP:MASTER:욕정 += 150
		DOWNBASE:MASTER:이성 += 100
	ELSE
		CUP:MASTER:욕정 += 50
		TCVAR:MASTER:이번발기 += 200
	ENDIF
ENDIF

;─
;바이브 장착중(쾌V, 중독충족, 트라우마)
;─
IF TEQUIP:바이브 && !IS_NOWACTNAME("바이브")
	A = 100 * (2 + ITEM:9 + TALENT:도구사용) / 2
	SOURCE:MASTER:쾌V += A
	SIF ABL:MASTER:V감각 > 2
		SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "V감각") * (GET_ABL(MASTER, "V감각") - 20) * (ITEM:9 + 1) / 10 + A / 5
	SIF CFLAG:MASTER:트라우마 == 21
		SOURCE:MASTER:트라우마 += 20 + GET_ABL(TARGET, "도구") + ITEM:9 * 100 + (TALENT:도구사용 + 1) * GET_ABL(TARGET, "기교") * GET_ABL(TARGET, "도구") / 20
	TCVAR:MASTER:전립선자극 += 1
	
	;장비시에 깨지기 때문에 만약을 위해이지만.
	;MASTER가 처녀라면, 처녀상실이 일어난다
	TCVAR:MASTER:처녀상실 = TALENT:MASTER:처녀
ENDIF

;─
;애널바이브 장착중(쾌A, 일탈, 중독충족)
;─
IF TEQUIP:애널바이브 && !IS_NOWACTNAME("애널바이브")
	A = 100 * (2 + ITEM:9 + TALENT:도구사용) / 2
	SOURCE:MASTER:쾌A += A
	;경험 부족
	IF EXP:MASTER:A경험 < 1
		SOURCE:MASTER:일탈 += 1500
	ELSEIF EXP:MASTER:A경험 < 6
		SOURCE:MASTER:일탈 += 800 - EXP:MASTER:A경험 * 100
	ELSEIF EXP:MASTER:A경험 < 11
		SOURCE:MASTER:일탈 += 200 - EXP:MASTER:A경험 * 15
	ELSEIF EXP:MASTER:A경험 < 21
		SOURCE:MASTER:일탈 += 40 - EXP:MASTER:A경험 * 2
	ENDIF
	SIF ABL:MASTER:A감각 > 2
		SOURCE:MASTER:중독충족 += (GET_ABL(MASTER, "A감각") + 1) * (GET_ABL(MASTER, "A감각") - 20) * (ITEM:9 + 1) / 4 + A / 5
	SIF TALENT:MASTER:남자
		TCVAR:MASTER:전립선자극 += 1
ENDIF

;─
;애널비즈 장착중(쾌A, 일탈, 중독충족)
;─
IF TEQUIP:애널비즈 && !IS_NOWACTNAME("애널비즈")
	A = 30 * (2 + ITEM:9 + TALENT:도구사용 + TEQUIP:애널비즈) / 3
	SOURCE:MASTER:쾌A += A
	;경험 부족
	IF EXP:MASTER:A경험 < 1
		SOURCE:MASTER:일탈 += 500
	ELSEIF EXP:MASTER:A경험 < 6
		SOURCE:MASTER:일탈 += 300 - EXP:MASTER:A경험 * 20
	ELSEIF EXP:MASTER:A경험 < 11
		SOURCE:MASTER:일탈 += 90 - EXP:MASTER:A경험 * 6
	ELSEIF EXP:MASTER:A경험 < 21
		SOURCE:MASTER:일탈 += 20 - EXP:MASTER:A경험 * 2
	ENDIF
	SIF ABL:MASTER:A감각 > 2
		SOURCE:MASTER:중독충족 += (GET_ABL(MASTER, "A감각") + 10) * (GET_ABL(MASTER, "A감각") - 20) / 4 + A / 5
ENDIF

;─
;관장＋애널 플러그 장착중
;─
IF TEQUIP:관장기＋플러그 && !IS_NOWACTNAME("관장세트")
	A = 20 * (CFLAG:MASTER:조교레벨 + 1) + (GET_ABL(MASTER, "기교") - 30) * GET_ABL(MASTER, "마조끼") * (TALENT:도구사용 + TALENT:MASTER:드M + TEQUIP:관장기＋플러그) * 30 / 100
	SIF A < 0
		A = 0
	B = 200 + TEQUIP:관장기＋플러그 * 100
	SOURCE:MASTER:쾌A += A
	SOURCE:MASTER:아픔 += B
	SOURCE:MASTER:구속 += 50 + TEQUIP:관장기＋플러그 * 100
	C = A / 2 + B / 2 - CFLAG:MASTER:조교레벨 * 20 - GET_ABL(MASTER, "마조끼") * 10
	SIF C < 0
		C = 0
	SOURCE:MASTER:일탈 += C
	SIF ABL:MASTER:마조끼 > 3
		SOURCE:MASTER:중독충족 += (GET_ABL(MASTER, "마조끼") - 30) * GET_ABL(MASTER, "A감각") * 25 * A / 10000

ENDIF



;클리캡·오나홀비전동화이기 때문에 comment out

[SKIPSTART]

;─
;클리캡 장착중(쾌C, 중독충족)
;─
IF TEQUIP:클리캡 && ! IS_NOWACTNAME("C애무도구")
	A = 100 * (2 + ITEM:9 + TALENT:도구사용) / 2
	SOURCE:MASTER:쾌C += A
	SIF ABL:MASTER:C감각 > 1
		SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "C감각") * (GET_ABL(MASTER, "C감각") - 10) * (ITEM:9 + 1) / 5 + A / 5
ENDIF

;─
;오나홀 장착중(쾌C, 중독충족)
;─
IF TEQUIP:오나홀 && ! IS_NOWACTNAME("C애무도구")
	A = 150 * (2 + ITEM:9 + TALENT:도구사용) / 2
	SOURCE:MASTER:쾌C += A
	SIF ABL:MASTER:C감각 > 1
		SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "C감각") * (GET_ABL(MASTER, "C감각") - 10) * (ITEM:9 + 1) / 5 + A / 5
	TCVAR:MASTER:이번발기 += 100
ENDIF


[SKIPEND]


;─
;니플캡 장착중(쾌B, 중독충족)
;─
IF TEQUIP:니플캡 && !IS_NOWACTNAME("니플캡")
	A = 100 * (2 + ITEM:9 + TALENT:도구사용) / 2
	SOURCE:MASTER:쾌B += A
	SIF ABL:MASTER:B감각 > 2
		SOURCE:MASTER:중독충족 += (GET_ABL(MASTER, "B감각") - 10) * (GET_ABL(MASTER, "B감각") - 20) * (ITEM:9 + 1) * 35 / 100 + A / 5
	TCVAR:MASTER:이번발기 += 30
ENDIF

;─
;줄로 긴박중(구속, 중독충족)
;─
IF TEQUIP:줄
	A = 200 + (GET_ABL(MASTER, "C감각") / 10 + GET_TRAINLV(TARGET)) * (GET_ABL(TARGET, "기교") + 10) * (TALENT:결박능숙 * 3 + 2) / 2
	SOURCE:MASTER:구속 += A
	SOURCE:MASTER:중독충족 += GET_ABL(MASTER, "마조끼") * (GET_ABL(MASTER, "구속중독") - 10) * (GET_ABL(MASTER, "구속중독") + 10) * A / 100000
	TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") - 20) * 3
ENDIF

;─
;아이마스크 장착중(구속, 중독충족, 트라우마)
;─
IF TEQUIP:아이마스크 && !IS_NOWACTNAME("아이마스크")
	A = 100 + (GET_ABL(TARGET, "가학") / 10 + GET_TRAINLV(TARGET)) * (GET_ABL(TARGET, "기교") + 10) * (TALENT:결박능숙 + 2) / 2
	B = 0
	SOURCE:MASTER:구속 += A
	SIF ABL:MASTER:구속중독 > 1
		B = GET_ABL(MASTER, "마조끼") * (GET_ABL(MASTER, "구속중독") - 10) * (GET_ABL(MASTER, "구속중독") + 10) * A / 100000
	SOURCE:MASTER:중독충족 += B
	C = A / 2 - B
	SIF C > 0
		SOURCE:MASTER:트라우마 += C
	SIF CFLAG:MASTER:트라우마 == 64
		TIMES SOURCE:MASTER:트라우마 , 1.50
	TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") - 20) * 2
ENDIF

;─
;볼개그 장착중(구속, 중독충족, 트라우마)
;─
IF TEQUIP:볼개그 && !IS_NOWACTNAME("볼개그")
	A = 350 + (GET_ABL(TARGET, "가학") / 10 + GET_TRAINLV(TARGET)) * 10 * (TALENT:결박능숙 + 1)
	B = 0
	SOURCE:MASTER:구속 += A
	SIF ABL:MASTER:구속중독 > 1
		B = GET_ABL(MASTER, "마조끼") * (GET_ABL(MASTER, "구속중독") - 10) * (GET_ABL(MASTER, "구속중독") + 10) * A / 100000
	SOURCE:MASTER:중독충족 += B
	C = A / 2 - B
	SIF C > 0
		SOURCE:MASTER:트라우마 += C
	SIF CFLAG:MASTER:트라우마 == 65
		TIMES SOURCE:MASTER:트라우마 , 1.50
	TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") - 20)
ENDIF

;─
;삼각목마 승마중(쾌V, 사랑역, 상해, 구속, 일탈, 중독충족, 트라우마)
;─
IF TEQUIP:삼각목마
	A = 0
	B = 1000 + GET_ABL(TARGET, "가학") * GET_ABL(TARGET, "기교") / 2 + GET_TRAINLV(TARGET) * (TALENT:새디즘 * 3 + 2) * 20
	C = 50 + (MARK:반항각인 + 1) * (GET_ABL(TARGET, "기교") + 10) 
	D = 1800 + B / 2 - CFLAG:MASTER:조교레벨 * 50 - GET_ABL(MASTER, "마조끼") * 20 - GET_ABL(MASTER, "고통중독") * 60
	E = 0
	F = 0
	G = 0
	SIF ABL:MASTER:마조끼 > 3
		A = (GET_ABL(MASTER, "마조끼") - 30) * GET_ABL(MASTER, "고통중독") * (TALENT:MASTER:드M + 2) / 2
	SOURCE:MASTER:쾌V += A
	SOURCE:MASTER:아픔 += B
	SOURCE:MASTER:구속 += C
	SIF D < 0
		D = 0
	SOURCE:MASTER:일탈 += D
	SIF ABL:MASTER:고통중독 > 3
		E = GET_ABL(MASTER, "마조끼") * (GET_ABL(MASTER, "고통중독") - 30) * (GET_ABL(MASTER, "고통중독") + 10) * B / 80000
	SOURCE:MASTER:중독충족 += E
	F = B / 2 + C / 5 - E
	SOURCE:MASTER:트라우마 += F
	SIF CFLAG:MASTER:트라우마 == 67
		TIMES SOURCE:MASTER:트라우마 , 1.50
	G = E - B - D - F
	SIF G < 0
		SOURCE:MASTER:사랑 += G
	TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") - 20) * 5
	;장비시에 깨지기 때문에 만약을 위해이지만.
	;MASTER가 처녀라면, 처녀상실이 일어난다
	TCVAR:MASTER:처녀상실 = TALENT:MASTER:처녀
ENDIF
;─
;안면승마 안(쾌C, 중독충족)
;─
IF TEQUIP:안면승마
	SOURCE:MASTER:불결 += 100
	SOURCE:쾌C += 150 +ACT_TO(TARGET, 1) * GET_ABL(MASTER, "순종") * ( 3 + TALENT:MASTER:혀사용 + TALENT:MASTER:고양이혀) / 500
ENDIF
;─
;안면승마애널(쾌C, 중독충족)
;─
IF TEQUIP:안면승마애널
	SOURCE:MASTER:불결 += 300
	SOURCE:쾌A += 150 +ACT_TO(TARGET, 3) * GET_ABL(MASTER, "순종") * ( 3 + TALENT:MASTER:혀사용 + TALENT:MASTER:고양이혀) / 500
ENDIF
;─
;야외플레이 안(구속, 노출, 일탈, 트라우마)
;─
IF TEQUIP:야외플레이 && !IS_NOWACTNAME("야외플레이")
	SOURCE:MASTER:구속 += 50 + MARK:반항각인 * 15
	SOURCE:MASTER:노출 += 300
	SIF TEQUIP:자위중 != 2
		SOURCE:MASTER:노출 += TEQUIP:자위중 * 200
	A = 150 + SOURCE:MASTER:구속 - GET_ABL(MASTER, "노출버릇") * 3 - CFLAG:MASTER:조교레벨 * 10
	B = TALENT:MASTER:겁쟁이 * 150 - TALENT:MASTER:무관심 * 50 + TALENT:MASTER:억압 * 150 - TALENT:MASTER:해방 * 50 + TALENT:MASTER:수줍음 * 250 - TALENT:MASTER:수치심옅음 * 150
	IF A > 0
		SOURCE:MASTER:일탈 += A
		B += A
	ENDIF
	SIF B > 0
		SOURCE:MASTER:트라우마 += B
	SIF CFLAG:MASTER:트라우마 == 44
		TIMES SOURCE:MASTER:트라우마 , 1.50
		TCVAR:MASTER:이번발기 += GET_ABL(MASTER, "노출버릇") * 5
ENDIF

;─
;수치플레이 안(노출)
;─
IF TEQUIP:대거울 && !IS_NOWACTNAME("수치플레이")
	SOURCE:MASTER:노출 += 200
	SIF TEQUIP:자위중 != 2
		SOURCE:MASTER:노출 += TEQUIP:자위중 * 150
		TCVAR:MASTER:이번발기 += GET_ABL(MASTER, "노출버릇") * 3
ENDIF

;─
;조교 대상이 자위중(쾌C, 쾌V, 쾌A, 쾌B, 성행동, 노출, 일탈, 중독충족)
;─
;이 턴으로 자위가 명령을 내렸을 경우는 거부될 가능성이 있으므로 처리를 뒷전 합니다
IF (TEQUIP:자위중 == 1 || TEQUIP:자위중 == 3) && !IS_NOWACTNAME("자위")
	A = 20 + GET_ABL(MASTER, "욕망") * (GET_ABL(MASTER, "기교") + 10) * CFLAG:MASTER:조교레벨 / 50 + GET_ABL(MASTER, "기교") * 25 * (TALENT:MASTER:기묘한손가락 + 1) / 10
	B = 0
	C = 0
	D = 0
	E = A
	;바이브 사용
	IF TEQUIP:바이브
		B = 50 + GET_ABL(MASTER, "욕망") * (CFLAG:MASTER:조교레벨 + 1) / 2 + (TALENT:MASTER:도구사용 + 1) * GET_ABL(MASTER, "기교") * (ITEM:9 + 1) * 3
		SOURCE:MASTER:쾌V += B
		TIMES A , 0.80
		;후타나리는 V자극으로 강제 발기
		IF TALENT:MASTER:후타나리
			IF !TCVAR:MASTER:요점강제발기
				TCVAR:MASTER:이번발기 += 300
			ELSE
				TCVAR:MASTER:발기도 += 200
			ENDIF
		ENDIF
	ENDIF
	;애널바이브 사용
	IF TEQUIP:애널바이브 && ABL:MASTER:A감각 > 1
		C = (GET_ABL(MASTER, "욕망") + 10) * (CFLAG:MASTER:조교레벨 + 1) * 6 / 10 + (TALENT:MASTER:도구사용 + 1) * GET_ABL(MASTER, "기교") * (ITEM:9 + 1) * 25 / 10
		SOURCE:MASTER:쾌A += C
		TIMES E , 0.60
		;남자는 A자극으로 강제 발기
		IF TALENT:MASTER:남자
			IF !TCVAR:MASTER:요점강제발기
				TCVAR:MASTER:이번발기 += 300
			ELSE
				TCVAR:MASTER:발기도 += 200
			ENDIF
		ENDIF
	ENDIF
	SOURCE:MASTER:쾌C += A
	SIF TALENT:MASTER:남자 == 0
		SOURCE:MASTER:쾌B += E
	SOURCE:MASTER:성행동 += 50 + GET_ABL(MASTER, "기교") * 2 + CFLAG:MASTER:조교레벨 * 10 + TEQUIP:바이브 * 50 + TEQUIP:애널바이브 * 50
	D = 150 + GET_TRAINLV(TARGET) * 10 + TEQUIP:야외플레이 * 200 + TEQUIP:대거울 * 150 + A / 2 + B / 3 + C / 3 + E / 2
	SOURCE:MASTER:노출 += D
	SOURCE:MASTER:일탈 += D / 2 - GET_ABL(MASTER, "노출버릇") * 2 - CFLAG:MASTER:조교레벨 * 10
	SOURCE:MASTER:중독충족 += (A + E + B + C + D) * (50 + GET_ABL(MASTER, "자위중독")) / 500
	TCVAR:MASTER:이번발기 += 100
ENDIF

;─
;조교자가 자위중(일탈, 욕정 추가, 쾌c, 쾌v, 쾌b)
;─
IF TEQUIP:자위중 > 1 && !IS_NOWACTNAME("자위과시하고")
	A = 50 + GET_ABL(TARGET, "욕망") * GET_ABL(TARGET, "기교") * GET_TRAINLV(TARGET) / 50 + GET_ABL(MASTER, "기교") * 25 * (TALENT:MASTER:기묘한손가락 + 1) / 10
	B = A
	C = 0
	;바이브 사용
	IF ITEM:로터 && TALENT:처녀 == 0 && TALENT:남자 == 0
		C = 50 + GET_ABL(TARGET, "욕망") * (GET_TRAINLV(TARGET) + 1) / 2 + (TALENT:도구사용 + 1) * GET_ABL(TARGET, "기교") * (ITEM:9 + 1) * 3
		TIMES A , 0.80
	ENDIF
	D = A + B + C
	SOURCE:쾌C += A
	SOURCE:쾌V += C
	SIF TALENT:남자 == 0
		SOURCE:쾌B += B
	;조교레벨과 욕망을 본다
	IF CFLAG:MASTER:조교레벨 < 1 || ABL:MASTER:욕망 < 1
		TIMES D , 0.10
	ELSEIF CFLAG:MASTER:조교레벨 < 3 || ABL:MASTER:욕망 < 2
		TIMES D , 0.25
	ELSEIF CFLAG:MASTER:조교레벨 < 5 || ABL:MASTER:욕망 < 3
		TIMES D , 0.40
	ELSEIF CFLAG:MASTER:조교레벨 < 8 || ABL:MASTER:욕망 < 4
		TIMES D , 0.60
