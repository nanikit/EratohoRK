도구난이도 = POWER(2, ABL:도구) * (100 - TALENT:습득빠름 * 20 + TALENT:습득느림 * 20)
;도구 EXP 취득
도구EXP = EXP:도구사용경험
;조교 레벨이 부족하면 EXP 보정을 줄인다(1 Lv에 대해 1/2)
LOCAL = (CFLAG:조교레벨 >= 20) ? 0 # (ABL:도구 * 2 - 1 - CFLAG:조교레벨)
도구보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;도구 달성 판정
SIF (CUP:MASTER:쾌C + CUP:MASTER:쾌V + CUP:MASTER:쾌A + CUP:MASTER:쾌B - CUP:MASTER:불쾌 + 도구EXP * 20 / 도구보정) > (도구난이도 * ABLUP_DIFFICULTY(22))
	CALL ABLUP_RISE(TARGET, 22)


;-------------------------------------------------
;성교 ABL
;-------------------------------------------------
@ABLUP_23T
#DIM 성교난이도
#DIM 성교EXP
#DIM 성교보정

;성교 난이도 설정
성교난이도 = POWER(2, ABL:성교) * (100 - TALENT:습득빠름 * 20 + TALENT:습득느림 * 20)
;성교 EXP 취득
성교EXP = EXP:성교경험
;조교 레벨이 부족하면 EXP 보정을 줄인다(1 Lv에 대해 1/2)
LOCAL = (CFLAG:조교레벨 >= 20) ? 0 # (ABL:성교 * 2 - 1 - CFLAG:조교레벨)
성교보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;성교 달성 판정
LOCAL = CUP:MASTER:쾌C + CUP:MASTER:쾌V + CUP:MASTER:쾌A + CUP:MASTER:욕정 + CUP:MASTER:공순 - CUP:MASTER:공포 - CUP:MASTER:억울
SIF (LOCAL / 3 + 성교EXP * 20 / 성교보정) > (성교난이도 * ABLUP_DIFFICULTY(23))
	CALL ABLUP_RISE(TARGET, 23)


;-------------------------------------------------
;수치 ABL
;『부끄러워하게 하는 능력』
;-------------------------------------------------
@ABLUP_24T
#DIM 수치난이도
#DIM 수치EXP
#DIM 수치보정

;수치 난이도 설정
수치난이도 = POWER(2, ABL:수치) * (100 - TALENT:습득빠름 * 20 + TALENT:습득느림 * 20)
;수치 EXP 취득
수치EXP = EXP:MASTER:노출경험 + EXP:MASTER:치욕쾌락경험 * 2
;조교 레벨이 부족하면 EXP 보정을 줄인다(1 Lv에 대해 1/2)
LOCAL = (CFLAG:조교레벨 >= 20) ? 0 # (ABL:수치 * 2 - 1 - CFLAG:조교레벨)
수치보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;수치 달성 판정
SIF (CUP:MASTER:욕정 + CUP:MASTER:치정 - CUP:MASTER:억울 + 수치EXP * 20 / 수치보정) > (수치난이도 * ABLUP_DIFFICULTY(24))
	CALL ABLUP_RISE(TARGET, 24)


;-------------------------------------------------
;봉사 ABL
;『봉사시키는 능력』
;-------------------------------------------------
@ABLUP_25T
#DIM 봉사난이도
#DIM 봉사EXP
#DIM 봉사보정

;봉사 난이도 설정
봉사난이도 = POWER(2, ABL:봉사) * (100 - TALENT:습득빠름 * 20 + TALENT:습득느림 * 20)
;봉사 EXP 취득
봉사EXP = EXP:MASTER:구음경험 + EXP:MASTER:수음경험 + EXP:MASTER:봉사쾌락경험 * 2 + EXP:MASTER:가게한경험 * 3 + EXP:MASTER:애정경험 * 4
;조교 레벨이 부족하면 EXP 보정을 줄인다(1 Lv에 대해 1/2)
LOCAL = (CFLAG:조교레벨 >= 20) ? 0 # (ABL:봉사 * 2 - 1 - CFLAG:조교레벨)
봉사보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;봉사 달성 판정
SIF (CUP:MASTER:굴복 + CUP:MASTER:공순 - CUP:MASTER:반항 + 봉사EXP * 10 / 봉사보정) > (봉사난이도 * ABLUP_DIFFICULTY(25))
	CALL ABLUP_RISE(TARGET, 25)


