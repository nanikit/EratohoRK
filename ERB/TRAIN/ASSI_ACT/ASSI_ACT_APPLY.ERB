@ASSI_ACT_APPLY, ARG
SELECTCASE NOWASSIACTNAME(ARG)
	;자위
	CASE "손으로 애무"
		SOURCE:MASTER:쾌C += 100 +ACT_FROM(ASSI, 1) * (2 + TALENT:ASSI:기묘한손가락) /2
		TCVAR:MASTER:이번발기 += GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:기묘한손가락 * 50
	;쿤닐링구스·펠라치오
	CASE "구에서 애무"
		SOURCE:MASTER:쾌C += 150 +ACT_FROM(ASSI, 1) * (3 + TALENT:ASSI:혀사용 + TALENT:ASSI:161) / 3
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:혀사용 * 50 + TALENT:ASSI:고양이혀 * 50)
	;가랑이·짝 맞추기 놀이
	CASE "가랑이"
		SOURCE:MASTER:쾌C += 100 +ACT_FROM(ASSI, 1) * (3 + TALENT:ASSI:젖기쉬움 - TALENT:ASSI:젖기어려움) / 3
		TCVAR:MASTER:이번발기 += GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무")
	;파이즈리
	CASE "파이즈리 한다"
		SOURCE:MASTER:쾌C += 100 +ACT_FROM(ASSI, 1) * (4 + TALENT:ASSI:거유 - TALENT:ASSI:빈유) / 4
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:거유 * 50 - TALENT:ASSI:빈유 * 50 + TALENT:ASSI:음유 * 50)
	;풋잡
	CASE "풋잡 한다"
		SOURCE:MASTER:쾌C += 50 + ACT_FROM(TARGET, 1) * (3 + TALENT:ASSI:미각) / 3
		TCVAR:MASTER:이번발기 += GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + GET_ABL(MASTER, "마조끼")
	;로터, 오나홀
	CASE "C도구"
		SOURCE:MASTER:쾌C += 300 + ITEM:9 * 100 + (TALENT:ASSI:도구사용 + 3) *ACT_FROM(ASSI, 11) / 3
		TCVAR:MASTER:이번발기 += GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "도구")
		SIF TFLAG:조수ACT파생 == 1
			TEQUIP:오나홀 = 1 + ITEM:9
	;역강간
	CASE "역강간"
		SOURCE:MASTER:쾌C += 300 + 2 *ACT_FROM(TARGET, 7)
		;--------EDIT 001 START--------
		;삽입중 플래그의 내용을 캐릭터 등록 번호로 변경
		;------------------------------
		TEQUIP:조수성교봉사중 = ARG
		TFLAG:조수에삽입중 = ARG == ASSI:1 ? 1 # 2
		TCVAR:MASTER:발기도 = 1500
		;--------EDIT 001 END  --------
	;애널 애무
	CASE "애널 애무"
		SOURCE:MASTER:쾌A += 60 +ACT_FROM(ASSI, 3) * (2 + TALENT:ASSI:기묘한손가락) / 2
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "A감각") * 10 + GET_ABL(MASTER, "마조끼") * GET_ABL(ASSI, "기교")) / 10
	;애널 빨고
	CASE "애널 빨고"
		SOURCE:MASTER:쾌A += 60 +ACT_FROM(ASSI, 3) * (3 + TALENT:ASSI:혀사용) / 3
		TCVAR:MASTER:이번발기 += GET_ABL(MASTER, "A감각")
	;애널 로터
	CASE "애널 로터"
		SOURCE:MASTER:쾌A += 100 + ITEM:9 * 100 + (TALENT:ASSI:도구사용 + 3) *ACT_FROM(ASSI, 13) / 3
		;남자는 A로 전립선 자극
		SIF TALENT:MASTER:남자
			TCVAR:MASTER:전립선자극 += 1
		TCVAR:MASTER:이번발기 += GET_ABL(MASTER, "A감각")
	;애널 바이브
	CASE "애널 바이브"
		SOURCE:MASTER:쾌A += 100 + ITEM:9 * 200 + (TALENT:ASSI:도구사용 + 3) *ACT_FROM(ASSI, 13) / 3
		;남자는 A로 전립선 자극
		SIF TALENT:MASTER:남자
			TCVAR:MASTER:전립선자극 += 1
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "A감각") * 10 + GET_ABL(MASTER, "마조끼") * GET_ABL(ASSI, "기교")) / 10
		TEQUIP:애널바이브 = 1 + ITEM:9
	;가슴 애무
	CASE "가슴 애무"
		SOURCE:MASTER:쾌B += 75 +ACT_FROM(ASSI, 4) * (2 + TALENT:ASSI:기묘한손가락) / 3
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:기묘한손가락 * 50) / 2
	;유두 빨고
	CASE "유두 빨고"
		SOURCE:MASTER:쾌B += 150 +ACT_FROM(ASSI, 4) * (3 + TALENT:ASSI:혀사용) / 3
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:혀사용 * 50) / 2
	;유두 로터
	CASE "유두 로터"
		SOURCE:MASTER:쾌B += 100 + ITEM:9 * 100 + (TALENT:ASSI:도구사용 + 3) *ACT_FROM(ASSI, 14) / 3
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:도구사용 * 50)/ 2
	;손가락 삽입
	CASE "손가락 삽입"
		SOURCE:MASTER:쾌V +=  100 +ACT_FROM(ASSI, 2) * (2 + TALENT:ASSI:기묘한손가락) / 2
		;후타나리는 V로 전립선 자극
		SIF TALENT:MASTER:후타나리
			TCVAR:MASTER:전립선자극 += 1
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:기묘한손가락 * 50) / 2
	;바이브
	CASE "바이브"
		SOURCE:MASTER:쾌V += 150 + ITEM:9 * 200 +ACT_FROM(ASSI, 2) * (TALENT:ASSI:도구사용 + 3) / 3
		;후타나리는 V로 전립선 자극
		SIF TALENT:MASTER:후타나리
			TCVAR:MASTER:전립선자극 += 1
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "V감각") + TALENT:ASSI:도구사용 * 50 + GET_ABL(ASSI, "기교"))
		TEQUIP:바이브 = 1 + ITEM:9
	;성교
	CASE "성교"
		SOURCE:MASTER:쾌V +=  300 +ACT_FROM(ASSI, 5) * 3 / 2
		;후타나리는 V로 전립선 자극
		SIF TALENT:MASTER:후타나리
			TCVAR:MASTER:전립선자극 += 1
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "성교")) / 2
	;키스
	CASE "키스"
		TCVAR:MASTER:이번발기 += 50 + (GET_ABL(ASSI, "기교") + GET_ABL(ASSI, "애무") + TALENT:ASSI:혀사용 * 50 + TALENT:ASSI:고양이혀 * 50) / 2
	;안면 승마
	CASE "안면 승마"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "욕망") + GET_ABL(MASTER, "마조끼")) * 2
	;다리를 빨게 한다
	CASE "다리를 빨게 한다"
		TCVAR:MASTER:이번발기 += (GET_ABL(ASSI, "가학") + GET_ABL(MASTER, "마조끼")) * 2
	;자위 과시하고
	CASE "자위 과시하고"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "욕망") + GET_ABL(ASSI, "수치")) * 2
	;매도
	CASE "매도"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") + GET_ABL(MASTER, "노출버릇"))
	;사진 촬영
	CASE "사진 촬영"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") + GET_ABL(MASTER, "노출버릇"))
	;조교자와 키스
	CASE "조교자와 키스"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") + GET_ABL(MASTER, "욕망"))
	;치태를 응시한다
	CASE "치태를 응시한다"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") + GET_ABL(MASTER, "노출버릇"))
	;두를 짓밟는다
	CASE "두를 짓밟는다"
		TCVAR:MASTER:이번발기 += (GET_ABL(MASTER, "마조끼") + TALENT:ASSI:새디즘 * 5) * 10
	;-------------------------------------------------
	;SP액션
	;-------------------------------------------------
	CASE "신문의 기사로 한다"
		SOURCE:MASTER:노출 += 25 * (2 + ABL:ASSI:기교 * 2 + ABL:ASSI:이상) * (2 + ITEM:9)
		SOURCE:MASTER:일탈 += 20 * SOURCE:MASTER:노출 / (3 + ABL:MASTER:순종 + ABL:MASTER:욕망 + ABL:MASTER:노출버릇 * 3 + ABL:MASTER:마조끼 * 2 + CFLAG:MASTER:조교레벨 + EXP:MASTER:피사경효험 + EXP:MASTER:이상경험 / 2)
		SOURCE:MASTER:트라우마 += (2 + TALENT:MASTER:수줍음 - TALENT:MASTER:수치심옅음) * (SOURCE:MASTER:일탈 + 1000) / (1000 + MARK:반항각인 * 100 - MARK:MASTER:트라우마 * 50 + EXP:MASTER:이상경험 * 10 + ABL:MASTER:노출버릇 * 250 + EXP:MASTER:피사경효험 * 100)
		SOURCE:MASTER:중독충족 += SOURCE:MASTER:노출 * (CFLAG:MASTER:조교레벨 + ABL:MASTER:노출버릇 + 1) * (ABL:MASTER:노출버릇 * 2 + 5) / (1000 + SOURCE:MASTER:트라우마)
		IF TEQUIP:자위중 == 1 || TEQUIP:자위중 == 3
			SOURCE:MASTER:노출 += 200 + SOURCE:MASTER:노출 / 5
			TIMES SOURCE:MASTER:일탈 , 1.35
			TIMES SOURCE:MASTER:중독충족 , 1.50
			TIMES SOURCE:MASTER:달성 , 1.50
		ENDIF
		;이번 조교로 촬영한 회수를 카운트
		TFLAG:51 += 1
ENDSELECT

