@ASSI_ACTABLE, ARGS, ARG
#FUNCTION
SELECTCASE ARGS
	;---------------------------------
	;C애무
	;---------------------------------
	CASE "C애무"
		SIF TFLAG:P사용중
			RETURNF 0
		SIF TFLAG:P삽입중
			RETURNF 0
		SIF TFLAG:조수에삽입중
			RETURNF 0
		SIF TEQUIP:성교봉사중
			RETURNF 0
		SIF TEQUIP:오나홀
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TEQUIP:삼각목마
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
		;지문·구상의 모순을 회피하기 위해(때문에)
		SIF TCVAR:MASTER:갈것같다
			RETURNF 0
	CASE "구에서 애무"
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TEQUIP:바이브
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
	CASE "가랑이"
		SIF TALENT:ARG:남자
			RETURNF 0
		SIF TEQUIP:바이브
			RETURNF 0
		SIF TALENT:ARG:다리없음
			RETURNF 0
		SIF !TFLAG:위로돌리고
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
		;귀찮은 것으로 연주한다
		SIF TEQUIP:페니스밴드
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 75
			RETURNF 0
	CASE "파이즈리 한다"
		SIF !PENIS(MASTER)
			RETURNF 0
		SIF TALENT:ARG:남자
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 75
			RETURNF 0
	CASE "풋잡 한다"
		SIF TALENT:ARG:다리없음
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 75
			RETURNF 0
	CASE "C도구"
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		SIF TEQUIP:클리캡
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			SIF ITEM:오나홀 == 0
				RETURNF 0
		ENDIF
		IF !PENIS(MASTER)
			SIF ITEM:로터 == 0
				RETURNF 0
		ENDIF
	CASE "역강간"
		SIF TCVAR:MASTER:발기도 < 1000
			RETURNF 0
		SIF ABL:ARG:V감각 < 3
			RETURNF 0
		SIF ABL:ARG:성교 < 3
			RETURNF 0
		SIF TEQUIP:바이브
			RETURNF 0
		;콤비 네이션의 일을 생각하면 하등의 성교가 들어가고 있을 때는 차 두는 편이 무난
		SIF TEQUIP:성교중
			RETURNF 0
		SIF TEQUIP:성교봉사중
			RETURNF 0
		SIF TEQUIP:조수성교중
			RETURNF 0
		SIF TEQUIP:조수성교봉사중
			RETURNF 0
		SIF !TFLAG:위로돌리고
			RETURNF 0
		SIF TALENT:ARG:남자
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
	CASE "손으로 애무"
		RETURNF 1
	;---------------------------------------------------------------
	;A애무
	;---------------------------------------------------------------
	CASE "A애무"
		SIF TFLAG:A사용중
			RETURNF 0
		SIF TEQUIP:애널바이브
			RETURNF 0
		SIF TEQUIP:애널비즈
			RETURNF 0
		SIF TEQUIP:관장기＋플러그
			RETURNF 0
		SIF RANGE(TEQUIP:성교중, 3, 5)
			RETURNF 0
		SIF TEQUIP:성교봉사중 == 6
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		;하반신 윗도리에 방해
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
	CASE "애널 애무"
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 500
			RETURNF 0
		;A경험이 불충분
		SIF EXP:MASTER:A경험 < 11
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "애널 빨고"
		SIF !TFLAG:위로돌리고
			RETURNF 0
		SIF TEQUIP:성교중 || TEQUIP:성교봉사중
			RETURNF 0
		IF !TALENT:ARG:벽통과
			;조교 대상의 하반신 속옷으로 강하게 방해된다
			SIF CLOTHESTYPE(1, 0)
				RETURNF 0
		ENDIF
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "애널 로터"
		SIF !ITEM:로터
			RETURNF 0
		;조교 대상의 하반신 윗도리로 방해된다
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		SIF ABL:MASTER:A감각 < 2
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "애널 바이브"
		SIF !ITEM:애널바이브
			RETURNF 0
		;조교 대상의 하반신 윗도리로 방해된다
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		;조교 대상의 하반신 속옷으로 방해된다
		SIF CLOTHESTYPE(1, 0) == 2
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 600
			RETURNF 0
		;A경험이 불충분
		SIF EXP:MASTER:A경험 < 11
			RETURNF 0
	;---------------------------------------------------------------
	;B애무
	;---------------------------------------------------------------
	CASE "B애무"
		SIF TFLAG:가슴사용중
			RETURNF 0
		SIF TEQUIP:니플캡
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "가슴 애무"
		;조교자에게 벽통과가 없고, 조교 대상이 남자
		IF !TALENT:ARG:벽통과 && TALENT:MASTER:남자
			;조교 대상의 상반신 윗도리로 방해된다
			SIF CLOTHESTYPE(1, 3) == 2
				RETURNF 0
			;조교 대상의 상반신 속옷으로 방해된다
			SIF CLOTHESTYPE(1, 2) == 2
				RETURNF 0
		ENDIF
	CASE "유두 빨고"
		IF !TALENT:ARG:벽통과
			SIF TEQUIP:성교중 == 4 || RANGE(TEQUIP:성교봉사중, 4, 5)
				RETURNF 0
			;조교 대상의 상반신 윗도리로 강하게 방해된다
			SIF CLOTHESTYPE(1, 3)
				RETURNF 0
			;조교 대상의 상반신 속옷으로 강하게 방해된다
			SIF CLOTHESTYPE(1, 2)
				RETURNF 0
		ENDIF
	CASE "유두 로터"
		SIF !ITEM:로터
			RETURNF 0
		IF !TALENT:MASTER:남자
			;조교 대상의 상반신 윗도리로 방해된다
			SIF CLOTHESTYPE(1, 3) == 2
				RETURNF 0
			;조교 대상의 상반신 속옷으로 방해된다
			SIF CLOTHESTYPE(1, 2) == 2
				RETURNF 0
		ENDIF
	;---------------------------------------------------------------
	;∨애무
	;---------------------------------------------------------------
	CASE "V애무"
		SIF TALENT:MASTER:처녀
			RETURNF 0
		SIF TFLAG:V사용중
			RETURNF 0
		SIF TEQUIP:바이브
			RETURNF 0
		SIF RANGE(TEQUIP:성교중, 1, 5)
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF TEQUIP:조수성교중
			RETURNF 0
		SIF TALENT:MASTER:남자
			RETURNF 0
		;하반신 윗도리에 방해
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:벽통과
			RETURNF 0
	CASE "손가락 삽입"
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 400
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		;경험이 불충분
		SIF EXP:MASTER:V경험 < 6
			RETURNF 0
		SIF TALENT:MASTER:남자
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "바이브"
		;조교 대상의 상반신 윗도리로 방해된다
		SIF CLOTHESTYPE(1, 3) == 2
			RETURNF 0
		;조교 대상의 상반신 속옷으로 강하게 방해된다
		SIF CLOTHESTYPE(1, 2)
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF !ITEM:바이브
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 500
			RETURNF 0
		SIF TALENT:MASTER:남자
			RETURNF 0
		;경험이 불충분
		SIF EXP:MASTER:V경험 < 6
			RETURNF 0
	CASE "성교"
		;콤비 네이션의 일을 생각하면 하등의 성교가 들어가고 있을 때는 차 두는 편이 무난
		SIF TEQUIP:성교봉사중
			RETURNF 0
		SIF TEQUIP:조수성교봉사중
			RETURNF 0
		SIF TEQUIP:페니스밴드
			RETURNF 0
		SIF !PENIS(ASSI)
			RETURNF 0
		;윤활이 불충분
		SIF PALAM:MASTER:윤활 < 500
			RETURNF 0
		SIF TALENT:MASTER:남자
			RETURNF 0
		;경험이 불충분
		SIF EXP:MASTER:V경험 < 11 && EXP:MASTER:성교경험 < 9
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
		SIF TEQUIP:자위중
			RETURNF 0
	;---------------------------------------------------------------
	;M애무
	;---------------------------------------------------------------
	CASE "M애무"
		SIF TFLAG:구사용중 >= 2
			RETURNF 0
		SIF TEQUIP:볼개그
			RETURNF 0
	CASE "키스"
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "안면 승마"
		SIF TFLAG:구사용중
			RETURNF 0
		SIF !TFLAG:위로돌리고
			RETURNF 0
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "다리를 빨게 한다"
		SIF TFLAG:구사용중
			RETURNF 0
		SIF TALENT:ARG:다리없음
			RETURNF 0
	;---------------------------------------------------------------
	;수치
	;---------------------------------------------------------------
	CASE "자위 과시하고"
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "매도"
	CASE "사진 촬영"
	SIF !TALENT:ARG:촬영기술
		RETURNF 0
	CASE "조교자와 키스"
		SIF TFLAG:벌플래그
			RETURNF 0
	CASE "치태를 응시한다"
	
	;---------------------------------------------------------------
	;가학
	;---------------------------------------------------------------
	CASE "두를 짓밟는다"
		SIF TALENT:ARG:다리없음
			RETURNF 0
		SIF !TFLAG:위로돌리고
			RETURNF 0
	;---------------------------------------------------------------
	;그 외
	;---------------------------------------------------------------
	CASE "탈의"
	CASE "약품"
	CASE "대기"
ENDSELECT
RETURNF 1
