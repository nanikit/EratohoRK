@ASSI_TRAIN_VARIATION, ARG, ARG:1
TCVAR:ARG:조수ACT파생 = 0
SELECTCASE GET_ASSIACTNAME(ARG:1)
	;애무→자위
	CASE "손으로애무"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;쿤닐링구스→페라
	CASE "입으로애무"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;조개 우려라→가랑이
	CASE "가랑이"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;로터, 오나홀
	CASE "C도구"
		SIF PENIS(MASTER) && ITEM:오나홀 > 0
			TCVAR:ARG:조수ACT파생 = 1
	;연속 교합
	CASE "역강간"
		SIF TFLAG:조수연속교합 == 1
			TCVAR:ARG:조수ACT파생 = 1
	;가슴애무
	CASE "가슴애무"
		SIF TALENT:MASTER:남자
			TCVAR:ARG:조수ACT파생 = 1
	;유두빨고→유두 살짝 깨물기		우선 테두리만 마련한다. 조건은 어떻게 하지@/L
	;CASE "유두빨고"
	;	SIF 0
	;		TCVAR:ARG:조수ACT파생 = 1
ENDSELECT


;조수 행동이 정해지면 여기로부터 행동의 메세지를 호출합니다.
@ASSI_TRAIN_MESSAGE, ARG
SELECTCASE NOWASSIACTNAME(ARG)
	;애무→자위
	CASE "손으로애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%PENIS_DESCRIPTION()%%CALLNAME:MASTER%의 남성기를 애무하기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 몸을 애무하기 시작했다…
		ENDIF
	;쿤닐링구스→페라
	CASE "입으로애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%PENIS_DESCRIPTION()%%CALLNAME:MASTER%의 남성기를 입으로애무 하기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 비순을 입으로애무 하기 시작했다…
		ENDIF
	;조개 우려라→가랑이
	CASE "가랑이"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%PENIS_DESCRIPTION()%%CALLNAME:MASTER%의 남성기를 자신의 사타구니에 맞게 한 켜기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는 두 명의 성기를 거듭해 켜기 시작했다…
		ENDIF
	;파이즈리
	CASE "파이즈리한다"
		PRINTFORML %CALLNAME:ARG%는%PENIS_DESCRIPTION()%%CALLNAME:MASTER%의 남성기를 자신의 가슴으로 사이에 두어 켜기 시작했다…
	;풋잡
	CASE "풋잡한다"
		PRINTFORML %CALLNAME:ARG%는%PENIS_DESCRIPTION()%%CALLNAME:MASTER%의 남성기를 자신의 다리로 켜기 시작했다…
	;로터, 오나홀
	CASE "C도구"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 남성기에 오나홀을 씌우고 상하시켰다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 비순에 떨리는 로터를 강압했다…
		ENDIF
	;역강간
	CASE "역강간"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER% 위에서 허덕이고 있다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 페니스를 억지로, 바기나의 안쪽까지 물어 허리를 흔들었다…
		ENDIF
	;애널애무
	CASE "애널애무"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 애널을 애무하기 시작했다…
	;애널빨고
	CASE "애널빨고"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 아누스를 정중하게 빨기 시작했다…
	;애널로터
	CASE "애널로터"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 애널에 떨리는 로터를 강압했다…
	;애널바이브
	CASE "애널바이브"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%에 애널바이브를 빼고 꽂기 했다…
	;애널섹스
	CASE "애널섹스"
	PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 애널에 삽입해 허리를 흔들었다…
	;가슴애무
	CASE "가슴애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 유두를 손가락으로 만지기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 가슴을 애무하기 시작했다…
		ENDIF
	;유두빨고
	CASE "유두빨고"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 유두를 살짝 깨물기 해 왔다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 유두를 들이마시기 시작했다…
		ENDIF
	;유두로터
	CASE "유두로터"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 유두에 떨리는 로터를 강압했다…
	;손가락삽입
	CASE "손가락삽입"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 비순에 손가락을 삽입해, 안을 휘저었다…
	;바이브
	CASE "바이브"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%에 바이브를 삽입 해내 가려 했다…
	;성교
	CASE "성교"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 비순을 관철해 허리를 흔들었다…
	;키스
	CASE "키스"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 얼굴에 가까워져 입술에 입술을 거듭했다…
	;안면승마
	CASE "안면승마"
		IF TEQUIP:성교봉사중 == 3
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 안면에 걸쳐 음순을 문지르면서%CALLNAME:TARGET%와 혀를 서로 걸었다…
		ELSE
			PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 안면에 걸쳐 음순을 문질렀다…
		ENDIF
	;다리를빨게한다
	CASE "다리를빨게한다"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%에 자신의 다리를 빨라고 명령했다…
	;자위과시하고
	CASE "자위과시하고"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%에 과시하도록(듯이), 눈앞에서 자위를 시작했다…
	;매도
	CASE "매도"
		PRINTFORML %CALLNAME:ARG%는 도발하는것 같이%CALLNAME:MASTER%의 치태를 소리에 냈다…
	;사진촬영
	CASE "사진촬영"
		PRINTFORM %CALLNAME:ARG%는 카메라를 지어,%CALLNAME:MASTER%의
		IF PALAM:MASTER:쾌C + PALAM:MASTER:쾌V + PALAM:MASTER:쾌A + PALAM:MASTER:쾌B + PALAM:MASTER:욕정 > 1500
			PRINT 흐트러진다
		ELSEIF PALAM:MASTER:공포 + PALAM:MASTER:고통 + PALAM:MASTER:불쾌 + PALAM:MASTER:억울 > 1000
			PRINT 비참한
		ELSEIF !(TEQUIP:MASTER:하반신윗도리 || TEQUIP:MASTER:상반신윗도리 || TEQUIP:MASTER:전신윗도리)
			PRINT 상스럽다
		ENDIF
		PRINTL 모습을 확실히 기록했다…
	;조교자와키스
	CASE "조교자와키스"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%에 과시하도록(듯이)%CALLNAME:TARGET%와 혀를 걸기 시작했다…
	;치태를응시한다
	CASE "치태를응시한다"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 치태를 응시하고 있다…
	;머리를짓밟는다
	CASE "머리를짓밟는다"
		PRINTFORML %CALLNAME:ARG%는%CALLNAME:MASTER%의 머리를 짓밟았다…
	;------------------------------
	;SP액션
	;------------------------------
	CASE "신문의기사로한다"
		PRINTFORML %CALLNAME:ARG%는 카메라로%CALLNAME:MASTER%의 모습을 찍어,
		PRINTFORML 그것을 재료에 기사를 쓰기 시작했다...
ENDSELECT
WAIT
RESULT = 0
CALL KOJO_ACT_ASSI
RETURN RESULT
