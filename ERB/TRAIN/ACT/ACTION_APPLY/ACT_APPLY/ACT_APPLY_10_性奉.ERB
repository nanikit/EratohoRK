	;중독 충족 SOURCE 소실
	SOURCE:MASTER:쾌C /= 3
	SOURCE:쾌V /= 5
	SOURCE:MASTER:접촉 /= 3
	SOURCE:MASTER:사랑 -= 500
	SOURCE:MASTER:성행동 /= 3
	SOURCE:MASTER:아픔 /= 3
	SOURCE:MASTER:일탈 /= 3
	SOURCE:MASTER:중독충족 = 0
	SOURCE:MASTER:트라우마 /= 3
	TFLAG:뽑지않고 = 0
	;REACT 분류가 날뛰는 것 경우
	IF IS_COMGRONAME("날뛴다")
		;애널 섹스 이외의 성교 봉사중
		IF V_SEX(MASTER)
			;소실 처리 회피
		;그 외
		ELSE
			;쾌C, 쾌vSOURCE 소실
			SOURCE:MASTER:쾌C = 0
			SOURCE:쾌V = 0
		ENDIF
		;성교 봉사중 해제
		TEQUIP:성교봉사중 = 0
	;REACT 분류가 요구에 응하지 않는 것 경우
	ELSE
		;애널 섹스 이외의 성교 봉사중
		IF V_SEX(MASTER)
			;소실 처리 회피
		;그 외
		ELSE
			;쾌C, 쾌vSOURCE 소실
			SOURCE:MASTER:쾌C = 0
			SOURCE:쾌V = 0
		ENDIF
		;성교 봉사중 해제
		TEQUIP:성교봉사중 = 0
	ENDIF
;그 외의 경우(수용)
ELSE
	;애무 보정, 조교 대상 순종, 음경소질에 응해 보정 가격인상승
	LOCAL = 100 + ACT_TO(TARGET, 5) * GET_ABL(MASTER, "순종") * (2 + TALENT:MASTER:음핵／음경) / 600
	;쾌vSOURCE 증가
	TIMES SOURCE:쾌V, 1.25
