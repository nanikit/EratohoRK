;─
;050, 자위/애무 강제
;─
;리액션전(S/N)(불결)
@ACTION_APPLY_50
;봉사계의 행동은 거부할 수 있으므로, 반응이 나올 때까지 신체의 접촉이 없도록 해 둡니다
;탈의의 처리
;조교 대상이 악세사리 장착중,1/3의 확률이나 조교자 C둔감시 액세서리 강제 탈의
SIF TEQUIP:MASTER:악세사리 && (!RAND:3 || TALENT:C둔감)
	TFLAG:강제탈의 = 6
;조교자 착의플레이선호 소질이 있는 경우
IF TALENT:착의플레이선호
	;속옷만 자주 탈의
	TFLAG:자주탈의 = 9
;해방 소질, 또는 확률(조교자 조교 레벨, 욕망, 조교 대상 매혹으로 증대)로
ELSEIF TALENT:해방 || (GET_TRAINLV(TARGET) + GET_ABL(TARGET, "욕망") / 10 + TALENT:MASTER:매혹 * 5 > 4 + RAND:5)
	;반반의 확률로 액세서리까지, 또는 전라까지 자주 탈의
	TFLAG:자주탈의 = 7 + RAND:2
ENDIF
;자위 파생시
IF TFLAG:ACT파생 == 1
	;불결 SOURCE 증가
	SOURCE:MASTER:불결 = 50
	;조교자의 페니스에 정액/애널 더러움이 존재하면 불결 SOURCE 나카마시가
	SIF STAIN:페니스 & (4 + 8)
		SOURCE:MASTER:불결 += 150
	;조교자의 페니스에 피/점액 더러움이 존재하면 불결 SOURCE 오마스가
	SIF STAIN:페니스 & (32 + 64)
		SOURCE:MASTER:불결 += 400
	;자주 탈의를 실시하지 않는 판정이면, 하반신 전부 자주 탈의로 변경
	SIF TFLAG:자주탈의 == 0
		TFLAG:자주탈의 = 5
;보통 애무시
ELSE
	;불결 SOURCE 증가
	SOURCE:MASTER:불결 = 25
	;조교자의 바기나에 정액/애널 더러움이 존재하면 불결 SOURCE 나카마시가
	SIF STAIN:바기나 & (4 + 8)
		SOURCE:MASTER:불결 += 150
	;조교자의 바기나에 피/점액 더러움이 존재하면 불결 SOURCE 오마스가
	SIF STAIN:바기나 & (32 + 64)
		SOURCE:MASTER:불결 += 400
	;자주 탈의를 실시하지 않는 판정이면, 상＋시모카미착자주 탈의로 변경
	SIF TFLAG:자주탈의 == 0
		TFLAG:자주탈의 = 3
ENDIF
;안면 승마계 해제
CALL FACESITTING_SET(0)
;조교 대상 자위를 중단
TEQUIP:자위중 &= ~1
;조교자 욕망에 응해 발기도 상승
TCVAR:MASTER:이번발기 += 100 + GET_ABL(TARGET, "욕망")
TFLAG:경과시간 += 2

;리액션 후(S/N)(불결)
@ACTION_APPLY2_50
;REACT 분류가 요구에 응하지 않는 것 경우(거절)
IF IS_COMGRONAME("거부/날뛴다/도망친다")
	;리액션이 자위하기 시작하는 것 경우
	IF IS_NOWCOMNAME("자위하기 시작한다")
		;트라우마, 굴종 SOURCE 감소
		TIMES SOURCE:MASTER:트라우마, 0.01
		TIMES SOURCE:MASTER:굴종, 0.10
	;치정선행
	ELSEIF PALAM:MASTER:치정 > PALAM:MASTER:반항
		;트라우마, 굴종 SOURCE 감소
		TIMES SOURCE:MASTER:트라우마, 0.03
		TIMES SOURCE:MASTER:굴종, 0.20
	;반항 선행
	ELSE
		;트라우마, 굴종 SOURCE 감소
		TIMES SOURCE:MASTER:트라우마, 0.05
		TIMES SOURCE:MASTER:굴종, 0.15
	ENDIF
;그 외의 경우(수용)
ELSE
	;쾌x용 보정치(LOCAL:1~4):애무 보정, 조교 대상 순종, 기묘한손가락소질에 응해 상승(쾌v는 계산하지만 사용하지 않는다)
	FOR LOCAL, 1, 5
		LOCAL:LOCAL = 100 + ACT_TO(TARGET, LOCAL) * GET_ABL(MASTER, "순종") * (2 + TALENT:MASTER:기묘한손가락) / 600
	NEXT
	;더러움이 서로 옮긴다
	STAIN:MASTER:손 |= STAIN:(PENIS(TARGET) ? 2 # 3)
	STAIN:(PENIS(TARGET) ? 2 # 3) |= STAIN:MASTER:손
	;조교 대상 조교 레벨, 쾌c용 보정치에 응해 접촉 SOURCE 증가
	SOURCE:MASTER:접촉 += CFLAG:MASTER:조교레벨 * 10 + LOCAL:1 / 10
	;쾌c용 보정치에 응해 사랑, 쾌cSOURCE 증가
	SOURCE:MASTER:사랑 += LOCAL:1 / 5
	SOURCE:쾌C += LOCAL:1 * 2
	;애무시
	IF !TFLAG:ACT파생
		;쾌b용 보정치에 응해 쾌bSOURCE 증가
		SOURCE:쾌B += LOCAL:4 * 2
		;조교 대상 기교가 높은 경우
		SIF ABL:MASTER:기교 > 2
			;쾌a용 보정치에 응해 쾌aSOURCE 증가
			SOURCE:쾌A += LOCAL:3 * 2
	ENDIF
	;불결 SOURCE 증가
	TIMES SOURCE:MASTER:불결 , 1.20
	;적극적·태연하게 따르는 것 경우
	IF IS_COMGRONAME("적극적 따른다/태연하게 따른다")
		;2/3로 앉아 자세가 무릎서기에 변화
		IF TCVAR:MASTER:자세 == 2 && RAND:3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:자세 = 10
		ENDIF
		;위로 돌리고가 아니면,1/2그리고 뒤로부터 애무하도록(듯이)
		IF TCVAR:MASTER:자세 != 1 && RAND:2 && TCVAR:MASTER:위치전후 != 3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:위치전후 = 3
		ENDIF
	ENDIF
ENDIF

;─
;051, 펠라치오/쿤닐링구스 강제
;─
;리액션전(S/N/H)(불결)
@ACTION_APPLY_51
;탈의의 처리
;조교자 착의플레이선호 소질이 있는 경우
IF TALENT:착의플레이선호
	;속옷만 자주 탈의
	TFLAG:자주탈의 = 9
;억압/수줍음 소질이 있는 경우
ELSEIF TALENT:억압 || TALENT:수줍음
	;하반신 윗도리 자주 탈의
	TFLAG:자주탈의 = 2
;그 외의 경우
ELSE
	;하반신 전부 자주 탈의
	TFLAG:자주탈의 = 5
ENDIF
;펠라치오계 강제시
IF TFLAG:ACT파생 == 1 || TFLAG:ACT파생 == 4
	;불결 SOURCE 증가
	SOURCE:MASTER:불결 = 150
	;조교자의 페니스에 정액/애널 더러움이 존재하면 불결 SOURCE 나카마시가
	SIF STAIN:페니스 & (4 + 8)
		SOURCE:MASTER:불결 += 280
	;조교자의 페니스에 피/점액 더러움이 존재하면 불결 SOURCE 오마스가
	SIF STAIN:페니스 & (32 + 64)
		SOURCE:MASTER:불결 += 650
;안면 승마 애널 강제시
ELSEIF TFLAG:ACT파생 == 3
	;불결 SOURCE 증가
	SOURCE:MASTER:불결 = 1000
;쿤닐링구스계 강제시
ELSE
	;불결 SOURCE 증가
	SOURCE:MASTER:불결 = 100
	;조교자의 바기나에 정액/애널 더러움이 존재하면 불결 SOURCE 나카마시가
	SIF STAIN:바기나 & (4 + 8)
		SOURCE:MASTER:불결 += 230
	;조교자의 바기나에 피/점액 더러움이 존재하면 불결 SOURCE 오마스가
	SIF STAIN:바기나 & (32 + 64)
		SOURCE:MASTER:불결 += 620
ENDIF
;안면 승마 강제시
IF TFLAG:ACT파생 == 2
	;안면 승마 상태를 세트
	CALL FACESITTING_SET(1)
;안면 승마 애널 강제시
ELSEIF TFLAG:ACT파생 == 3
	;안면 승마 상태를 세트
	CALL FACESITTING_SET(2)
;그 외의 경우
ELSE
	;안면 승마계 해제
	CALL FACESITTING_SET(0)
ENDIF
;조교자 자위를 중단
TEQUIP:자위중 &= ~2
;조교자 욕망에 응해 발기도 상승
TCVAR:MASTER:이번발기 += 100 + GET_ABL(TARGET, "욕망")
TFLAG:경과시간 += 2

;리액션 후(S/N/H)(불결)
@ACTION_APPLY2_51
;REACT 분류가 요구에 응하지 않는 것 경우(거절)
IF IS_COMGRONAME("거부/날뛴다/도망친다")
	;치정선행
	IF PALAM:MASTER:치정 > PALAM:MASTER:반항
		;트라우마, 굴종 SOURCE 감소
		TIMES SOURCE:MASTER:트라우마, 0.03
		TIMES SOURCE:MASTER:굴종, 0.20
	;반항 선행
	ELSE
		;트라우마, 굴종 SOURCE 감소
		TIMES SOURCE:MASTER:트라우마, 0.05
		TIMES SOURCE:MASTER:굴종, 0.15
	ENDIF
;그 외의 경우(수용)
ELSE
	;커맨드 파생에 의해 분기
	SELECTCASE TFLAG:ACT파생
		;펠라치오 강제
		CASE 1
			;애무 보정, 조교 대상 순종, 혀 사용해, 고양이혀 소질에 의해 보정 가격인상승
			LOCAL = 250 + ACT_TO(TARGET, 1) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀사용 + TALENT:MASTER:고양이혀) / 900
			;더러움이 서로 옮긴다
			STAIN:MASTER:구 |= STAIN:페니스
			STAIN:페니스 |= STAIN:MASTER:구
			;보정치에 응해 쾌cSOURCE 증가
			SOURCE:쾌C = LOCAL * 2
		;안면 승마 애널
		CASE 3
			;애무 보정, 조교 대상 순종, 혀 사용해, 고양이혀 소질에 의해 보정 가격인상승
			LOCAL = 250 + ACT_TO(TARGET, 3) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀사용 + TALENT:MASTER:고양이혀) / 900
			;더러움이 서로 옮긴다
			STAIN:MASTER:구 |= STAIN:애널
			STAIN:애널 |= STAIN:MASTER:구
			;보정치에 응해 쾌aSOURCE 증가
			SOURCE:쾌A = LOCAL * 2
		;상기 이외(쿤닐링구스 강제/안면 승마)
		CASEELSE
			;애무 보정, 조교 대상 순종, 혀 사용해, 고양이혀 소질에 의해 보정 가격인상승
			LOCAL = 250 + ACT_TO(TARGET, 1) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀사용 + TALENT:MASTER:고양이혀) / 900
			;더러움이 서로 옮긴다
			STAIN:MASTER:구 |= STAIN:바기나
			STAIN:바기나 |= STAIN:MASTER:구
			;보정치에 응해 쾌cSOURCE 증가
			SOURCE:쾌C = LOCAL * 2
	ENDSELECT
	;조교 대상 조교 레벨, 보정치에 응해 접촉 SOURCE 증가
	SOURCE:MASTER:접촉 += CFLAG:MASTER:조교레벨 * 10 + LOCAL / 10
	;보정치에 응해 사랑 SOURCE 증가
	SOURCE:MASTER:사랑 += LOCAL / 5
	;불결 SOURCE 증가
	TIMES SOURCE:MASTER:불결 , 1.20
	;적극적/태연하게 따르는 것 경우
	IF IS_COMGRONAME("적극적 따른다/태연하게 따른다") && TFLAG:ACT파생 == 1
		;2/3로 앉아 자세가 무릎서기에 변화
		IF TCVAR:MASTER:자세 == 2 && RAND:3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:자세 = 10
		ENDIF
	ENDIF
ENDIF

