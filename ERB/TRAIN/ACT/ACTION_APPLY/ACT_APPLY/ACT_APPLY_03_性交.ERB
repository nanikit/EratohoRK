	TIMES SOURCE:MASTER:쾌A , 0.55
	SOURCE:MASTER:아픔 += 1000
	SOURCE:MASTER:일탈 += 1500
	SOURCE:MASTER:트라우마 += 500
ELSEIF EXP:MASTER:A경험 < 13 && EXP:MASTER:성교경험 < 21
	TIMES SOURCE:MASTER:쾌A , 0.70
	SOURCE:MASTER:아픔 += 600
	SOURCE:MASTER:일탈 += 500
	SOURCE:MASTER:트라우마 += 100
ELSEIF EXP:MASTER:A경험 < 26 && EXP:MASTER:성교경험 < 31
	TIMES SOURCE:MASTER:쾌A , 0.85
	SOURCE:MASTER:아픔 += 200
	SOURCE:MASTER:일탈 += 150
ENDIF
;조교 레벨에 응해 접촉 SOURCE 증가
SOURCE:MASTER:접촉 = 200 + GET_TRAINLV(TARGET) * 10
;접촉 SOURCE에 응해 성행동SOURCE 증가
SOURCE:MASTER:성행동 = SOURCE:MASTER:접촉 / 5
;트라우마(각인)의 원이 이 조교였던 경우
IF CFLAG:MASTER:트라우마 == 35
	;접촉, 일탈 SOURCE를 트라우마 SOURCE에 가산해, 1.5배로 한다
	SOURCE:MASTER:트라우마 += SOURCE:MASTER:접촉 + SOURCE:MASTER:일탈
	TIMES SOURCE:MASTER:트라우마 , 1.50
ENDIF
;쾌ASOURCE에 응해 사랑 SOURCE 증가, 상해, 트라우마 SOURCE에 응해 감소
SOURCE:MASTER:사랑 = SOURCE:MASTER:쾌A / 5 - SOURCE:MASTER:아픔 / 5 - SOURCE:MASTER:트라우마 / 5
;사랑 SOURCE가 정이라면 조교 대상 A감각, 사랑 SOURCE에 응해 중독 충족 SOURCE 증가
SOURCE:MASTER:중독충족 = SOURCE:MASTER:사랑 > 0 ? (GET_ABL(MASTER, "A감각") + 10) * (GET_ABL(MASTER, "A감각") - 30) * SOURCE:MASTER:사랑 / 100 # SOURCE:MASTER:중독충족
;중독 충족 SOURCE가 정의 값이라면 쾌ASOURCE에 응해 증가
SOURCE:MASTER:중독충족 = SOURCE:MASTER:중독충족 > 0 ? SOURCE:MASTER:중독충족 + SOURCE:MASTER:쾌A / 5 # 0
;중독 충족 SOURCE가 부의 값을 받지 않게 조정
SOURCE:MASTER:중독충족 = SOURCE:MASTER:중독충족 < 0 ? 0 # SOURCE:MASTER:중독충족
;중독 충족 SOURCE에 응해 사랑 SOURCE 가산(피드백)
SOURCE:MASTER:사랑 += SOURCE:MASTER:중독충족 / 5
;더러움이 서로 옮긴다
STAIN:MASTER:애널 |= STAIN:페니스
STAIN:페니스 |= STAIN:MASTER:애널
;안면 승마계 해제
CALL FACESITTING_SET(0)
;조교자, 조교 대상 자위를 중단
TEQUIP:자위중 = 0

;남자는 A로 전립선 자극
SIF TALENT:MASTER:남자
	TCVAR:MASTER:전립선자극 += 1
;A감각, 마조끼에 응해 발기도 상승
TCVAR:MASTER:이번발기 += 100 + (GET_ABL(MASTER, "A감각") + GET_ABL(MASTER, "마조끼"))

TFLAG:경과시간 += 4
;애널 바이브, 애널 비즈 해제
TEQUIP:애널바이브 = 0
TEQUIP:애널비즈 = 0

