	SIF STAIN:손 & (32 + 64)
		SOURCE:MASTER:불결 += 550
	;더러움이 서로 옮긴다
	STAIN:MASTER:가슴 |= STAIN:손
	STAIN:손 |= STAIN:MASTER:가슴
	;조교자, 조교 대상 자위를 중단
	TEQUIP:자위중 = 0
	;조교자 기교, 애무, 기묘한손가락소질에 응해 발기도 상승
	TCVAR:MASTER:이번발기 += 80 + (GET_ABL(TARGET, "기교") + GET_ABL(TARGET, "애무") + TALENT:기묘한손가락 * 50) / 3
ENDIF
;안면승마 계 해제
CALL FACESITTING_SET(0)
TFLAG:경과시간 += 2

;리액션 후(접촉, 사랑, 성행동, 달성, 쾌b)
@ACTION_APPLY2_11
;젖의 몸싸움 파생시
IF TFLAG:ACT파생 == 2
	;실행했다
	IF IS_COMGRONAME("소극적 따른다/태연하게 따른다/적극적 따른다/받아들인다")
		;조교 대상 조교레벨에 응해 접촉 SOURCE 증가
		SOURCE:MASTER:접촉 += 70 + CFLAG:MASTER:조교레벨 * 3
		;조교 대상 호의, 봉사정신에 응해 사랑 SOURCE 증가
		SOURCE:MASTER:사랑 += 20 + CFLAG:M호의 / 40 + GET_ABL(MASTER, "봉사정신") * 5
		;애무 보정, 조교 대상기묘한손가락소질에 응해 보정 가격인상승
		LOCAL = 35 + ACT_TO(TARGET, 4) * (3 + TALENT:MASTER:기묘한손가락) / 5
		;보정치에 응해 성행동, 달성, 쾌bSOURCE 증가
		SOURCE:MASTER:성행동 += 50 + LOCAL / 8
		SOURCE:MASTER:달성 += LOCAL / 4
		SOURCE:쾌B += LOCAL
		;더러움이 서로 옮긴다
		STAIN:MASTER:손 |= STAIN:가슴
		STAIN:가슴 |= STAIN:MASTER:손
	;그 외의 경우
	ELSE
		;반항 선행시, REACT분류에 요구에 응하지 않는 것을 세트
		SIF PALAM:MASTER:치정 <= PALAM:MASTER:반항
			CALL SET_COMGRO("거부")
	ENDIF
ENDIF

