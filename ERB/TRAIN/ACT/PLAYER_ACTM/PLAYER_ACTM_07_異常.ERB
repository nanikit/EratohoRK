;----------------------------------------------------------------------
;피스트 적성
;경험과 쌍방의 소질이 관련되므로 소질로부터 독립
;
;다만, 작은체형 장신에 의해 곱셈을 걸치는 의의가 그다지 느껴지지 않고
;각각 경험과 소질로, 보통으로 가감(상태)하면 좋다고 생각하는 것과
;경험이 없을 때, 마이나스가 된다 라고 하는 것을 채용한다면 기초치의 개념 사용해
;----------------------------------------------------------------------
@ACT_ACTM7_FIST
;작은체형 체형/거구/ＶＡ경험/확장 경험/음계 소질 따위로 피스트의 처리를 해 본다
VARSET LOCAL

;Ｖ경험/확장 경험/음계 소질에 의해 증감
IF EXP:MASTER:Ｖ경험 < 10
	LOCAL:0 = -6
ELSEIF EXP:MASTER:Ｖ경험 < 20 && EXP:MASTER:Ｖ확장경험 < 1
	LOCAL:0 = -4
ELSEIF EXP:MASTER:Ｖ경험 < 40 && EXP:MASTER:Ｖ확장경험 < 3
	LOCAL:0 = -2
ENDIF
SIF TALENT:MASTER:음호
	LOCAL:0 += 4

;Ａ경험/확장 경험/음계 소질에 의해 증감
IF EXP:MASTER:Ａ경험 < 10
	LOCAL:1 = -6
ELSEIF EXP:MASTER:Ａ경험 < 20 && EXP:MASTER:Ａ확장경험 < 1
	LOCAL:1 = -4
ELSEIF EXP:MASTER:Ａ경험 < 40 && EXP:MASTER:Ａ확장경험 < 3
	LOCAL:1 = -2
ENDIF
SIF TALENT:MASTER:음고
	LOCAL:1 += 4

;작은체형 체형/장신에 의해 증감
IF TALENT:MASTER:작은체형 || TALENT:장신
	LOCAL:0 = LOCAL:0 * 3 / 4
	LOCAL:1 = LOCAL:1 * 3 / 4
ELSEIF TALENT:MASTER:장신 || TALENT:작은체형
	LOCAL:0 = LOCAL:0 * 4 / 3
	LOCAL:1 = LOCAL:1 * 4 / 3
ENDIF

;가산 처리
CALL ADD_CRI_VAR("피스트퍽", LOCAL:0)
CALL ADD_CRI_VAR("애널피스트", LOCAL:1)
CALL ADD_CRI_VAR("양구멍피스트", LOCAL:0 + LOCAL:1)

