CALL ADD_CRI_VAR("공중육변기플레이", LINE_CALC(BASE:기력, "0→3, 750→0, 1000→-3, LIMIT/-3～3") + TFLAG:조교자소모도 * LINE_CALC(BASE:기력, "0→3, 750→0, LIMIT/0～3"))

