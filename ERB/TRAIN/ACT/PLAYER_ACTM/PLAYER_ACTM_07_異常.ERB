CALL ADD_CRI_VAR("피스트퍽", LINE_CALC(BASE:MASTER:체력, "0→-1, 500→0, LIMIT/-1～0"))
;조교 대상의 체력이[0→-3, 1000→0]을 가산(-3～0)
CALL ADD_CRI_VAR("애널피스트", LINE_CALC(BASE:MASTER:체력, "0→-3, 1000→0, LIMIT/-3～0"))
;조교 대상의 체력이[0→-4, 1000→0]을 가산(-4～0)
CALL ADD_CRI_VAR("양구멍피스트", LINE_CALC(BASE:MASTER:체력, "0→-4, 1000→0, LIMIT/-4～0"))
;※캔슬시는 정의 보정
;조교 대상의 체력이[0→9/-9, 1500→0]을 가산(-9～9)
