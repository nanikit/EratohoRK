CALL ADD_CRI_VAR("피스트퍽", LINE_CALC(BASE:흥미, "0→-3, 550→0, LIMIT/-3～0"))
CALL ADD_CRI_VAR("애널피스트", LINE_CALC(BASE:흥미, "0→-3, 550→0, LIMIT/-3～0"))
CALL ADD_CRI_VAR("양구멍피스트", LINE_CALC(BASE:흥미, "0→-3, 550→0, LIMIT/-3～0"))
;조교자의 흥미가[0→3, 700→-3]을 가산(-3～3)
CALL ADD_CRI_VAR("방치플레이", LINE_CALC(BASE:흥미, "0→3, 700→-3, LIMIT/-3～3"))
;조교자의 흥미가[0→5, 700→-5]를 가산(-5～5)
CALL ADD_CRI_VAR("공중육변기플레이", LINE_CALC(BASE:흥미, "0→5, 700→-5, LIMIT/-5～5"))
;조교자의 흥미가[0→-5, 700→0]을 가산(-5～0)
CALL ADD_CRI_VAR("풋잡한다", LINE_CALC(BASE:흥미, "0→-5, 700→0, LIMIT/-5～0"))
CALL ADD_CRI_VAR("밟핥기강제", LINE_CALC(BASE:흥미, "0→-5, 700→0, LIMIT/-5～0"))

