	CALL ADD_CRI_VAR("아이마스크", LINE_CALC(PALAM:MASTER:반항, "0→0, 5→4, LIMIT/0～4"), LINE_CALC(PALAM:MASTER:반항, "0→0, 5→3, LIMIT/0～3"))
