;-------------------------------------------------------------
;오늘의 방침
;오늘의 방침(0=언제나 대로/1=A개발/2=벌/3=하드/4=러브러브/5=이상/6=야외/7=아이 만들기/8=휴게/9=안아/10= 봉사/11=귀여워해)
;-------------------------------------------------------------
@ACT_ACTM1_POLYCY
SELECTCASE POLICY("오늘")
	CASE "애널"
		CALL ADD_CRI_VAR("애널 애무", 15)
		CALL ADD_CRI_VAR("애널 빨고", 15)
	CASE "러브러브"
		CALL ADD_CRI_VAR("키스", 15)
	CASE "귀여워해"
		CALL ADD_CRI_VAR("손으로 애무", -10)
		CALL ADD_CRI_VAR("가슴 애무", TALENT:MASTER:남자 ? 5 # 20)
		CALL ADD_CRI_VAR("애널 애무", -10)
		CALL ADD_CRI_VAR("구에서 애무", 15)
		CALL ADD_CRI_VAR("키스", 15)
		CALL ADD_CRI_VAR("풋잡 한다", -10)
		SIF PENIS(MASTER)
			CALL ADD_CRI_VAR("파이즈리 한다", 15)
	CASEELSE
		;none
ENDSELECT

