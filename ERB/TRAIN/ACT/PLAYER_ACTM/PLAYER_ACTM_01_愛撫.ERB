@ACT_ACTM1_POLYCY
SELECTCASE POLICY("오늘")
	CASE "애널"
		CALL ADD_CRI_VAR("애널애무", 15)
		CALL ADD_CRI_VAR("애널핥기", 15)
	CASE "러브러브"
		CALL ADD_CRI_VAR("키스", 15)
	CASE "귀여워해"
		CALL ADD_CRI_VAR("손으로애무", -10)
		CALL ADD_CRI_VAR("가슴애무", TALENT:MASTER:남자 ? 5 # 20)
		CALL ADD_CRI_VAR("애널애무", -10)
		CALL ADD_CRI_VAR("입으로애무", 15)
		CALL ADD_CRI_VAR("키스", 15)
		CALL ADD_CRI_VAR("풋잡한다", -10)
		SIF PENIS(MASTER)
			CALL ADD_CRI_VAR("파이즈리한다", 15)
	CASEELSE
		;none
ENDSELECT

