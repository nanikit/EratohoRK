@ACT_ACTM1_CORRECTION(ARG)
#DIM LCOUNT

;ARG가 0이외라면 보정치를 계산한다
IF ARG
	FOR LCOUNT, 10, 20
		CALL ACT_M_RES_CALC(LCOUNT, GET_CRI_VAR(GET_ACTNAME(LCOUNT) + "누계"))
	NEXT
	CALL ACT_M_RES_CALC(27, GET_CRI_VAR("쓰러트린다누계"))
;ARG가 0이라면 보정치를 판정치에 가산한다
ELSE
	;보정치를 판정치에 가산한다
	CALL ACT_ACTM_CORRECTION(1)
ENDIF

