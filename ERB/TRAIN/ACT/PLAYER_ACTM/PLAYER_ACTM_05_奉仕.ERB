IF LOCAL == -1
	SIF FLAG:디버그
		PRINTL (봉사 ACT분류에 실행 가능한 ACT가 존재하지 않습니다)
	TFLAG:ACT = -1
	CALL ACT_MENU_RESULT(2, "봉사")
