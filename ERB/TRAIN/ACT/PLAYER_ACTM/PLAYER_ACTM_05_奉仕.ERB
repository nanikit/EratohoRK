CALL ANA_CRI_VAR()

;-------------------------------------------------------------------------
;기준치
;-------------------------------------------------------------------------
@ACT_ACTM5_REFERENCE

;필요하지 않을지도라고 생각했으므로 시험삼아 무효화@이것 보고
[SKIPSTART]
CALL ADD_CRI_VAR("손으로애무를강제", 10)
CALL ADD_CRI_VAR("입으로애무를강제", 10)
CALL ADD_CRI_VAR("파이즈리", 10)
CALL ADD_CRI_VAR("가랑이", 10)
CALL ADD_CRI_VAR("밟핥기강제", 2)
CALL ADD_CRI_VAR("이라마치오", 0)
