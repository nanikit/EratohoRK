	IF ALI(0)
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「햐, 아……」
				PRINTFORMW 「굉장히, 좋았어요……」
			CASE 1
				PRINTFORMW 「다음에, 나도 해 주기 때문에……」
