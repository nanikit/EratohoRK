	;파생:애원이라면 COM:용서를빈다에 날린다
	CALL KOJO_COM_K40_7
	RETURN RESULT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 0, 여리게대답한다(REACT분류:소극적으로 한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	SELECTCASE RANDCOM(3, 10)
		CASE 0
			PRINTFORMW 「응, 알았어……」
			PRINTFORMW 「그렇지만, 좀 더 확실해 주면 기쁘구나」
		CASE 1
			PRINTFORMW 「고마워요」
			PRINTFORMW 「역시, 솔직한 것이 제일 좋지요」
		CASE 2
			PRINTFORMW 「좀, 무서워하게 해 버렸는지?」
			PRINTFORMW 「괜찮아, 나쁘게는 하지 않는다고……」
	ENDSELECT
	RETURN 1
ENDIF

