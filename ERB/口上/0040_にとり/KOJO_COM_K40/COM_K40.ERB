	;アライメント正
	IF ALI(0)
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「もしかして、やり過ぎた……？」
				PRINTFORMW 「反応してくれると、嬉しいなあ……」
			CASE 1
				PRINTFORMW 「どうしたの……？」
				PRINTFORMW 「飽きちゃった？　それとも……」
			CASE 2
				PRINTFORMW 「気持ち良くすれば、戻ってくるかなあ」
				PRINTFORMW 「大丈夫……？」
		ENDSELECT
	;アライメント負
	ELSE
		SELECTCASE RANDCOM(3, 1)
			CASE 0
				PRINTFORMW 「もっと、強いほうがいいかな？」
				PRINTFORMW 「聞いてる？」
			CASE 1
				PRINTFORMW 「反応無いと、つまらないなあ」
				PRINTFORMW 「ほら、早く戻って」
			CASE 2
				PRINTFORMW 「ちょっと、やり過ぎちゃったかな……」
				PRINTFORMW 「まあ、いいか……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

