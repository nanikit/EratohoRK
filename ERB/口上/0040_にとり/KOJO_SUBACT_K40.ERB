;·상반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(6)
	ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	IF 구상 기입 체크(0) && ONCEDATUI(6)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
