;────────────────────────────────────
;脱衣アクション口上
;────────────────────────────────────
;・調教者アクションの前に強制脱衣や自主脱衣が発生したときに表示
;・この口上の時点ではまだTEQUIPは更新されていない
;・初回分岐は「初めて見た/見せた」とは限らない。
;　ショップメニューで脱がせたことがあるかもしれないし、調教の流れで完全に脱がせずに露出だけしている場合もある。
;　もしかすると助手だったときに見たことがあるかもしれない。そういう意味では簡易的なものである。
;　厳密な意味での「初見」フラグを用意するかどうかは要望次第ということで。
;-------------------------------------------------
@KOJO_DATUI_K40
;-------------------------------------------------
;関連変数…
;強制脱衣：TFLAG:102　自主脱衣：TFLAG:103
;=(-1, 強制脱衣に抵抗、1, 上着、2, 下半身上着、3, 上衣、4, 上半身全部、5, 下半身全部、
;　6, アクセサリー、7, アクセサリー以外全て、8, 全裸まで、9, 下着だけ)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;強制脱衣に抵抗された FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 < 0
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTDATUI(0)
		PRINTFORMW 「服脱ぐ位、いいじゃないかあ」
		PRINTFORMW 「人間の裸、見たいんだけどねえ」
		RETURN 1
	ENDIF
	
	;=================================================
	;自分は脱いだ
	;=================================================
	IF 口上記入チェック(100) && TFLAG:自主脱衣
		SELECTCASE RANDDATUI(3, 0)
			CASE 0
				PRINTFORMW 「あらら……」
				PRINTFORMW 「私は脱いでるんだけど……」
			CASE 1
				PRINTFORMW 「恥ずかしいなあ」
				PRINTFORMW 「一緒に恥ずかしくなろうよ」
			CASE 2
				PRINTFORMW 「脱ぎたくならない？」
				PRINTFORMW 「私の体、魅力無いのかなあ……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;アライメント正
	IF ALI(0)
		SELECTCASE RANDDATUI(3, 1)
			CASE 0
				PRINTFORMW 「何で……」
				PRINTFORMW 「気持ち良いこと、したくないの……？」
			CASE 1
				PRINTFORMW 「強引なのは嫌だったかな？」
				PRINTFORMW 「でも、脱いで欲しいなあ」
			CASE 2
				PRINTFORMW 「ううっ……」
				PRINTFORMW 「あんまり、抵抗しないでよお……」
		ENDSELECT
	;アライメント負
	ELSE
		SELECTCASE RANDDATUI(3, 2)
			CASE 0
				PRINTFORMW 「乱暴なのが好みだと思ってたんだけど」
				PRINTFORMW 「まだ足りないのかなあ」
			CASE 1
				PRINTFORMW 「嫌がるふりはもういいからさあ」
				PRINTFORMW 「とりあえず、脱いでよ」
			CASE 2
				PRINTFORMW 「どうしたもんだろうねえ……」
				PRINTFORMW 「そんなに嫌なのかなあ」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;二人とも何か脱いだ FIRSTDATUI(10)
;------------------------------------------------
;・主にセックス系行動の予備動作で発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 > 0 && TFLAG:自主脱衣 && !IS_NOWASSIACTNAME(, "脱衣")
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTDATUI(10)
		PRINTFORMW 「何か、恥ずかしいね……」
		PRINTFORMW 「え……っと、よろしくね……」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者が[情欲]状態
	;=================================================
	IF 口上記入チェック(100) && T_COND("情欲")
		;アライメント正
		IF ALI(0)
			SELECTCASE RANDDATUI(3, 10)
				CASE 0
					PRINTFORMW 「もう、我慢できないよ……」
					PRINTFORMW 「早く、良くなろ……？」
				CASE 1
					PRINTFORMW 「顔、見ないで……」
					PRINTFORMW 「きっと、凄く赤くなっちゃってる……」
				CASE 2
					PRINTFORMW 「乱暴に、しちゃいそうだあ……」
					PRINTFORMW 「今の内に、謝っとくね……」
			ENDSELECT
		;アライメント負
		ELSE
			SELECTCASE RANDDATUI(3, 11)
				CASE 0
					PRINTFORMW 「何だか、良い気分だよ……」
					PRINTFORMW 「さっさと、寝転がって？」
				CASE 1
					PRINTFORMW 「私、加減できないかも」
					PRINTFORMW 「まあ、いいよねえ？」
				CASE 2
					PRINTFORMW 「はぁ……っ」
					PRINTFORMW 「鎮めさせてもらおうかな……」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;アライメント正
	IF ALI(0)
		SELECTCASE RANDDATUI(3, 12)
			CASE 0
				PRINTFORMW 「あんまり、見ないでよお……」
				PRINTFORMW 「何か、不公平だ」
			CASE 1
				PRINTFORMW 「ほらっ、%CNAME(MASTER, TARGET)%……」
				PRINTFORMW 「私、どうかなっ？」
			CASE 2
				PRINTFORMW 「やっぱり、まだ慣れないなあ」
				PRINTFORMW 「回数こなさないとねえ」
		ENDSELECT
	;アライメント負
	ELSE
		SELECTCASE RANDDATUI(3, 13)
			CASE 0
				PRINTFORMW 「鼻息、荒いねえ……」
				PRINTFORMW 「何興奮してるのさ？」
			CASE 1
				PRINTFORMW 「さて、始めようか？」
				PRINTFORMW 「おとなしく、してなよ？」
			CASE 2
				PRINTFORMW 「服、脱いだんだからねえ」
				PRINTFORMW 「まあ、期待してもいいんじゃあない？」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

