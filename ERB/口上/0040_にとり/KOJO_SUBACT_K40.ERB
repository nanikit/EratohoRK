;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GETBIT(TFLAG:媚薬等使用口上, 0) && GETBIT(TFLAG:媚薬等使用口上, 1)
	;アライメント正
	IF ALI(0)
		SELECTCASE RANDUSE(3, 0)
			CASE 0
				PRINTFORMW 「これ、楽しみ？」
			CASE 1
				PRINTFORMW 「ぬるぬるで、凄いよ？」
			CASE 2
				PRINTFORMW 「薬でどうなっちゃうんだろうねえ？」
		ENDSELECT
	;アライメント負
	ELSE
		SELECTCASE RANDUSE(3, 1)
			CASE 0
				PRINTFORMW 「面倒だから、使っちゃうね」
			CASE 1
				PRINTFORMW 「乱れる顔、見せて？」
			CASE 2
				PRINTFORMW 「あ……凄い、音……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

