;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·만p FIRSTDATUI(30)
;------------------------------------------------
;·이번 탈의로 성기가 보이게 되었을 경우에 표시
;·팬티 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(3)
	ENDIF
	
	;=================================================
	;벌써 가슴이 보이고 있다
	;=================================================
	IF 구상 기입 체크(0) && B_DATUICHECK(0) == 1
	ENDIF
	
	;=================================================
	;응 이렇게 p
	;=================================================
	IF 구상 기입 체크(0) && PENIS(TARGET)
	ENDIF
	
	;그 외
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·말하는 p FIRSTDATUI(40)
;------------------------------------------------
;·이번 탈의로 젖가슴이 보이게 되었을 경우에 표시
;·브라 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && ! TALENT:거유 && FIRSTDATUI(4)
	ENDIF
	
	;=================================================
	;벌써 성기가 보이고 있다
	;=================================================
	IF 구상 기입 체크(0) && P_DATUICHECK(0) == 1
	ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	IF 구상 기입 체크(0) && ONCEDATUI(4)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·팬티 보이고 FIRSTDATUI(50)
;------------------------------------------------
;·이번 탈의로 팬티（캐릭터에 의해 드로워즈·트렁크스）가 보이게 되었을 경우에 표시
;·하반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(5)
	ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	IF 구상 기입 체크(0) && ONCEDATUI(5)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
