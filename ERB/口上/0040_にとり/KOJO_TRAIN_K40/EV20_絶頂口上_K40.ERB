					PRINTFORMW 「仕方が無かったんだよっ……」
			ENDSELECT
		;アライメント負
		ELSE
			SELECTCASE RANDEVENT(3, 15)
				CASE 0
					PRINTFORMW 「気持ち良かったでしょ？」
					PRINTFORMW 「感謝して欲しいぐらいだなあ」
				CASE 1
					PRINTFORMW 「何泣いてるのさ？」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%の使い道なんて、これくらいだよね」
				CASE 2
					PRINTFORMW 「良かったよ……」
					PRINTFORMW 「これからも、出したげるからさ」
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;アライメント正
	IF ALI(0)
		SELECTCASE RANDEVENT(3, 16)
			CASE 0
				PRINTFORMW 「これっ、凄いよ……」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%の、吸い付いて……っ！」
			CASE 1
