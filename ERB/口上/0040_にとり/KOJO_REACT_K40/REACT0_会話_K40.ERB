	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT1_4, 분명히 좋아한다고 고했다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 적극적 따르는 것으로, 조교 대상 호의가 1000이상
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 4
	;부하 2(무리를 한 행동)의 경우는 연주해 둡니다@/L
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTREACT(40) && TFLAG:REACT부하 < 2
		PRINTFORMW 「갑자기, 무엇 말하기 시작해……」
		PRINTFORMW 「나는…… 은, 말할 수 있을 이유 없지 않은가」
		RETURN 1
	ENDIF
	
	;=================================================
	;성교 봉사계로 질내사정의 뒤
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질내사정 == 2
		SELECTCASE RANDREACT(3, 40)
			CASE 0
				PRINTFORMW 「그래, 나도 좋아해……」
				PRINTFORMW 「기분이 좋으니까」
			CASE 1
				PRINTFORMW 「사실이라면, 기쁘구나」
				PRINTFORMW 「거짓말이 아니, 지요……?」
			CASE 2
				PRINTFORMW 「그렇게, 기분 좋았어?」
				PRINTFORMW 「정직하다……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;성교계로 질내사정의 뒤
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질내사정 == 1
		SELECTCASE RANDREACT(3, 41)
			CASE 0
				PRINTFORMW 「기뻐……」
				PRINTFORMW 「좀 더 가득, 내 주기 때문에」
			CASE 1
				PRINTFORMW 「에……」
				PRINTFORMW 「정말로?   내가……?」
			CASE 2
				PRINTFORMW 「이런 일, 되어 버렸는데 응」
				PRINTFORMW 「바보야, 사실……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;호감도 1000이상
	;=================================================
	IF 구상기입체크(100) && AFFECTION("권애")
		SELECTCASE RANDREACT(3, 42)
			CASE 0
				PRINTFORMW 「나도, 좋아해!」
				PRINTFORMW 「쭉, 사이좋게 지내자……?」
			CASE 1
				PRINTFORMW 「고마워요……」
				PRINTFORMW 「절대, 떼어 놓지 않으니까……」
			CASE 2
				PRINTFORMW 「…… 읏!?」
				PRINTFORMW 「거짓말에서도, 기뻐……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(100)
		SELECTCASE RANDREACT(3, 43)
			CASE 0
				PRINTFORMW 「조금……」
				PRINTFORMW 「이상한 일, 하지 말아」
			CASE 1
				PRINTFORMW 「네네」
				PRINTFORMW 「아니, 너무나 당돌했기 때문에」
			CASE 2
				PRINTFORMW 「나의 일, 좋아하는 것이야?」
				PRINTFORMW 「흐음…… 그런가 아……」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT1_5, 「좀 더 굉장한 일 해 주면, 좀 더 좋아하게 될 수 있을지도」
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 받아들이는 것 경우
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 5
	;=================================================
	;성교 봉사계로 질내사정의 뒤
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질내사정 == 2
		SELECTCASE RANDREACT(3, 50)
			CASE 0
				PRINTFORMW 「좀 더, 카아……」
				PRINTFORMW 「절륜이다, 이제(벌써)」
			CASE 1
				PRINTFORMW 「상당히 이것, 힘들지만……」
				PRINTFORMW 「다음은%CALLNAME:MASTER%가 움직여?」
			CASE 2
				PRINTFORMW 「무책임한 일, 해 주어 버려」
				PRINTFORMW 「더 이상 하면, 임신해 버려?」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;성교계로 질내사정의 뒤
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질내사정 == 1
		SELECTCASE RANDREACT(3, 51)
			CASE 0
				PRINTFORMW 「아직 사랑받지 않았어?」
				PRINTFORMW 「그러면, 좀 더 내지 않으면」
			CASE 1
				PRINTFORMW 「좀 더, 수줍음이라든지 없는 것인지」
				PRINTFORMW 「뭐, 좋은가……」
			CASE 2
				PRINTFORMW 「정말로, 좋은거야?」
				PRINTFORMW 「더 이상 하면, 임신해 버려?」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;호감도 1000이상
	;=================================================
	IF 구상기입체크(100) && AFFECTION("권애")
		SELECTCASE RANDREACT(3, 52)
			CASE 0
				PRINTFORMW 「그러면, 어떻게 하시고 싶어?」
				PRINTFORMW 「할 수 있는 한, 노력한다」
			CASE 1
				PRINTFORMW 「나, 노력할테니까」
				PRINTFORMW 「아니, 역시, 아무것도 아니야……」
			CASE 2
				PRINTFORMW 「멋대로이다, 이제(벌써)……」
				PRINTFORMW 「나, 거절할 수 없고흩어졌다……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(100)
		;아라이먼트정
		IF ALI(0)
			SELECTCASE RANDREACT(3, 53)
				CASE 0
					PRINTFORMW 「난제다……」
					PRINTFORMW 「구체적인 의견을 갖고 싶구나」
				CASE 1
					PRINTFORMW 「빨리 이 환경에 익숙했으면 좋지만……」
					PRINTFORMW 「이것은 다른 의미로 어려울 것 같다」
				CASE 2
					PRINTFORMW 「엣……?」
					PRINTFORMW 「나, 상당히 노력하고 있지만……?」
			ENDSELECT
		;아라이먼트부
		ELSE
			SELECTCASE RANDREACT(3, 54)
				CASE 0
					PRINTFORMW 「따로 사랑받고 싶지는 않지만」
					PRINTFORMW 「굉장한 일, 에는 찬성이구나」
				CASE 1
					PRINTFORMW 「아직, 부족한거야……?」
					PRINTFORMW 「굉장한 것 잡고 흩어졌군……」
				CASE 2
					PRINTFORMW 「꽤, 매우 엄하다」
					PRINTFORMW 「어떻게 한 것일 것이다?」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;REACT1, 자신에게로의기분을얘기한다

