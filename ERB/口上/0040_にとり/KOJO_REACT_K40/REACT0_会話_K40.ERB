;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT派生,0 歯切れ悪く質問に答えた
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が消極的従うで、リアクションが皮肉を言う以外の場合
;-------------------------------------------------
IF 口上記入チェック(100) && TFLAG:ACT派生 == 0
	;アライメント正
	IF ALI(0)
		SELECTCASE RANDREACT(3, 0)
			CASE 0
				PRINTFORMW 「あれ、良くなかった？」
				PRINTFORMW 「もう少し、改良の余地があるみたいだあね……」
			CASE 1
				PRINTFORMW 「うーん、結構頑張ったんだけど」
				PRINTFORMW 「満足させてあげられなかったかあ」
			CASE 2
				PRINTFORMW 「まあ、方向性としては」
				PRINTFORMW 「間違ってない……よねえ？」
		ENDSELECT
	;アライメント負
	ELSE
		SELECTCASE RANDREACT(3, 1)
			CASE 0
				PRINTFORMW 「ありゃ、いまいち？」
				PRINTFORMW 「次行こう、次！」
			CASE 1
				PRINTFORMW 「あれだけやって、まだ足りないの？」
				PRINTFORMW 「本当に、困ったねえ……」
			CASE 2
				PRINTFORMW 「何だよお……」
				PRINTFORMW 「一体、何が気に食わないのさ？」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

