;*************************************************
;ACT9, 기분나쁘게웃는다
;*************************************************
;-------------------------------------------------
;·기분 나쁜 미소를 띄운다
;·이상계로부터 발생하는 경우도 있다
;·오늘의방침가 "러브러브"라면 발생하지 않는다
;·화제 『비웃는다』가 기출이 되어, 이후, 동일 조교내에서는 억제된다
;-------------------------------------------------
;=================================================
@KOJO_ACT_K48_9
;=================================================
;함수@TARGET_DESCRIPTION에 의해 TEQUIP(성교중/조교대상Ｃ사용/성교봉사중/자위중 & 2)로 지문은 변화한다
;그 외·조건 없음을 최초로 묻은 다음, 뒤는 기호로 분기를 증감해 주세요
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT9, 기분나쁘게웃는다
;기입 체크(=0, 비표시, 1, 표시, 2, 표시해 미약등 구상을 스킵)
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL
	;=================================================
	;첫회
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && FIRSTACT(0)
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;PENIS(TARGET)/! PENIS(TARGET)/TEQUIP:조교자페니스밴드 등으로 분기 해도 괜찮다
	;=================================================
	;성교중
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TEQUIP:성교중
		;체위에 의한 파생
		SELECTCASE TEQUIP:성교중
			;정상위				체중을 맡겨 %CALLNAME:MASTER%의 질내에 묻고
			CASE 1
				PRINTFORMW 
			;후배위				배후로부터 %CALLNAME:MASTER%의 질내에 묻고
			CASE 2
				PRINTFORMW 
			;기승위				%CALLNAME:MASTER%의 암컷 구멍에 근원까지 물어지고
			CASE 3
				PRINTFORMW 
			;대면좌위			밀착한 %CALLNAME:MASTER%의 질에 애무되고
			CASE 4
				PRINTFORMW 
			;배면좌위			%CALLNAME:MASTER%에 삽입해 등을 껴안고
			CASE 5
				PRINTFORMW 
			;애널섹스		%CALLNAME:MASTER%의 항문에 묻고
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
	
	;조교 대상의Ｃ에의 애무를 하고 있는 상황. TEQUIP 관리인 것으로 가능성은 비교적 있다
	;=================================================
	;조교대상Ｃ사용
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GROUPMATCH(TEQUIP:조교대상Ｃ사용, 1, 2)
		;자위·애무 계속중
		IF TEQUIP:조교대상Ｃ사용 == 1
			PRINTFORMW 
		;페라·쿤닐링구스 계속중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;PENIS(MASTER)/! PENIS(MASTER)/TEQUIP:페니스밴드등으로 분기 해도 괜찮다
	;=================================================
	;성교봉사중
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TEQUIP:성교봉사중
		;체위에 의한 파생
		SELECTCASE TEQUIP:성교봉사중
			;정상위시킨다				크게 벌어진 가랑이의 사이에 맞아들이고
			CASE 1
				PRINTFORMW 
			;후배위시킨다				배후로부터 질내에 삽입되고
			CASE 2
				PRINTFORMW 
			;역강간					미끈미끈의 질로 큐우큐우 짜내고
			CASE 3
				PRINTFORMW 
			;대면좌위시킨다				체를 밀착시켜 질 안쪽으로 환영해
			CASE 4
				PRINTFORMW 
			;배면좌위시킨다				등을 맡겨 만끽해
			CASE 5
				PRINTFORMW 
			;애널섹스시킨다		장액에 미끌거리는 항문으로 억압
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;조교자가 자위중
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GETBIT(TEQUIP:자위중, 1)
		;조교자가 페니스밴드 몸에 걸치고 있다
		IF TEQUIP:조교자페니스밴드
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;그 외·조건 없음
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF
