	;전COM가가게해!
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_PREVCOMNAME("가게해!")
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;전COM가 기분좋게해
	;기입 체크
	LOCAL:1 = 0
	;=================================================
