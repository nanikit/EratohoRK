;────────────────────────────────────
;愛撫系
;────────────────────────────────────
;・各REACT内の分け方としてTFLAG:REACT分類(IS_COMGRONAME)やSELECTCOM(IS_NOWCOMNAME)が考えられます
;・ただ、範囲の広い条件で拾ってしまうのは基本的にお勧めしません
;・ACT分類:愛撫系の場合、基本的に愛撫系COM(COM10~COM16)が選択肢となりますが
;・状況によってはその他のものが選択肢に含まれる可能性もあります
;・リアクション(REACT)で処理されなかったものは汎用リアクション(COM)で補完されるので
;・無理に全て埋める必要はありません
;・必要と思うものを絞り込み、COMとの連携を考えて効率よく構築すべきです
;-------------------------------------------------
;・各REACT及びACT派生内は、&& IS_NOWCOMNAMEなり、&& IS_COMGRONAME等の条件で決めうちしましょう
;・ここに条件無し分岐があると、リアクションに関わらず、全て拾ってしまって汎用リアクションに流れません
;・要するに、想定されたシステムの破壊に繋がります
;-------------------------------------------------
;　基本的に地の文はTFLAG:REACT分類(受け入れる/積極的従う/平然と従う/許しを乞う/消極的従う)で分岐し、
;　各REACT分類内でTFLAG:REACT負荷(自然な行動/無理のない行動/無理をした行動)で派生
;　ACT_MESSAGE.ERBを参照しつつの作業をお勧めします
;-------------------------------------------------
;　取り敢えずACT派生毎に枠を切り分けてありますが、一つに纏めた上で汎用的な記述で手間を減らすという手もあります
;　正直、やり方はいくらでもあるので、以下に示す方法は一つの例と思ってください
;-------------------------------------------------

;*************************************************
;REACT10, 手で愛撫・個別リアクション
;*************************************************
;・TFLAG:ACT派生(0=愛撫/1=手淫/2=ペニスバンド手淫)
;-------------------------------------------------
;=================================================
@KOJO_REACT_K48_10
;=================================================

;ACT派生0においては参考として快楽系COMを全て網羅してあります
;必要に応じて分岐を増減して下さい
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT派生0, 愛撫
;記入チェック
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && !TFLAG:ACT派生
	;=================================================
	;COM10, 恥ずかしがる(REACT分類"消極的従う")
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("恥ずかしがる")
		;快感に恥ずかしがる
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 
		;状況に恥ずかしがる
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM11, 嫌がる
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("嫌がる")
		;嫌がって妨害した			REACT分類"暴れる"
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 
		;嫌がった					REACT分類"消極的従う"
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM12, 下手だと罵る
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("下手だと罵る")
		;強がって					REACT分類"消極的従う"
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 
		;未熟なテクニックに対して	REACT分類"平然と従う"
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM13, 不敵に笑う(REACT分類"平然と従う")
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("不敵に笑う")
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM14, 快感を我慢する
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("快感を我慢する")
		;派生:哀願					REACT分類"許しを乞う"
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 
		;派生:耐久					REACT分類"消極的従う"
		ELSE
			PRINTFORMW
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM15, 快感を受け入れる
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("快感を受け入れる")
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM16, 気持ち良い！
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("気持ち良い！")
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
;条件にヒットしなければ汎用リアクションに流す
RETURN 0
ENDIF

;ACT派生1は派生0と同じ種類のものを違う形で提示してみます
;まぁこんなやりかたもあるという事で
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT派生1, 手淫
;記入チェック
LOCAL = 1
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && TFLAG:ACT派生 == 1
	;=================================================
	;COM10, 恥ずかしがる(REACT分類"消極的従う")
	;記入チェック
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("恥ずかしがる")

		;=================================================
		;射精直前
		;記入チェック
		LOCAL:1 = 1
		;=================================================
		IF LOCAL:1 && NOWEX:MASTER:射精 && KFLAG:152 == 1
			PRINTFORMW 「……だーめっ、イき顔ちゃんと見せてくれないと嫌ですっ」
			PRINTFORMW 「意地悪する子は、途中で止めちゃいますよ？」
			PRINTFORMW 「ちゃんとびゅーびゅーする時も、阿求にお顔見せて下さい♪」
		ENDIF

		IF CFLAG:153 == 0		;CFLAG:153は代入が行われていないので0にしかなりません。ELSE以降は死に口上になります@/L
			PRINTFORMW 「少し、刺激が強すぎました？」
			PRINTFORMW 「それとも……ふふ、誘ってらっしゃるとか？」
		ELSE
			PRINTFORMW 「その通り、少し反省しなさい」
			PRINTFORMW 「男の尊厳に係る事なのですから、この早漏」
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM12, 下手だと罵る
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("下手だと罵る")

		;=================================================
		;射精直前
		;記入チェック
		LOCAL:1 = 1
		;=================================================
		IF LOCAL:1 && NOWEX:MASTER:射精 && KFLAG:152 == 1
			PRINTFORMW 「
		ENDIF

		;強がって					REACT分類"消極的従う"
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 
		;未熟なテクニックに対して	REACT分類"平然と従う"
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;手間を厭わないのであれば、IFなり三項なりを使って内部で書き分けるのもあり
	;=================================================
	;REACT分類4, 受け入れる＆COM, 快感を受け入れる/気持ち良い！
	;記入チェック
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("受け入れる") && IS_NOWCOMNAME("快感を受け入れる/気持ち良い！")
	
		;=================================================
		;射精直前
		;記入チェック
		LOCAL:1 = 1
		;=================================================
		IF LOCAL:1 && NOWEX:MASTER:射精 && KFLAG:152 == 1
			PRINTFORMW 「ええ、私に全て任せて下さい」
			PRINTFORMW 「この指と、ぷにぷにの手の平で……あなた様の特濃せーえき、きっちり搾り取って差し上げます」
		ENDIF

		;消費気力による派生
		IF CFLAG:153 == 0
			SELECTCASE GET_COM_RANK()
				;自然な行動
				CASE "自然な行動"
					PRINTFORMW 「まぁ……お褒めの言葉ありがとうございます、あなた様」
					PRINTFORMW 「このまま、阿求の技に蕩けて下さいね♪」
				;無理のない行動
				CASE "無理のない行動"
					PRINTFORMW 「まぁ……お褒めの言葉ありがとうございます、あなた様」
					PRINTFORMW 「このまま、阿求の技に蕩けて下さいね♪」
				;無理をした行動
				CASE "無理をした行動"
					PRINTFORMW 「少し、素直になって頂けたようで……嬉しいです♪」
					PRINTFORMW 「もちろん、これからも精進を止めるつもりは有りませんけど」
			ENDSELECT
		ELSE
			PRINTFORMW 「……無惨にあえいでいる場合ですか？」
			PRINTFORMDW 阿求は眉を顰め、ペニスをしごく指を強く握りしめた……
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;REACT分類2, 平然と従う＆COM, 不敵に笑う
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("平然と従う") && IS_NOWCOMNAME("不敵に笑う")
		;消費気力による派生
		SELECTCASE GET_COM_RANK()
			;自然な行動
			CASE "自然な行動"
				PRINTFORMW 
			;無理のない行動
			CASE "無理のない行動"
				PRINTFORMW 
			;無理をした行動
			CASE "無理をした行動"
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
	
	;COMが妨害/哀願に派生したらCOMへ流す
	;&& TFLAG:REACT派生 < 1自体はなくてもREACT分類の決めうちで同様の結果を得られますが、仕様の可視化ということで
	;=================================================
	;REACT分類2, 消極的従う
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("消極的従う") && IS_NOWCOMNAME("嫌がる/快感を我慢する") && TFLAG:REACT派生 < 1
		;消費気力による派生
		SELECTCASE GET_COM_RANK()
			;自然な行動
			CASE "自然な行動"
				PRINTFORMW 
			;無理のない行動
			CASE "無理のない行動"
				PRINTFORMW 
			;無理をした行動
			CASE "無理をした行動"
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
;条件にヒットしなければ汎用リアクションに流す
RETURN 0
ENDIF

