IF LOCAL && TFLAG:ACT파생 == 1
	;=================================================
	;한 장 입었다(소극적 반응)
	;기입 체크
	LOCAL:1 = 1
	;=================================================
	IF LOCAL && IS_COMGRONAME("소극적으로 한다/받아들인다")
		PRINTFORMW 「에에, 좋다.」
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;2매 입었다(적극적 반응)
	;기입 체크
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		PRINTFORMW 「에에, 좋다.」
		RETURN LOCAL:1
	ENDIF
	
	;지문에서는 "싫어해"라고 하는 취급
	;=================================================
	;입지 않았다
	;기입 체크
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("거부")
		PRINTFORMW 「벌거벗은 채로로 있고 싶습니까……?」
		PRINTFORMW 「머지않아 부친이 될 때에, 그러면 조금 곤란해 버립니다만……」
		RETURN LOCAL:1
	ENDIF
ENDIF
