IF LOCAL && IS_COMGRONAME("태연하게 한다")
	;=================================================
	;별로 싫지 않다고 말했다(자연스러운 행동)
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "자연스러운 행동"
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;마음대로 하면 좋다고 대답했다(무리가 없는 행동)
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "무리가 없는 행동"
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;무리를 한 행동
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "무리를 한 행동"
		;실은 적극			하고 싶으면 좋다고 말했다
		IF COMCOR_POSI()
			PRINTFORMW 
		;실은 혐				마음대로 하면 좋다고 대답했다
