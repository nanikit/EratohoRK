;-------------------------------------------------
;=================================================
@KOJO_REACT_K48_6
;=================================================

;REACT분류가 소극적 따르는 것 경우
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_0, 작은 소리로 승낙한다
;기입 체크
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && IS_COMGRONAME("소극적으로 한다")
	;=================================================
	;각오를 결정한 것처럼, 작은 소리로 승낙했다(자연스러운 행동)
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "자연스러운 행동"
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;작은 소리로 승낙했다(무리가 없는 행동)
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "무리가 없는 행동"
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;무리를 한 행동
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GET_COM_RANK() == "무리를 한 행동"
		;실은 적극			부끄러움에 스러질 것 같은 소리로 승낙했다
		IF COMCOR_POSI()
			PRINTFORMW 
		;실은 혐				분함에 얼굴을 비뚤어지게 하면서도 승낙했다
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
ENDIF

;REACT분류가 태연하게 따르는 것 경우
;━━━━━━━━━━━━━━━━━━━━━━━━
