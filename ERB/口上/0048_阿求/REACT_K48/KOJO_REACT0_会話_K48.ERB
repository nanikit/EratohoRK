;REACT5, 의상의변경을요구한다·개별 리액션
;*************************************************
;·조교 대상의 리액션에 의해 한 장 벗는, 2매 벗는, 벗지 않는으로 나누어진다
;·REACT분류가"소극적으로 한다", "받아들인다"의 경우는 한 장 벗는다
;·REACT분류가"적극적으로 한다", "태연하게 한다"의 경우는 2매 벗는다
;·그 외의 경우는 벗지 않는다(PALAM치정/반항의 수치에 의해, 부끄러워서 벗지 않는, 싫어해 벗지 않는으로 나누어진다)
;-------------------------------------------------
;·TFLAG:ACT파생(0=옷을 벗도록(듯이) 명령/1=옷을 입도록(듯이) 명령)
;-------------------------------------------------
;=================================================
@KOJO_REACT_K48_5
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_0, 옷을 벗도록(듯이) 명령
;기입 체크
LOCAL = 1
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && TFLAG:ACT파생 == 0
	;=================================================
	;한 장 벗었다(소극적 반응)
	;기입 체크
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("소극적으로 한다/받아들인다")
		;-------------------------------------------------
		;마지막 한 장을 벗어전라가 되었다
		;기입 체크
		LOCAL:2 = 0
		;-------------------------------------------------
		IF LOCAL:2 && (TEQUIP:MASTER:악세사리 + TEQUIP:MASTER:양말 + TEQUIP:MASTER:하반신속옷 + TEQUIP:MASTER:상반신속옷 + TEQUIP:MASTER:하반신윗도리 + TEQUIP:MASTER:상반신윗도리 + TEQUIP:MASTER:전신윗도리) == 0
			;REACT분류받아들인다			전라가 되어, 기대와 흥분으로 피부가 달아오르는 것을 느꼈다
			IF IS_COMGRONAME("받아들인다")
				PRINTFORMW 
			;REACT분류소극적 따른다			어쩔 수 없이 마지막 한 장을 벗어, 전라가 되었다
			ELSE
				PRINTFORMW 
			ENDIF
			RETURN LOCAL:2
		ENDIF
		
		;-------------------------------------------------
		;완만한 움직임으로 옷을 한 장 벗어 던졌다
		;기입 체크
		LOCAL:2 = 0
		;-------------------------------------------------
		IF LOCAL:2 && IS_COMGRONAME("받아들인다")
			PRINTFORMW 
			RETURN LOCAL:2
		ENDIF
		;-------------------------------------------------
		;어쩔 수 없이 옷을 한 장 벗어 던졌다
		;기입 체크
		LOCAL:2 = 1
		;-------------------------------------------------
		IF LOCAL:2 && IS_COMGRONAME("소극적으로 한다")
			PRINTFORMW 「……?」
			PRINTFORMW 「절제 계속이라고, 좀 더 짐승과 같이 된다고 들었습니다만」
			PRINTFORMW 「의외로, 신사이군요?」
			PRINTFORMDW 아큐는 미소지으면서 고개를 갸웃했다……
			RETURN LOCAL:2
		ENDIF
		
		;상층의 분기를 모두 묻었을 경우는 불필요한 개소
		;반대로 묻지 않는 경우는 이 개소가 보완이 책임을 다합니다
		;-------------------------------------------------
		;그 외·조건 없음
		;기입 체크
		LOCAL:2 = 1
		;-------------------------------------------------
		IF LOCAL:2
			PRINTFORMW 「초조하게 하도록(듯이), 라고 하는 것도 나쁘지 않네요」
			PRINTFORMW 「평상시, 애태움이라고 있을 뿐이기 때문에……」
			RETURN LOCAL:2
		ENDIF
	ENDIF
	
	;=================================================
	;2매 벗었다(적극적 반응)
	;기입 체크
	LOCAL = 1
	;=================================================
	IF LOCAL && IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;-------------------------------------------------
		;전라가 되었다
		;기입 체크
		LOCAL:2 = 1
		;-------------------------------------------------
		IF LOCAL:2 && (TEQUIP:MASTER:악세사리 + TEQUIP:MASTER:양말 + TEQUIP:MASTER:하반신속옷 + TEQUIP:MASTER:상반신속옷 + TEQUIP:MASTER:하반신윗도리 + TEQUIP:MASTER:상반신윗도리 + TEQUIP:MASTER:전신윗도리) == 0
			PRINTFORMW 「뭐……♪」
			PRINTFORMW 「건강의 좋은 일……」
			PRINTFORMDW 아큐는%CALLNAME:MASTER%의 사타구니의 근처를 응시하면서, 킥킥웃고 있다.
			RETURN LOCAL:2
		ENDIF
		
		;-------------------------------------------------
		;적극적으로 벗어 던졌다
		;기입 체크
		LOCAL:2 = 0
		;-------------------------------------------------
		IF LOCAL:2 && IS_COMGRONAME("적극적으로 한다")
			PRINTFORMW 
			RETURN LOCAL:2
		ENDIF
		
		;-------------------------------------------------
		;솜씨 좋게 벗어 던졌다
		;기입 체크
		LOCAL:2 = 0
		;-------------------------------------------------
		IF LOCAL:2 && IS_COMGRONAME("태연하게 한다")
			PRINTFORMW 
			RETURN LOCAL:2
		ENDIF
		
		;상층의 분기를 모두 묻었을 경우는 불필요한 개소
		;반대로 묻지 않는 경우는 이 개소가 보완이 책임을 다합니다
		;-------------------------------------------------
		;그 외·조건 없음
		;기입 체크
		LOCAL:2 = 1
		;-------------------------------------------------
		IF LOCAL:2
			PRINTFORMW 「후후, 빨리 개방해 받고 싶습니까?」
			PRINTFORMW 「그리고 아주 조금의 인내예요……」
			RETURN LOCAL:2
		ENDIF
	ENDIF
	
	;=================================================
	;벗지 않았다
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_COMGRONAME("거부")
		;부끄러워서 벗겨지지 않았다
		IF PALAM:MASTER:치정 > PALAM:MASTER:반항
			PRINTFORMW 
		;싫어해 벗지 않았다
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
ENDIF


;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_1, 옷을 입도록(듯이) 명령
