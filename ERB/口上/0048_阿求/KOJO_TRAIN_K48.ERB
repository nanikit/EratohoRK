IF LOCAL && (TFLAG:전회질내사정 == 6 || TFLAG:전회질내사정 == 8)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;뽑지않고에 속행
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:뽑지않고
		;동시절정(조교자가 동시에 a절정)
		IF NOWEX:A절정
				;그 외
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 
						CASEELSE
							PRINTFORMW 
					ENDSELECT
					RETURN LOCAL
		;통상
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 
				;조루
				CASE 4
					PRINTFORMW 
				;그 외
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL
		ENDIF
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;동시절정(조교자가 동시에 a절정)
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:A절정
		SELECTCASE NOWEX:MASTER:사정
			;드라이사정
			CASE 1
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
			;그 외
			CASEELSE
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
		ENDSELECT
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;초조하게함으로부터
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:MASTER:초조하게해번
		;초조하게 해 안
		IF TCVAR:MASTER:초조하게해번 > 1
			SELECTCASE NOWEX:MASTER:사정
				;A책사정
				CASE 5
					PRINTFORMW 
				;그 외(조루)
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		;결정타
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 
				;그 외
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;사정의 파생
	SELECTCASE NOWEX:MASTER:사정
		;드라이사정
		CASE 1
			PRINTFORMW 
		;대량사정
		CASE 3
			PRINTFORMW 
			RETURN LOCAL
		;조루
		CASE 4
			PRINTFORMW 
		;A책사정
		CASE 5
			PRINTFORMW 
		;그 외(사정)
		CASEELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL
	ENDSELECT
	RETURN 2
ENDIF

