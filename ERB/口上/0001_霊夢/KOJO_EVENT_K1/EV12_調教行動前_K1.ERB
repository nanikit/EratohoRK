;========================================================================
;EV12, 調教者行動前口上
;========================================================================
;・TRAIN_PROCCES.ERB内、@TRAIN_CANCELから呼び出される
;・口上主のパラメータ表示の直後に表示。調教指示は既に決定されている
;・調教中止宣言の差し替えや、調教ターン開始前にご主人様から一言、のように書くことも可能
;・午前、午後の調教開始ではなく、調教ターンの開始前には毎回呼び出される箇所
;・その為もあって、調教中止時と経過時間0以外の箇所はIF 口上記入チェック(25)等として確率表示する事を推奨
;・調教中止時にのみ地の文が存在する
;・文章表示後に本体側のDRAWLINE処理あり
;-------------------------------------------------
;FIRSTEVENT(0)～や、FIRSTEVENT(10)～などとして枠毎に割り当てていますが
;FIRSTEVENT/ONCEEVENT/RANDEVENT/ORDEREVENTは事実上無限にイベント番号を増やせるので
;必要に応じて任意に割り当てて下さい
;-------------------------------------------------
@KOJO_EVENT_K1_12(ARG)
;-------------------------------------------------
;引数内訳…
;ARG(0=調教可/1=口上主の疲労で中止/2=調教対象の疲労で中止/3=ご褒美による中止)
;-------------------------------------------------

;────────────────────────────────────
;調教中止 ARG:1 ARG:2 ARG:3 FIRSTEVENT(0)～
;────────────────────────────────────
;・疲労、もしくはご褒美による調教中止の場合の口上
;・記入チェック=1の場合は口上表示後本体側でPRINTLの空行が入る
;・記入チェックを=2にすると、地の文を表示しない
;・というかRESULT > 2で判定されるので2以上なら同じ挙動になりますが一応非推奨ということで
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ご褒美による中止
;RESULT値（1, 口上の後に地の文を表示(非推奨)、2, 口上のみ表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 3
	;記入チェックを2以下にした場合は以下の地の文が表示される。表示タイミングは口上表示の後
	;%CALLNAME:TARGET%は今日の調教を中止し、
	;%CALLNAME:MASTER%とまったりとした時間を過ごした…
	
	SELECTCASE TIME
		;昼
		CASE 0
			;腰掛ける描写は無い。既に隣に座っている。
			PRINTFORMDW 休憩中、%CALLNAME:MASTER%の隣に座る%CALLNAME:TARGET%がポンポンと膝を叩き誘惑してきた…
			PRINTFORMW 「ほら、こっちきなさい」
		;夜
		CASEELSE
			PRINTFORMDW %CALLNAME:TARGET%は頬杖をついて、食事を取る%CALLNAME:MASTER%を見つめている…
			SELECTCASE RAND:3
				CASE 0
					;なにもなし
				CASE 1
					PRINTFORML
					PRINTFORMDW %CALLNAME:TARGET%が視線を窓に向ける
					PRINTFORMW 「今夜は月見酒ね」
					;言わせない方が良い味出そう
					;PRINTFORMW 「あんたにも付き合って貰うわよ。お晩酌」
				CASEELSE
					PRINTFORML
					PRINTFORMW 「何か付いてるのかって？」
					PRINTFORMW 「ごはんつぶ、付いてるわよ」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;調教者の疲労による中止
;RESULT値（1, 口上の後に地の文を表示(非推奨)、2, 口上のみ表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 1
	;記入チェックを2以下にした場合は以下の地の文が表示される。表示タイミングは口上表示の後
	;調教の時間が来たが、入ってきた%CALLNAME:TARGET%はなにやら疲れている様子で、
	;今回は休みだと言って出ていった…
	
	;基本的に「インデント」は「ネスト1階層を意味する」という論理的な意味を持っていることが条件、と考えます@/L
	SELECTCASE TIME
		;昼間
		CASE 0
			PRINTFORMW 「今日は中止にしましょ」
			PRINTFORML
			PRINTFORMDW %CALLNAME:TARGET%は居間でゴロゴロしている…
			PRINTFORMDW %CALLNAME:MASTER%は思いつくままにお煎餅を手に取り、%CALLNAME:TARGET%の口元に近づけてみた…
			PRINTFORML
			;米食動物 あざらしのようにねそべり奈良の鹿のように横暴に食らう
			PRINTFORMW もっ！…ぱりっ…ぽり…もっ…もっ……
			PRINTFORML
			PRINTFORMDW %CALLNAME:MASTER%は欲望に衝き動かされるまま、次なるお煎餅に手を伸ばした…
			;夜が更けるに繋がる
		;夜
		CASEELSE
			PRINTFORMDW %CALLNAME:TARGET%は部屋から出ると、すぐさま枕を抱えて戻ってきた
			PRINTFORML 
			PRINTFORMW 「休みとは言ったけど、相手はしてもらうわよ」
			PRINTFORMW 「あんたの腕、貸してもらうわね」
			PRINTFORML 
			PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕を枕にして眠ってしまった
	ENDSELECT
	;口上のみ表示
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;調教対象の疲労による中止
;RESULT値（1, 口上の後に地の文を表示(非推奨)、2, 口上のみ表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 2
	;記入チェックを2以下にした場合は以下の地の文が表示される。表示タイミングは口上表示の後
	;調教の時間が来たが、%CALLNAME:TARGET%は酷い様子の%CALLNAME:MASTER%を見てしばらく考えると、
	;今回は休んでいいと言って出ていった…
	
	;上の地の分は実際には口上の下に表示される
	SELECTCASE TIME
		;昼間
		CASE 0
	PRINTFORMDW 調教の時間が来たが、%CALLNAME:TARGET%は酷い様子の%CALLNAME:MASTER%を見てしばらく考えると、今回は休んでいいと言って出ていった…
	
			PRINTFORMDW %CALLNAME:TARGET%は部屋から出ると、お盆を手に戻ってきた
			PRINTFORML
			PRINTFORMW こぽこぽ…
			PRINTFORML
			PRINTFORMDL ずぞぞっ…
			PRINTFORMW ずずっ…
			PRINTFORML 
			PRINTFORMDW そして、二人並び茶をすする
		;夜
		CASEELSE
			PRINTFORMDW %CALLNAME:TARGET%は部屋から出ると、すぐさま枕を抱えて戻ってきた
			PRINTFORMW 「休んでもいいとは言ったけど、相手はしてもらうわよ」
			PRINTFORML 
			PRINTFORMDW %CALLNAME:MASTER%は抱き枕にされてしまった
			PRINTFORML 
			PRINTFORMW 「次は、今回の分もまとめて全部搾り取ってやるんだから」
			PRINTFORMW 「覚悟しておきなさいよ」
	ENDSELECT
	;口上のみ表示
	RETURN 2
ENDIF
	
;-------------------------------------------------
;・以下、ARG:0(調教可)相当
;・TRAIN_PROCCES.ERB内、@BEFORE_ACTから呼び出される
;・この時点でACTはすでに決定しているがACT派生の決定はなされていない
;・NOWEXのリセットはなされていない
;・これらの箇所を抜けた後にACT開始になる
;・分岐の増減は自由にできますが、優先度には注意しましょう
;-------------------------------------------------

;────────────────────────────────────
;調教開始直後 FIRSTEVENT(10)～
;────────────────────────────────────
;・経過時間が0である場合に表示される
;・その回の調教開始直後に一度きりで、以後その調教中には表示されない
;・この箇所を抜けた後にACT開始になる
;-------------------------------------------------
;　ここでIS_POLICY("今日", "お仕置き")等で今日の方針を喋らせる事も可能だったりしますが
;　EVENT17, 本日の方針口上と被る可能性があります
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;調教開始直後
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:経過時間

	;=================================================
	;できちゃった　孕ませてから産むまで物足りない為展開強化
	;=================================================
	IF 口上記入チェック(0) && CFLAG:妊娠 == 10
		PRINTFORML 「そろそろ聞こえるんじゃないかしら」
		PRINTFORML 
		PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%にお腹に耳を当てさせた
		RETURN 1
	ENDIF

	IF 口上記入チェック(100) && CFLAG:妊娠 == 4
		PRINTFORMDL %CALLNAME:TARGET%の表情がにまにまと緩みに緩みきっている
		PRINTFORML 「えへへへへへ～♪」
		PRINTFORMW 「あとで教えてあげるわ%UNICODE(0x2661)%」
		RETURN 1
	ENDIF

	;=================================================
	;調教者が少し疲れている
	;=================================================
	IF 口上記入チェック(0) && CFLAG:消耗 > 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ARG:3の分岐を書いた場合は不要な箇所
	;=================================================
	;ご褒美による調教中止の場合
	;=================================================
	IF IS_POLICY("今日", "休憩")
		;消し忘れ
		;PRINTFORMW てすと　休憩
		RETURN 1
	ENDIF
	
	;ARG:1の分岐を書いた場合は不要な箇所
	;=================================================
	;調教対象の疲労による中止
	;=================================================
	IF 口上記入チェック(0) && CFLAG:MASTER:消耗 > 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象が少し疲れている
	;=================================================
	IF 口上記入チェック(0) && CFLAG:MASTER:消耗 > 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象への好感度1000以上
	;=================================================
	IF 口上記入チェック(0) && AFFECTION("眷愛")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;お仕置き FIRSTEVENT(20)～
;────────────────────────────────────
;・TFLAG:お仕置きフラグが立っている場合に表示される箇所
;・お仕置きフラグが立っていても、加虐系以外のACTになる場合がある
;-------------------------------------------------
;加虐系ACT、派生する事により加虐系に含まれる可能性のあるイラマチオ
;衣装の変更を要求する/不気味に笑う/アナルバイブ/後背位/アナルセックス/自慰/野外プレイ/放尿/足舐め強制
;異常系ACT/性交奉仕系ACT
;上記のACT以外はTFLAG:お仕置きフラグによってACT_ABLEで規制される(0616版時点)
;-------------------------------------------------
;-------------------------------------------------
;・一回の調教で一回のみ表示される
;━━━━━━━━━━━━━━━━━━━━━━━━
;お仕置き開始 
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:お仕置きフラグ && ONCEEVENT(20)
	;=================================================
	;前回ACTがお仕置き宣言
	;=================================================
	IF 口上記入チェック(0) && IS_PREVACTNAME("お仕置きと宣言する")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(100)
		PRINTFORMW 「少しお灸をすえてあげるわ！」
		PRINTFORMDW %CALLNAME:TARGET%がお祓い棒を構える
		RETURN 1
	ENDIF
ENDIF

;-------------------------------------------------
;お仕置き開始以外を処理する箇所
;━━━━━━━━━━━━━━━━━━━━━━━━
;お仕置き中
;口上記入チェック(確率％, 入力した数字の確率で口上を表示)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && TFLAG:お仕置きフラグ
	;=================================================
	;前回ACTがお仕置き宣言かつ今回ACTが加虐系
	;=================================================
	IF 口上記入チェック(0) && MENUMATCH(TFLAG:ACT, "加虐") && IS_PREVACTNAME("お仕置きと宣言する")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前回ACTがお仕置き宣言
	;=================================================
	IF 口上記入チェック(0) && IS_PREVACTNAME("お仕置きと宣言する")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;膣内、腸内射精直後 FIRSTEVENT(30)～
;────────────────────────────────────
;NOWEX:射精ないしNOWEX:MASTER:射精で射精種の参照も可能だったりしますが
;EVENT20, 絶頂口上に同種の分岐がると被る可能性もあります
;-------------------------------------------------
;関連変数…
;
;TFLAG:前回中出し(1=Ｖ性交/2=Ｖ性交奉仕/3=Ｖ性交(空射)/4=Ｖ性交奉仕(空射)/5=Ａ性交/6=Ａ性交奉仕/7=Ａ性交(空射)/8=Ａ性交奉仕(空射)
;NOWEX:射精(1=空射精/2=射精/3=大量射精/4=射精(早漏)/5=A責射精)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;膣内射精直後(調教対象→調教者)
;口上記入チェック（確率％, 入力した数字の確率で口上を表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
;-------------------------------------------------
;次のコマンドとの合間。事後。
;この時点では挿入が解除されている場合がある。
;-------------------------------------------------
IF 口上記入チェック(100) && GROUPMATCH(TFLAG:前回中出し, 2, 4)
	;=================================================
	;妊娠中膣内射精
	;=================================================
	;CFLAG:TARGET:妊娠は調教中も立つので不自然さが否めない TALENTに修正
	IF 口上記入チェック(100) && TALENT:TARGET:妊娠
		PRINTFORMW 「あんたの底無しの性欲は、私%RAND_SPLIT("がしっかりと/が全部/のカラダで全部")%、%RAND_SPLIT("受け止めて/面倒見て")%あげる%RAND_SPLIT("/わ")%」
		RETURN 1
	ENDIF
	;=================================================
	;空射精
	;=================================================
	IF 口上記入チェック(100) && TFLAG:前回中出し == 4
		IF V_SEX(MASTER)
		;Ｐアイテムが大きくなりました
			PRINTFORMDL %CALLNAME:TARGET%は笑顔で、息を乱す%CALLNAME:MASTER%の口に[Ｐ]を捻じ込んできた
			PRINTFORMW 「まだ、できるわよね？」
			RETURN 1
		ENDIF
	ENDIF
	;=================================================
	;膣内大量射精
	;=================================================
	IF 口上記入チェック(100) && NOWEX:射精 == 3
		PRINTFORML 「%RAND_SPLIT("一杯/盛大にどぷどぷ/随分沢山")%射精したわねぇ…」
		IF V_SEX(MASTER)
			PRINTFORM 「%RAND_SPLIT("これだけ出るなら、まだ射精そうよね/当たっちゃうかしら/そんなに孕ませたいのかしら/あんたの子種で一杯にされちゃった")%
			CALL HEARTBW
		ELSE
			;中出し後精液見せ付け ちんちんくてってして挿入解除分岐
			IF !RAND:2
				PRINTFORMW 「柔らかくなっちゃったわね」
				PRINTFORMW 「一仕事終えて、くてってしてるおちんちんも、可愛いわよ♪」
			ELSE
				PRINTFORM 「ほら？私のお・ま・ん・こ。あんたの%RAND_SPLIT("精液/子種")%で%RAND_SPLIT("真っ白/どろどろ/たぽたぽ")%
				CALL HEARTBW
			ENDIF
		ENDIF
		;妊娠中は抑制 上流に別分岐を作り
		SIF !RAND:4 && !CFLAG:TARGET:妊娠
			PRINTFORMDW ちゅるんと、%CALLNAME:MASTER%の精子が%CALLNAME:TARGET%の卵子に潜り込んだ
		RETURN 1
	ENDIF
	;=================================================
	;膣内早漏射精
	;=================================================
	IF 口上記入チェック(100) && NOWEX:射精 == 4
		PRINTFORMW 「早漏」
		PRINTFORMDW %CALLNAME:TARGET%が%RAND_SPLIT("いぢわるな/勝ち誇ったような/にまーっとした")%笑みを浮かべる
		RETURN 1
		;妊娠中は抑制
		SIF !RAND:10 && !CFLAG:TARGET:妊娠
			PRINTFORMDW ちゅるんと、%CALLNAME:MASTER%の精子が%CALLNAME:TARGET%の卵子に潜り込んだ
	ENDIF
	;=================================================
	;空射精以外
	;=================================================
	IF 口上記入チェック(100)
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「気持ち良かったかしら？本懐を遂げたような顔しちゃって」
				PRINTFORMW 「でも生憎、回避には自信があるのよ…だから、ね？」
			CASE 1
				PRINTFORML 「私のお腹の中で、あんたの精子と私の卵子が」
				PRINTFORM 「和姦中
				CALL HEARTBW
			CASE 2
				PRINTFORMW 「んふふ。赤ちゃんできるかしら？」
				PRINTFORMW 「できるといいわね」
			CASE 3
				PRINTFORMW 「もう一回、いいわよね？」
			CASEELSE
				PRINTFORM 「あんたのおちんちんは、私を孕ませる為にあるのよ
				CALL HEARTBW
		ENDSELECT
		;妊娠中は抑制
		SIF !RAND:5 && !CFLAG:TARGET:妊娠
			PRINTFORMDW ちゅるんと、%CALLNAME:MASTER%の精子が%CALLNAME:TARGET%の卵子に潜り込んだ
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;腸内射精直後(調教対象→調教者)
;口上記入チェック（確率％, 入力した数字の確率で口上を表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && GROUPMATCH(TFLAG:前回中出し, 6, 8)
	;=================================================
	;空射精
	;=================================================
	IF 口上記入チェック(0) && TFLAG:前回中出し == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
		
	;=================================================
	;空射精以外
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;膣内射精直後(調教者→調教対象)
;口上記入チェック（確率％, 入力した数字の確率で口上を表示）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GROUPMATCH(TFLAG:前回中出し, 1, 3)
	;=================================================
	;空射精
	;=================================================
	IF 口上記入チェック(0) && TFLAG:前回中出し == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;空射精以外
	;=================================================
	IF 口上記入チェック(100)
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「あかちゃん、できるといいわね♪」
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の下腹部をそっと撫で、囁いた
			CASE 1
				PRINTFORML 「あんたのおまんこでいかされちゃった」
				PRINTFORM 「気持ち良かったわよ
				CALL HEARTBW
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の頬に唇を当ててきた
