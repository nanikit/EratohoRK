;========================================================================
;EV13, 조교 종료 구상
;========================================================================
;·@EVENTCOMEND로부터 불려 간다
;·구상주가 조교 종료하기 직전에 표시
;·ARG:1, ARG:2의 경우는 지문 표시 후에 PRINTL로 공행. 그 뒤로 구상을 표시
;·ARG:0의 경우는 고유의 지문 없음. 구상 표시전에 PRINTL로 공행이 들어간다
;·RESULT 0이상을 돌려주면 구상 표시 후에 PRINTL로 공행이 들어간다
;-------------------------------------------------
;관련 변수…
;FLAG:일상제어 조교 종료시 상황(1=통상/2=정력 0/3=한번휴식/4=실신했다)
;-------------------------------------------------
@KOJO_EVENT_K1_13(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 보통으로 종료, 1, 조교 대상이 피곤한, 2, 조교 대상이 실신했다)
;-------------------------------------------------
IF TALENT:Ｖ민감 == 1
	PRINTFORML
	PRINTFORMDW 자 만들기 섹스에 반응하고 있었다%CALLNAME:TARGET%의 질은, 원래의 감도에 돌아왔다
	TALENT:Ｖ민감 = 0
ENDIF
IF TALENT:매혹 == 1
	PRINTFORML
	PRINTFORMDW %CALLNAME:TARGET%의 목욕마침 상태와 함께 매혹은 해제되었습니다
	TALENT:매혹 = 0
ENDIF

;────────────────────────────────────
;ARG:1, ARG:2, 피로·실신에 의한 조교 종료 FIRSTEVENT(0)～
;────────────────────────────────────
;·조교 대상 소모도에 응해 피로/실신으로 나누어진다
;·호출 타이밍은 지문의 표시→PRINTL→구상의 표시
;·EVENT201, 간호(일상 이벤트)의 개소를 쓰는 경우는, 능숙하게 제휴할 수 있도록(듯이)하면 좋을 것입니다
;------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 피곤함
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 1
	PRINTFORMW 「슬슬 끝맺을까요」
	PRINTFORMW 「넘어질 때까지 해 주었으면 했어? 그렇게 초조해 하지 않아도, 나는」
	CALL FADEIN(20, "「놓치지 않아요♪」")
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 실신했다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 2
	PRINTFORMDW %CALLNAME:TARGET%는 의식을 잃었다%CALLNAME:MASTER%를 메었다
	RETURN 1
ENDIF

;────────────────────────────────────
;굴복 종료  이번에 취득했는지? 를 참조하지 않으면 의미가 없다
;────────────────────────────────────
;【굴복:25】질내사정 취득 첫회
IF 구상기입체크(100) && GETBIT(CFLAG:각인, 25) && FIRSTEVENT(125)
	PRINTFORML 「저기. 어땠어? 무엇은 그」
	CALL FADEOUT(6, "  머뭇머뭇")
	PRINTFORML 「개만들기.」
	PRINTFORML 「개만들기 엣치, 좋아하게 될 수 있을 것 같다?」
	RETURN 1
ENDIF
;【굴복:11】키스 취득 첫회
IF 구상기입체크(100) && GETBIT(CFLAG:각인, 11) && FIRSTEVENT(111)
	CALL FADEOUT(4, "  등")
	PRINTFORMDL %CALLNAME:TARGET%는 행위를 끝맺으려고 하면서도,
	CALL FADEOUT(4, "  등")
	PRINTFORMDL 끊임없이%CALLNAME:MASTER%의 입가에 시선을 향하여 왔다…
	PRINTFORML 
	PRINTFORMDL 스스로의 입술에 손가락을 댄다%CALLNAME:TARGET%
	PRINTFORML 「입맞추고 한 것이니까, 이것으로 우리들[연인]군요」
	PRINTFORMW 「좋지요, 부부겸연인이야.」
	CALL FADEIN(6, "「응후후♪」")
	PRINTFORMW 「이봐요, 우리들은 그러한 시기가 없었잖아」
	PRINTFORML 
	PRINTFORMDW %CALLNAME:TARGET%는 조교를 끝맺으면 땀을 개수대에 욕실에 향했다…
	;안보이는 뒷모습으로 들리는 콧노래 튀는 발걸음에 맞추어 소녀 기상곡
	CALL FADEOUT(12, "「들 응등응등─들 -들―♪라라응등─들 -들♪」")
	RETURN 1
ENDIF
;────────────────────────────────────
;ARG:0보통으로 종료 FIRSTEVENT(10)～
;────────────────────────────────────
;·통상의 조교 종료시의 구상
;·지문 없음. PRINTL에 의한 공행의 뒤로 구상 표시
;·테두리마다 이벤트번호를 털어 있습니다만, 임의로 설정하는 일은 가능합니다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;대만족 보너스를 얻고 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:이번에대만족
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTEVENT(10)
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상의 정력 0
	;=================================================
	IF 구상기입체크(100) && FLAG:일상제어 == 2
		PRINTFORML 「오늘도 즐거웠어요」
		PRINTFORMDL %CALLNAME:TARGET%는 반질반질하면서%CALLNAME:MASTER%에 생긋 한 웃는 얼굴을 향했다
		RETURN 1
	ENDIF

	;=================================================
	;가능하게 되었다  임신하게 하고 나서 낳을 때까지 어딘지 부족하기 때문에 전개 강화
	;=================================================
	IF 구상기입체크(0) && CFLAG:임신 == 10
		PRINTFORMDL 
		RETURN 1
	ENDIF

	IF 구상기입체크(100) && CFLAG:임신 == 4
		PRINTFORMDW %CALLNAME:TARGET%는 체온계와 같은 것을 전해 왔다
		PRINTFORMDL 체온계와 같은 것에는 환에 종선이 2개 줄표시되고 있다…
		PRINTFORML 
		PRINTFORMDL 아무래도 내일과 모레는 개이는 것 같다…
		PRINTFORMDL %CALLNAME:TARGET%에는 햇빛이 잘 어울린다
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		RETURN 1
	ENDIF
	;무슨이야기였는지 망실
	;여기서 리턴 시키고 하부를 대만족 하고 있지 않는 %CALLNAME:TARGET%씨로 만들어 낸다
	RETURN 1
ENDIF

;*************************************************
;이하, 호감도에 의한 파생
;*************************************************

;EVENT11 근처에서 임의의 KFLAG를 세워 조건으로 해 준다고 이야기의 이치는 맞추기 쉽습니다
;━━━━━━━━━━━━━━━━━━━━━━━━
;상사상애（호의 3000이상·호감도 3000이상）
;-------------------------------------------------
;·서로 서로 좋아하고 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && AFFECTION("상사상애")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTEVENT(20)
		PRINTFORMDW %CALLNAME:MASTER%의 방을 나오려고 해, 한 번 뒤돌아 봐 입가를 피기 시작하게 한다
		PRINTFORML 「행복한 표정 해 버려.」
		RETURN 1
	ENDIF
	[SKIPSTART]
	;=================================================
	;후타나리 %CALLNAME:TARGET%씨가 대만족 하고 있지 않는가 개성교 굴복 획득이 끝난【굴복:22】질내사정 되었다
	;=================================================
		;=================================================
		;%CALLNAME:MASTER%의 처녀를 받아 기분
		;=================================================
		
		;=================================================
		;%CALLNAME:MASTER%가 남자 또한 애널 개발 끝난 경우 엉덩이를 사용된다
		;=================================================
		
	[SKIPEND]
	;=================================================
	;대만족 하고 있지 않는가 개성교 봉사가 끝난 상태
	;=================================================
	;%CALLNAME:TARGET%과 엣치한 일 있는 경우의 조건 설정이 생각해내지 못하기 때문에 임신하게 한 후의 포상에 자리 매김을 한다
	;대만족은 발생하지 않아도 매일밤 만족은 시킨다. 사티스파크션!
	IF 구상기입체크(100)
		PRINTFORML 「응. 여기로부터는 너가 마음대로 해도 좋아요」
		PRINTFORMDW %CALLNAME:TARGET%는 아직 만족하지 않는 것 같다…
		PRINTFORML 
		;%CALLNAME:MASTER%가 %CALLNAME:TARGET%에 반하고 있는 일을 암시
		PRINTFORMDL %CALLNAME:MASTER%는%CALLNAME:TARGET%가 만족할 때까지 천천히 후희를 즐겼다
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		RETURN 1
	ENDIF
ENDIF

;호감도의 성장적에 몹시 사랑함의 뒤가 된다고 생각됩니다
;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 완료（호의 2000이상）
;-------------------------------------------------
;·조교 대상이 구상 주로 이제(벌써) 헤롱헤롱
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && AFFECTION("조교 완료")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTEVENT(30)
		PRINTFORMDW 지금의 관계를 일방적으로 쌓아 올려지고 나서 상당히 길 때가 지난 것처럼 생각한다
		PRINTFORMDW 시원한 듯한%CALLNAME:TARGET%의 옆 얼굴만 보고 있던 이전을 돌아본다
		PRINTFORMDW 이 관계가 시작되고 나서, 다양한%CALLNAME:TARGET%의 표정을 볼 수가 있었다
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;몹시 사랑함（호감도 2000이상）
;-------------------------------------------------
;·조교자가 조교 대상에 빠져 버려, 깊기도 하고 도착 한 사랑을 따르고 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && AFFECTION("몹시 사랑함")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTEVENT(40)
		PRINTFORMDW 방을 나오려고 했다%CALLNAME:TARGET%가 되돌아 본다
		;여기는 당신 표기가 분위기가 나온다
		PRINTFORMW 「저기.%CALLNAME:MASTER%」
		;첫 번째는 (들)물어 놓칠 수도 있다.
		CALL FADEOUT(20, "「좋아해요」")
		;소중한 일인 것으로 2회째는 분명히.
		CALL FADEIN(20, "「좋아」")
		PRINTFORML
		PRINTFORMDW %CALLNAME:TARGET%는 그렇게 고하면%CALLNAME:MASTER%의 대답도 (듣)묻지 않고, 허둥지둥 가 버렸다
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교가 끝난 상태（호의 1000이상）
;-------------------------------------------------
;·조교 대상이 구상 주로 조교되는 것을 행복하게 느끼고 있다
