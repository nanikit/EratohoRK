	;=================================================
	;조교대상Ｃ사용
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TEQUIP:조교대상Ｃ사용, 1, 2)
		;자위·애무 계속중
		IF TEQUIP:조교대상Ｃ사용 == 1
			PRINTFORMW 
		;페라·쿤닐링구스 계속중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/! PENIS(MASTER)/TEQUIP:페니스밴드등으로 분기 해도 괜찮다
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(0) && TEQUIP:성교봉사중
		;체위에 의한 파생
		SELECTCASE TEQUIP:성교봉사중
			;정상위시킨다				크게 벌어진 가랑이의 사이에 맞아들이고
			CASE 1
				PRINTFORMW 
			;후배위시킨다				배후로부터 질내에 삽입되고
			CASE 2
				PRINTFORMW 
			;역강간					미끈미끈의 질로 큐우큐우 짜내고
			CASE 3
				PRINTFORMW 
			;대면좌위시킨다				체를 밀착시켜 질 안쪽으로 환영해
			CASE 4
				PRINTFORMW 
			;배면좌위시킨다				등을 맡겨 만끽해
			CASE 5
				PRINTFORMW 
			;애널섹스시킨다		장액에 미끌거리는 항문으로 억압
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자가 자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:자위중, 1)
		;조교자가 페니스밴드 몸에 걸치고 있다
		IF TEQUIP:조교자페니스밴드
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「오케이!%CALLNAME:TARGET%에 맡겨!」
		RETURN 1
