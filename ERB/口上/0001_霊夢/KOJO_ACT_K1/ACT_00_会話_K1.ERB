IF 口上記入チェック(100)
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;PENIS(TARGET)/!PENIS(TARGET)/TEQUIP:調教者ペニスバンド等で分岐してもよい
	;=================================================
	;性交中
	;=================================================
	IF 口上記入チェック(0) && TEQUIP:性交中
		;体位による派生
		SELECTCASE TEQUIP:性交中
			;正常位				体重を預けて%CALLNAME:MASTER%の膣内に埋め
			CASE 1
				PRINTFORMW 
			;後背位				背後から%CALLNAME:MASTER%の膣内に埋め
			CASE 2
				PRINTFORMW 
			;騎乗位				%CALLNAME:MASTER%の雌穴に根元まで咥え込まれ
			CASE 3
				PRINTFORMW 
			;対面座位			密着した%CALLNAME:MASTER%の膣に愛撫され
			CASE 4
				PRINTFORMW 
			;背面座位			%CALLNAME:MASTER%に挿入し背を抱き締め
			CASE 5
				PRINTFORMW 
			;アナルセックス		%CALLNAME:MASTER%の肛門に埋め
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;調教対象のＣへの愛撫が行われている状況。TEQUIP管理なので可能性は割りとある
	;=================================================
	;調教対象Ｃ使用
	;=================================================
	IF 口上記入チェック(0) && GROUPMATCH(TEQUIP:調教対象Ｃ使用, 1, 2)
		;手淫・愛撫継続中
		IF TEQUIP:調教対象Ｃ使用 == 1
			PRINTFORMW 
		;フェラ・クンニ継続中
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/!PENIS(MASTER)/TEQUIP:ペニスバンド等で分岐してもよい
	;=================================================
	;性交奉仕中
	;=================================================
	IF 口上記入チェック(0) && TEQUIP:性交奉仕中
		;体位による派生
		SELECTCASE TEQUIP:性交奉仕中
			;正常位させる				大きく開いた股の間に迎え入れ
			CASE 1
				PRINTFORMW 
			;後背位させる				背後から膣内に挿入され
			CASE 2
				PRINTFORMW 
			;逆レイプ					ぬるぬるの膣できゅうきゅうと絞り上げ
			CASE 3
				PRINTFORMW 
			;対面座位させる				体を密着させて膣奥に歓迎し
			CASE 4
				PRINTFORMW 
			;背面座位させる				背を預けて堪能し
			CASE 5
				PRINTFORMW 
			;アナルセックスさせる		腸液にぬめる肛門で締め付け
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者が自慰中
	;=================================================
	IF 口上記入チェック(0) && GETBIT(TEQUIP:自慰中, 1)
		;調教者がペニスバンド身に付けている
		IF TEQUIP:調教者ペニスバンド
			PRINTFORMW
		;その他
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(100)
		PRINTFORMW 「おっけー！%CALLNAME:TARGET%に任せてっ！」
		RETURN 1
	ENDIF
ENDIF