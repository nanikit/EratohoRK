			;초조하게함 안
			CASE IS > 1
				PRINTFORMDW %CALLNAME:TARGET%는%CALLNAME:MASTER%의 표정을 보자, 봉사를 멈추어 버렸다
				PRINTFORMDW %CALLNAME:TARGET%의 눈은 심술궂은 것 같게 웃고 있다…
			;초조하게함 종료
			CASE 1
				IF IS_CONDITION(TARGET, "정욕")
					CALL FADEOUT(8, "(내, 내)")
					[SKIPSTART]
					;말하게 하지 않는 것이 좋은 것 같다
					PRINTFORM 「키─해 먹여
					CALL HEARTBW
					[SKIPEND]
				ELSE
					CALL FADEOUT(12, "총… 츄파… 쥿… 쥿…")
					PRINTFORMW 「마셔 주기 때문에, 사정해 버리세요」
					CALL FADEOUT(6, "「아뭇!」")
					PRINTFORMDW %CALLNAME:TARGET%는%CALLNAME:MASTER%의 음경을 깊게 물면 조금 강하게 들이마셔 세웠다…
					IF !RAND:3
						PRINTFORMDW 가지고 갈 곳이 곤란한다%CALLNAME:MASTER%의 시선은%CALLNAME:TARGET%의 가는 어깨에 빨려 들여갔다
						PRINTFORMDW 그 가는 선은%CALLNAME:MASTER%안의수컷에게%CALLNAME:TARGET%의 암컷을 의식시켰다…
					ELSEIF !RAND:2
						PRINTFORMDW %CALLNAME:TARGET%의 요염한 흑발이%CALLNAME:MASTER%의 시선을 붙잡는다…
					ELSE
						PRINTFORMDW 일심 불란에 육봉을 들이마셔 세운다%CALLNAME:TARGET%의 표정은 긴 머리카락에 덮여 안보인다…
					ENDIF
				ENDIF
			;초조하게함 없음
			CASEELSE
				PRINTFORMDW %CALLNAME:TARGET%는 리본을 흔들면서%CALLNAME:MASTER%의 음경을 목 안까지 사용해 봉사하고 있다…
		ENDSELECT
		RETURN 1
	ENDIF
	
	;갈것같다는 상류에 있는+3회 연속은 우선 나오지 않기 때문에, 다른 플레이로 이행해 사정시키는 일을 의식하는 것이 좋은 것인지도 모른다
	;가게 할 생각이 아닌 형태도 성격이 잘 맞을지도 모른다
	;=================================================
	;페라 연속 실행
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;안면승마 안
		;-------------------------------------------------
		IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;연속 실행 페라 %CALLNAME:MASTER%정욕
		;-------------------------------------------------
		IF 구상기입체크(50) && IS_CONDITION(MASTER, "정욕")
			CALL FADEOUT(8, "(이상한 소리 내 버려)")
			CALL FADEIN(8, "(사랑스럽기 때문에)")
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;연속 실행 페라 %CALLNAME:TARGET%정욕
		;-------------------------------------------------
		IF 구상기입체크(50) && IS_CONDITION(TARGET, "정욕")
			[SKIPSTART]
			;병콜라가 존재하므로 구형 패키지라고 강변 하면 캇빠 새우 선도 환상향에 존재할 수 있다. 플레이어씨가 곤란하므로 없음
			PRINTFORMW (아─… 그만두고들 응 없어요 이것…)
			PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%의 캇빠 새우 선사이즈의 페니스에 달라붙고 있다…
			;비명과의 제휴를 생각하면 없이는 없지만, 아프기 때문에 없음
			PRINTFORMW (그다지 맛있지 않네요… 씹으면 육즙 나올까…?)
			[SKIPEND]
			PRINTFORMDW %CALLNAME:TARGET%는 맛있을 것 같게%CALLNAME:MASTER%의 음경을 맛보고 있다
			RETURN 1
		ENDIF
		
		;연속 실행의 네스트를 개방한다면 여기는 반드시 써 둡시다
		;-------------------------------------------------
		;그 외
		;-------------------------------------------------
		IF 구상기입체크(100)
			PRINTFORML 「계속해요? 아뭇…」
			PRINTFORMDW %CALLNAME:TARGET%는 헌신적으로%CALLNAME:MASTER%의 일물을 입으로 봉사하고 있다…
			RETURN 1
		ENDIF
	ENDIF
	
	;-------------------------------------------------
	;안면승마 안
	;-------------------------------------------------
	IF 구상기입체크(100) && TRUECHECK(TEQUIP:안면승마) > 0
		PRINTFORMW 「나의%RAND_SPLIT("/보지/소중한 곳/저기")%로 자지 이렇게 해 버려…」
		CALL FADEOUT(4, "「응후후」")
		CALL FADEIN(12, "「어쩔 수 없네요」")
		PRINTFORMW 「나로 이렇게 한 것이니까… 책임과 간장은 배달시켜 주지 않으면…%RAND_SPLIT("/안 돼요")%군요」
		;앵글적으로 %CALLNAME:MASTER%에는 안보이기 때문에 없음
		;PRINTFORMDW %CALLNAME:TARGET%는 기쁜듯이 %CALLNAME:MASTER%의 음경을 물었다
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;LOWERJACKET(1) == 1 「」LOWERUNDER(1) == 1 「비켜 놓아」등으로 확장하는 것도 좋을 것입니다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		;지문
		;%CALLNAME:TARGET%는 %CALLNAME:MASTER%의 페니스를 잡아 천천히 상하시키고 있다…
		;%CALLNAME:TARGET%는 %CALLNAME:MASTER%의 사타구니에 얼굴을 묻어 터질듯이 노장 한 남성기를 입으로애무하기 시작했다…
		;구상 여기
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMDW %CALLNAME:TARGET%가%CALLNAME:MASTER%의 가랑이에 얼굴을 묻으면, 늘어지는 흑발이%CALLNAME:TARGET%의 표정을 숨긴다
				PRINTFORMDW 안보이는 앵글이, 오히려%CALLNAME:MASTER%를 어지럽혔다
			CASEELSE
				PRINTFORMDW %CALLNAME:TARGET%가%CALLNAME:MASTER%를 높이려고 열심히 머리를 움직일 때마다, 리본이 팔랑팔랑춤추었다…
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;─
;ACT13, 애널애무
;─
;·애널애무/전립선자극/애널바이브 출납
;·애널바이브 장비중은, 애널바이브 출납에 파생
;·연속 실행의 경우, 전립선자극에 파생
;·갈것같다or쾌C가 5000이상의 경우, 조교자의 애무 스킬에 응해 전립선자극에 파생
;·발기도가 낮은 경우, 조교자의 애무 스킬에 응해 전립선자극에 파생
;·요점강부디기의 경우, 전립선자극에 파생
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·턴방침이 "소프트", "이상"이라면 발생하지 않는다
;·TEQUIP(삼각목마/페니스밴드/애널비즈) 장착중에는 발생하지 않는다
;·성교봉사중에는 발생하지 않는다
;-------------------------------------------------
@KOJO_ACT_K1_13
;-------------------------------------------------
;·TFLAG:ACT파생(0=애널애무/1=전립선자극/2=애널바이브 출납)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_0, 애널애무
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORML 「응… 아마 여기서 맞고 있네요」
		PRINTFORMDL %CALLNAME:TARGET%는 되어가는 대로 하게%CALLNAME:MASTER%의 국문을 꾸짖어 왔다
		RETURN 1
	ENDIF

	;=================================================
	;탄 떨어짐 또한 고추 서 있지 않다
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:발기도 <= 1000 && BASE:MASTER:사정 == 0
		PRINTFORML 「총알 떨어짐 하면 허락해 받을 수 있다고 생각했을까?」
		PRINTFORML 「%RAND_SPLIT("나는 아직 만족하지 않아요/좀 더 교제하세요/허락하지 않아요/허없·어·요♪")%」
		RETURN 1
	ENDIF

	;=================================================
	;안면승마 안
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;하반신 복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(0) && PANTSCHECK(1, 1)
		;하반신윗도리를 걷고
		IF LOWERJACKET(1) == 1
			PRINTFORMW 
		;속옷 너머로
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		SELECTCASE ABL:MASTER:A감각
			CASE 0 TO 2
				PRINTFORML 「아파? 괜찮아, 곧 좋아져요」
				CALL FADEOUT(4, "「… 아마」")
			CASEELSE
				PRINTFORMW 「엉덩이로 기분 좋아져 버리다니 너는%RAND_SPLIT("/사실")%」
				PRINTFORMW 「어쩔 수 없다%RAND_SPLIT("변태/와")%군요?」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_1, 전립선자극(애널애무)
;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 남자의 경우에만 파생
;후타나리의 전립선은 요도, 즉 질 위에 있다고 해, G스팟 자극에
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 1
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORMW 「아 여기군요? 대개 알아 왔어요」
		RETURN 1
	ENDIF
	;=================================================
	;발기 강제
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:발기도 <= 1000
		PRINTFORMW 「이봐요! 자지 서게 해 주세요!」
		PRINTFORMW 「…%RAND_SPLIT("너의 엉덩이는 편리하구나/너의 자지는 편리하구나/편리하구나 이것")%」
		RETURN 1
	ENDIF
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「응, 편리라고 할까 단순이라고 할까… 이것은 이것인 의미, 남자다운 것일까요?」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_2, 애널바이브 출납
;━━━━━━━━━━━━━━━━━━━━━━━━
;·애널바이브 장비중은, 애널바이브 출납이 된다
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 2
	;조교 대상이 남자
	IF TALENT:MASTER:남자
		PRINTFORMW 「엉덩이라고%RAND_SPLIT("는 히는 히 말한다/응 몸부림친다")%의 것은 좋지만…」
		PRINTFORMW 「헛된 공격 하면, 벌 해요」
		RETURN 1
	;그 외
	ELSE
		;PRINTFORMW 
		RETURN 0
	ENDIF
	;RETURN 1
ENDIF

;─
;ACT14, 애널빨고
;─
;·애널빨고
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·턴방침이 "소프트", "이상"이라면 발생하지 않는다
;·TEQUIP(애널바이브/애널비즈/관장기＋플러그/페니스밴드/삼각목마) 장착중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·TCVAR:MASTER:초조하게해번이 실로 되어 있는 경우는 발생하지 않는다
;·혀에 의한 전립선자극에의 파생은 「그런 곳까지 혀가 닿을 이유 없지 않은가www」라고 하는 일로 숙청되고 있습니다
;·현시점에서 ACT가 파생하는 일은 없지만, ACT 개변의 가능성에 대비해, TFLAG:ACT파생 == 0으로 지정
;-------------------------------------------------
@KOJO_ACT_K1_14
;-------------------------------------------------
;·ACT파생없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT14_0, 애널빨고
;━━━━━━━━━━━━━━━━━━━━━━━━
;구 음은 물소리에 두어 그다지 말하게 하지 않는다
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자가 자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:자위중, 1)
		;조교자가 페니스밴드 몸에 걸치고 있다
		IF TEQUIP:조교자페니스밴드
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마 안
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:자위중, 0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;하반신 복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(0) && PANTSCHECK(1, 1)
		;하반신윗도리를 걷고
		IF LOWERJACKET(1) == 1
			PRINTFORMW 
		;속옷 너머로
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

