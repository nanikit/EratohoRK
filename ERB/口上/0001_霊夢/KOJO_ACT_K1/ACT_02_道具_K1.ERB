	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT23_2, 애널비즈 추가
;━━━━━━━━━━━━━━━━━━━━━━━━
;·비즈의 추가는 1～3의 사이에 랜덤으로 정해진다
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:ACT파생 == 2
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		;비즈의 개수에 의한 파생
		SELECTCASE TEQUIP:애널비즈
			;4개, 혹은 5번째. 사양상, 5개 이상이 되는 일은 없다
			CASE IS >= 4
				PRINTFORMW 
			;세번째
			CASE 3
				PRINTFORMW 
			;그 외
			CASEELSE
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT24, Ｃ애무도구
;────────────────────────────────────
;·클리캡/오나홀의 장착, 해제
;·오나홀을 소지 또한 조교 대상으로 페니스가 있는 경우는 「오나홀」에 파생한다
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·턴방침이 "하드", "이상"의 경우는 발생하지 않는다
;·TEQUIP(삼각목마/페니스밴드) 장착중에는 발생하지 않는다
;·오나홀 장착은 발기도가 1000 이하(TCVAR:MASTER:발기도 < 1000)의 경우에 연주해져 발생하지 않는다
;·성교봉사중에는 발생하지 않는다
;-------------------------------------------------
@KOJO_ACT_K1_24
;-------------------------------------------------
;·TFLAG:ACT파생(0=클리캡 장착/1=클리캡 해제/2=오나홀 장착/3=오나홀 해제)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT24_0, 클리캡 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT24_1, 클리캡 해제
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이미 장비 하고 있는 경우, 클리캡 해제가 된다
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:ACT파생 == 1
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT24_2, 오나홀 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:ACT파생 == 2
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(1)
		;조교 대상이 동정
		IF TALENT:MASTER:동정
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「이런 것 좋아하는 것 같아요? 너는, 변태인걸…」
		PRINTFORMDW %CALLNAME:TARGET%는%CALLNAME:MASTER%의 음경에 소매를 씌워 왔다
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT24_3, 오나홀 해제
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이미 장비 하고 있는 경우, 오나홀 해제가 된다
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 3
	;=================================================
	;정액 서로 더러워져
	;=================================================
	IF 구상기입체크(100) && GET_STAIN("페니스", "정액", MASTER)
		SELECTCASE RAND:3
			CASE 0
				CALL FADEOUT(4, "「………」")
			CASE 1
				CALL FADEOUT(4, "「우와아…」")
			CASEELSE
				CALL FADEOUT(4, "「싫다…」")
		ENDSELECT
		;정액이라고 하는 단어를 모를 가능성이 부상했으므로 국물 표기（아이를 만드는 방법은 직감과 본능으로 이해하고 있다）
		PRINTFORMW 「뇌물에 국물이 모여, 하고 있다…」
		PRINTFORMW 「… 스스로 해 두어이지만」
		SELECTCASE RAND:3
			CASE 0
				CALL FADEOUT(12, "조금… 후회했어요…")
			CASEELSE
				CALL FADEOUT(12, "냄새… 잡힐까…")
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「응… 아무리 너가 변태에서도, 이것은 없네요…」
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT25, 니플캡
;────────────────────────────────────
;·니플캡 장착/해제
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·턴방침이 "하드", "이상"이라면 발생하지 않는다
;·니플캡이라고 말해도, 지문에서는 클립 로터라고 하는 취급
;-------------------------------------------------
@KOJO_ACT_K1_25
;-------------------------------------------------
;TFLAG:ACT파생(0=장착/1=해제)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT25_0, 니플캡 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT25_1, 니플캡 해제
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이미 장비 하고 있는 경우, 니플캡 해제가 된다
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:ACT파생 == 1
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT26, 페니스밴드
;────────────────────────────────────
;·페니스밴드장착/해제
;·조교자가 조교 대상으로 쌍두 페니스밴드장착 시키려고 하는 장면
;·조교 대상이 처녀라면, 처녀상실이 일어난다
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·TEQUIP:(애널비즈/애널바이브/관장기＋플러그/삼각목마) 장착중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·조교 대상으로 페니스가 있으면 발생하지 않는다
;·TEQUIP(바이브/클리캡)은 양립하지 않는 것으로서 해제된다
;-------------------------------------------------
@KOJO_ACT_K1_26
;-------------------------------------------------
;·TFLAG:ACT파생(0=장착/1=해제)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT26_0, , 페니스밴드를 장착시킨다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		;조교 대상이 처녀
		IF TALENT:MASTER:처녀
			PRINTFORMW 
		;그 외
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT26_1, 페니스밴드를 해제시킨다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && TFLAG:ACT파생 == 1
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF