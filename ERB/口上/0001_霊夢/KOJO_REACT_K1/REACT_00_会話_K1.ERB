	;自然な行動
	;=================================================
	;REACT負荷0, 質問にはっきり答えることができなかった
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "自然な行動"
		RETURN 1
	ENDIF
	
	;無理のない行動
	;=================================================
	;REACT負荷1, 歯切れ悪く質問に答えた
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理のない行動"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;無理をした行動
	;=================================================
	;REACT負荷2, 出掛かった言葉を飲み込み答えを濁した
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理をした行動"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;REACT負荷分岐を全て埋めたら不要な箇所
	;逆に上層の分岐を書かないのであればここで補完させましょう
	;=================================================
	;その他
	;=================================================
	IF 口上記入チェック(100)
		PRINTFORMW 「自分でもよく解ってないのかしら」
		RETURN 1
	ENDIF
ENDIF

