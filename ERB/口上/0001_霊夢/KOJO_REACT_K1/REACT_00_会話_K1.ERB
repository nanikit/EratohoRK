	;=================================================
	;妊娠 逆レイプで子供出来ちゃってどんな気持ちどんな気持ち もうすぐ生まれそう
	;=================================================
	IF 口上記入チェック(0) && TALENT:妊娠
		RETURN 1
	ENDIF
	;自然な行動
	;=================================================
	;REACT負荷0, 一緒に居られることが嬉しいと素直に口にした
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "自然な行動"
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「ふーん」
				PRINTFORML
				PRINTFORMW 「…おゆはん。期待していいわよ」
			CASEELSE
				PRINTFORMW 「私もよ」
				PRINTFORMW 「ずっと先の事だけど…私がおばあちゃんになっても、その言葉が聞きたいわ」
			ENDSELECT
		RETURN 1
	ENDIF
	
	;無理のない行動
	;=================================================
	;REACT負荷1, はっきりと今の気持ちを告げた
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理のない行動"
		PRINTFORMW 「ほんと？」
		PRINTFORMDW %CALLNAME:TARGET%は期待の籠った目で%CALLNAME:MASTER%を見つめている
		RETURN 1
	ENDIF
	
	;無理をした行動
	;=================================================
	;REACT負荷2, 機嫌を損ねないよう、取り繕って答えた
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		PRINTFORMW 「無理してるでしょ」
		PRINTFORML
		PRINTFORMW 「言霊ってあるのよ」
		PRINTFORMW 「その言葉、本当にしてあげるわ」
		RETURN 1
	ENDIF
	
	;REACT負荷分岐を全て埋めたら不要な箇所
	;逆に上層の分岐を書かないのであればここで補完させましょう
	;=================================================
	;その他
	;=================================================
	IF 口上記入チェック(0)
		RETURN 1
	ENDIF
ENDIF

