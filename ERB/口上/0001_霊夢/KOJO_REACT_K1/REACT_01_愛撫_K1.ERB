;────────────────────────────────────
;REACT27, 押し倒す・個別リアクション
;────────────────────────────────────
;TFLAG:ACT派生（=0,押し倒す/=1,押し倒し終了）
;-------------------------------------------------
;想定されるCOM
;会話系反応
;	COM:気弱く応答する/無愛想に応答する/愛想よく応答する/強気に応答する/許しを乞う/気持ち良くして
;苦痛系反応
;	COM:怯える
;-------------------------------------------------
@KOJO_REACT_K1_27
;-------------------------------------------------
;地の文はREACT分類(受け入れる/積極的従う, 平然と従う/許しを乞う/消極的従う)で分岐
;ACT派生毎の分岐はなしかつ、内容的にはACT派生0のもの
;ACT派生1の場合は整合性が崩れると思われる
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT派生0, 押し倒す
;━━━━━━━━━━━━━━━━━━━━━━━━
;この箇所は個別のCOM毎に記述するには適さないと思われるので
;REACT分類で分けておきます
;-------------------------------------------------
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;REACT分類受け入れる
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("受け入れる")
		PRINTFORMW 「抱き%RAND_SPLIT("つか/締め")%ないでよ…%RAND_SPLIT("恥ずかしいじゃない/苦しいでしょ/動けないじゃない")%」
		PRINTFORMW 「…%RAND_SPLIT("少し/ちょっと")%だけ%RAND_SPLIT("だからね/なんだからね？")%」
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT分類積極的従う/平然と従う
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("積極的従う/平然と従う")
		IF AFFECTION("相思相愛", TARGET)
			PRINTFORMW 「きゃ…」
			PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%を引き倒した
			PRINTFORMDW %CALLNAME:TARGET%はされるがままに%CALLNAME:MASTER%の胸の中に納まり硬直している
			PRINTFORML （あっ）
			PRINTFORM 「～～
			CALL HEARTBW
			PRINTFORMW 「……何すんのよ」
			PRINTFORMDW %CALLNAME:TARGET%は存分に%CALLNAME:MASTER%の%RAND_SPLIT("体温を交換して/腕の中を十分に堪能して/腕の中を楽しんで")%から抗議してきた
		ELSE
			PRINTFORMW 「きゃ…」
			PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%を引き倒した
			PRINTFORMDW %CALLNAME:TARGET%はされるがままに%CALLNAME:MASTER%の胸の中に納まり硬直している
			PRINTFORMW 「……何すんのよ」
		ENDIF
		RETURN 1
	ENDIF

	;=================================================
	;REACT分類許しを乞う
	;=================================================
	IF 口上記入チェック(100) && (IS_COMGRONAME("許しを乞う") || IS_NOWCOMNAME("怯える"))
		[SKIPSTART]
		;描写が淡泊すぎるのでコメントアウト
		;PRINTFORMW 「なんか…興奮してきたわ」
		;COM許しを乞うではなくREACT分類許しを乞う系。無言で怯える際に矛盾。コメントアウト
		;PRINTFORMW 「%RAND_SPLIT("涙声になっ/慌て")%ちゃって♪」
		[SKIPEND]
		[SKIPSTART]
		;描写の調整につきコメントアウト
		;押し倒す→怯える時、「観念して」が被るのでコメントアウト
		PRINTFORMW 「そんなに怯えなくても、何も取って食おうって訳じゃ…」
		PRINTFORMW 「無くもなかったわ」
		PRINTFORMW 「さあ！観念して食べられちゃいなさい！」
		[SKIPEND]
		PRINTFORMW 「そんなに怯えなくても、何も取って食おうって訳じゃ…」
		CALL FADEIN(8, "「無くもなかったわね」")
		RETURN 1
	ENDIF
;=================================================
	;REACT分類消極的従う
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("消極的従う")
		PRINTFORMW 「本当にやめて欲しい？それとも、やめて欲しくない？」
		RETURN 1
	ENDIF
;条件にヒットしなければ汎用リアクションに流す
RETURN 0
ENDIF

