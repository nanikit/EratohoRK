;
;	레이무 구상 탈의·추가 액션 구상 파일
;
;	조교자가 조교 지시 이외에 실시하는 행동의 파일
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;//잠정 사양@/L

;─
;탈의 액션 구상
;─
;·조교자 액션의 앞에 강제탈의나 자주탈의가 발생했을 때에 표시
;·이 구상의 시점에서는 아직 TEQUIP는 갱신되어 있지 않다
;·첫회 분기는 「처음 보았다/보였다」란 할 수 없다.
;  숍 메뉴로 벗게 했던 적이 있을지도 모르고, 조교의 흐름으로 완전하게 벗게 하지 않고서 노출만 하고 있는 경우도 있다.
;  어쩌면 조수였던 때에 본 적이 있을지도 모른다. 그러한 의미에서는 간이적인 것이다.
;  엄밀한 의미에서의 「초견」플래그를 준비할지 어떨지는 요망하는 대로라고 하는 일로.
;-------------------------------------------------
@KOJO_DATUI_K1
;-------------------------------------------------
;관련 변수…
;강제탈의:TFLAG:102  자주탈의:TFLAG:103
;=(-1, 강제탈의에 저항, 1, 윗도리, 2, 하반신윗도리, 3, 웃도리, 4, 상반신 전부, 5, 하반신 전부,
;  6, 액세서리, 7, 액세서리 이외 모두, 8, 전라까지, 9, 속옷만)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의에 저항된 FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 < 0
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTDATUI(0)
		RETURN 1
	ENDIF
	
	;=================================================
	;자신은 벗었다
	;=================================================
	IF 구상기입체크(0) && TFLAG:자주탈의
		PRINTFORMW 
		RETURN 1
	ENDIF
	;=================================================
	;발기중 지금 커지고 있기 때문에놈들
	;=================================================
	IF 구상기입체크(100) && PENIS(MASTER) && TCVAR:MASTER:발기도 >= 1000
		CALL FADEOUT(12, "「기를쓰고 되어 숨겨도」")
		PRINTFORMW 「이제(벌써)%RAND_SPLIT("발기해/크고/준비 성과/건강하게")%비치는 것, 옷 위로부터라도」
		CALL FADEIN(8, "「들키고 들키고야?」")
		CALL FADEOUT(8, "「쿠스」")
		RETURN 1
	ENDIF
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「그누누…」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;두 사람 모두 뭔가 벗은 FIRSTDATUI(10)
;------------------------------------------------
;·주로 섹스계 행동의 예비 동작으로 발생한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTDATUI(10)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자가[정욕]상태
	;=================================================
	IF 구상기입체크(0) && T_COND("정욕")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMDW 앞으로의 행위에 기대해인가,%CALLNAME:TARGET%는%CALLNAME:MASTER%를 힐끔힐끔 관찰하면서 옷을 벗어 갔다
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·액세서리만 FIRSTDATUI(20)
;------------------------------------------------
;·주로 양말. 그 밖에 장갑이나 소매 따위
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 == 6 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;전라가 되었다(벗기 전은 양말 이외 알몸이라든지 그러한 느낌)
	;=================================================
	IF 구상기입체크(0) && TFLAG:자주탈의 && ZENRACHECK(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORML 「이것은 방해가 되어요…」
		PRINTFORMDW %CALLNAME:TARGET%가 소매를 제외하면,%CALLNAME:MASTER%는 무심코 소리를 흘려 버렸다
		PRINTFORMW 「붙인 채로가 좋았을까?」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·만p FIRSTDATUI(30)
;------------------------------------------------
;·이번 탈의로 성기가 보이게 되었을 경우에 표시
;·팬티 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTDATUI(30)
		;낮
		;착오이기 때문에 수정
		;IF TIME == 1
		IF TIME == 0
		;밤
		ELSE
			PRINTFORMW 「빛은 끄지 않는 것이 좋네요」
		ENDIF
		PRINTFORMW 「이런 곳, 너이니까 보이기 때문에」
		PRINTFORMDW %CALLNAME:TARGET%의 치부에는 희미하게음모가 나, 그 수풀은 그녀가 어린 소녀는 아니고 한명의 여성인 것이라고 무언의 주장을 하고 있었다
		RETURN 1
	ENDIF
	
	;=================================================
	;응 이렇게 p
	;=================================================
	IF 구상기입체크(100) && PENIS(TARGET)
		PRINTFORMW 「이것으로 지금부터 어떻게 하셔 버릴까… 아네요?」
		PRINTFORMW %CALLNAME:TARGET%는 일물을 과시하면서,%CALLNAME:MASTER%에 추잡한 시선을 향하여 있다
		RETURN 1
	ENDIF
	
	;=================================================
	;벌써 가슴이 보이고 있다
	;=================================================
	IF 구상기입체크(100) && B_DATUICHECK(0) == 1
		SIF !TEQUIP:성교봉사중 && TCVAR:MASTER:발기도 >= 1000
			PRINTFORMDL %CALLNAME:TARGET%는 격분한다%CALLNAME:MASTER%의 사타구니를 응시하면서 속옷을 내려 갔다
			;PRINTFORMDL %CALLNAME:TARGET%는 맥동 하는 %CALLNAME:MASTER%의 사타구니를 응시하면서 속옷을 내려 갔다
			;PRINTFORML 「무엇 벗은 것 뿐으로 사정할 것 같게 되어 있어\@ TALENT:MASTER:조루? 조루 #  \@」
		SIF IS_CONDITION(MASTER, "정욕")
			PRINTFORML 「그렇게 물고 늘어지도록(듯이) 볼 수 있으면, 그, 곤란했군요」
		IF IS_CONDITION(TARGET, "정욕")
			PRINTFORML (젖고 있는 것 알아 버리네요)
			;PRINTFORMDW %CALLNAME:TARGET%의 비순으로부터 내퇴를 꿀이 탄다
			PRINTFORMDW %CALLNAME:TARGET%의 내퇴를 꿀이 탄다
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMDW 속옷을 내리는 그 일순간, 무심코 비밀의 틈에 정신을 빼앗긴다
		PRINTFORMW 「어떻게, 일까? … 어, 어떻게든 말하세요」
		IF TEQUIP:MASTER:자위중
			PRINTFORMW 「손, 격렬해지고 있다」
			IF IS_CONDITION(TARGET, "정욕")
				CALL FADEOUT(8, "(꿀꺽)")
				;우리 레이무씨는 초심이니까 쿠빠아가 어울리지 않는 것에 주의
			ELSEIF TCVAR:MASTER:갈것같다
				PRINTFORML 「그렇게 격렬하게 해 괜찮아? 읏, 뭔가 안돼 그렇구나.」
			ELSE
				PRINTFORML 
				PRINTFORMDW %CALLNAME:TARGET%는 손을 입가에 수신자명 무늬%CALLNAME:MASTER%의 자위를 응시하고 있다
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·말하는 p FIRSTDATUI(40)
;------------------------------------------------
;·이번 탈의로 젖가슴이 보이게 되었을 경우에 표시
;·브라 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && !TALENT:거유 && FIRSTDATUI(40)
		PRINTFORMDW 사라시아래의 2개의 부푼 곳은, 지금, 여성과 소녀의 경계에 있는 것 같다
		PRINTFORMW 「요즘이군요, 커져 왔어」
		;소스는 사나에 젖가슴이 싫은 여자는 있을까
		PRINTFORMW 「남자는 모두 큰 가슴을 좋아한다고 사나에가 말했지만, 너는 어때?」
		RETURN 1
	ENDIF
	
	;=================================================
	;벌써 성기가 보이고 있다
	;=================================================
	IF 구상기입체크(0) && P_DATUICHECK(0) == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	;=================================================
	;몹시 사랑함  사이에 두어 주는 정도의 진행 상태
	;=================================================
	IF 구상기입체크(100) && AFFECTION("몹시 사랑함")
		PRINTFORMW 「사이에 두었으면 좋아? 읏훗후… 어떻게 하지?」
		RETURN 1
	ENDIF
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「너에게 엣치한 눈으로 볼 수 있는 것은\@ AFFECTION("상사상애", TARGET) ? 기쁘지만 # 그렇지만도 않지만 \@… 역시 부끄러워요…」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·팬티 보이고 FIRSTDATUI(50)
;------------------------------------------------
;·이번 탈의로 팬티(캐릭터에 의해 드로워즈·트렁크스)가 보이게 되었을 경우에 표시
;·하반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTDATUI(50)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·브라 보이고 FIRSTDATUI(60)
;------------------------------------------------
;·이번 탈의로 브라(캐릭터에 의해 스포트브라·사라시)가 보이게 되었을 경우에 표시
;·상반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTDATUI(60)
		;당신을 말하게 하지 않는 편이 보다 좋다
		PRINTFORMDW 색기가 없다.
		PRINTFORMW 「시끄러운 바보」
		PRINTFORMW (색기가 있는 속옷… 앨리스 근처가 자세할까…)
		PRINTFORMDW 색기가 없다. 하지만 그것이 좋다…!
		PRINTFORMW (어떻게 한 것일까요. 이 바보 주인)
		RETURN 1
		
		;후일, 이라고 가루차응에 스포브라에 대해 상담하는 천연드 S레이무씨
		;가슴에 대한 상담받아 상태 코키 걷는이라고 가루차응
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·전라에 FIRSTDATUI(70)
;------------------------------------------------
;·「전라까지 벗었다」만이 아니고, 「지금 벗었으므로 전라가 되었다」라고 하는 의미
;·「윗도리 밖에 남지 않은 상태로 윗도리를 벗었다」등도 포함된다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMDW %CALLNAME:TARGET%는 수치에 홍조 하면서도, 한가닥의 실 감기지 않는 모습이 되었다
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·액세서리만 FIRSTDATUI(80)
;------------------------------------------------
;·주로 양말. 그 밖에 장갑이나 소매 따위
;·벗긴 중에 접어들지 않는다! 되기 십상인 것으로 이것만은 부위별 텐프레를 준비
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && TFLAG:강제탈의 == 6
	;=================================================
	;전라가 되었다
	;=================================================
	IF 구상기입체크(0) && ZENRACHECK(1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·고추 보여라 FIRSTDATUI(90)
;------------------------------------------------
;·이번 탈의로 남성기가 보이게 되었을 경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && PENIS(MASTER)
	;=================================================
	;지금 커지고 있기 때문에놈들
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:발기도 >= 1000
		PRINTFORMW 「너의 자지, 보여 받아요」
		PRINTFORMW 「… 건강하구나♪」
		RETURN 1
	ENDIF
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「이봐요, 좋으니까 벗으세요! 벗어라!」
		SIF IS_CONDITION(TARGET, "정욕")
			PRINTFORML 「벗어랏!!」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·그대로응 보여라 FIRSTDATUI(100)
;------------------------------------------------
;·이번 탈의로 여성기가 보이게 되었을 경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && !TALENT:MASTER:남자
	;=================================================
