;=================================================
;EV101, 衣装変更
;=================================================
;・ショップから衣装変更を行った場合に表示
;-------------------------------------------------
@KOJO_EVENT_K71_101(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=
;　一の位…（0, もう着てる、1, 着替えた、2, 恥ずかしくて着られない、3, 嫌がって着ない、9, 持ってない）
;　十の位…（0, 通常、10, 半裸、20, 全裸、30, 下着無し）
;　百の位…コスプレのために保留
;　千の位…（0, 調教者, 1000, 調教対象）
;　例（調教対象が恥ずかしがって全裸にできなかった＝1022）
;-------------------------------------------------

;-------------------------------------------------
;調教者が着替えた
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;通常になった
	IF ARG == 1
		PRINTFORMW 「ま、普通が一番」
		RETURN 2
	ENDIF
	
	;半裸になった
	IF ARG == 11
		PRINTFORMW 「にひひっ♪むらむらする？」
		RETURN 2
	ENDIF
	
	;全裸になった
	IF ARG == 21
		PRINTFORMW 「ま、手っ取り早いよね」
		RETURN 2
	ENDIF
	
	;……すーすーする
	IF ARG == 31
		PRINTFORMW 「チラリズム～♪ ほらほらどうよぉ」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;調教者が恥ずかしがって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;半裸だって恥ずかしい
	IF ARG == 12
		PRINTFORMW 「変態思考め…」
		RETURN 2
	ENDIF
	;普段から全裸はちょっと……
	IF ARG == 22
		PRINTFORMW 「これはー…ちょっと…」
		RETURN 2
	ENDIF
	;パンツが無いから恥ずかしいもん！
	IF ARG == 32
		PRINTFORMW 「…全裸より恥ずかしいんだけど」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;調教者が嫌がって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;半裸は嫌
	IF ARG == 13
		PRINTFORMW 「何で私が？」
		RETURN 2
	ENDIF
	;普段から全裸なんて嫌
	IF ARG == 23
		PRINTFORMW 「お前、立場分かってないんじゃないの」
		RETURN 2
	ENDIF
	;すーすーするのは嫌
	IF ARG == 33
		PRINTFORMW 「馬鹿にしてる？」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;調教対象を着替えさせた
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;小分岐
	;通常になった
	IF ARG == 1001
		PRINTFORMW 「ま、良いけど。」
		RETURN 2
	ENDIF
	;半裸になった
	IF ARG == 1011
		PRINTFORMW 「期待しちゃってる見たいね～♪」
		RETURN 2
	ENDIF
	;全裸になった
	IF ARG == 1021
		PRINTFORMW 「あははっ、恥ずかしく無いわけ？」
		PRINTFORMW 「奴隷の自覚が出てきたの？良い子良い子♪」
		RETURN 2
	ENDIF
	;……すーすーする
	IF ARG == 1031
		PRINTFORMW 「へ～…ほら、スカートめくってみなよ」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;調教対象が恥ずかしがって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;小分岐
	;半裸だって恥ずかしい
	IF ARG == 1012
		PRINTFORMW 「うーん…まだダメか」
		RETURN 2
	ENDIF
	;普段から全裸なんて恥ずかしい
	IF ARG == 1022
		PRINTFORMW 「もうちょい躾けてあげないとダメね」
		RETURN 2
	ENDIF
	;パンツがないから恥ずかしいもん！
	IF ARG == 1032
		PRINTFORMW 「残念～」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;調教対象が嫌がって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;小分岐
	;半裸は嫌
	IF ARG == 1013
		PRINTFORMW 「チッ…まぁいいわ」
		RETURN 2
	ENDIF
	;普段から全裸なんて嫌
	IF ARG == 1023
		PRINTFORMW 「まだ人間で居たいってわけ？」
		RETURN 2
	ENDIF
	;すーすーするのは嫌
	IF ARG == 1033
		PRINTFORMW 「ふーん…」
		RETURN 2
	ENDIF
ENDIF

;=================================================
;EV110, 魔力使用
;=================================================
;・魔力を消費して何らかの特典を購入したときに表示
;-------------------------------------------------
@KOJO_EVENT_K71_110(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(0, 魔力使用前、1, 調教技巧、2, 知識を得る前、3, 調合知識取得、4, 魅惑取得、5, 禁断の知識取得、6, 道具使い取得、
;	7, 技術を得る前、8, 舌使い取得、9, 器用な指先取得、10, 縛り上手、11, サド消去/サド取得、12, 汚れ無視取得)
;イベント口上：アイテム
;引数内訳…
;ARG=(13, アイテム練成前、14, ローター練成、15,バイブ練成、16, アナルビーズ練成、 17, ペニスバンド練成、 18, アナルバイブ練成
; 19, クリキャップ練成、 20, ニプルキャップ練成、21, オナホール練成)
;イベント口上：責め具錬成
;引数内訳…
;ARG=(22, 責め具練成前、23, アイマスク練成、24, 鞭練成、 25, 縄練成、 26, ボールギャグ練成、27,  針練成、28, 浣腸セット練成
; 29, 三角木馬練成、 30, 大鏡練成、 31, 首輪練成、 32, 道具をカスタマイズした)
;その他イベント
;引数内訳…
;ARG=(33, ふたなり化/戻す、34, 巨乳化/戻す、35, 貧乳化/戻す、36, 精力の限界値上昇)
;-------------------------------------------------

;-------------------------------------------------
;魔力使用前
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 0
	PRINTFORMW 「くくくっ…久しぶりに力がみなぎる感じ…」
	PRINTFORMW 「アイツから搾り取った魔力だし、アイツにも還元してやらなきゃね…♪」
	RETURN 2
ENDIF

;-------------------------------------------------
;調教技巧がアップ
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 1
	;アライメント正
	IF ALI(0)
		PRINTFORMW 「さぁて、この力で…」
		PRINTFORMW 「ひんひん鳴かせてやるとするかなっ。にひひっ♪」
	;アライメント負
	ELSE
		PRINTFORMW 「さぁて、この力で…」
		PRINTFORMW 「どんな目に合わせてやろうか。くくっ、くくくくっ♪」
	ENDIF
	RETURN 2
ENDIF

