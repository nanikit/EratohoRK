@KOJO_COM_K71_20
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;상사상애
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(3000, 3000)
		SELECTCASE RAND:3
			CASE 0
					;@QUAKE 인수로 진동 회수를 변경 할 수 있다
					CALL QUAKE(4)
				PRINTFORMW 
					CALL QUAKE(4)
				PRINTFORMW 
			CASE 1
					CALL QUAKE(4)
				PRINTFORMW 
					CALL QUAKE(4)
				PRINTFORMW 
			CASEELSE
					CALL QUAKE(4)
				PRINTFORMW 
					CALL QUAKE(4)
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;몹시 사랑함
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 2000)
		IF LOCAL:1 && NOWEX:MASTER:사정
			SELECTCASE RAND:3
				CASE 0
						CALL QUAKE(4)
					PRINTFORMW 「좋아요, 그쪽이 그 기분이라면」
						CALL QUAKE(4)
					PRINTFORMW 「사익코의 사정을 시켜 주자가 아니다…♪」
				CASE 1
						CALL QUAKE(4)
					PRINTFORMW (아앗, 이런 찔리면서 사정되면…)
						CALL QUAKE(4)
					PRINTFORMW (여기도 위험할 정도, 쳐날아 버릴지도…♪)
				CASEELSE
						CALL QUAKE(4)
					PRINTFORMW 「(이)든지♪죽는다♪죽어버린다♪」
						CALL QUAKE(4)
					PRINTFORM 「나도 죽기 때문에, 너도 하지 않는닷
							CALL HEARTCB(RGBCOLOR(221,160,221))
					PRINTFORMW 」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
						CALL QUAKE(4)
					PRINTFORMW 「하아, 아핫… 그렇게 짜내지고 싶은거네…」
						CALL QUAKE(4)
					PRINTFORMW 「쿠응, 응응… 이봐요, 짜 버리는, …♪」
				CASE 1
						CALL QUAKE(4)
					PRINTFORMW 「아하앗… 여기로부터, 여기로부터…♪」
						CALL QUAKE(4)
					PRINTFORMW 「어디까지나… 트바 해 올린닷…♪」
				CASEELSE
						CALL QUAKE(4)
					PRINTFORMW 「좀 더, 좀 더……」
						CALL QUAKE(4)
					PRINTFORMW 「격렬하게 하세요, 이 고기 바이브……」
			ENDSELECT
			ENDIF
		RETURN LOCAL
	ENDIF 
	
	;-------------------------------------------------
	;연모
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 1000)
			CALL QUAKE(4)
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;일반
	IF LOCAL:1 && NOWEX:MASTER:사정
		IF RAND:2 == 0
			PRINTFORMW 「필사적으로 허리 흔들어… 갈 것 같다?」
			PRINTFORMW 「쿠쿳, 겨우 안쪽에 내던지고♪」
		ELSE
			PRINTFORMW 「갈 것 같다는 것은 자주(잘) 아는, 솔직한 자지구나♪」
			PRINTFORM 「좋아♪영혼마다 바칠 생각으로 사정하고
						CALL HEARTCB(RGBCOLOR(221,160,221))
			PRINTFORMW 」
		ENDIF
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「그래요, 좀 더 원숭이같이 허리 흔들어 주세요…」
			PRINTFORMW 「기분 좋고, 멈출 수 없지요?」
			PRINTFORMW 「그 앞을 지옥이라고 알아도… 말야. 에 히히♪」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「좀 더 오나홀 교체로 할 생각으로 좋아?」
			PRINTFORMW 「그 대상은… 쿠쿳♪」
		ELSE
			PRINTFORMW 「뭐, 그 헵피리허리♪」
			PRINTFORMW 「혹시 나를 기분 좋게 시키려고 하고 있는 거야?」
		ENDIF
	ENDIF
	RETURN LOCAL
ENDIF

