;記入チェック（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG:3
	SETBIT LOCAL:1, 3
	SELECTCASE ARG:3
		;反抗刻印Lv2
		CASE 2
			PRINTFORMW 
		;反抗刻印Lv5
		CASE 5
			PRINTFORMW 
		;反抗刻印Lv7
		CASE 7
			PRINTFORMW 
		;反抗刻印Lv10
		CASE 10
			PRINTFORMW 
		;表示しない
		CASEELSE
			CLEARBIT LOCAL:1, 3
	ENDSELECT
	SIF GETBIT(LOCAL:1, 3) && LOCAL == 1
		RETURN LOCAL
ENDIF

;-------------------------------------------------
;[屈服刻印]取得
;-------------------------------------------------
;記入チェック（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG:2
	SETBIT LOCAL:1, 2
	SELECTCASE ARG:2
		;屈服刻印Lv2
		CASE 2
			PRINTFORMW 
		;屈服刻印Lv5
		CASE 5
			PRINTFORMW 
		;屈服刻印Lv7…[反抗的]がある場合、それを失う
		CASE 7
			PRINTFORMW 
		;屈服刻印Lv10…[プライド高い]がある場合、それを失う
		CASE 10
			PRINTFORMW 
		;表示しない
		CASEELSE
			CLEARBIT LOCAL:1, 2
	ENDSELECT
	SIF GETBIT(LOCAL:1, 2) && LOCAL == 1
		RETURN LOCAL
ENDIF

;-------------------------------------------------
;[快楽刻印]取得
;-------------------------------------------------
;記入チェック（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG:1
	SETBIT LOCAL:1, 1
	SELECTCASE ARG:1
		;快楽刻印Lv2
		CASE 2
			PRINTFORMW 
			PRINTFORMW 
		;快楽刻印Lv5…[快感の否定]がある場合、それを失う
		CASE 5
			PRINTFORMW 
		;快楽刻印Lv7…[抑圧]がある場合、それを失う
		CASE 7
			PRINTFORMW 
		;快楽刻印Lv10…[快感に素直]がない場合、それを得る
		CASE 10
			PRINTFORMW 
		;表示しない
		CASEELSE
			CLEARBIT LOCAL:1, 1
	ENDSELECT
	SIF GETBIT(LOCAL:1, 1) && LOCAL == 1
		RETURN LOCAL
ENDIF

;刻印取得口上「すべて順に表示」のための終端処理
RETURN LOCAL:1 != 0

