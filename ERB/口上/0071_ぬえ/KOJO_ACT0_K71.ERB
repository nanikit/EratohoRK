;*************************************************
;調教アクション・コマンド口上
;*************************************************
;=================================================
;アクション口上 FIRSTTIME(100～199)
;=================================================
;・調教者アクションが発生したときに表示。小分岐の増減は自由に
;・「@KOJO_ACT_K71_(行動番号)」で行動ごとの関数を作成して書く。関数ごとコピペして改変を推奨
;・この後媚薬・ローションを使う場合は「媚薬等使用口上」が呼ばれるが、アクション口上の記入チェックを=2にすると呼ばれない
;・媚薬関連汎用口上を出したくない分岐に設定できる
;-------------------------------------------------
;↑セットで口上を書くことになるため、並べて設置↓
;=================================================
;個別リアクション口上 FIRSTTIME(200～299)
;=================================================
;・調教対象がコマンドを決定した後に表示。小分岐の増減は自由に
;・調教者アクション別に用意する固定のリアクション口上
;・「@KOJO_REACT_K71_(行動番号)」で行動ごとの関数を作成して書く。関数ごとコピペして改変を推奨
;・個別リアクションを表示しなかった場合は「汎用リアクション口上」が呼ばれる
;-------------------------------------------------
;関連変数…
;TFLAG:90, 91=(今回, 前回のアクションの番号)
;TFLAG:93=(今回のアクションの派生内容　0～4)
;TFLAG:79, 80=(前回, 今回のメニュー　0, 会話、1, 愛撫、2, 道具、3, 性交、4, 羞恥、5, 奉仕、6, 加虐、7, 異常、8, 使役、9, 休憩)
;TFLAG:104=(媚薬・ローション使用　1, 媚薬使用、2, ローション使用、3, 両方使用)
;TFLAG:105=(前回のコマンドで膣内射精　1, した、2, された)
;
;TFLAG:70=(現在の調教方針　1, ノーマルプレイ、2, 異常プレイ、3, 休憩、4, ハードプレイ、5, ソフトプレイ)
;TFLAG:69=(0, 通常、1, お仕置き中)
;TFLAG:60, 61=(調教者, 調教対象の状態　0, 通常、1, 疲弊、2, 衰弱、3, 無気力、4, 朦朧、5, 情欲、6, 怒り、7, 退屈、8, 狂乱)
;
;TFLAG:REACT分類=(リアクション分類 1, 消極的、2, 積極的、3, 許しを乞う、4, 暴れる、5, 拒否するor従えない)
;TFLAG:106=(今回のリアクションの派生内容　0～4)
;-------------------------------------------------

;=================================================
;会話系 FIRSTTIME(100～109)
;=================================================
;・調教開始直後、膣内射精の直後などに選ばれやすい性質がある
;・ここの時点では問いかけているだけで、調教対象の反応は未定
;-------------------------------------------------

;-------------------------------------------------
;ACT0, 今の気分を聞く FIRSTTIME(100)
;-------------------------------------------------
@KOJO_ACT_K71_0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示、2, 表示して媚薬等口上をスキップ）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(100)
		PRINTFORMW 「何がなんだか分からないって顔してるけどさぁ」
		PRINTFORMW 「何されるかくらい、わかってんでしょ…くくっ♪
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;逆レイプで膣内射精の後
	;（調教対象が調教者に搾り取られた後）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:前回中出し == 2
			PRINTFORMW 「た～っぷり搾られちゃって♪」
			PRINTFORMW 「こんなに簡単に屈服しちゃってさぁ、情けないとか思わない？」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;羞恥プレイ中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:大鏡
			PRINTFORMW 「自分の調教される姿を客観的に見て、どんな気分よ？」
			PRINTFORMW 「人間として死にたくなったりしない？」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;野外プレイ中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:野外プレイ
			PRINTFORMW 「ほーら、ポチ。お散歩たのしい？」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;勃起
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:MASTER:勃起度 >= 1000
		IF (TEQUIP:顔面騎乗 & 1) || (TEQUIP:顔面騎乗アナル & 1) 
			PRINTFORMW 「私のお尻しゃぶらされてぇ、チンポビンビンにして幸せそうね…♪」
			PRINTFORMW 「それとも…死ぬ前に一際強く輝くっていうアレかなぁ？」
			PRINTFORMW 「どーよ、そんなに苦しいの？」
		ELSE
			PRINTFORMW 「わーぉ、ギンギンじゃない♪」
			PRINTFORMDW ぬえは指先でピシピシと男性器を弾き始めた…
			PRINTFORMW 「くくっ、ビクンビクンしてる。ほらほらぁ、気持ちいい？」
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;調教対象自慰中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GETBIT(TEQUIP:自慰中, 0)
		PRINTFORMW 「くくっ…オナニー気持ちいいでちゅかぁ？」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;セックスで膣内射精の後
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:前回中出し == 1
		;アライメント正
		IF ALI(0)
			PRINTFORMW 
			PRINTFORMW 
		;アライメント負
		;ねえどんな気持ち？中出しされてどんな気持ち？
		ELSE
			PRINTFORMW 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;その調教の最初のアクション
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:経過時間 == 1
		PRINTFORMW 「そんじゃ始めましょうかね～」
	ENDIF
	
;一般
	PRINTFORMW 「気分はどーよ、%CALLNAME:MASTER%」
	RETURN LOCAL
ENDIF

