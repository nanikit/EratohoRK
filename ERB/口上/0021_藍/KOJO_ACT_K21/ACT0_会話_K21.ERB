	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 남자의 경우
	;=================================================
	IF 구상기입체크(0) && TALENT:MASTER:남자
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 후타나리의 경우
	;=================================================
	IF 구상기입체크(0) && TALENT:MASTER:후타나리
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 여자 아이의 경우
	;=================================================
	IF 구상기입체크(0) && !PENIS(MASTER)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;성별 마다의 나누어 쓰고를 하지 않으면, 여기만 써 둡시다
	;남자, 후타나리, 여자 아이 분기를 모두 묻었을 경우는 불필요합니다. 삭제합시다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;─
