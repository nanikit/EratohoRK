	IF 구상기입체크(0) && IS_PREVCOMNAME("기분 좋게 해")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「좀 더 하드하게 해」(이 돼지째!)
	;=================================================
	IF 구상기입체크(0) && IS_PREVCOMNAME("좀 더 하드하게 해")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「허가를 청한다」(허락해 받을 수 없었습니다)
	;=================================================
	IF 구상기입체크(0) && IS_PREVCOMNAME("허가를 청한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「자위하기 시작한다」(사람의 이야기를 들어라!)
	;=================================================
	IF 구상기입체크(0) && IS_PREVCOMNAME("자위하기 시작한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

