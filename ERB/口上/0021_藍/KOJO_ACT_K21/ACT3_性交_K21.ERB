		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;実質的にTFLAG:ACT派生 == 0(通常)の箇所
	;=================================================
	;その他・条件無し
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
