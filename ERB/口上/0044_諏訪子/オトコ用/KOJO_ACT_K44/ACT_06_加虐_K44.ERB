;────────────────────────────────────
;ACT60, スパンキング
;────────────────────────────────────
;・調教者が調教対象の尻を平手で叩く
;・ACT開始時点ですでに叩いている
;・性交奉仕中には発生しない
;・顔面騎乗系は解除される
;・調教者が自慰中である場合は解除される
;-------------------------------------------------
@KOJO_MALE_ACT_K44_60
;-------------------------------------------------
;・ACTの派生なし
;-------------------------------------------------
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		PRINTFORMW 「え？　痛いのはちょっとって？」
		CALL HTMLPRINTW(@"「大丈夫よ、それが良くなるから%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;調教対象のマゾッ気3以上かつ50%で真
	;=================================================
	IF 口上記入チェック(50) && ABL:MASTER:マゾっ気 >= 3 ;//マゾっ気 >= 3の場合でも1/2の確率で下層に流す
		SELECTCASE ABL:MASTER:マゾっ気
			;マゾっ気5以上
			CASE IS >= 5
				PRINTFORMW 「ついに悦んでケツ振りながらおねだり始めるようになったのね…」
				PRINTFORMW 「恥ずかしくないのかしらっ！　この豚がッ！♪」
			;マゾっ気3以上
			CASEELSE
				PRINTFORMW 「アンタ、だんだん叩かれるのクセになってきてない…？」
				PRINTFORMW 「隠したって無駄なんだからっ！　バレバレなのよっ！♪」
		ENDSELECT
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件無し
	;=================================================
	IF 口上記入チェック(100)
		;苦痛中毒による派生
		SELECTCASE ABL:MASTER:苦痛中毒
			;苦痛中毒5以上
			CASE IS > 5
				PRINTFORMW 「そんな悩ましい目で私を見るなんて…」
				PRINTFORMW 「この変態っ！　ド変態っ！！　変態大人ッ！！！」
			;苦痛中毒3以上
			CASE IS > 3
				PRINTFORMW 「最近イイ声で鳴くじゃない…♪」
				PRINTFORMW 「ケツ叩かれて感じちゃってるのっ！　この恥知らずっ！♪」
			;苦痛中毒1以上
			CASE IS > 1
				PRINTFORMW 「だんだん痛みの中に別の感覚が見えてきたんじゃない？」
				PRINTFORMW 「気持ちいいのはっ！　これからよっ！」
			CASEELSE
				PRINTFORMW 「ほらっケツ出しなさいよ」
				PRINTFORMW 「ちょっとばかり躾をしてあげる…♪」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

