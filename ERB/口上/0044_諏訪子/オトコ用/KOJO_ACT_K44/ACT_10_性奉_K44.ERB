;·지문은 ACT파생 1(연속), ACT파생 2 TO 5(A성교/V성교 봉사/V성교), 그 외로 분기
;·각파생내에서 페니스밴드의 유무로 분기 한다
;·필요에 따라서 분기를 증감해 주세요
;-------------------------------------------------
IF 구상기입체크(100)
	;=================================================
	;뽑지않고에 속행
	;=================================================
	IF 구상기입체크(100) && TFLAG:뽑지않고
		;//숙청된 커맨드로부터 사용할 수 있는 것을 회수
		SELECTCASE RANDACT(4, 0)
			CASE 0
				CALL HTMLPRINTW(@"「힛%HTML_HEARTB()%  응%HTML_HEARTB()%  좋은 것…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「여기도 좋지요…%HTML_HEARTB()%  응응%HTML_HEARTB()%」")
			CASE 1
				CALL HTMLPRINTW(@"「훗%HTML_HEARTB()%  후~%HTML_HEARTB()%  응응%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「뜨겁고 좋다…%HTML_HEARTB()%  %HTML_HEARTB()%」")
			CASE 2
				CALL HTMLPRINTW(@"「이봐요 분명하게 푹푹 해?   엉덩이 휘저어…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「%HTML_HEARTB()%  %HTML_HEARTB()%  그 상태%HTML_HEARTB()%」")
			CASE 3
				CALL HTMLPRINTW(@"「응읏%HTML_HEARTB()%  너의 몸… 따뜻하다…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「이봐요%HTML_HEARTB()%  좀 더 엉덩이의 구멍 맛 깨어엇%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 1
	ENDIF

	;=================================================
	;연속(전회 ACT가애널섹스시킨다)
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:ACT파생 == 1
	;ENDIF

	;=================================================
	;A성교계로부터
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:ACT파생 == 2
	;ENDIF

	;=================================================
	;V성교 봉사계로부터
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 3
		CALL HTMLPRINTW(@"「엉덩이도 갖고 싶어져 버렸다…%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「이봐요…%HTML_HEARTB()%  빨리 와아…%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;V성교계로부터
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:ACT파생 == 4
	;ENDIF

	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		SELECTCASE RANDACT(2, 10)
			CASE 0
				CALL HTMLPRINTW(@"「이봐요 들어갈 수 있어…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「그쪽이 아니고 엉덩이의 (분)편%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「준비 되어 있으니… 까요, 빨리…%HTML_HEARTB()%」")
			CASE 1
				CALL HTMLPRINTW(@"「이봐요 이봐요, 넣게 해 준다…%HTML_HEARTB()%엉덩이(분)편에 말야%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「는─나─구%HTML_HEARTB()%  그렇게 자주, 그대로… 자, 찔러%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF


