IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;勃起中
	;=================================================
	IF 口上記入チェック(100) && TCVAR:MASTER:勃起度 >= 1000
		PRINTFORMW 「許可なくこんなにガッチガチにしたチンポには…」
		PRINTFORMW 「ちょっと躾が必要だよね♪」
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(100)
		SELECTCASE RANDACT(3, 10)
			CASE 0
				PRINTFORMW 「ちょーっと大人しくしてもらうよ？」
			CASE 1
				PRINTFORMW 「よいしょっと」
				CALL HTMLPRINTW(@"「ふふ、ちょろいちょろい♪　さてどうしようかしら%HTML_HEARTB()%」")
			CASE 2
				PRINTFORMW 「抵抗しないでくれる？」
				CALL HTMLPRINTW(@"「なーに、ちゃんと気持ちよくしてあげるわよ%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

