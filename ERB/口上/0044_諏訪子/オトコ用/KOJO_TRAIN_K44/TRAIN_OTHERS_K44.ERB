;─
;EV21, 대만족 구상
;─
;·조교자의 대만족 보너스를 취득했을 경우에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	CALL HTMLPRINTW(@"「응…%HTML_HEARTB()%」")
	CALL HTMLPRINTW(@"「지금의 좋았어요…%HTML_HEARTB()%」")
	RETURN 2
ENDIF

;─
;EV22, 상태변화 구상
;─
;·상태 이상이 붙었을 때에 표시
;·TCVAR:상태변화가 조교자, TCVAR:MASTER:상태변화가 조교 대상
;·상태변화가 일어나면 3 턴 지속한다
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_22(ARG)
;-------------------------------------------------

;EV22_0, 조교자 상태변화
;**********************
;━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님[피폐]화  소모시:피로를 위해 소극적 행동이 우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(50) && IS_CONDITION(TARGET, "피폐")
	;=================================================
	;조교내 첫회
	;=================================================
	IF ONCEEVENT(0)
		PRINTFORMW 「우─, 조금 타임…」
		PRINTFORMW 「과연 너무 까불며 떠들었을까…」
		RETURN 2
	ENDIF

	;조건 없음
	PRINTFORMW 「뭔가 지쳐 왔어요…」
	PRINTFORMW 「조금은 돌보세요…」
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님[쇠약]화  강소모시:피로를 위해 소극적 행동이 우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_CONDITION(TARGET, "쇠약")
	;=================================================
	;조교내 첫회
	;=================================================
	IF ONCEEVENT(1)
		PRINTFORMW 「… 아직!」
		PRINTFORMW 「아직도 이것이나… 오로로로」
		RETURN 2
	ENDIF

	;조건 없음
	PRINTFORMW 「어째서 이렇게 지쳤을까…」
	PRINTFORMW 「어제 카나코와 도철 너무 한 탓…?」
	RETURN 2
ENDIF

;EV22_1, 조교 대상 상태변화
;************************
;기입 개소 없음

;─
;EV23, 조교 대상 능력치 업 구상
;─
;·조교중에 조교 대상의 능력치가 상승했을 때에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_23(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(이번 상승한 ABL 번호/각 ARG 번호는 ABL의 거기에 상당)

;ARG:0 순종/1욕망/2기교/3 C감각/4 V감각/5 A감각/6 B감각/7봉사정신/8노출버릇
;    9레즈끼/10 ＢＬ끼/11마조끼/12자위중독/13정액중독/14레즈중독/15고통중독/16구속중독

;ARG:20 회화/21애무/22도구/23성교/24수치/25봉사/26가학/27이상
;-------------------------------------------------
IF 구상기입체크(100)
	;일행 비운다
	PRINTL
	;이번 상승한 ABL의 파생을 본다
	SELECTCASE ARG
		;조교 대상[순종]업
		CASE 0
			PRINTFORMW 「순수에… 순수에…」
			RETURN 2
		;조교 대상[욕망]업
		CASE 1
			PRINTFORMW 「거기, 하아하아 하지 않는다」
			RETURN 2
		;조교 대상[기교]업
		CASE 2
			PRINTFORMW 「조금 능숙해졌지 않아…♪」
			RETURN 2
		;조교 대상[C감각]업
		CASE 3
			PRINTFORMW 「너무 민감해도 큰 일이지 않아?」
			RETURN 2
		[SKIPSTART]
		//남자용인 것으로 comment out
		;조교 대상[V감각]업
		CASE 4
			PRINTFORMW 「어디에 보지 붙어 있는거야」
			RETURN 2
		[SKIPEND]
		;조교 대상[A감각]업
		CASE 5
			PRINTFORMW 「눈을 뜸이야…!」
			RETURN 2
		;조교 대상[B감각]업
		CASE 6
			PRINTFORMW 「좋았어, 스스로 빨거나 하면 좋아요」
			RETURN 2
		;그 외
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

