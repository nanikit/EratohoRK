;────────────────────────────────────
;EV21, 大満足口上
;────────────────────────────────────
;・調教者の大満足ボーナスを取得した場合に表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;諏訪子さま大満足
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	CALL HTMLPRINTW(@"「ん…%HTML_HEARTB()%」")
	CALL HTMLPRINTW(@"「今の良かったわよ…%HTML_HEARTB()%」")
	RETURN 2
ENDIF

;────────────────────────────────────
;EV22, 状態変化口上
;────────────────────────────────────
;・状態異常が付いたときに表示
;・TCVAR:状態変化が調教者、TCVAR:MASTER:状態変化が調教対象
;・状態変化が起こると3ターン持続する
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_22(ARG)
;-------------------------------------------------

;EV22_0, 調教者状態変化
;**********************
;━━━━━━━━━━━━━━━━━━━━━━━━
;諏訪子さま[疲弊]化  消耗時：疲労のため消極的行動が優先
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(50) && IS_CONDITION(TARGET, "疲弊")
	;=================================================
	;調教内初回
	;=================================================
	IF ONCEEVENT(0)
		PRINTFORMW 「うー、ちょっとタンマ…」
		PRINTFORMW 「流石にはしゃぎすぎたかしら…」
		RETURN 2
	ENDIF

	;条件なし
	PRINTFORMW 「なんだか疲れてきたわ…」
	PRINTFORMW 「ちょっとは労わりなさいよ…」
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;諏訪子さま[衰弱]化  強消耗時：疲労のため消極的行動が優先
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_CONDITION(TARGET, "衰弱")
	;=================================================
	;調教内初回
	;=================================================
	IF ONCEEVENT(1)
		PRINTFORMW 「ぜーっぜーっ…まだよっ！」
		PRINTFORMW 「まだまだこれか…ｵﾛﾛﾛｫ」
		RETURN 2
	ENDIF

	;条件なし
	PRINTFORMW 「なんでこんなに疲れたのかしら…」
	PRINTFORMW 「昨日神奈子と桃鉄やりすぎたせい…？」
	RETURN 2
ENDIF

;EV22_1, 調教対象状態変化
;************************
;記入箇所なし

;────────────────────────────────────
;EV23, 調教対象能力値アップ口上
;────────────────────────────────────
;・調教中に調教対象の能力値が上昇した際に表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_23(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(今回上昇したABL番号/各ARG番号はABLのそれに相当)

;ARG:0 従順/1 欲望/2 技巧/3 Ｃ感覚/4 Ｖ感覚/5 Ａ感覚/6 Ｂ感覚/7 奉仕精神/8 露出癖
;    9 レズっ気/10 ＢＬっ気/11 マゾっ気/12 自慰中毒/13 精液中毒/14 レズ中毒/15 苦痛中毒/16 拘束中毒

;ARG:20 会話/21 愛撫/22 道具/23 性交/24 羞恥/25 奉仕/26 加虐/27 異常
;-------------------------------------------------
IF 口上記入チェック(100)
	;一行空ける
	PRINTL
	;今回上昇したABLの派生を見る
	SELECTCASE ARG
		;調教対象[従順]アップ
		CASE 0
			PRINTFORMW 「素直になぁれっ…素直になぁれっ…」
			RETURN 2
		;調教対象[欲望]アップ
		CASE 1
			PRINTFORMW 「そこ、ハァハァしない」
			RETURN 2
		;調教対象[技巧]アップ
		CASE 2
			PRINTFORMW 「ちょっと巧くなったんじゃないの…♪」
			RETURN 2
		;調教対象[Ｃ感覚]アップ
		CASE 3
			PRINTFORMW 「敏感過ぎても大変じゃない？」
			RETURN 2
		[SKIPSTART]
		//オトコ用なのでコメントアウト
		;調教対象[Ｖ感覚]アップ
		CASE 4
			PRINTFORMW 「どこにマンコついてんのよ」
			RETURN 2
		[SKIPEND]
		;調教対象[Ａ感覚]アップ
		CASE 5
			PRINTFORMW 「目覚めよ…！」
			RETURN 2
		;調教対象[Ｂ感覚]アップ
		CASE 6
			PRINTFORMW 「よかったわね、自分で舐めたりするといいわ」
			RETURN 2
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;────────────────────────────────────
;EV24, 調教者能力値アップ口上
;────────────────────────────────────
;・調教中に調教者の能力値が上昇した際に表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_24(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(今回上昇したABL番号)

;ARG:0 従順/1 欲望/2 技巧/3 Ｃ感覚/4 Ｖ感覚/5 Ａ感覚/6 Ｂ感覚/7 奉仕精神/8 露出癖
;    9 レズっ気/10 ＢＬっ気/11 マゾっ気/12 自慰中毒/13 精液中毒/14 レズ中毒/15 苦痛中毒/16 拘束中毒

;ARG:20 会話/21 愛撫/22 道具/23 性交/24 羞恥/25 奉仕/26 加虐/27 異常
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;諏訪子さま[欲望]アップ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 1
	PRINTL
	PRINTFORMW 「私の欲望ならマントルより深いわよ？」
	RETURN 2
ENDIF

[SKIPSTART]
;────────────────────────────────────
;EV29, 屈服・苦痛刻印による反抗刻印上限処理
;────────────────────────────────────
;・反抗刻印Lv上限の処理
;・反抗刻印は10-(苦痛刻印+屈服刻印)/2を超えない
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_29(ARG)
;-------------------------------------------------
;%CALLNAME:TARGET%の与える快楽\@ MARK:MASTER:苦痛刻印 ? と苦痛 # \@が%CALLNAME:MASTER%から反抗の気力を奪っていく…
;反抗刻印は{MARK:反抗刻印}になった
;-------------------------------------------------
IF 口上記入チェック(0)
ENDIF

;========================================================================
;刻印取得口上
;========================================================================
;・調教中に刻印を取得させた場合に表示。どのレベルで口上表示するかは加減自由なので個々で調整するのが望ましい
;・記入チェックが=1の場合、同時に複数取得したときはこのファイルで一番上のものだけを表示する
;・=2の場合、取得した刻印の口上すべてを上から順に表示する
;・刻印取得による素質変動の前に表示するため、素質取得/喪失するかはこの時点でのTALENTの有無で判定できる
;-------------------------------------------------
@KOJO_MALE_MARK_K44(ARG:0, ARG:1, ARG:2, ARG:3, ARG:4)
#DIM 記入チェック
#DIM すべて順に表示
すべて順に表示 = 0	;刻印取得口上「すべて順に表示」のための初期化処理
;-------------------------------------------------
;引数内訳…
;ARG:(刻印番号)=(今回取得したレベル)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;[トラウマ]取得
記入チェック = 0	;0.非表示/1.同時に取得した刻印をこれ以下表示しない/2.すべて順に表示する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 記入チェック && ARG:4
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[苦痛刻印]取得
記入チェック = 0	;0.非表示/1.同時に取得した刻印をこれ以下表示しない/2.すべて順に表示する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 記入チェック && ARG:0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[反抗刻印]取得
記入チェック = 0	;（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG:3
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[屈服刻印]取得
記入チェック = 0	;（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 記入チェック && ARG:2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[快楽刻印]取得
記入チェック = 0	;（0, 非表示、1, 同時に取得した刻印をこれ以下表示しない、2, すべて順に表示する）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 記入チェック && ARG:1
ENDIF

;刻印取得口上「すべて順に表示」のための終端処理
RETURN すべて順に表示 != 0
[SKIPEND]