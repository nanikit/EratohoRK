;========================================================================
;EV20, 절정 구상
;========================================================================
;·조교자나 조교 대상이 절정에 이를 때 표시
;· 각 절정은 개별의 호출을 가지지 않고, 모두 파생으로 처리된다
;-------------------------------------------------
;=================================================
;관련 변수…
;
;TFLAG:전회질내사정, (1=Ｖ성교/2=Ｖ성교 봉사/3=Ｖ성교(하늘 쏘아 맞히고)/4=Ｖ성교 봉사(하늘 쏘아 맞히고)/5=Ａ성교/6=Ａ성교 봉사/7=Ａ성교(하늘 쏘아 맞히고)/8=Ａ성교 봉사(하늘 쏘아 맞히고))
;TFLAG:REACT분류, (0=없음/1=소극적으로 한다/2=적극적으로 한다/3=용서를빈다/4=날뛴다/5=요구에 응하지 않는, 거부/6=도망치는, 발버둥 친다)
;NOWEX:사정, (1=드라이사정/2=사정/3=대량사정/4=사정(조루)/5=A책사정)
;TEQUIP:자위중, (bit0=조교 대상/bit1=조교자)
;
;=================================================
;함수류…
;
;MASTER_EX
;인  수:ARG:0… 애무자→1. 조교 대상 자신/2. 조교자/4. 조수 1/8. 조수 2/16. 조수 3
;/64. 도구/128. 변소 이용자  생략 하면 애무자를 묻지 않는다
;              비트화에서의 지정이 가능
;       ARG:1…(생략가능) 절정의 종별→1Ｃ절정/2Ｖ절정/3Ａ절정/4Ｂ절정
;/11. 분유/12. 사정/13. 방뇨
;ARG:0(bit0 =조교 대상 자신/bit1 =조교자/bit2 =조수 1/bit3 =조수 2/bit4 =조수 3/bit6 =도구/bit7 =변소 이용자)
;-------------------------------------------------
;TARGET_EX
;인  수:ARG:0… 애무자→1. 조교 대상/2. 조교자 자신/4. 조수 1/8. 조수 2/16. 조수 3
;              비트화에서의 지정이 가능. 생략 하면 애무자를 묻지 않는다
;       ARG:1…(생략가능) 절정의 종별→1. c절정/2. v절정/3. a절정/4. b절정
;/11. 분유/12. 사정
;ARG:0(bit0 =조교 대상/bit1 =조교자 자신/bit2 =조수 1/bit3 =조수 2/bit4 =조수 3)
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_20(ARG)
;-------------------------------------------------
;TFLAG:전회질내사정;(1=Ｖ성교/2=Ｖ성교 봉사/3=Ｖ성교(하늘 쏘아 맞히고)/4=Ｖ성교 봉사(하늘 쏘아 맞히고)/5=Ａ성교/6=Ａ성교 봉사/7=Ａ성교(하늘 쏘아 맞히고)/8=Ａ성교 봉사(하늘 쏘아 맞히고))
;-------------------------------------------------
;────────────────────────────────────
;질내사정(MASTER→TARGET)
;────────────────────────────────────
;·Ｖ성교 봉사계로 질내사정 하는 경우에 표시
;·TFLAG:뽑지않고는 뽑지 않는 판정 마다+1된다
;·사정량에 의한 분기는 필요하게 응하고 추가해 사용하는 것이 바람직함
;-------------------------------------------------
IF 구상기입체크(100) && GROUPMATCH(TFLAG:전회질내사정, 2, 4) > 0
	;=================================================
	;뽑지않고에 속행
	;=================================================
	IF TFLAG:뽑지않고
		;동시절정(조교자가 동시에 v절정)
		IF TCVAR:TARGET:Ｖ절정 && TCVAR:MASTER:사정
			SELECTCASE GET_SHOOTING(MASTER)
				CASE "드라이사정"
					CALL HTMLPRINTW(@"「간다앗%HTML_HEARTB()%  자지로 간닷!」")
					PRINTFORMW 「하… 핫… 이제(벌써) 나오지 않는다니이…」
					CALL HTMLPRINTW(@"「그렇지만 나카로 아직 움찔움찔 커지고 있지 않아…%HTML_HEARTB()%  계속해 버려?」")
				;그 외
				CASEELSE
					CALL HTMLPRINTW(@"「응하앗%HTML_HEARTB()%  간다앗%HTML_HEARTB()%」")
					CALL HTMLPRINTW(@"「… 벌써… 건강하게 드퓨드퓨 해 버려…%HTML_HEARTB()%」")
					CALL HTMLPRINTW(@"「아직 전혀 딱딱한 그대로야?   이 호색가가%HTML_HEARTB()%  좀 더 짜내 준다…%HTML_HEARTB()%」")
			ENDSELECT
		;통상
		ELSE
			SELECTCASE GET_SHOOTING(MASTER)
				CASE "드라이사정"
					PRINTFORMW 「아랴, 이제(벌써) 최후야…?」
					CALL HTMLPRINTW(@"「여기는 아직 가지 않았는데 달콤해%HTML_HEARTB()%  이대로 속행%HTML_HEARTB()%」")
				CASE "조루"
					PRINTFORMW 「아무것도 사정하고 있어 이 조루!」
					CALL HTMLPRINTW(@"「빠뜨릴 수 없어요…?   갈 때까지 노력해 받기 때문에%HTML_HEARTB()%」")
				;그 외
				CASEELSE
					CALL HTMLPRINTW(@"「아핫%HTML_HEARTB()%  자지 쫑긋쫑긋 말하고 있다아%HTML_HEARTB()%」")
					CALL HTMLPRINTW(@"「그렇지만 나 아직 가지 않지만 말야─%HTML_HEARTB()%  쿠스, 이대로 속행이군요%HTML_HEARTB()%」")
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF

	;=================================================
	;동시절정(조교자가 동시에Ｖ절정)
	;=================================================
	IF TCVAR:TARGET:Ｖ절정 && TCVAR:MASTER:사정
		SELECTCASE GET_SHOOTING(MASTER)
			CASE "드라이사정"
				CALL HTMLPRINTW(@"「함께%HTML_HEARTB()%  함께 와앗%HTML_HEARTB()%  후~아%HTML_HEARTB()%」")
				PRINTFORMW 「하아… 하아… 뭐야, 이제 다 써버려 버리고 있는 것이 아니야…」
			;그 외
			CASEELSE
				CALL HTMLPRINTW(@"「간닷…!   나도 가 버린닷!   나%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「하… 핫%HTML_HEARTB()%  들어 온다아… 정액… 가득…%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 2
	ENDIF

	;=================================================
	;초조하게함으로부터
	;=================================================
	IF TCVAR:MASTER:애태움수 && TCVAR:MASTER:사정
		;애태움중
		IF TCVAR:MASTER:애태움수 > 1
			SELECTCASE GET_SHOOTING(MASTER)
				CASE "전립선사정"
					CALL HTMLPRINTW(@"「이제(벌써) 아직이라고 말하고 있는데%HTML_HEARTB()%」")
					PRINTFORMW 「엉덩이로 너무 느껴 버렸어?   완전히…♪」
				CASEELSE
					CALL HTMLPRINTW(@"「하아읏…… 아%HTML_HEARTB()%」")
					PRINTFORMW 「, 아이참─, 뭐사정하고 있는거야… 너무 빠른 것이야 너는!」
			ENDSELECT
		;결정타
		ELSE
			SELECTCASE GET_SHOOTING(MASTER)
				CASE "드라이사정"
					PRINTFORMW 「뭐야 이제(벌써) 나오지 않잖아…」
					PRINTFORMW 「빨리 불알로 정자 만드세요…♪」
				;그 외
				CASEELSE
					CALL HTMLPRINTW(@"「아하앗%HTML_HEARTB()%  열있고…%HTML_HEARTB()%」")
					CALL HTMLPRINTW(@"「그렇게 기분 좋은 것 같이 사정된 것은 견딜 수 없어요…%HTML_HEARTB()%」")
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF

	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		;사정종의 파생을 본다
		SELECTCASE GET_SHOOTING(MASTER)
			CASE "드라이사정"
				PRINTFORMW 「이제(벌써) 정액 다 써버려 버렸어…?」
				PRINTFORMW 「평상시부터 정력 붙여 두세요… 나참…」
			CASE "대량사정"
				CALL HTMLPRINTW(@"「도… 도…, 너무 사정해요 바보…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「아직 나오고 있다아…%HTML_HEARTB()%  드로는…%HTML_HEARTB()%」")
			CASE "조루"
				PRINTFORMW 「이제(벌써) 바보!   아직 사정하지 말라고 말하고 있는데…」
				PRINTFORMW 「이 조루…♪」
			CASE "전립선사정"
				CALL HTMLPRINTW(@"「애널로 느껴 사정해 버린 것이다아…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「어쩔 수 없네요…%HTML_HEARTB()%」")
			CASE "사정"
				CALL HTMLPRINTW(@"「하아읏%HTML_HEARTB()%  나왔다앗%HTML_HEARTB()%  이제(벌써)…」")
				CALL HTMLPRINTW(@"「이봐요 봐아… 이렇게 사정해 버려…%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 2
	ENDIF
ENDIF

;────────────────────────────────────
;애널내 사정(MASTER→TARGET)
;────────────────────────────────────
;·Ａ성교 봉사계로 장내 사정하는 경우에 표시
;·TFLAG:106, 뽑지않고는 뽑지 않는 판정 마다+1된다
;·사정량에 의한 분기는 필요하게 응하고 추가해 사용하는 것이 바람직함
;-------------------------------------------------
IF 구상기입체크(100) && GROUPMATCH(TFLAG:전회질내사정, 6, 8) > 0
	;=================================================
	;동시절정(조교자가 동시에Ａ절정)
	;=================================================
	IF TCVAR:TARGET:Ａ절정 && TCVAR:MASTER:사정
		SELECTCASE GET_SHOOTING(MASTER)
			CASE "드라이사정"
				CALL HTMLPRINTW(@"「햐아아아%HTML_HEARTB()%  이제 안돼엣%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「… 엉덩이로 움찔움찔 움직이고 있다아…%HTML_HEARTB()%  이제 낼 수 없는거야…?」")
			;그 외
			CASEELSE
				CALL HTMLPRINTW(@"「야아앗%HTML_HEARTB()%  엉덩이 간다앗%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「도─바보…%HTML_HEARTB()%  너무 사정해요…%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 2
	ENDIF

	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		;사정종의 파생을 본다
		SELECTCASE GET_SHOOTING(MASTER)
			CASE "드라이사정"
				CALL HTMLPRINTW(@"「이미 맥 치고 있을 뿐이지 않아…%HTML_HEARTB()%」")
				PRINTFORMW 「기분 좋았다아…?」
			CASE "대량사정"
				CALL HTMLPRINTW(@"「애널로 이렇게 줄줄 사정하다니이…%HTML_HEARTB()%」")
				PRINTFORMW 「엉덩이로 임신시킬 생각…?」
			CASE "조루"
				PRINTFORMW 「아이참─사정해 버렸어?」
				CALL HTMLPRINTW(@"「조금 과잉긴축 일까…%HTML_HEARTB()%」")
			CASE "전립선사정"
				CALL HTMLPRINTW(@"「애널 꾸짖어지면서 엉덩이에 사정하다니…%HTML_HEARTB()%」")
				PRINTFORMW 「이 엉덩이 매니아♪」
			CASE "사정"
				CALL HTMLPRINTW(@"「아하아…%HTML_HEARTB()%  뜨겁다…%HTML_HEARTB()%  엉덩이 녹아 버린다…%HTML_HEARTB()%」")
				CALL HTMLPRINTW(@"「너무 했어… 이제(벌써)…%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 2
	ENDIF
ENDIF

;────────────────────────────────────
;질내사정(TARGET→MASTER)
;────────────────────────────────────
;·Ｖ성교계로 질내사정 하는 경우에 표시
;·사정량에 의한 분기는 필요하게 응하고 추가해 사용하는 것이 바람직함
;·현재의 사양에서는 조교자가 드라이사정 하는 것은 매우 희(거의 없어?)
;-------------------------------------------------
;IF 구상 기입 체크(0) && GROUPMATCH(TFLAG:전회질내사정, 1, 3) > 0
;ENDIF

;────────────────────────────────────
;애널내 사정(TARGET→MASTER)
;────────────────────────────────────
;·Ａ성교계로 장내 사정하는 경우에 표시
;·사정량에 의한 분기는 필요하게 응하고 추가해 사용하는 것이 바람직함
;·현재의 사양에서는 조교자가 드라이사정 하는 것은 매우 희(거의 없어?)
;-------------------------------------------------
;IF 구상 기입 체크(0) && GROUPMATCH(TFLAG:전회질내사정, 5, 7) > 0
;ENDIF

;────────────────────────────────────
;조교 대상 사정
;────────────────────────────────────
;·조교 대상이 사정하는 경우에 표시
;·사정량에 의한 분기는 필요하게 응하고 가감(상태)해 사용하는 것이 바람직함
;-------------------------------------------------
IF 구상기입체크(100) && TCVAR:MASTER:사정 && !TCVAR:MASTER:방뇨 ;//방뇨를 수반하는 경우는 연주한다
	;=================================================
	;전립선사정(Ａ절정시에 쾌Ｃ에 관련되지 않고 사정)
	;=================================================
	IF SHOOTING(MASTER, "전립선사정")
		SELECTCASE MA_PLAYER(1)
			;구에서(애널핥기)//현재의 사양에서는 발생 할 수 없다
			;//혀가 전립선까지 닿을 이유 없다! 그렇다고 하는 시스템측의 의향입니다만
			;//스와코님이라면 가능할 것 같은 감은 아무튼 있습니다 w 다만 사양인 것으로, 라는 것으로@/L
			;CASE 1
			;	PRINTFORMW 「날름날름 %UNICODE(0 x2661)%  응%UNICODE(0 x2661)%  %UNICODE(0 x2661)%」
			;	PRINTFORMW 「너무 느껴 사정해 버렸어? 좋았어요―%UNICODE(0 x2661)%」
			;팔로(피스트)
			CASE 10
				PRINTFORMW 「아리데베르치…(안녕)」
				PRINTFORMW 「제 3부완!!」
			;상기 이외
			CASEELSE
				CALL HTMLPRINTW(@"「질척질척%HTML_HEARTB()%  어떨까…♪… 앗」")
				CALL HTMLPRINTW(@"「사정한다면 사정한다 라고 하세요…%HTML_HEARTB()%」")
		ENDSELECT
		RETURN 2
	ENDIF

	;=================================================
	;가랑이로 동시절정
	;=================================================
	IF TCVAR:TARGET:Ｃ절정 && IS_NOWACTNAME("성기를서로켠다") && MC_PLAYER(1) == 8
		CALL HTMLPRINTW(@"「사정해 버리는 거야…?   는 이대로… 보지에 걸쳐엇%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「아하앗♪  뜨거운 정액 가득…%HTML_HEARTB()%  이제 보지 굿체그체가 아닌 것…%HTML_HEARTB()%」")
		RETURN 2
	ENDIF

	;=================================================
	;파이즈리로 동시절정
	;=================================================
	IF IS_NOWACTNAME("파이즈리한다") && TCVAR:TARGET:Ｂ절정 && !TCVAR:TARGET:분유
		CALL HTMLPRINTW(@"「좋아…%HTML_HEARTB()%  그대로%HTML_HEARTB()%  그대로 젖가슴에 사정햇%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「응하아…%HTML_HEARTB()%  뜨겁고 질척질척이 아니다…%HTML_HEARTB()%」")
		RETURN 2
	ENDIF

	;=================================================
	;자위로 사정
	;=================================================
	IF GETBIT(TEQUIP:자위중, 0)
		LOCAL:3 = RAND:3
		;구 쏘아 맞히고
		IF ME_PLAYER(1) == 9
			SELECTCASE GET_SHOOTING(MASTER)
				CASE "드라이사정"
					CALL HTMLPRINTW(@"「이봐요, 구에들 해…%HTML_HEARTB()%」")
					CALL HTMLPRINTW(@"「아─, 이제(벌써) 나오지 않지 않은 것…%HTML_HEARTB()%」")
				CASE "조루"
					PRINTFORMW 「이제(벌써) 지금 마셔 주기 때문에 대… 햐앗!?」
					CALL HTMLPRINTW(@"「정액얼굴에 걸려 버렸지 않아아… 이제(벌써)…%HTML_HEARTB()%」")
				;그 외
				CASEELSE
					SELECTCASE RANDEVENT(3, 40)
						CASE 0
							PRINTFORMW 「아하, 스스로 쫄깃쫄깃 하면서 가 버린다♪」
							CALL HTMLPRINTW(@"「그렇지만 어차피라면… 네, 날름%HTML_HEARTB()%」")
							CALL HTMLPRINTW(@"「두는 히에 전부등 해…%HTML_HEARTB()%  왔다앗…%HTML_HEARTB()%」")
						CASE 1
							PRINTFORMW 「나와? 이제(벌써)갈것같다?   자제 할 수 없는거야?」
							CALL HTMLPRINTW(@"「어쩔 수 없구나…%HTML_HEARTB()%  이봐요, 구하세요%HTML_HEARTB()%」")
							CALL HTMLPRINTW(@"「%HTMLFONT("… 뷰룻", , 0xFFFFFF)%%HTML_HEARTB()%　%HTMLFONT("", , 0xFFFFFF)%%HTML_HEARTB()%  응응…%HTML_HEARTB()%  뜨겁고 진하다아…%HTML_HEARTB()%」")
						CASE 2
							CALL HTMLPRINTW(@"「손굉장한 빨라?   이제(벌써) 정액 사정낳았다%HTML_HEARTB()%」")
							CALL HTMLPRINTW(@"「슝슝… 냄새나는 진해지고 있다…%HTML_HEARTB()%  좋아… 앙%HTML_HEARTB()%」")
							CALL HTMLPRINTW(@"「퓻%HTML_HEARTB()%  %HTML_HEARTB()%  … 만나%HTML_HEARTB()%」")
					ENDSELECT
			ENDSELECT
		;통상
