	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTREACT(4)
		PRINTFORMW 「！？…ってアンタ早苗が好きなんでしょうが」
		PRINTFORMW 「あんまりからかわないでよ…」
		RETURN 1
	ENDIF

	;=================================================
	;負荷0, 負荷1 愛想よく笑って大好きだと答えた
	;=================================================
	IF 口上記入チェック(100) && GROUPMATCH(GET_COM_RANK(), "自然な行動", "無理のない行動")
		IF RAND:2
			PRINTFORMW 「う、…あ、ありがと…」
			PRINTFORMW 「なにニヨニヨしてんのよこのばか…」
		ELSE
			PRINTFORMW 「そんなド直球に好きとかうー…」
			PRINTFORMW 「ま、まぁうれしいケドさ…」
		ENDIF
		RETURN 1
	ENDIF

	;=================================================
	;REACT負荷2, 作り笑いを浮かべて、好きだよと答えた
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;RESULTを返して汎用リアクションに流さず完結させる
		RETURN 1
	ENDIF
ENDIF

