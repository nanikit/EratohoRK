;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("許しを乞う")
	PRINTFORMW 「何？まだ何も言ってないよ？」
	PRINTFORMW 「興味ある？ねぇ興味あるのぉ？」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM31, 悲鳴を上げる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("悲鳴を上げる")
	PRINTFORMW 「そうそうそんな感じでいい声が毎晩…」
	PRINTFORMW 「まぁ今やる気は無いから安心してね」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM33, 怯える
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("怯える")
	PRINTFORMW 「小便は済ませたかしら？　お祈りは？　部屋の隅でガタガタ震えて命乞いをする準備はＯＫ！？」
	PRINTFORMW 「…ちょっと顔色ブルーレイみたいになってるわよ。冗談よ冗談」
	RETURN 1
ENDIF


