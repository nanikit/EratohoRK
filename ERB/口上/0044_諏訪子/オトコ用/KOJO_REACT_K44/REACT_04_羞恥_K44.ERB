;────────────────────────────────────
;REACT42, 自慰みせつけ・個別リアクション
;────────────────────────────────────
;・ACT派生なし
;-------------------------------------------------
;想定されるCOM…
;会話系反応
;	COM:気持ち良くして
;愛撫系反応
;	COM:恥ずかしがる
;その他反応
;	COM:自慰し始める
;-------------------------------------------------
@KOJO_MALE_REACT_K44_42
;-------------------------------------------------
;・地の文はREACT分類(受け入れる, 積極的従う/平然と従う/許しを乞う/消極的従う)で分岐
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;調教者にペニスなしの場合
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !PENIS(TARGET)
	;RANDで分けてしまうと辻褄が合わないので、リアクション毎に切り分ける
	SELECTCASE COMGRONAME(TFLAG:REACT分類)
		CASE "受け入れる"
			CALL HTMLPRINTW(@"「くちゅくちゅ…はぁ…%HTML_HEARTB()%」")
			CALL HTMLPRINTW(@"「気持ちいいよぉ…%HTML_HEART()%　んっ%HTML_HEARTB()%」")
			RETURN 1
		CASE "平然と従う", "積極的従う"
			PRINTFORMW 「あは、なんか息荒いよ…？」
			CALL HTMLPRINTW(@"「興奮しちゃってるんだ…%HTML_HEARTB()%」")
			RETURN 1
		CASE "消極的従う", "許しを乞う"
			PRINTFORMW 「ちゃんと…見て…？」
			CALL HTMLPRINTW(@"「目ぇそらしちゃ…だめ…%HTML_HEARTB()%　はぁ%HTML_HEARTB()%」")
			RETURN 1
		CASEELSE
			;条件から漏れたらRETURN 0
			RETURN 0
	ENDSELECT
	;終端でRESULTは返さない
ENDIF

