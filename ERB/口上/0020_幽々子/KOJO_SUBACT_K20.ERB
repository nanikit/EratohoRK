;========================================================================
;조교 액션·커맨드 구상
;========================================================================

;오리지날로 기술이 없었던 개소는 comment out 해 둡니다@/L

;─
;탈의 액션 구상
;─
;·조교자 액션의 앞에 강제탈의나 자주탈의가 발생했을 때에 표시
;·이 구상의 시점에서는 아직 TEQUIP는 갱신되어 있지 않다
;·첫회 분기는 「처음 보았다/보였다」란 할 수 없다.
;  숍 메뉴로 벗게 했던 적이 있을지도 모르고, 조교의 흐름으로 완전하게 벗게 하지 않고서 노출만 하고 있는 경우도 있다.
;  어쩌면 조수였던 때에 본 적이 있을지도 모른다. 그러한 의미에서는 간이적인 것이다.
;  엄밀한 의미에서의 「초견」플래그를 준비할지 어떨지는 요망하는 대로라고 하는 일로.
;-------------------------------------------------
@KOJO_DATUI_K20
;-------------------------------------------------
;관련 변수…
;강제탈의:TFLAG:102  자주탈의:TFLAG:103
;=(-1, 강제탈의에 저항, 1, 윗도리, 2, 하반신윗도리, 3, 웃도리, 4, 상반신 전부, 5, 하반신 전부,
;  6, 액세서리, 7, 액세서리 이외 모두, 8, 전라까지, 9, 속옷만)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의에 저항된 FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 < 0
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && FIRSTDATUI(0)
	;ENDIF
	
	;=================================================
	;자신은 벗었다
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:자주탈의
	;ENDIF
	
	;아라이먼트정
	IF ALI(0)
		PRINTFORMW 「～응, 그런 일,～응」
		PRINTFORMW 「착의 플레이가 소망인 거네」	
	;아라이먼트부
	ELSE
		PRINTFORMW 「… 아라. 무슨 생각」
		PRINTFORMW 「벌이 소망일까?」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;두 사람 모두 뭔가 벗은 FIRSTDATUI(1)
;------------------------------------------------
;·주로 섹스계 행동의 예비 동작으로 발생한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && FIRSTDATUI(1) && ZENRACHECK(0)
	;ENDIF
	
	;=================================================
	;조교자가[정욕]상태
	;=================================================
	IF 구상기입체크(100) && T_COND("정욕")
		PRINTFORMW 「읏, 이런 것 입지 말고」
		PRINTFORMW 「서로 칠하는 칠하는에, 군요?」
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「그런데 그쪽도 벗어 해요～?」
	PRINTFORMW 「소망이라면 찢어 줄까요?」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·액세서리만 FIRSTDATUI(2)
;------------------------------------------------
;·주로 양말. 그 밖에 장갑이나 소매 따위
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 == 6 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;전라가 되었다(벗기 전은 양말 이외 알몸이라든지 그러한 느낌)
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:자주탈의 && ZENRACHECK(0)
	;ENDIF
	
	;=================================================
	;조교 대상이 남자
	;=================================================
	IF 구상기입체크(100) && TALENT:MASTER:남자
		PRINTFORMW 「～응, 나 그다지 양말은 좋아하지 않네요」
		PRINTFORMW 「그렇다!   이것으로 끝 쪽을 괴롭혀 줄까」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·만p FIRSTDATUI(3)
;------------------------------------------------
;·이번 탈의로 성기가 보이게 되었을 경우에 표시
;·팬티 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(3)
	;ENDIF
	
	;=================================================
	;벌써 가슴이 보이고 있다
	;=================================================
	;IF 구상 기입 체크(0) && B_DATUICHECK(0) == 1
	;ENDIF
	
	;=================================================
	;응 이렇게 p(조교자가 후타나리)
	;=================================================
	;IF 구상 기입 체크(0) && PENIS(TARGET)
	;ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「우후후, 어때? 넣고 싶다」
	PRINTFORMW 「아무래도라고 한다면 날름날름 시켜 주어도 괜찮아요?」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·말하는 p FIRSTDATUI(4)
;------------------------------------------------
;·이번 탈의로 젖가슴이 보이게 되었을 경우에 표시
;·브라 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && ! TALENT:거유 && FIRSTDATUI(4)
	;ENDIF
	
	;=================================================
	;벌써 성기가 보이고 있다
	;=================================================
	;IF 구상 기입 체크(0) && P_DATUICHECK(0) == 1
	;ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	;IF 구상 기입 체크(0) && ONCEDATUI(4)
	;ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	;현행 사양에서는 가슴의 크기는 변경 가능한 것으로 거유의 경우를 절단 분리@/L
	PRINTFORMW 「어때? 　\@ TALENT:거유 ? 크지요?  # 나의 가슴 \@」
	PRINTFORMW 「들이마시고 싶어?   비비고 싶은이야?   끼워지고 싶어?   어떤 것일까?」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·팬티 보이고 FIRSTDATUI(5)
;------------------------------------------------
;·이번 탈의로 팬티(캐릭터에 의해 드로워즈·트렁크스)가 보이게 되었을 경우에 표시
;·하반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(5)
	;ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	;IF 구상 기입 체크(0) && ONCEDATUI(5)
	;ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「어때? 꽤 추잡하지요?」
	PRINTFORMW 「씌워 줄까요? 랄까」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·브라 보이고 FIRSTDATUI(6)
;------------------------------------------------
;·이번 탈의로 브라(캐릭터에 의해 스포트브라·사라시)가 보이게 되었을 경우에 표시
;·상반신속옷이 없는 캐릭터는 쓰는 필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;첫회
	;=================================================
	;IF 구상 기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(6)
	;ENDIF
	
	;현행 사양은 가슴의 사이즈가 변경 가능한 것으로 조건을 짜 둡니다@/L
	;=================================================
	;조교자가 거유
	;=================================================
	IF 구상기입체크(100) && TALENT:거유
		PRINTFORMW 「우후후, 크지요?」
		PRINTFORMW 「이런 사이즈의 사람은 그래그래 없어요～」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·전라에 FIRSTDATUI(7)·RANDDATUI(8) ～ (11)
;------------------------------------------------
;·「전라까지 벗었다」만이 아니고, 「지금 벗었으므로 전라가 되었다」라고 하는 의미
;·「윗도리 밖에 남지 않은 상태로 윗도리를 벗었다」등도 포함된다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !IS_NOWASSIACTNAME(, "탈의")
	PRINTFORMW 「자, 엣치한 일 합시다?」
	PRINTFORMW 「이 신체가 당신에 대한 제일의 무기야?」
	RETURN 1
ENDIF

[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·액세서리만 FIRSTDATUI(8)
;------------------------------------------------
;·주로 양말. 그 밖에 장갑이나 소매 따위
;·벗긴 중에 접어들지 않는다! 되기 십상인 것으로 이것만은 부위별 텐프레를 준비
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && TFLAG:강제탈의 == 6
	;=================================================
	;전라가 되었다
	;=================================================
	IF 구상 기입 체크(0) && ZENRACHECK(1)
	ENDIF
ENDIF
[SKIPEND]

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·고추 보여라 FIRSTDATUI(9)
;------------------------------------------------
;·이번 탈의로 남성기가 보이게 되었을 경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && PENIS(MASTER)
	PRINTFORMW 「그런데 벗어 해요～」
	PRINTFORMW 「아라, 커지고 있어요」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·그대로응 보여라 FIRSTDATUI(10)
;------------------------------------------------
;·이번 탈의로 여성기가 보이게 되었을 경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상 기입 체크(0) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && ! TALENT:MASTER:남자
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·젖가슴 보여라 FIRSTDATUI(11)
;------------------------------------------------
;·이번 탈의로 젖가슴이 보이게 되었을 경우에 표시
;·브라 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;·조교 대상이 남자라고 표시하지 않기 때문에, 남자 한정 구상에서는 쓸 필요가 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상 기입 체크(0) && TFLAG:강제탈의 && B_DATUICHECK(1) == 3 && ! TALENT:MASTER:남자
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·팬티 보여라 FIRSTDATUI(12)
;------------------------------------------------
;·이번 탈의로 하반신속옷이 보이게 되었을 경우에 표시
;·조교 대상이 하반신속옷이 없는 캐릭터 한정의 구상이면 쓸 필요가 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
