	;=================================================
	;初回
	;=================================================
	;IF 口上記入チェック(0) && FIRSTDATUI(1) && ZENRACHECK(0)
	;ENDIF
	
	;=================================================
	;調教者が[情欲]状態
	;=================================================
	IF 口上記入チェック(100) && T_COND("情欲")
		PRINTFORMW 「んっ、こんなの着てないで」
		PRINTFORMW 「お互いぬるぬるになろ、ね？」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 「さーてそっちも脱ぎ脱ぎしましょうね～？」
	PRINTFORMW 「ご所望なら引き裂いてあげましょうか？」
	RETURN 1
