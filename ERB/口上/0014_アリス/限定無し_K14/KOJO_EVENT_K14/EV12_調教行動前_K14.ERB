;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(25)
	;=================================================
	;만족 50％이상
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 8) == "높다" ;BASERATIO(TARGET, 8, 80)
		PRINTFORMW 「좋아요… 꽤 좋은 느낌이야」
		PRINTFORM 「자, 계속해요
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;이성 25％이하
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 5) == "낮다" ;! BASERATIO(TARGET, 5, 20)
		PRINTFORMW 「나… 나… 이, 이제(벌써)……」
		RETURN 1
	ENDIF
	
	;=================================================
	;흥미 25％이하
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 6) == "낮다" ;! BASERATIO(TARGET, 6, 20)
		PRINTFORMW 「뭐라고 할까…%RANDTEXT_K14("흥을 깸이, 군요", "질려 왔군요")%」
		RETURN 1
	ENDIF
	
	;=================================================
	;초조 50％이상
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 7) == "높다" ;BASERATIO(TARGET, 7, 50)
		PRINTFORMW 「그렇게 나를 화나게 하고 싶은 것일까요?」
		PRINTFORMW 「좋아요, 깨닫게 해 준다……」
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	IF GET_KFLAG_K14("상사상애") && !RAND:3
		PRINTFORM 「후후…… 즐겁네요
		CALL PRINT_SYMBOL_K14
	ELSE
		PRINTFORMW 「그런데,%RAND_SPLIT("는 다음은/다음은 어떻게 하지")%……」
	ENDIF
	RETURN 1
ENDIF