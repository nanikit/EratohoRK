;────────────────────────────────────
;ASSI_7,愛撫→手淫
;────────────────────────────────────
@KOJO_ACT_ASSI_K14_7
;-------------------------------------------------
;手淫
IF TCVAR:ASSI:助手ACT派生 == 1
	;アリスがご主人様
	IF TALENT:ASSI:ご主人様
		;調教者も器用な指
		IF TALENT:TARGET:器用な指先
			;相思相愛
			IF FAVOR(3000, 3000, ASSI)
				PRINTFORMW 「フフっ、気持ち良さそうね？」
				PRINTFORMW 「腰まで動かしちゃって、そんなにいいのかしら」
				PRINTFORMW 「ほら、私に扱かれてだらしなくなってる顔、もっと良く%CNAME(TARGET, (FLAG:ご主人様))%に見せてあげなさい♪」
			;それ以外
			ELSE
				PRINTFORMW 「%CNAME(TARGET, (FLAG:ご主人様))%も中々器用だけど、私と比べてどうなのかしら？」
				PRINTFORMW 「と言っても、%CNAME(MASTER, (FLAG:ご主人様))%の立場じゃ、下手な事は言えないわよねぇ？　ウフフ…」
			ENDIF
		;それ以外
		ELSE
			;相思相愛
			IF FAVOR(3000, 3000, ASSI)
				PRINTFORMW 「私の手で扱かれるのは、堪らないでしょう？」
				PRINTFORMW 「%CNAME(TARGET, (FLAG:ご主人様))%じゃこんな指捌きは楽しめないでしょうから、仕方無いかしらね？」
				PRINTFORMW 「ほら、イきたければ何時でもいいのよ…？」
				PRINTFORMW 「私の手でどれだけ気持ち良くなっちゃうのか、%CNAME(TARGET, (FLAG:ご主人様))%によく見てもらいなさい♪」
			;それ以外
			ELSE
				PRINTFORMW 「%CNAME(TARGET, (FLAG:ご主人様))%も悪くないんでしょうけど、やっぱりちょっと違う感じかしらね？」
				PRINTFORMW 「まぁこの反応見れば、一目瞭然だけど、ね♪」
			ENDIF
		ENDIF
	;普通の助手
	ELSE
		;調教者も器用な指
		IF TALENT:TARGET:器用な指先
			PRINTFORMW 「%CNAME(TARGET, ASSI)%も器用だけど、私だって負けてないのよ？」
			PRINTFORMW 「ほら、どうなの？　気持ち良いんじゃないの？　ウフフ…体は正直みたいね」
		ELSE
			PRINTFORMW 「ねぇ、私に扱かれるのは、%CNAME(MASTER, ASSI)%のご主人様と比べてどうなのかしら？」
			PRINTFORMW 「あら？　%CNAME((FLAG:ご主人様), ASSI)%には聞かれたくないの？　中々正直なのね、ウフフ…」
		ENDIF
	ENDIF
;愛撫
ELSE
	;アリスがご主人様
	IF TALENT:ASSI:ご主人様
		;相思相愛
		IF FAVOR(3000, 3000, ASSI)
			PRINTFORMW 「ほら、ココがいいんでしょう？　こっちも一緒に弄るのはどうかしら？」
			PRINTFORMW 「あらあら、そんなに感じたら%CNAME(TARGET, (FLAG:ご主人様))%が嫉妬しちゃうわよ？　うふっ…」
		;それ以外
		ELSE
			PRINTFORMW 「私に弄られるのが嬉しいみたいね？」
			PRINTFORMW 「ねぇ、気持ち良いのは分かるけど、そんなに甘えた声を出しては駄目よ？」
			PRINTFORMW 「今は%CNAME(TARGET, (FLAG:ご主人様))%も一緒にいるんだから。ウフフ…」
		ENDIF
	;普通の助手
	ELSE
		PRINTFORMW 「気持ち良いのは分かるけど、そんな甘えた声出されても困るわね」
		PRINTFORMW 「%CNAME(MASTER, ASSI)%のご主人様は%CNAME((FLAG:ご主人様), ASSI)%なのよ？　ウフフ…」
	ENDIF
ENDIF
RETURN 1

