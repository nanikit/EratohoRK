;-------------------------------------------------
RETURN 0

[SKIPSTART]
;射精させた
IF CARESS_SPOT(ASSI) == 1 && NOWEX:MASTER:射精
	SELECTCASE NOWASSIACTNAME(ASSI)
		CASE "口で愛撫"
		CASE "素股"
		CASE "パイズリする"
		CASE "足コキする"
		CASE "逆レイプ"
		CASE "手で愛撫"
	ENDSELECT
;助手として絶頂させた
ELSE
ENDIF
RETURN 3
