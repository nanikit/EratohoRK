@KOJO_EVENT_K14_212(ARG)
;-------------------------------------------------
;引数内訳…ARG(=0,お茶はいかが？/=1,楽しいお茶たいむでした)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;日常0_0 お茶はいかが？
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 0
	;相思相愛
	IF TALENT:DAILY_TARGET:ご主人様 && AFFECTION("相思相愛", (FLAG:ご主人様))
		SELECTCASE RANDEVENT(3, 0, DAILY_TARGET)
			CASE 0
				PRINTFORMW 「さ、ティータイムにしましょう」
				PRINTFORMW 「ティーポットを新調してみたんだけど、どう？ なかなか素敵だと思わない？」
			CASE 1
				PRINTFORMW 「ねぇ見て。ティーカップをお揃いにしてみたんだけど」
				CALL HTMLPRINTW(@"「なに？ 照れてるの？ うふっ、面白い人ね%HTML_PS_K14()%」")
			CASE 2
				PRINTFORMW 「ティータイムと言えば、やっぱりスコーンでしょ？」
				PRINTFORML 「オーブンじゃなくて、フライパンでさくっと焼いてみたんだけど。…って」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%の興味は、食べること自体にしかないみたいね…」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RANDEVENT(3, 1, DAILY_TARGET)
			CASE 0
				PRINTFORMW 「丁度良かったわ、一緒にお茶しましょう」
				PRINTFORMW 「たまにはこういうのもいいでしょう？」
			CASE 1
				PRINTFORMW 「ねぇ、クッキーを焼いたんだけど%CNAME(MASTER, DAILY_TARGET)%も食べるでしょう？」
				CALL HTMLPRINTW(@"「この紅茶にきっと合うと思うわ%HTML_PS_K14()%」")
			CASE 2
				PRINTFORML 「あら、変に警戒しなくていいのよ。別に毒なんて入ってないから」
				PRINTFORMW 「本当にその気があったとして…、そんな回りくどい事する必要なんてないもの」
				PRINTFORMW 「…って、冗談としてはちょっとキツイかしらね？」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

