;COM14, 쾌감을 참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;애원에 파생하면 연주한다
IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 참는다") && !TFLAG:REACT파생
	;2/3로 진
	IF RAND:3
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				;페이드 아웃
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				;콘피그 참조
				SIF ANIME_CONFIG("FADE")
					;FADE가 ON라면 행 삭제
					CLEARLINE 1
				PRINTFORMW 「뭐야 그것, 애매하네요.\@ TALENT:MASTER:남자 ? 남의 주제에# %CALLNAME:MASTER% \@……」
			CASE "무리가 없는 행동"
				SELECTCASE RANDREACT(3, 141)
					CASE 0
						PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 정말로, 속상해」
					CASE 1
						PRINTFORMW 「%CNAME(MASTER, TARGET)%가 분명하게 하지 않으면, 나부터 빼앗을 때까지…」
					CASE 2
						CALL FADEIN(8, "「…………」")
						;WAIT 사이에 두지 않고 연결한다			과연 조금 괴로운 느낌이 들지 않는 것은 아니다. 아이마스크라든지 죽어w
						PRINTFORMDL %CALLNAME%는 가만히%CALLNAME:MASTER%\@ !TEQUIP:아이마스크 ? 의 눈동자# \@를 응시해
						PRINTFORMDW 그\@ !TEQUIP:아이마스크 ? 시선# 분위기\@에 뒷걸음질쳤다%CALLNAME:MASTER%가\@ !TEQUIP:아이마스크 ? 눈# 얼굴\@를 덮으려고 한 순간, 입술을 빼앗았다…
				ENDSELECT
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "무리를 한 행동"
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				IF ANIME_CONFIG("FADE")
					CLEARLINE 1
				ENDIF
				;실은 적극
				IF COMCOR_POSI()
					PRINTFORMW 「적극적으로 응할 생각은 없지만, 거절할 생각도 없다는 것?」
					PRINTFORMW 「뭐 좋아요. 지금은 그런데도……」
				;실은 싫어
				ELSE
					PRINTFORMW 「받아들여지고 있는 실감은 없지만, 그렇다고 해서 거역할 것도 아니다, 라고」
					PRINTFORMW 「흥. 좋아요, 그렇다면…… 좋을대로 시켜 받기 때문에」
				ENDIF
		ENDSELECT
		RETURN 1
	;1/3로 진
	ELSE
		;부하 2라면 처리하지 않고 범용 리액션에 날린다
		IF GET_COM_RANK() == "무리를 한 행동"
			RETURN 0
		;조교자가 "분노"(이)가 아닌 경우, 상사상애/몹시 사랑함/권애의 어느 쪽인가에 히트 했을 경우에 진. FADE를 관련되지 않는 것과 분리한다
		ELSEIF TRUECHECK(AFFECTION("상사상애"), AFFECTION("몹시 사랑함"), AFFECTION("권애")) > 0 && !T_COND("분노")
			;페이드 아웃
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
			;콘피그를 참조
			IF ANIME_CONFIG("FADE")
				;FADE가 ON라면 행 삭제
				CLEARLINE 1
			ENDIF
			;이하, 호감도의 파생을 본다
			IF 0
			;상사상애
			ELSEIF AFFECTION("상사상애")
				PRINTFORMW 「저기, 무슨 일이야…? 좀 더, 언제나와 같이 해요…」
				PRINTFORMDW 앨리스는 소극적인%CALLNAME:MASTER%에도 치워를 느끼면서 입술을 탐내고 있다…
			;몹시 사랑함
			ELSEIF AFFECTION("몹시 사랑함")
				PRINTFORMW 「뭐야, 조금 소극적이구나… 응츗…… 츄웃…」
			;권애
			ELSE
				PRINTFORMW 「이봐요, 그렇지 않아…… 속상하네요」
			ENDIF
			RETURN 1
		ELSEIF AFFECTION("혐오") || T_COND("분노")
			PRINTFORMW 「나에게 입술이 유린되는 기분은 어떠세요…?」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%의 기분이 어떻게든, 멈출 생각은 없어요…」
			RETURN 1
		ELSE
			PRINTFORMW 「\@ TALENT:MASTER:작은체형 ? 이 아이,# \@키스도 온전히 할 수 없는 것일까…?」
			PRINTFORMW 「%RAND_SPLIT("퇴색해요/우둔")%군요…」
			SIF TALENT:MASTER:작은체형
				PRINTFORMW 「라는 아무튼, 방법 없는 생각도 들지만」
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF
