;COM15, 쾌감을받아들인다/COM16, 기분좋다！등을 상정. ACT103, 역강간의 경우, COM20, 허리를흔드는도 REACT분류받아들이는이 된다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류, 받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다")
	;=================================================
	;조교 대상이갈것같다		자궁입구에 닿을 만큼 깊게 운운…
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:갈것같다
		CALL PRINT_ACTGR_10_ACCEPT_0_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자가갈것같다
	;=================================================
	IF 구상기입체크(100) && PALAM:쾌V > 8500
		CALL PRINT_ACTGR_10_ACCEPT_1_K14
		RETURN 1
	ENDIF
	
	;조교 대상/조교자가갈것같다인 경우 이외를 처리
	;=================================================
	;그 외
	;=================================================
	IF 	구상기입체크(70)
		CALL PRINT_ACTGR_10_ACCEPT_2_K14
		RETURN 1
	ENDIF
;조건으로부터 빠지면 범용 리액션(COM)에
RETURN 0
ENDIF

;소극적 반응을 정리한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;소극적으로 따랐다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다/용서를빈다")
	;=================================================
	;REACT부하 2(무리를 한 행동)
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
		;실은 적극
		IF COMCOR_POSI()
			PRINTFORMW 「… 응.\@ IS_COMGRONAME("용서를빈다") ? 이 모습이 부끄럽다고…,%CNAME(MASTER, TARGET)%가 부끄러워해 어떻게 하는거야!# 부끄러워해 보인 곳에서, 허리는 멈추지 않는거네… \@」
			PRINTFORMW 「이봐요, 그런 것은 나…, 전혀 부족한거야. 좀 더 격렬하게 시라고 줘…」
		;실은 싫어
		ELSE
			PRINTFORMW 「…\@ IS_COMGRONAME("소극적 따른다") ? 단조롭고 시시해요 # 나의 움직임에 맞추는 일도 할 수 없다든가 \@. 지루하구나,%CNAME(MASTER, TARGET)%…」
			PRINTFORMW 「그러니까, 그렇지 않아서! 아 이제(벌써)…, 정말로%RAND_SPLIT("속상하다/안타깝다/안절부절 한다")%\@ TENSION(TARGET, 7) == "높다" ? 의 것!  # 원이군요… \@」
		ENDIF
		RETURN 1
	ENDIF
	
	;REACT파생 1, 소극적인의가 마음에 들지 않는 것 경우는 COM로 처리
	;=================================================
	;COM41, 소극적한다 「소극적이지만 좋은가」
	;=================================================
	IF 구상기입체크(55) && IS_NOWCOMNAME("소극적한다") && TFLAG:REACT파생 == 0
		IF AFFECTION("상사상애")
			PRINTFORMW 「저기…… 좀 더…… 언제나와 같이 범해요……」
			PRINTFORMW 「고기 인형을 취급하는 것 같이…… 망가질 정도로, 격렬하게 찔러요……!」
		ELSEIF AFFECTION("몹시 사랑함")
			PRINTFORMW 「어떻게 한거야…… 이봐요, 좀 더 찔러요」
			PRINTFORMW 「얕은 곳 희롱해지고…… 안타깝고… 미칠 것 같아…」
		ELSEIF AFFECTION("권애")
			PRINTFORMW 「뭔가 소극적이구나…… 안타까워요…」
		ELSE
			PRINTFORMW 「좀 더 허리 흔드세요…」
			PRINTFORMW 「이봐요, 좀 더 마음껏…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;허리를흔드는
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("허리를흔드는")
	;=================================================
	;갈것같다상태
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:갈것같다
		SELECTCASE TCVAR:MASTER:초조하게해번
			CASE 1 ;초조하게해 종료
				;HEART가 있는 없음만의 간단 처리로 적당히 얼버무린다!
				IF AFFECTION("상사상애")
					;RR 2
					SELECTCASE RANDREACT(2, 2)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응앗! 바, 바보! 그렇게 격렬하게 찔리면!」
							CALL QUAKE(4)
							PRINTFORM 「겨, 격렬해져! 아읏
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아, 아앗!!」")
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「하웃! 앗
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아! 이, 이런 건… 은!」")
							CALL QUAKE(4)
							PRINTFORMW 「마, 망가진닷! 부서져 버린다앗!」
					ENDSELECT
				ELSE
					;RR 3
					SELECTCASE RANDREACT(2, 3)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응앗! 바, 바보! 그렇게 격렬하게 찔리면!」
							CALL QUAKE(4)
							PRINTFORMW 「겨, 격렬해져! 아웃, 아앗!!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「하웃! 아, 아앗! 이, 이런 건… 은!」
							CALL QUAKE(4)
							PRINTFORMW 「마, 망가진닷! 부서져 버린다앗!」
					ENDSELECT
				ENDIF
				PRINTFORMDW 배후로부터의 격렬한 공격에, 앨리스는%RAND_SPLIT("머리카락을 흩뜨려/미친 것처럼")%허덕여 몸부림치고 있다…
			CASEELSE ;초조하게해 없음
				IF AFFECTION("상사상애")
					;RR 4
					SELECTCASE RANDREACT(2, 4)
						CASE 0
							CALL QUAKE(4)
							PRINTFORM 「아앗! 아, 안쪽에 해당되엇! 아웃, 아앗
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORMW 「야앗, 거기! 동글동글 되면!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「아앗, 앗
							CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
							PRINTFORM   팥고물, 하앗! 응응앗
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORM 「이, 이런 건…! 겨, 견딜 수 있는 것……
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					ENDSELECT
				ELSE
					;RR 5
					SELECTCASE RANDREACT(2, 5)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「아앗! 아, 안쪽에 해당되엇! 아웃, 아앗!」
							CALL QUAKE(4)
							PRINTFORMW 「야앗, 거기! 동글동글 되면!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「아앗, 앗! 아읏, 하앗! 응응앗!!」
							CALL QUAKE(4)
							PRINTFORMW 「이, 이런 건…! 겨, 견딜 수 있는 것……!」
					ENDSELECT
				ENDIF
				PRINTFORMDW 배후로부터의 고문에 피학심이 자극되어 앨리스는 전신을 진동시키면서 허덕여 몸부림치고 있다…
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;서로 정욕
	;=================================================
	IF 구상기입체크(100) && M_COND("정욕") && T_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「하웃, 아읏! 아아…… 아… 아앗, 아앗!!」
		CALL QUAKE(4)
		PRINTFORMW 「응앗! 이, 이제(벌써)…… 정말! 이, 이상해져 버린닷!」
		PRINTFORMDW 정욕에 끓어오르는 두 명은 서로 서로 미칠 듯이 요구해, 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 정욕
	;=================================================
	IF 구상기입체크(100) && M_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「앗, 후앗! 시, 싫닷! 이런 모습… 아… 아″…」
		CALL QUAKE(4)
		PRINTFORMW 「앗, 하앗! 야… 도, 안됏! 아, 아앗!」
		PRINTFORMDL 정욕에 끓어오른다%CALLNAME:MASTER%는 앨리스의 양손을 잡아,
		PRINTFORMDW 활처럼 된 그 신체를 배후로부터 마음껏 탐냈다…
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자가 정욕
	;=================================================
	IF 구상기입체크(100) && T_COND("정욕") ;조교자가 정욕
		CALL QUAKE(4)
		PRINTFORMW 「후앗! 시, 싫다…… 허리… 멈추지 않게 되어 버린다앗!」
		CALL QUAKE(4)
		PRINTFORMW 「읏, 후웃… 핫, 아읏! 거, 거기! 아웃, 아아…」
		PRINTFORMDW 정욕에 끓어오르는 앨리스는 상체를 뒤로 젖히게 하면서 격렬하게 허리를 흔들어, 쾌락을 탐내고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(60)
		IF AFFECTION("상사상애")
			;RR 6
			SELECTCASE RANDREACT(2, 6)
				CASE 0
					CALL QUAKE(4)
					PRINTFORM 「아
					CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
					PRINTFORM   아…… 아, 앗
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					PRINTFORMW 「이, 이나다 이것! 굉장히, 흥분해… 라고!」
				CASE 1
					CALL QUAKE(4)
					PRINTFORM 「읏…, 앗
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  … 응읏…!」")
					PRINTFORM 「싫다, 나… 이런 모습인데…… 하웃, 앗
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아!!」")
					CALL QUAKE(4)
			ENDSELECT
		ELSE
			;RR 7
			SELECTCASE RANDREACT(2, 7)
				CASE 0
					CALL QUAKE(4)
					PRINTFORMW 「아…… 아아…… 아, 앗!」
					PRINTFORMW 「이, 이나다 이것! 나…… 굉장히, 흥분해… 라고!」
				CASE 1
					CALL QUAKE(4)
					PRINTFORMW 「읏…, 앗! 후우… 응읏…!」
					PRINTFORMW 「싫다, 나… 이런 모습인데…… 하웃, 아, 아앗!!」
					CALL QUAKE(4)
			ENDSELECT
		ENDIF
		PRINTFORMDL 배후로부터의 공격에,
		PRINTFORMDW 앨리스는%PLEASURE_DESCRIPTION(TARGET)%허덕이고 있다…
		RETURN 1
	ENDIF
ENDIF

