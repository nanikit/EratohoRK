	;派生：「私に意見するなんて生意気ね！」
	;=================================================
	IF 口上記入チェック(100) && TFLAG:REACT派生
		SELECTCASE RANDREACT(2, 0)
			CASE 0
				PRINTFORMW 「ウルサイわねぇ。%CNAME(MASTER, TARGET)%の意見なんて聞いてないわ！」
				PRINTFORMW 「大人しくしなさいよ」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER, TARGET)%がそう言う度に許してたんじゃ、話にならないのよ」
				PRINTFORMW 「いい加減、諦めたらどうなの！？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;派生：「ふーん」
	;=================================================
	IF 口上記入チェック(100) && !TFLAG:REACT派生
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「フフっ、駄目よ。そんな事言っても許してあげないわ」
