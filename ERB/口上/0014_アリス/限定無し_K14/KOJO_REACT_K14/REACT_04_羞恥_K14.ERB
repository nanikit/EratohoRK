;消極的なのが気に入らないの場合はCOMへ流す
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM41, 消極的やる RANDREACT(20)~
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("消極的やる") && TFLAG:REACT派生 != 1 && TFLAG:REACT負荷 < 2
	IF 0
	;FAVOR(3000, 3000)
	ELSEIF AFFECTION("相思相愛") && !T_COND("怒り") && PERCENT(90)
		PRINTFORMW 「いまさら恥ずかしがる事ないでしょ…？」
		PRINTFORM 「でも、そんなとこも可愛いわね
		CALL PRINT_SYMBOL_K14
	;FAVOR(0, 2000)
	ELSEIF AFFECTION("溺愛") && !T_COND("怒り") && PERCENT(80)
