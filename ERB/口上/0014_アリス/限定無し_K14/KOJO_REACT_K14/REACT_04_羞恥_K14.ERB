;REACT負荷の処理もここで完結させる
;COMへは流さない
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM44, 平然とやる RANDREACT(30)~
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("平然とやる")
	SELECTCASE GET_COM_RANK()
		CASE "自然な行動"
			;調教者BASE:満足派生
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORMW 「それが%CNAME(MASTER, TARGET)%の普段通りのオナニーって訳ね？」
					PRINTFORMW 「ま、正直…少しは恥じらいがないと面白くはないのは確かなんだけど…」
					PRINTFORM 「この際だから、じっくり鑑賞させて貰う事にするわ
				CASEELSE
					PRINTFORMW 「私に見られる事に抵抗はないのね？ まぁいいわ」
					PRINTFORM 「それならそれで、じっくり見させて貰うから
			ENDSELECT
			;PSでまとめて閉じる
			CALL PRINT_SYMBOL_K14
			;1/2
			IF RAND:2
				PRINTFORMW 「ほら、もっと私によく見えるようにシて%RANDTEXT_K14("頂戴", "＿")%…」
			;1/2
			ELSE
				PRINTFORMW 「%CNAME(MASTER, TARGET)%がどこでどう良くなっちゃうのか、私に見せて頂戴…」
			ENDIF
		CASE "無理のない行動"
			PRINTFORMW 「流石に完全に%RAND_SPLIT("普段/いつも")%通りとはいかないみたいね？」
