			CASE 1
				PRINTFORMW 「いい調子ね？ フフ…」
				PRINTFORMW 「そう、目を背けないで。しっかりこっちを見てやるのよ」
				PRINTFORM 「私はじっくり鑑賞させて貰うから…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ELSE
		SELECTCASE RANDREACT(2, 43)
			CASE 0
				PRINTFORMW 「恥ずかし気もなく、よくやるわね？」
				PRINTFORMW 「まぁ、私が言えた義理じゃないけど。フフ…」
			CASE 1
				PRINTFORMW 「へぇ、積極的なのね？ 私に見られて興奮してるのかしら」
				PRINTFORM 「でもまぁ…、ふふっ。悪くない眺めね
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ENDIF
	RETURN 1
