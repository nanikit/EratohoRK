;
;	수치계 리액션 구상을 정리한 파일
;
;Version		Reason
;1. 71			@구상 기입 체크 사양에. 전체적으로 손질
;1. 68 Rel3		IS_NOWACTNAME의 명칭이 부정하기 때문에 에러 초월하는 개소를 수정
;1. 67			REACT40, 자위/REACT45, 방뇨에 COM44, 태연하게한다분기를 추가. REACT43에 거절 처리를 추가
;1. 65			REACT부하는 COM에 흐르도록(듯이). 일부 REACT파생시에는 COM에 흐르도록(듯이)
;1. 62			@IS_NOWCOMNAME의 신설에 수반해 SELECTCOM를 IS_NOWCOMNAME로 변경
;1. 60			ACT분류단체 파일로서 독립
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;수치계 REACT40～45
;========================================================================
;·ACT분류:수치계의 경우, 기본적으로 회화계 COM, 애무계 COM, 봉사계 COM가 선택지가 됩니다만
;·상황에 따라서는 그 외의 것이 선택지에 포함될 가능성도 있습니다
;-------------------------------------------------
;  REACT의 지문은 ACT_MESSAGE.ERB의 TRAIN_MESSAGE2_{TFLAG:ACT}로 처리됩니다
;  기본적으로 지문은 TFLAG:REACT분류(받아들인다/적극적 따른다/태연하게 따르는/용서를빈다/소극적 따른다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운 행동/무리가 없는 행동/무리를 한 행동)으로 파생
;  고유의 TRAIN_MESSAGE2가 없는 경우는, REACTION_MESSAGE로 COM마다 범용 처리됩니다
;-------------------------------------------------

;─
;REACT40, 자위·개별 리액션
;─
;·ACT파생없음. PENIS(MASTER)나 TEQUIP:페니스밴드로 나누는 일은 가능
;-------------------------------------------------
;상정되는 COM…
;회화계 반응
;	COM:용서를빈다
;애무계 반응
;	COM:부끄러워한다/싫어한다/겁없게웃는다/쾌감을참는다/쾌감을받아들인다/기분좋다！
;봉사계 반응
;	COM:거부한다/소극적한다/적극적한다/태연하게한다
;-------------------------------------------------
@KOJO_REACT_K14_40
;-------------------------------------------------
;·지문은 REACT분류로 분기
;·REACT분류가 소극적 따른다/태연하게 따른다/적극적 따른다/받아들인다인 경우는 수용 취급
;·그 외의 반응의 경우는 REACT분류는 모두 거부에 세트 된다(용서를빈다등도 거부 취급인 일에 주의)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 거절했을 경우
;━━━━━━━━━━━━━━━━━━━━━━━━
;잠정적으로 대상 한정 구상측을 호출하고 있었지만 멈춘다@v1. 71
;------------------------------------------------
IF 구상기입체크(100) && IS_COMGRONAME("거부")
	;=================================================
	;부끄러워서 명령 대로로 할 수 없었다((치정선행)
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:치정 > PALAM:MASTER:반항
		;BASE:만족 파생		초조로 주워도 괜찮을지도이지만, BASE:7은 변화가 적기 때문에
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「하? 뭐야. 부끄럽고 할 수 없어?」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 아직도 수치심 같은거 말하는 것을 가지고 있던 거네」
				PRINTFORMW 「약간의 놀라, 라는 곳일까. 저기? 후후」
			CASE "일반"
				PRINTFORMW 「뭐, 확실히 부끄럽겠지만…」
				PRINTFORMW 「그런 일 되면 퇴색하는거야. 재미없네요,%CNAME(MASTER, TARGET)%……」
			CASE "낮다"
				PRINTFORMW 「그렇게,%CNAME(MASTER, TARGET)%를 부끄러워하게 하는 것도 나쁘지 않지만. 그렇지만, 말야…」
				PRINTFORMW 「말해진 일을 하지 않으면,%CNAME(MASTER, TARGET)%에 있어 바람직하지 않은 결과가 되어요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;싫어해 요구에 응하지 않는다(반항 선행)
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:반항 > PALAM:MASTER:치정 
		;BASE:만족 파생
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「그다지 칭찬할 수 있던 것이 아니네요, 그러한 태도…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 자신의 입장을 분별하는 것이 좋아요」
				;만족도가 높은 경우는 놓쳐 받을 수 있는적인
				;그 외의 경우는 벌!
			CASE "일반"
				PRINTFORMW 「말하는 것이 (들)물을 수 없다면, 상응하는 처사를 하는 일이 되지만…」
				PRINTFORMW 「뭐, 방법 없네요?  %CNAME(MASTER, TARGET)%가 바란 결과인 걸」
				TCVAR:행동예약 = 60
			CASE "낮다"
				PRINTFORMW 「나에 따를 수 없다고 말한다면, 상응하는 보답을 받아요」
				PRINTFORMW 「이유는 어떻든지 좋아요.%CNAME(MASTER, TARGET)%는, 자신의 행동에 대해 벌을 받아」
				CALL FADEIN(12, @"「각오%RAND_SPLIT("하군요/하세요")%……」")
				WAIT
				TCVAR:행동예약 = 60
		ENDSELECT
		RETURN 1
	ENDIF
RETURN 0
ENDIF

;조교 대상이 수용 했을 경우만
;━━━━━━━━━━━━━━━━━━━━━━━━
;첫회
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다/적극적 따른다/받아들인다/태연하게 따른다") && FIRSTREACT(0)
	PRINTFORMW 「후응, 과연.\@ PENIS(MASTER) ? 그렇게 단순하게 잡아당기는 것만으로 기분 좋은 것 까 # %CNAME(MASTER, TARGET)%는 그러한 느낌으로 하는 것 \@?」
	PRINTFORMW 「좋아요, 그대로 계속해. 분명하게 보고 있어 주기 때문에…」
	PRINTFORMDL %CALLNAME:MASTER%는%CALLNAME%의 시선의 원이라고 하는 이상한 상황에\@ IS_COMGRONAME("소극적 따른다") ? 부끄러움을 기억하면서도 # 흥분하면서 \@、
	PRINTFORMDW \@ IS_COMGRONAME("소극적 따른다") ? 어쩔 수 없이 # 열중해 \@자신의\@ PENIS(MASTER) ? 페니스를 계속 훑어 냈다 # 비밀스런 장소를 만지작거렸다 \@…
	PRINTFORMDW %CALLNAME%는 웃음을 띄워, 스스로를 위로한다%CALLNAME:MASTER%의 모습을 응시하고 있다…
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;얼굴에 사정하기 자위절정
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ME_PLAYER(1) == 9
	;BASE:만족 파생
	SELECTCASE TENSION(TARGET, 8)
		CASE "높다"
			PRINTFORMW 「이봐요, 여기에 내도 좋아요. 나의 입에…」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%도 그렇게 하고 싶겠지요? 나의 얼굴을 정액으로 질척질척해 보고 싶겠지요?」
			PRINTFORMW 「나의 흥이 깨기 전에, 빨리 하는 것이 좋아요」
			CALL FADEIN(15, "「이런 기회, 좀처럼 없기 때문에……」")
			WAIT
		CASE "일반"
			PRINTFORMW 「뭐 멍청히 하고 있는거야. 입에 내도 좋다고 말하고 있어. 알아?」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%도, 이런 것 싫지 않을 것입니다」
			PRINTFORMW 「나의 기분이 바뀌기 전에, 빨리 시(분)편이 좋다고 생각해요」
		CASE "낮다"
			PRINTFORMW 「이봐요, 여기에 내는거야. 알았어?」
			PRINTFORMW 「스스로도 잘 모르지만, 왠지 그렇게 하고 싶은 기분인 것이군요」
	ENDSELECT
	RETURN 1
ENDIF

;REACT분류로 줍는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:적극적한다/받아들일까 개갈것같다상태로 초조하게해중은 아니다(즉 초조하게해 종료/없음) 경우
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM70, 가게해！로 파생:가게해 주지 않는은 초조하게해번으로 연주하고 있으므로, 여기의 조건에는 걸리지 않는다
IF 구상기입체크(100) && IS_COMGRONAME("적극적 따른다/받아들인다") && TCVAR:MASTER:갈것같다 && TCVAR:MASTER:초조하게해번 < 2
	LOCALS = \@ ALI(0) ? 후후 # 후후 \@
	;=================================================
	;첫회 한편, 확실히 절정에 이르는 경우
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(NOWEX:MASTER:사정, NOWEX:MASTER:C절정) && FIRSTREACT(1)
		PRINTFORMW 「그래, 갈것같다야. 뭐, 그것은 좋지만……」
		PRINTFORMW 「마음대로\@ PENIS(MASTER) ? 사정한다 # 이크 \@의 것은 허락하지 않아요. 내가 좋다고 말할 때까지 인내 하세요」
		PRINTFORM 「내가 허가하기 전에 가면……
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "오·시·오·키·", 1)
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		RETURN 1
	ENDIF
	
	;남자와 후타나리로 나누는 것은 과연 힘들기 때문에 그만두고. 뭐 기분이 내키면 생각하지 않는 것도 아니다w
	;=================================================
	;조교 대상으로 페니스 있어
	;=================================================
	IF 구상기입체크(100) && PENIS(MASTER)
		SELECTCASE RANDREACT(5, 1)
			CASE 0
				PRINTFORMW 「헤에, 이제(벌써) 나와 버려. 그것은 큰 일이구나」
				PRINTFORMW 「그렇다면 뭔가 말하는 것이 있을까요? 시치미를 떼어도 안 돼요」
				PRINTFORMW 「이봐요, 말하세요. 『사정시켜 주세요』는…」
			CASE 1
				PRINTFORMW 「나에게 허가를 요구한다든가, 꽤 기특한 배려군요.%LOCALS%」
				PRINTFORMW 「여기서 보고 있어 주기 때문에, 마음껏 하면 좋아요」
			CASE 2
				PRINTFORMW 「흐음, 그렇게. 나에게 보여지면서 정액 털어 놓고 싶은거네. 이 변태…」
				PRINTFORM 「그러면, 그렇구나…『
				CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), @"\@ TALENT:MASTER:남자 ? 나 # 나 \@가 사정하는 곳 봐 주세요", 1)
				PRINTFORMW 』라고라도 말해 받을까.%LOCALS%…」
			CASE 3
				PRINTFORMW 「뭐야, 이제(벌써) 나와 버리는 거야? 나에게 보여지면서, 쫄깃쫄깃 해 사정해 버리는 것?」
				PRINTFORMW 「바보 같은 얼굴 하면서, 필사적으로 다뤄 버려… 그렇게 기분 좋아? 이 변태!」
			CASE 4
				PRINTFORMW 「이제 내자? 상당히 진전된 것 같아 무엇보다네」
				PRINTFORM 「나는 여기서 보고 있기 때문에, 기분이 풀릴 때까지 쫄깃쫄깃 하면 좋아요. 변태\@ TALENT:MASTER:남자 ? 쿠 # 씨 \@…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 페니스밴드장착 안
	;=================================================
	IF 구상기입체크(100) && TEQUIP:페니스밴드
		SELECTCASE RANDREACT(3, 2)
			CASE 0
				PRINTFORMW 「그래, 갈것같다야. 그런 것을 몸에 대고 있어도 좋아져 버리는 거네?」
				PRINTFORMW 「오히려, 몸에 대고 있기 때문이야말로… 인지도이지만.%LOCALS%…」
			CASE 1
				PRINTFORMW 「아라아라, 사양이 없는 사람이네. 남자에라도 된 것 같은 기분으로 흥분해 버렸다든가?」
				PRINTFORMW 「뭐,%CNAME(MASTER, TARGET)%의 경우… 보여져와 흥분해 버리는, 은만의 이야기일까」
			CASE 2
				PRINTFORMW 「아라, 좋네요. 여자 아이가 그런 것 몸에 대면서, 어떤 식으로 가 버리는 것인가…」
				PRINTFORMW 「모처럼의 기회이니까, 차분히 감상시켜 받아요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외(조교 대상이 여자 아이 한편 페니스밴드장착 안이 아닌 경우)
	;-------------------------
	SELECTCASE RANDREACT(5, 3)
		CASE 0
			PRINTFORMW 「방법 없네요. 이제 와서 안된다고 말해도, 이제 멈추지 않는 것 같고….%LOCALS%」
			PRINTFORM 「뭐, 손을 대지 않고… 다만 보고 있다는 것도 이따금은 좋네요
			CALL PRINT_SYMBOL_K14
		CASE 1
			PRINTFORMW 「마음대로 가지 않았던 것은 칭찬해 주어도 괜찮아요. 좋은 배려군요」
			PRINTFORMW 「나는 여기서 보고 있기 때문에, 좋을대로하면 좋아요」
		CASE 2
			PRINTFORMW 「헤에, 그렇게. 나에게 보여지면서 가 버리는 것…」
			PRINTFORMW 「오히려 나에게…, 가는 곳을 보았으면 좋은 것일까. 저기?」
		CASE 3
			PRINTFORMW 「-알았어요. 자위 하면서 가 버리는 곳, 내가 보았으면 좋겠지요?」
			PRINTFORMW 「여기서 보고 있어 주기 때문에,%CNAME(MASTER, TARGET)%의 기분이 풀리도록(듯이) 하면 돼요」
			;1/3로 진
			IF !RAND:3
				PRINTFORM 「이런 것, 싫지 않아%RAND_SPLIT(". 오히려 좋아하고/해")%. 저기…
				CALL PRINT_SYMBOL_K14
			ENDIF
		CASE 4
			PRINTFORMW 「\@ GET_KFLAG_K14("상사상애") ? 에, 가 버린다 # -응, 그렇게… 갈것같다야 \@。%RAND_SPLIT("나의 눈앞/남의 앞")%로 자위 하면서. 저기?  %LOCALS%」
			PRINTFORMW 「그래서? 내가 보았으면 좋다는 것? 그렇겠지요」
			PRINTFORMW 「이봐요, 정직하게 말\@ GET_KFLAG_K14("상사상애") ? 는 보세요 # 우노 \@야……」
	ENDSELECT
	RETURN 1
ENDIF

;쾌락에 대해서부끄러워한다의 경우는 COM에 흘린다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM10, 부끄러워한다(상황에 대해서) RANDREACT(10)~
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다") && TFLAG:REACT파생 == 0
	SELECTCASE RANDREACT(2, 10)
		CASE 0
			PRINTFORMW 「부끄러우면 그만두면 좋다고 생각하지만」
			PRINTFORMW 「그렇지만, 손은 멈추지 않네요?  \@ AFFECTION("상사상애") ? 후후 # 후후 \@」
		CASE 1
			PRINTFORMW 「아라, 그렇게부끄러워한다일 없어요」
			PRINTFORM 「지금의%CNAME(MASTER, TARGET)%、\@ AFFECTION("상사상애") ? 매우 # 꽤 \@사랑스러워요
			CALL PRINT_SYMBOL_K14
	ENDSELECT
	RETURN 1
ENDIF

;소극적인의가 마음에 들지 않는 것 경우는 COM에 흘린다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM41, 소극적한다 RANDREACT(20)~
