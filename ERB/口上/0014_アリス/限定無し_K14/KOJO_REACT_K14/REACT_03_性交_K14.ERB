;-------------------------------------------------
@KOJO_REACT_K14_34
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속 실행/2=다른 V성교로부터/3=A성교로부터/4=V성교 봉사로부터/5=A성교 봉사로부터)
;-------------------------------------------------
;·캔슬 처리는 ACT_MESSAGE_OTHERS에서 범용 처리
;·주도권은 조교자(SEX_INITIATIVE(TARGET))
;-------------------------------------------------
;오토모드 용처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을받아들인다/기분좋다！/허리를흔드는")
	;=================================================
	;COM20, 허리를흔드는		80%
	;=================================================
	IF 구상기입체크(80) && IS_NOWCOMNAME("허리를흔드는")
		LOCALS = %RAND_SPLIT("하, 후~/응읏, 구/아…, 응응/하, 아/쿠웃, 응/핫, 하아, 하아")%……
		;소비 기력에 의한 파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORM 「%LOCALS%
				;조교자 BASE:만족 파생
				SELECTCASE TENSION(TARGET, 8)
					CASE "높다"
						PRINTFORMW 그렇구나, 말해질 것도 없이 그럴 생각이었지만. 키득」
					CASE "일반"
						PRINTFORMW 졸라댐이라니 드무네요. 후후」
					CASE "낮다"
						PRINTFORMW 후후, 욕심쟁이인」
				ENDSELECT
				;조교자의 기능에 의해 분기
				IF ABL:TARGET:성교 > 4
					PRINTFORMW 「이봐요, 이렇게?\@ PENIS(MASTER) ? 다뤄# 손가락끝으로 집어\@갖고 싶었던 것이지요?」
					PRINTFORMW 「나의 손가락 처리해, 마음껏 만끽하면 좋아요……」
				ELSE
					PRINTFORMW 「응, 그렇지만 이것…,%RAND_SPLIT("생각했던 것보다/꽤/의외로")%어렵네요……」
				ENDIF
			CASE "무리가 없는 행동"
				PRINTFORM 「%LOCALS%
				PRINTFORMW 좋은 소리로 울어 주어 버려…」
				PRINTFORMW 「어디가 제일 기분 좋은 것일까. 저기? 후후」
			CASE "무리를 한 행동"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%를 완구로 하고 있는 것 같고 나쁘지 않네요, 이것…」
				PRINTFORM 「무엇보다…,%CNAME(MASTER, TARGET)%라도 그렇지만도 않은 것 같지만
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		;조교자가갈것같다인 경우 FADEOUT
		IF TCVAR:갈것같다
			CALL FADEOUT(15, @"(라든지 말하면서, 나도%RAND_SPLIT("슬슬 갈것같다인/여유 없다")%의 것이군요……)")
			;콘피그를 참조
			SIF ANIME_CONFIG("FADE")
				;FADE가 ON라면 행 삭제
				CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;COM15/16, 쾌감을받아들인다/기분좋다！
	;=================================================
	IF 구상기입체크(100) && ((IS_NOWCOMNAME("쾌감을받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분좋다！") && PERCENT(65)))
		CALL PRINT_REACT_30_COM15_COM16_K14
		RETURN RESULT
	ENDIF
RETURN 0
ENDIF

