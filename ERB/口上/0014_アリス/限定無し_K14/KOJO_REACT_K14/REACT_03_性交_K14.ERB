;========================================================================
;성교계 REACT30～35
;========================================================================
;· 각 REACT내의 나누는 방법으로서 TFLAG:REACT분류(IS_COMGRONAME)나 SELECTCOM(IS_NOWCOMNAME)가 생각됩니다
;·ACT분류:성교계의 경우, 기본적으로 애무계 COM(COM10~COM16), 성교계 COM(COM20~COM22), 고통계 COM(COM30~COM33)가 선택지가 됩니다만
;·상황에 따라서는 그 외의 것이 선택지에 포함될 가능성도 있습니다
;-------------------------------------------------
;=================================================
;관련 변수…
;
;TFLAG:REACT분류 (0=없음/1=소극적 따른다/2=적극적 따른다/3=허가를청한다/4=날뛴다/5=요구에 응하지 않는, 거부/6=도망치는, 발버둥 친다)
;IS_COMGRONAME("없음/소극적 따른다/적극적 따른다/허가를청한다/날뛴다/거부/도망친다")
;
;FIRSTREACT
;첫회 ACT 판정 함수
;TFLAG:ACT(TFLAG:90)를 자동으로 참조, 현재 실행되고 있는 REACT 마다 번호가 독립해 보존된다
;
;ONCEREACT
;조교내미독판정 함수  조교전에 초기화된다
;그 외의 사양은 거의 FIRSTREACT와 같이
;=================================================

;─
;REACT30, 정상위·개별 리액션
;─
;상정되는 COM…
;회화계 반응
;	COM:좀더하드하게해
;애무계 반응
;	COM:싫어한다/서투르다고매도한다/쾌감을참는다/쾌감을받아들인다/기분좋다！
;성교계 반응
;	COM:허리를흔드는/안에내！/안에내지마！
;고통계 반응
;	COM:날뛴다
;-------------------------------------------------
@KOJO_REACT_K14_30
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속 실행/2=다른 V성교로부터/3=A성교로부터/4=V성교 봉사로부터/5=A성교 봉사로부터)
;·주도권은 조교자(SEX_INITIATIVE(TARGET))
;-------------------------------------------------
;오토모드 용처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을받아들인다/기분좋다！/허리를흔드는")
	;=================================================
	;COM20, 허리를흔드는		80%
	;=================================================
	IF 구상기입체크(80) && IS_NOWCOMNAME("허리를흔드는")
		LOCALS = %RAND_SPLIT("하, 후~/응읏, 구/아…, 응응/하, 아/쿠웃, 응/핫, 하아, 하아")%……
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORM 「%LOCALS%
				;조교자가갈것같다. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:갈것같다 || PALAM:쾌C > 6000
					PRINTFORMW 사랑스러운 소리로 울어 주어 버려…」
					PRINTFORMW 「나%RAND_SPLIT(", 견딜 수 없게 되어 버려요/머, 멈추지 않게 될 것 같다")%……」
				;그 외
				ELSE
					PRINTFORMW 후후, 응석부린 소리 내 버려…」
					PRINTFORMW 「좀 더, 좀 더 좋은 소리로 울려지고 싶어져 버려요……」
				ENDIF
			CASE "무리가 없는 행동"
				PRINTFORM 「%LOCALS%
				;조교자가갈것같다. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:갈것같다 || PALAM:쾌C > 6000
					PRINTFORMW %RAND_SPLIT("엣치한/추잡하다/꽤")%허리사용,…」
					PRINTFORMW 「그런 일 당하면, 이제(벌써)%RAND_SPLIT("나……, 스윗치 들어가 버리기 때문/…… 그만두어 줄 수 없으, 니까")%…」
				;그 외
				ELSE
					PRINTFORMW 후후, 좋은 느낌이야…」
					PRINTFORMW 「이봐요, 좀 더 나를 느껴……」
				ENDIF
			CASE "무리를 한 행동"
				;조교자가갈것같다. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:갈것같다 || PALAM:쾌C > 6000
					PRINTFORMW 「%LOCALS%얌전하고, 몸을 맡기기로 했다는 것?」
					PRINTFORMW 「뭐, 그런 일이라면……,%RAND_SPLIT("소망 대로, 범해 주어요/마음껏 범해 주기 때문에")%…」
				;그 외
				ELSE
					PRINTFORMW 「그래그래, 그것으로 좋은거야…」
					PRINTFORMW 「언제까지나 쾌락에 저항하는 것은,%CNAME(MASTER, TARGET)%라도 괴로울 것입니다?」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;COM측의 충실도에 의해 확률을 치우치게 한다든가 그런 생각. REACT마다 나누어 쓰는 것이 귀찮게 되었으므로 범용 처리로 끝마친다
	;=================================================
	;COM15/16, 쾌감을받아들인다/기분좋다！
	;=================================================
	IF 구상기입체크(100) && ((IS_NOWCOMNAME("쾌감을받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분좋다！") && PERCENT(65)))
		;범용 처리를 부른다
		CALL PRINT_REACT_30_COM15_COM16_K14
		;RESULT치는 함수측에서 지정. 나중에 바꾸는 경우 이 (분)편이 락이라든지 그런 생각
		RETURN RESULT
	ENDIF
RETURN 0
ENDIF

;일단 COM 조건에 허가를청한다를 포함하고 있지만, 우선 없는 것이라고 생각하고 있다. 무서워하고는 너무 귀찮으므로 고려하지 않는다. 발생하면 COM로 어떻게든 한다고 할 자세
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:허가를청한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("허가를청한다") && IS_NOWCOMNAME("허가를청한다/쾌감을참는다")
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운 행동"
			;BASE:만족에 의한 파생(높다=50%이상/일반=25%이상/낮다=25% 미만)
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「나, 말하지 않았던가? 그런 일 하는 것은 역효과래…」
					PRINTFORMW 「오히려%CNAME(MASTER, TARGET)%를, 좀 더 엉망진창으로 하고 싶어져 버려요……」
				CASE "일반"
					PRINTFORMW 「자주(잘) 말해요. 사실은 느껴 버리고 있는 주제에…」
					PRINTFORMW 「거짓말쟁이인 아이에게는, 벌 해 주지 않으면. 저기?」
				CASE "낮다"
					PRINTFORMW 「이제 와서 그런 일 말해져도, 제지당할 이유 없잖아」
					PRINTFORMW 「얌전하게 단념하는 거네…」
			ENDSELECT
		CASE "무리가 없는 행동"
			;BASE:만족에 의한 파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「그만두어라고 말해지면, 더욱 더 그만두고 싶지 않게 되어 버리는거네요…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%도…, 내가 그 정도는 그만두지 않는다고, 알아 하고 있는 것이 아닌거야?」
				CASE "일반"
					PRINTFORMW 「지금 그만두면, 괴로운 것은%CNAME(MASTER, TARGET)%의 (분)편이라고 생각하지만 말야?」
					PRINTFORMW 「정말로 그만두었으면 좋으면, 그런 소리는 나오지 않을 것이라고 생각하지만」
				CASE "낮다"
					PRINTFORMW 「그런 일 하는 것 치고, 상당히 좋은 소리가 나와 버리고 있는 것 같지만 말야?」
					PRINTFORMW 「『이런 일 그만두어』는…, 그것이%CNAME(MASTER, TARGET)%의 본심인 것인가, 이상한 곳이네」
			ENDSELECT
		CASE "무리를 한 행동"
			;실은 기분이 좋다
			IF COMCOR_POSI()
				PRINTFORMW 「허락해라고 말하면서, 상당히 좋은 느낌의 허리사용?」
				PRINTFORMW 「사실은 싫지 않겠지요. 이봐요, 정직하게 말해 보세요…」
			;실은 싫어
			ELSE
				PRINTFORMW 「말해져 그만둘 정도라면, 최초부터 하고 있지 않아요」
				PRINTFORMW 「적당, 단념하면 어때?」
			ENDIF
	ENDSELECT
	PRINTL
	PRINTFORMDW %CALLNAME%는 몸을 비튼다%CALLNAME:MASTER%를 억누르면서 계속 범했다…
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적 따른다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류로 단락짓고 있는 이상, REACT파생는 고려하는 필요없음
;------------------------------------------------
IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다") && IS_NOWCOMNAME("부끄러워한다/싫어한다/서투르다고매도한다/쾌감을참는다")
	;=================================================
	;COM10, 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「솔직하지 않은거네.%RAND_SPLIT("나에게 소리를 들려/혹시, 소리를 내")%가 부끄럽다든가?」
				PRINTFORMW 「뭐, 그건 그걸로%RAND_SPLIT("나쁘지 않다/좋다")%지만…」
				PRINTFORM 「억지로라도 울려지고 싶고\@ TENSION(TARGET, 8) == "높다" ? 되어 버려요# 되어요\@……
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는 행동"
				PRINTFORMW 「참은 곳에서, 좋은 느낌의 소리가 나와 버리고 있어요」
				PRINTFORM 「이제 와서 부끄러워해도, 이제(벌써) 뒤늦음이라고 생각해요……
				CALL PRINT_SYMBOL_K14
			CASE "무리를 한 행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「새빨간 얼굴 해 부끄러워하고 있는 것 치고는…, 나를 떼어 놓으려고 하지 않는거네\@ TENSION(TARGET, 8) == "높다" ? ?# \@」
					PRINTFORMW 「뭐, 그런 일이라면…. 나\@ TENSION(TARGET, 8) == "높다" ? 에 있어서도 형편상 좋다는 것은. 후후# 도 최대한 즐겁게 해 받아요\@……」
				;실은 싫어
				ELSE
					PRINTFORMW 「그렇게 부끄러워해 보이고 있지만, 사실은 기분 좋겠지요」
					IF AFFECTION("몹시 사랑함")
						PRINTFORMW 「적당, 참는 것도 괴로운 듯이 보이지만 말야」
					ELSE
						PRINTFORMW 「솔직해질 생각이 없다면…, 좀 더 괴롭혀 주기 때문에……」
					ENDIF
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM11, 싫어한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「싫다던가 말하고 있는 것 치고는, 상당히 좋은 기분이 들어 버리고 있는 것 같지만. 저기?」
			CASE "무리가 없는 행동"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 그렇게 싫어해 보이고 있다\@ TENSION(TARGET, 8) == "높다" ? 버릇에# 지만\@……」
				PRINTFORMW 「\@ TENSION(TARGET, 8) == "높다" ? 상당히 좋은 소리로 우는 거네? 후후…# 좋은 소리가 나와 버리고 있는 일을 알아차리지 않은 것일까. 저기?\@」
			CASE "무리를 한 행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					CALL FADEIN(10, "「저기 사실은……」")
					PRINTFORMW 「그렇지만도 않은, 나쁘지 않다고 생각하고 있겠지요」
					PRINTFORMW 「왜냐하면%CNAME(MASTER, TARGET)%……, 싫다던가 말하면서 나를 떼어 놓으려고 하지 않고. 저기?」
				;실은 싫어
				ELSE
					PRINTFORMW 「후후, 소리가 떨고 있어요. 말할 정도 싫어 하고 것이 아닌 것 같구나?」
					PRINTFORMW 「좋은 느낌의 소리도 나와 버리고 있는데, 이제 와서 싫다던가 조금 무리가 있다고 생각해요」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM12, 서투르다고매도한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("서투르다고매도한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「느끼지 않으면, 좀 더 여유가 있어도 좋은 것 같은 것이지만 말야?」
				PRINTFORMW 「거짓말쟁이인 아이에게는, 조금 벌이 필요할까……」
			CASE "무리가 없는 행동"
				PRINTFORMW 「헤에, 말해 주지 않아. 뭐, 그것이 사실이라고 해…」
				PRINTFORMW 「그런 나에게 좋은 느낌에 울어져 버리고 있다%CNAME(MASTER, TARGET)%는, 도대체 뭐야?」
				IF !RAND:3
					PRINTFORMW 「라고 하는 이야기가 되지만.%CNAME(MASTER, TARGET)%, 거기까지는 생각하지 않는 것 같네」
					PRINTFORMW 「무엇보다…, 그런 여유도 없는 것일까. 저기?」
				ENDIF
			CASE "무리를 한 행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「과연 그것은…, 조금 설득력이 부족하다고 생각하지만 말야?」
					PRINTFORMW 「지금의 자신이 어떤 상태인 것인가, 자주(잘) 생각해 보는 일을 추천해요」
				;실은 싫어
				ELSE
					PRINTFORMW 「이 상황으로, 아직 그렇게 말할 수 있는 거네」
					PRINTFORMW 「무엇보다…, 이제 와서 뭐 말한 곳에서, 설득력이 부족한 것이 슬픈 곳이지만」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;조건마다 RESULT를 돌려준다. 낡은 기술을 살려 통합하기 위한 고육지책이라든지 그런 응
	;=================================================
	;COM14, 쾌감을참는다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을참는다")
		;2/3로 진
		IF RAND:3
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운 행동"
					PRINTFORMW 「그러한 것도 나쁘지는 않지만…」
					PRINTFORMW 「나로서는, 좀 더%CNAME(MASTER, TARGET)%의 소리를 듣고 싶네요……」
				CASE "무리가 없는 행동"
					PRINTFORMW 「소리가 나와 버리고 있어요.%CNAME(MASTER, TARGET)%…, 참는 것이 아니었어?」
					PRINTFORMW 「그렇지만…, 후후. 나쁘지 않아요. 이봐요, 좀 더 소리를 들려줘……」
				CASE "무리를 한 행동"
