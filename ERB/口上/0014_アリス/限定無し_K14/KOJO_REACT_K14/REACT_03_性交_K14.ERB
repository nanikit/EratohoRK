	CALL PRINT_REACT_30_COM7_COM14_K14
	RETURN RESULT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적 따른다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다") && IS_NOWCOMNAME("부끄러워한다/싫어한다/서투르다고매도한다/쾌감을참는다")
	;=================================================
	;COM10, 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
