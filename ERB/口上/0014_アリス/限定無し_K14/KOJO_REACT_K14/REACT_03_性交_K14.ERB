;성교계 반응
;	COM:허리를흔드는/안에내！/안에내지마！
;고통계 반응
;	COM:아픔을참는다/비명을올린다/날뛴다
;-------------------------------------------------
@KOJO_REACT_K14_33
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속 실행/2=다른 V성교로부터/3=A성교로부터/4=V성교 봉사로부터/5=A성교 봉사로부터)
;-------------------------------------------------
;·캔슬 처리는 ACT_MESSAGE_OTHERS에서 범용 처리
;·주도권은 조교자(SEX_INITIATIVE(TARGET))
;-------------------------------------------------
;오토모드 용처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을받아들인다/기분좋다！/허리를흔드는")
	;=================================================
	;COM20, 허리를흔드는		80%
	;=================================================
	IF 구상기입체크(80) && IS_NOWCOMNAME("허리를흔드는")
		LOCALS = %RAND_SPLIT("하, 후~/응읏, 구/아…, 응응/하, 아/쿠웃, 응/핫, 하아, 하아")%……
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORM 「%LOCALS%
				PRINTFORM 응, 사랑스럽다%RAND_SPLIT("원/원이군요/")%…
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「%RAND_SPLIT("이봐요, 키스 해 준다/자, 키스 합시다")%……」
			CASE "무리가 없는 행동"
				PRINTFORM 「%LOCALS%
				PRINTFORMW %CNAME(MASTER, TARGET)%는 꽉 되는 것 좋아하네요. 후후」
				PRINTFORMW 「이봐요, 여기를 향해. 키스 해 주어요……」
			CASE "무리를 한 행동"
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
				PRINTFORMW 「후후, 완전히 솔직해져 버려」
				PRINTFORMW 「그렇지만 나쁘지 않네요. 이런 것도……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM15/16, 쾌감을받아들인다/기분좋다！
	;=================================================
	IF 구상기입체크(100) && ((IS_NOWCOMNAME("쾌감을받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분좋다！") && PERCENT(65)))
		CALL PRINT_REACT_30_COM15_COM16_K14
		RETURN RESULT
	ENDIF
RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:허가를청한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("허가를청한다") && IS_NOWCOMNAME("허가를청한다/쾌감을참는다")
	CALL PRINT_REACT_30_COM7_COM14_K14
	RETURN RESULT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적 따른다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다") && IS_NOWCOMNAME("부끄러워한다/싫어한다/서투르다고매도한다/쾌감을참는다")
	;=================================================
	;COM10, 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「후훗, 부끄러워해 버려 사랑스럽네요」
				PRINTFORMW 「그렇지만 모처럼인 것이니까, 분명하게%CNAME(MASTER, TARGET)%의 얼굴을 보였으면 좋아요」
			CASE "무리가 없는 행동"
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
				PRINTFORM 「후후, 잘 먹었어요…
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「읏,%CNAME(MASTER, TARGET)%를 더욱 더 부끄러워하게 해 버렸을까?」
			CASE "무리를 한 행동"
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「응…, 내켜하는 마음이 아니지만, 그렇다고 해서 거역할 것도 아닌거네?」
					PRINTFORMW 「뭐 좋아요. 지금은 그런데도……」
				;실은 싫어
				ELSE
					PRINTFORMW 「후후, 어때? 이것이라고 도망치는 일도 숨는 일도 할 수 없어요」
					PRINTFORMW 「부끄러워하는 것은%CNAME(MASTER, TARGET)%의 제멋대로이지만…」
					PRINTFORMW 「나는 나대로, 좋아하게 시켜 받기 때문에……」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM11, 싫어한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「재미있는 일 말해요? 그렇게 싫으면, 어째서 나의 팔 중(안)에서 얌전하게 하고 있을까」
				PRINTFORMW 「사실은 그렇지만도 않겠지요. 이봐요, 정직하게 말하세요……」
			CASE "무리가 없는 행동"
				PRINTFORMW 「후후…, 혐로 어쩔 도리가 없네요. 이 모습이다」
				PRINTFORMW 「무엇보다… 진심으로 싫으면 날뛰든지든지, 방법은 있다고 생각하지만」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 그만큼 싫어 하고 것이 아닌 것 같구나?」
			CASE "무리를 한 행동"
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「지금에서야 싫어해 보여도, 저기?」
					PRINTFORMW 「얌전하고, 나에게 키스 되었었던 것은 어디의 누구였을까. 후후…」
				;실은 싫어
				ELSE
					PRINTFORMW 「헤에, 그렇게. 싫어. 뭐, 그런 것 나의 알 바는 아니네요」
					PRINTFORMW 「나쁘지만, 나의 하고 싶은 것 같이 시켜 받기 때문에……」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;파생:강한척 하는 취급
	;=================================================
	;COM12, 서투르다고매도한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("서투르다고매도한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「떨리면서 그런 일 말해져도, 저기?」
				PRINTFORMW 「뻔히 보인 거짓말은 그만두는 거네……」
			CASE "무리가 없는 행동"
				PRINTFORMW 「나에게 키스 되고 있는 때는 묘하게 얌전했던 주제에, 자주(잘) 말해요…」
				PRINTFORMW 「포즈를 치는 것도 적당히 하면 어때?」
			CASE "무리를 한 행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「그렇게 말하는 것 치고는, 키스 되고 있는 때에 넋을 잃고 있던 것 같지만. 저기?」
					PRINTFORMW 「거짓말을 한다면, 좀 더 능숙하게 하지 않으면. 후후…」
				;실은 싫어
				ELSE
					PRINTFORMW 「하이 하이, 마음대로 말하면?」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%가 허세를 치고 있는 것은 미에미에인 것이야」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM14, 쾌감을참는다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을참는다")
		LOCALS = \@ ALI(0) ? 후후# 후후\@
		;2/3로 진
		IF RAND:3
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운 행동"
					PRINTFORMW 「아라, 상당히 얌전한거네? 혹시, 소리가 나올 것 같은 것을 참고 있다든가…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%에도, 그렇게 사랑스러운 점이 있던 거네.%LOCALS%」
				CASE "무리가 없는 행동"
					PRINTFORMW 「아무리 속이자고 해도, 이만큼 얼굴이 가까우면 자주(잘) 알아요」
					PRINTFORMW 「사실은 느끼고 있겠지요? 후후, 보면 아는 것. 이제 와서 무슨 말해도 늦어요」
				CASE "무리를 한 행동"
					;실은 기분이 좋다
					IF COMCOR_POSI()
						PRINTFORMW 「%LOCALS%, 완전히 얌전해져 버려」
						PRINTFORMW 「뭐, 그런 일이라면…, 여기는 즐기는 것에 한정해요」
					;실은 싫어
					ELSE
						PRINTFORMW 「%CNAME(MASTER, TARGET)%의 입술을 억지로 빼앗는다는 것도 나쁘지 않네요」
						PRINTFORMW 「고집 센 아이를 괴롭히는 것은, 오슬오슬 해 버려요……」
					ENDIF
			ENDSELECT
			;분기내에서 RESULT를 돌려준다
			RETURN 1
		;1/3로 진
		ELSE
			;부하 0, 1이라면 RETURN 1
			IF GROUPMATCH(GET_COM_RANK(), "자연스러운 행동", "무리가 없는 행동") > 0
				IF AFFECTION("상사상애") && PERCENT(80)
					PRINTFORMW 「뭐야 그것. 매우 얌전하지 않아」
					PRINTFORMW 「자신으로부터 움직이는 관심이 없다면, 괴롭혀 버려요……」
				ELSEIF AFFECTION("몹시 사랑함") && PERCENT(70)
					PRINTFORMW 「어차피 곧 기분 좋아져 버리는 주제에…」
					PRINTFORMW 「이봐요, 좀 더 솔직해지세요….%KOJO_RAND_K14(("키스"), 1)%」
				ELSE
					PRINTFORMW 「뭐야, 그다지 시 싶지 않다는 것…?」
					PRINTFORMW 「그렇지만, 그런 것 최초 뿐이야…」
				ENDIF
				RETURN 1
			;부하 2라면 RETURN 0
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	ENDIF
RETURN 0
ENDIF
