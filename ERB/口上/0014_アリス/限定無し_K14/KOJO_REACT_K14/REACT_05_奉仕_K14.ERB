;*************************************************
;REACT50, 손으로애무를강제·개별 리액션
;*************************************************
;=================================================
@KOJO_REACT_K14_50
;=================================================

;범용 처리
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 요구에 응하지 않는다(거절)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("거부/날뛴다/도망친다")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
	RETURN 1
ENDIF

;범용 처리. REACT부하도 함수내에서 처리하므로 COM에 흘리지 않고 여기서 완결시킨다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && !IS_NOWCOMNAME("하는대로")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;적극적한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(55) && IS_NOWCOMNAME("적극적한다") && TFLAG:REACT부하 < 2
	IF AFFECTION("상사상애")
		PRINTFORMW 「앗… 하…… 야…… 거기, 만지작거려서는 안됏…」
		PRINTFORMW 「읏… 이제(벌써) … 그렇게 하면… 가 버리기 때문에……!」
	ELSEIF AFFECTION("몹시 사랑함")
		PRINTFORMW 「핫… 아… 아읏… 좋아요, 능숙해요……」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%의 손만으로, 가게 될 것 같구나……」
	ELSEIF AFFECTION("권애")
		PRINTFORMW 「읏… 앗…… 꽤… 좋아요…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%, 전망이 있을 듯 하구나…」
	ELSE
		PRINTFORMW 「읏… 그런대로라는 곳일까…」
		PRINTFORMW 「내가 느끼는 곳, 분명하게 기억해 받아요…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;부끄러워한다(상황에부끄러워한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(55) && IS_NOWCOMNAME("부끄러워한다") && TFLAG:REACT파생 == 0
	IF AFFECTION("상사상애")
		PRINTFORMW 「이제 와서 나에게 손대는 것이 부끄럽다는건 어떻게 말하는 일?」
		PRINTFORMW 「이제(벌써), 초조하게 하는 것은 그만두어!」
	ELSE
		PRINTFORMW 「아라아라, 이런 일로 부끄러워하다니」
		PRINTFORM 「%RAND_SPLIT("우브/초심")%로 사랑스럽네요
		CALL PRINT_SYMBOL_K14
	ENDIF
	RETURN 1
ENDIF

;파생 == 1(소극적인의가 마음에 들지 않는다)의 경우는 COM에
;━━━━━━━━━━━━━━━━━━━━━━━━
;소극적한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(55) && IS_NOWCOMNAME("소극적한다") && TFLAG:REACT파생 < 1 && TFLAG:REACT부하 < 2
	IF AFFECTION("상사상애")
		PRINTFORMW 「앗… 하…… 야…… 이래서야, 부족해요…」
		PRINTFORMW 「저기, 언제나와 같이 해요…」
	ELSEIF AFFECTION("몹시 사랑함")
		PRINTFORMW 「앗… 하…… 응응… 안타깝네요…」
		PRINTFORMW 「저기, 좀 더 적극적으로 해요…」
	ELSEIF AFFECTION("권애")
		PRINTFORMW 「마음이 내키지 않는 것일까…?」
		PRINTFORMW 「좀 더 적극적으로 해 얻음원이군요…」
	ELSEIF AFFECTION("혐오")
		PRINTFORMW 「완전히, 사용할 수 없는 개군요…」
		PRINTFORMW 「꾸물거리고 미련해서 바보로 어쩔 수 없어요…」
	ELSE
		PRINTFORMW 「서투르고 서투르구나…」
		PRINTFORMW 「좀 더 성실하게 해 주었으면 하는 거네…」
	ENDIF
	RETURN 1
ENDIF
;REACT50, 손으로애무를강제

;*************************************************
;REACT51, 입으로애무를강제·개별 리액션 FIRSTTIME(251)
;*************************************************
;=================================================
@KOJO_REACT_K14_51
;=================================================

;범용 처리
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 요구에 응하지 않는다(거절)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("거부/날뛴다/도망친다")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
	RETURN 1
ENDIF

;범용 처리. REACT부하도 함수내에서 처리하므로 COM에 흘리지 않고 여기서 완결시킨다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && !IS_NOWCOMNAME("하는대로")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가용서를빈다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("용서를빈다") && TFLAG:REACT파생 < 1
	SELECTCASE RANDREACT(2, 0)
		CASE 0
			PRINTFORMW 「… 완전히. 안타깝기 이를 데 없네요!」
			PRINTFORMW 「그렇지만 그렇구나?  %CNAME(MASTER, TARGET)%에 무리하게 이런 일 시키는 것도, 나쁘지 않은 생각도 들어요. 후훗」
		CASE 1
			PRINTFORMW 「마지못해서 하고 있는 것이 잘 알아요. 정말, 안타깝다…」
			PRINTFORMW 「그렇지만 왜일까?  %CNAME(MASTER, TARGET)%에 이런 일을 강요하는 것은 조금 흥분하는거야」
			PRINTFORMW 「나로서도 구제할 길 없다고는 생각하지만……, 뭐, 방법 없네요? 후후」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;소극적한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("소극적한다") && TFLAG:REACT파생 == 0 && TFLAG:REACT부하 < 2
	;=================================================
	;정액 빨고 강제
	;=================================================
	{
	IF 구상기입체크(100) && !PENIS(TARGET) && ((GET_STAIN("바기나", "정액", TARGET) && INRANGE(TCVAR:MASTER:질내사정후경과턴, 1, 2)) 
                             || (GET_STAIN("애널", "정액", TARGET) && INRANGE(TCVAR:MASTER:장내사정후경과턴, 1, 2)))
	}
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「시켜 두어 말하는 것도 뭐 하지만, 자주(잘) 자신이 낸 것 빨 수 있어요?」
				PRINTFORMW 「뭐, 그다지 마음은 내키지 않는 것 같지만. 후훗」
			CASE 1
				PRINTFORMW 「과연 조금 싫은 것 같다지만… 결국 하는 거네?」
				PRINTFORMW 「그렇지만 방법 없네요, 자신이 더럽힌 것이니까. 킥킥…」
		ENDSELECT
		PRINTFORMDW 앨리스는 스스로의 정액을 빨아낸다%CALLNAME:MASTER%의 모습을 웃음을 띄워 응시하고 있다…
		;더러움이 서로 이동
		;CALL MOVE_STAIN("바기나", TARGET, "입", MASTER)
		RETURN 1
	ENDIF

	;=================================================
	;BASE:만족 50%이상
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 8) == "높다"
		CALL TEXTDISPLAY_CATEGORY_5_UNMOTIVATED_K14
		RETURN 1
	ENDIF
	
	;소극적한다분기에는 100%로 떨어뜨려 여기서 확률로 COM에 흘린다
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(55)
		IF AFFECTION("상사상애") && TENSION(TARGET, 8) != "높다"
			PRINTFORMW 「아읏…… 하아읏…… 응읏…」
			PRINTFORMW 「이런 것 싫어…… 응, 좀 더 진지하게 해요……」
		ELSEIF AFFECTION("몹시 사랑함") && TENSION(TARGET, 8) != "높다"
			PRINTFORMW 「앗… 항…… 앗…」
			PRINTFORMW 「저기, 좀 더 적극적으로 빨아요…」
		ELSEIF AFFECTION("권애") && TENSION(TARGET, 8) != "높다"
			PRINTFORMW 「너무 하고 싶지 않은 것 같구나…?」
			PRINTFORMW 「이봐요, 좀 더 적극적으로 빨아 줘…」
		ELSE
			SELECTCASE RANDREACT(3, 2)
				CASE 0
					PRINTFORMW 「개에는 적당한 모습이군요…」
					PRINTFORMW 「이봐요, 좀 더 주인님에 봉사하세요…」
				CASE 1
					PRINTFORMW 「개같아 무님 마지막 없네요…」
					PRINTFORMW 「이봐요, 좀 더 진지하게 하세요」
				CASE 2
					PRINTFORMW 「개의 주제에 록에 빠는 방법도 모르는 것일까…」
					PRINTFORMW 「사실, 사용할 수 없는 똥개군요」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;적극적한다/태연하게한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("적극적한다/태연하게한다") && TFLAG:REACT부하 < 2
	;=================================================
	;정액 빨고 강제
	;=================================================
	{
	IF 구상기입체크(100) && !PENIS(TARGET) && ((GET_STAIN("바기나", "정액", TARGET) && INRANGE(TCVAR:MASTER:질내사정후경과턴, 1, 2)) 
                             || (GET_STAIN("애널", "정액", TARGET) && INRANGE(TCVAR:MASTER:장내사정후경과턴, 1, 2)))
	}
		SELECTCASE RANDREACT(3, 3)
			CASE 0
				PRINTFORMW 「정말… 자신의 정액은 자주(잘) 빨 수 있어요? 게다가 개같이 기뻐해 버려…」
				PRINTFORM 「이봐요, 아직 남아 있어요. 모처럼이니까 전부 깨끗이 해 줘
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			CASE 1
				PRINTFORMW 「자신의 정액을 빠는 일도%CNAME(MASTER, TARGET)%에 있어서는 어떻게라는 일 없는거네?」
				PRINTFORMW 「내가 말하는 것도 뭐 하지만, 이 때 말하게 해 받아요…」
				PRINTFORMW 「어쩔 수 없는 변태군욧!」
			CASE 2
				PRINTFORMW 「자주(잘) 아무렇지도 않게 빨 수 있어요? 그것도 기쁜듯이…」
				PRINTFORMW 「그렇지만 이것…… 보고 있는 나도 뭔가 오슬오슬 해 와 버려요」
		ENDSELECT
		PRINTFORMDW 앨리스는 희희낙락 해 정액을 빨아낸다%CALLNAME:MASTER%의 모습을, 만족인 모습으로 응시하고 있다…
		;더러움이 서로 이동
		;CALL MOVE_STAIN("바기나", TARGET, "입", MASTER)
		RETURN 1
	ENDIF
	
	;상층의 분기는 COM에 흘리고 싶지 않아서적극적한다분기에는 100%로 떨어뜨려, 여기서 확률로 COM(범용 리액션)에 흘린다
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(55)
		IF 구상기입체크(100) && AFFECTION("상사상애")
			SELECTCASE RANDREACT(3, 4)
				CASE 0
					PRINTFORMW 「읏후아…… 앗… 항…… 앗… 응읏…」
					PRINTFORMW 「야앗… 혀가…… 기분 좋게……… 라고」
				CASE 1
					PRINTFORMW 「읏후아…… 노… 녹는…… 녹아 버리는……」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%의…… 혀로…… 탕그라고 끝낸다…… 야아…!」
				CASE 2
					PRINTFORMW 「힛…… 아…… 아아읏…… 읏…… 응쿳…」
					PRINTFORMW 「… 그렇게…… 상냥하고… 빨 수 있으면…… 나……」
			ENDSELECT
		ELSEIF AFFECTION("몹시 사랑함")
			SELECTCASE RANDREACT(3, 5)
				CASE 0
					PRINTFORMW 「아읏…… 앗…… 하아읏…… 응쿳…」
					PRINTFORMW 「추잡한 혀사용이군요… 아아…」
				CASE 1
					PRINTFORMW 「아읏…… 응앗…… 하아읏…… 응쿳…」 
					PRINTFORMW 「좋아요…… 좀 더, 좀 더… 아아…」
				CASE 2
					PRINTFORMW 「… 아…… 아읏…… 햐… 햐아……」
					PRINTFORMW 「그런 곳…… 살짝 깨물어, 되면……!」
			ENDSELECT
		ELSEIF AFFECTION("권애")
			SELECTCASE RANDREACT(2, 6)
				CASE 0
					PRINTFORMW 「그렇게…… 능숙해요…」
					PRINTFORMW 「나의 기분이 좋은 곳, 기억해 줘…」
				CASE 1
					PRINTFORMW 「앗…… 능숙함, …」
					PRINTFORMW 「저기, 좀 더 혀로 뭉그러뜨려…」
			ENDSELECT
		ELSEIF 구상기입체크(100) && AFFECTION("혐오")
			PRINTFORMW 「이봐요! 개라면 개인것 같고, 확실히 빠세요!」
			PRINTFORMW 「정말, 사용할 수 없는 똥개군요…」
		ELSE
			SELECTCASE RANDREACT(2, 7)
				CASE 0
					PRINTFORMW 「나쁘지 않네요,%CNAME(MASTER, TARGET)%에 이런 느낌으로 봉사되는 것도…」
					PRINTFORMW 「이봐요, 좀 더 혀를 사용해. 구석구석까지 정중하게 빨아……」
				CASE 1
					PRINTFORMW 「이봐요, 좀 더 확실히 빨아 줘…」
					PRINTFORMW 「사실, 개같구나…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
