;*************************************************
;REACT50, 手で愛撫を強制・個別リアクション
;*************************************************
;=================================================
@KOJO_REACT_K14_50
;=================================================

;汎用処理
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が要求に応じない(拒絶)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("拒否/暴れる/逃げる")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
	RETURN 1
ENDIF

;汎用処理。REACT負荷も関数内で処理するのでCOMに流さずここで完結させる
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類受け入れる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("受け入れる") && !IS_NOWCOMNAME("なすがまま")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;積極的やる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(55) && IS_NOWCOMNAME("積極的やる") && TFLAG:REACT負荷 < 2
	IF AFFECTION("相思相愛")
		PRINTFORMW 「あっ…は…っ…や……ソコ、弄っちゃだめっ…」
		PRINTFORMW 「んっ…もうっ…そんなにしたら…イっちゃうからぁっ…ッ…！」
	ELSEIF AFFECTION("溺愛")
		PRINTFORMW 「はっ…あっ…あんっ…いいわ、上手よ……」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%の手だけで、イかされそうね……」
	ELSEIF AFFECTION("眷愛")
		PRINTFORMW 「んっ…あっ……なかなか…いいわよ…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%、見込みがありそうね…」
	ELSE
		PRINTFORMW 「んっ…まずまずってとこかしら…」
		PRINTFORMW 「私の感じるところ、ちゃんと覚えてもらうわよ…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;恥ずかしがる（状況に恥ずかしがる）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(55) && IS_NOWCOMNAME("恥ずかしがる") && TFLAG:REACT派生 == 0
	IF AFFECTION("相思相愛")
		PRINTFORMW 「今更私に触るのが恥ずかしいってどういう事？」
		PRINTFORMW 「もうっ、焦らすのはやめて！」
	ELSE
		PRINTFORMW 「あらあら、こんな事ではずかしがるなんて」
		PRINTFORM 「%RAND_SPLIT("ウブ/初心")%で可愛らしいわね
		CALL PRINT_SYMBOL_K14
	ENDIF
	RETURN 1
ENDIF

;派生 == 1（消極的なのが気に入らない）の場合はCOMへ
;━━━━━━━━━━━━━━━━━━━━━━━━
;消極的やる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(55) && IS_NOWCOMNAME("消極的やる") && TFLAG:REACT派生 < 1 && TFLAG:REACT負荷 < 2
	IF AFFECTION("相思相愛")
		PRINTFORMW 「あっ…は…っ…や……これじゃ、足りないわ…」
		PRINTFORMW 「ねぇ、いつもみたいにやってよ…」
	ELSEIF AFFECTION("溺愛")
		PRINTFORMW 「あっ…は…っ…んん…もどかしいわね…」
		PRINTFORMW 「ねぇ、もっと積極的にやってよ…」
	ELSEIF AFFECTION("眷愛")
		PRINTFORMW 「気が進まないのかしら…？」
		PRINTFORMW 「もっと積極的にしてもらいわね…」
	ELSEIF AFFECTION("嫌悪")
		PRINTFORMW 「まったく、使えない犬ね…」
		PRINTFORMW 「愚図で愚鈍で馬鹿でどうしようもないわ…」
	ELSE
		PRINTFORMW 「下手で不器用だわ…」
		PRINTFORMW 「もっと真面目にやって欲しいものね…」
	ENDIF
	RETURN 1
ENDIF
