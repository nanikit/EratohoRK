;=================================================
@KOJO_REACT_K14_51
;=================================================

;汎用処理
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が要求に応じない(拒絶)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("拒否/暴れる/逃げる")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
	RETURN 1
ENDIF

;汎用処理。REACT負荷も関数内で処理するのでCOMに流さずここで完結させる
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類受け入れる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("受け入れる") && !IS_NOWCOMNAME("なすがまま")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が許しを乞う
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("許しを乞う") && TFLAG:REACT派生 < 1
	SELECTCASE RANDREACT(2, 0)
		CASE 0
			PRINTFORMW 「…まったく。もどかしいったらないわね！」
			PRINTFORMW 「でもそうね？ %CNAME(MASTER, TARGET)%に無理矢理こういう事させるのも、悪くない気もするわ。ふふっ」
		CASE 1
			PRINTFORMW 「嫌々やってるのがよくわかるわ。ホント、もどかしい…」
			PRINTFORMW 「でもなぜかしら？ %CNAME(MASTER, TARGET)%にこんな事を強要するのってちょっと興奮するのよ」
			PRINTFORMW 「我ながら度し難いとは思うけど……、ま、仕方無いわね？ ふふ」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;消極的やる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("消極的やる") && TFLAG:REACT派生 == 0 && TFLAG:REACT負荷 < 2
	;=================================================
	;精液舐め強制
	;=================================================
	{
	IF 口上記入チェック(100) && !PENIS(TARGET) && ((GET_STAIN("ヴァギナ", "精液", TARGET) && INRANGE(TCVAR:MASTER:膣内射精後経過ターン, 1, 2)) 
                             || (GET_STAIN("アナル", "精液", TARGET) && INRANGE(TCVAR:MASTER:腸内射精後経過ターン, 1, 2)))
	}
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「やらせておいて言うのもなんだけど、よく自分の出したの舐められるわね？」
				PRINTFORMW 「ま、あまり気は進まないようだけど。ふふっ」
			CASE 1
				PRINTFORMW 「流石にちょっと嫌そうだけど…結局やるのね？」
				PRINTFORMW 「でも仕方無いわよねぇ、自分が汚したんだから。クスクス…」
		ENDSELECT
		PRINTFORMDW アリスは自らの精液を舐め取る%CALLNAME:MASTER%の姿を目を細めて見つめている…
		;汚れが相互に移動
		;CALL MOVE_STAIN("ヴァギナ", TARGET, "口", MASTER)
		RETURN 1
	ENDIF

	;=================================================
	;BASE:満足50%以上
	;=================================================
	IF 口上記入チェック(100) && TENSION(TARGET, 8) == "高い"
		CALL TEXTDISPLAY_CATEGORY_5_UNMOTIVATED_K14
		RETURN 1
	ENDIF
	
	;消極的やる分岐には100%で落としてここで確率でCOMに流す
	;=================================================
	;その他
	;=================================================
	IF 口上記入チェック(55)
		IF AFFECTION("相思相愛") && TENSION(TARGET, 8) != "高い"
			PRINTFORMW 「あんっ……はぁんっ……んんっ…」
			PRINTFORMW 「こんなの嫌……ねぇ、もっと真剣にやってよ……」
		ELSEIF AFFECTION("溺愛") && TENSION(TARGET, 8) != "高い"
			PRINTFORMW 「あっ…はん…っ…あっ…」
			PRINTFORMW 「ねぇ、もっと積極的に舐めてよ…」
		ELSEIF AFFECTION("眷愛") && TENSION(TARGET, 8) != "高い"
			PRINTFORMW 「あまりやりたくないみたいね…？」
			PRINTFORMW 「ほら、もっと積極的に舐めて頂戴…」
		ELSE
			SELECTCASE RANDREACT(3, 2)
				CASE 0
					PRINTFORMW 「犬には相応しい姿ね…」
					PRINTFORMW 「ほら、もっとご主人様に奉仕しなさいよ…」
				CASE 1
					PRINTFORMW 「犬みたいで無様極まりないわね…」
					PRINTFORMW 「ほら、もっと真剣にやりなさいよ」
				CASE 2
					PRINTFORMW 「犬の癖に録に舐め方も知らないのかしら…」
					PRINTFORMW 「本当、使えない駄犬ね」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;積極的やる/平然とやる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_NOWCOMNAME("積極的やる/平然とやる") && TFLAG:REACT負荷 < 2
	;=================================================
	;精液舐め強制
	;=================================================
	{
	IF 口上記入チェック(100) && !PENIS(TARGET) && ((GET_STAIN("ヴァギナ", "精液", TARGET) && INRANGE(TCVAR:MASTER:膣内射精後経過ターン, 1, 2)) 
                             || (GET_STAIN("アナル", "精液", TARGET) && INRANGE(TCVAR:MASTER:腸内射精後経過ターン, 1, 2)))
	}
		SELECTCASE RANDREACT(3, 3)
			CASE 0
				PRINTFORMW 「ホント…自分の精液なんてよく舐められるわね？ しかも犬みたいに喜んじゃって…」
				PRINTFORM 「ほら、まだ残ってるわ。折角だから全部綺麗にして頂戴
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			CASE 1
				PRINTFORMW 「自分の精液を舐める事も%CNAME(MASTER, TARGET)%にとってはどうって事ないのね？」
				PRINTFORMW 「私が言うのもなんだけど、この際言わせて貰うわ…」
				PRINTFORMW 「どうしようもない変態ねっ！」
			CASE 2
				PRINTFORMW 「よく平気で舐められるわね？ それも嬉しそうに…」
				PRINTFORMW 「でもこれ……見てる私もなんかゾクゾクしてきちゃうわね」
		ENDSELECT
		PRINTFORMDW アリスは嬉々として精液を舐め取る%CALLNAME:MASTER%の姿を、満足げな様子で見つめている…
		;汚れが相互に移動
		;CALL MOVE_STAIN("ヴァギナ", TARGET, "口", MASTER)
		RETURN 1
	ENDIF
	
	;上層の分岐はCOMに流したくないんで積極的やる分岐には100%で落として、ここで確率でCOM（汎用リアクション）に流す
	;=================================================
	;その他
	;=================================================
	IF 口上記入チェック(55)
		IF 口上記入チェック(100) && AFFECTION("相思相愛")
			SELECTCASE RANDREACT(3, 4)
				CASE 0
					PRINTFORMW 「っふあ……あっ…はん…っ…あっ…んんっ…」
					PRINTFORMW 「やぁっ…舌が……気持ちよく…ッ……て」
				CASE 1
					PRINTFORMW 「っふあ……やっ…蕩ける…っ…蕩けちゃう……ッ」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%の……舌で……蕩かされちゃう…っ…やぁぁ…！」
				CASE 2
					PRINTFORMW 「ひっ……あっ……あぁんっ……んっ……んくっ…」
					PRINTFORMW 「…そんなに…っ…優しく…舐められると……私…ッ…」
			ENDSELECT
		ELSEIF AFFECTION("溺愛")
			SELECTCASE RANDREACT(3, 5)
				CASE 0
					PRINTFORMW 「あんっ……あっ……はぁんっ……んくっ…」
					PRINTFORMW 「いやらしい舌使いね… あぁ…ッ」
				CASE 1
					PRINTFORMW 「あんっ……んあっ……はぁんっ……んくっ…」 
					PRINTFORMW 「いいわ……もっと、もっとっ…あぁ…ッ」
				CASE 2
					PRINTFORMW 「…あっ……あんっ……ひっ…ひゃぁ…ッ…」
					PRINTFORMW 「そんなとこ……甘噛み、されたら…っ…！」
			ENDSELECT
		ELSEIF AFFECTION("眷愛")
			SELECTCASE RANDREACT(2, 6)
				CASE 0
					PRINTFORMW 「そう……上手よ…」
					PRINTFORMW 「私の気持ちいいところ、覚えて頂戴…」
				CASE 1
					PRINTFORMW 「あっ……上手、よ…」
					PRINTFORMW 「ねぇ、もっと舌でこねまわして…」
			ENDSELECT
		ELSEIF 口上記入チェック(100) && AFFECTION("嫌悪")
			PRINTFORMW 「ほらっ！ 犬なら犬らしく、しっかり舐めなさいよ！」
			PRINTFORMW 「ホント、使えない駄犬ね…」
		ELSE
			SELECTCASE RANDREACT(2, 7)
				CASE 0
					PRINTFORMW 「悪くないわね、%CNAME(MASTER, TARGET)%にこんな感じで奉仕されるのも…」
					PRINTFORMW 「ほら、もっと舌を使って。隅々まで丁寧に舐めて……」
				CASE 1
					PRINTFORMW 「ほら、もっとしっかり舐めて頂戴…」
					PRINTFORMW 「本当、犬みたいね…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
