;地の文をKOJO_TRAIN_MESSAGEで捏造。戻り値2で拾う

;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡

;正直、付けた姿はあまり見栄えのいいものじゃない。クリ派には大抵喜ばれる
;これを付けたまま正常位すると凄くいい感じになるｗ
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT24_0, クリキャップ装着
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;REACT分類派生
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("消極的従う/受け入れる/積極的従う/平然と従う/許しを乞う") && FLAG:地の文制御 == 2
		LOCALS = \@ AFFECTION("溺愛") ? ふふふ # フフフ \@
		;REACT分類派生
		SELECTCASE COMGRONAME(TFLAG:REACT分類)
			CASE "積極的従う", "平然と従う"
				PRINTFORMW 「まるで生えちゃったみたいな感じね？ 小さなおちんちん。%LOCALS%」
				PRINTFORMW 「折角だからシコシコしてあげるわ。ま、この場合扱かれるのは…中身の方だけど」
				PRINTFORMDL %CALLNAME%は充血し、膨らみきった%CALLNAME:MASTER%のクリトリスをキャップを使いしごきあげた
				PRINTFORMDW 吸引しながらポンと外すとまた根元まではめ込み吸引する…
				PRINTFORMDW 快楽に朦朧となった%CALLNAME:MASTER%の様子を満足げに見つめながら、%CALLNAME%は何度も、何度も繰り返した……
			CASE "受け入れる"
				PRINTFORMW 「そうよね？ キモチ良いわよね？ でもこんなのは序の口…」
				PRINTFORMDL %CALLNAME%はクリキャップをそっと引っ張り、キャップの輪でクリトリスをこすりあげる
				PRINTFORMDW 抜けそうになるとまた奥まではめ込み、そしてまた引っ張り…
				PRINTFORMDW まるでペニスをしごくかのように、キャップでクリトリス全体をしごきあげた……
				PRINTFORMW 「ほら、凄くきちゃうでしょう？ おかしくなっちゃうくらいキモチ良いでしょう？ %LOCALS%」
			CASE "消極的従う"
				PRINTFORMW 「気が進まない素振りしたところで、これがキモチ良くない訳がない！ って思うんだけど」
				PRINTFORMW 「むしろ、ちょっと刺激が強すぎるかもね？ %LOCALS%…」
			CASE "許しを乞う"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…、そういう態度してるけど……」
				PRINTFORMW 「こっちの方はうるうるしてきちゃったみたいよ？ %LOCALS%…」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT24_1, クリキャップ解除
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 1
	;=================================================
	;受容の場合1/2で表示。ヒットしなければCOM毎の処理へ
	;=================================================
	IF 口上記入チェック(50) && IS_COMGRONAME("消極的従う/受け入れる/積極的従う/平然と従う/許しを乞う") && FLAG:地の文制御 == 1
		PRINTFORMDW 吸引しつつポンと小さな音をたててクリキャップが外されると、%CLITORIS_DESCRIPTION(MASTER)%陰核が露わになる
		IF TCVAR:MASTER:イきそう
			PRINTFORMW 「こんなに膨らませちゃって。剥けたままで凄くイヤラシイわね…」
		ELSEIF PALAM:MASTER:快Ｃ >= 4000 && !TCVAR:MASTER:イきそう
			PRINTFORMW 「ふふ、愉しんでもらえたみたいね。でも、本当はこのままイきたかったんじゃないかしら？」
		ELSEIF PALAM:MASTER:快Ｃ < 4000 && !TCVAR:MASTER:イきそう
			PRINTFORMW 「あまりよくなかったかしら？ %CNAME(MASTER, TARGET)%も難しい人ねぇ」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;COMがもっとハードにして/恥ずかしがる/嫌がる/下手だと罵る/快感を我慢する/快感を受け入れる/気持ち良い！/暴れるのいずれかの場合
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("もっとハードにして/恥ずかしがる/嫌がる/下手だと罵る/快感を我慢する/快感を受け入れる/気持ち良い！/暴れる")
		IF 0
		;もっとハードにして
		ELSEIF IS_NOWCOMNAME("もっとハードにして")
			PRINTFORMW 「おねだりなんて珍しいわね？ でも残念。これはお終いよ」
			PRINTFORMW 「ま、別の事で楽しませてあげるわ。ただし…」
			PRINTFORM 「それが%CNAME(MASTER, TARGET)%の望むものかは判らないけど、ね…
			CALL PRINT_SYMBOL_K14
		;恥ずかしがる
		ELSEIF IS_NOWCOMNAME("恥ずかしがる")
			PRINTFORMW 「今更恥ずかしがるとか、ちょっと面白いわね。ふふ」
			PRINTFORMW 「ほら、大人しくして。今、外してあげるわ」
		;嫌がる（妨害なし）
		ELSEIF IS_NOWCOMNAME("嫌がる") && !IS_COMGRONAME("暴れる")
			PRINTFORMW 「聞き分けのない事いわないで頂戴…」
			PRINTFORMW 「終わりと言ったら終わりなのよ、悪いけど」
		;下手だと罵る		75%
		ELSEIF IS_NOWCOMNAME("下手だと罵る") && PERCENT(75)
			PRINTFORMW 「外すのに下手も何も無いと思うけどね？」
		;快感を我慢する
		ELSEIF IS_NOWCOMNAME("快感を我慢する")
			IF 0
			ELSEIF IS_COMGRONAME("許しを乞う")
				PRINTFORMW 「ま、今回はこれで終わりだから安心していいわよ。でも…」
				PRINTFORMW 「そんな事いわれると、またシてあげたくなっちゃうかもだけど。\@ AFFECTION("溺愛") ? ふふ # フフ \@…」
			ELSEIF IS_COMGRONAME("消極的従う")
				PRINTFORMW 「ふふっ、こうやって外される時もちょっと気持ちいいのかしら」
				PRINTFORMW 「我慢したところで、見れば判っちゃうのね」
			ENDIF
		;快感を受け入れる/気持ち良い！
		ELSEIF IS_NOWCOMNAME("快感を受け入れる/気持ち良い！")
			PRINTFORMW 「愉しんでいるみたいで何よりね？ 今外されちゃうのは名残惜しいかしら」
			PRINTFORMW 「まぁでも……、他にやりたいこともあるし。仕方無いわね。ふふ…」
		;暴れる（暴れたが抑えられる）
		ELSEIF IS_NOWCOMNAME("暴れる") && !IS_COMGRONAME("暴れる")
			PRINTFORMW 「外してあげるんだから、大人しくなさい……」
			PRINTFORMW 「それとも%CNAME(MASTER, TARGET)%…、外されるのが嫌なわけ？」
		;道具を外す	これで何が外されるかは優先順位次第。REACT派生で参照できるが面倒なのでしない。大抵の場合、当該REACTとは無関係のものが外されそうｗ
		ELSEIF IS_NOWCOMNAME("道具を外す")
			PRINTFORMW 「あら、私の手を煩わせるまでもないって事？ でもねぇ…」
			PRINTFORMW 「勝手な真似を許すつもりはないのよ。どんな場合であっても、ね」
		ENDIF
		RETURN 1
	ENDIF
RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT24_2, オナホール装着
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 2
	;=================================================
	;REACT分類派生
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("消極的従う/受け入れる/積極的従う/平然と従う/許しを乞う") && FLAG:地の文制御 == 2
		LOCALS = \@ AFFECTION("溺愛") ? ふふふ # フフフ \@
		;REACT分類派生
		SELECTCASE COMGRONAME(TFLAG:REACT分類)
			CASE "積極的従う", "平然と従う"
				PRINTFORMW 「あらあら、積極的ねぇ。%CNAME(MASTER, TARGET)%…、オナホとセックスでも始めるつもり？ %LOCALS%…」
				PRINTFORMDL 思わず腰をくいくいと動かしてしまう%CALLNAME:MASTER%の様子を、
				PRINTFORMDW %CALLNAME%は時折くすくすと笑いながら、愉しそうに見つめている…
			CASE "受け入れる"
				PRINTFORMW 「ねぇどう？ キモチ良い？ キモチ良いの？ これで私にシコシコして欲しいの？ %LOCALS%…」
				PRINTFORMDL %CALLNAME%は吐息を漏らしながら頷く%CALLNAME:MASTER%の姿に満足げな表情をみせ
				PRINTFORMDW ゆっくりと、大きなストロークで、オナホールごと%CALLNAME:MASTER%のペニスをしごいている…
			CASE "消極的従う"
				PRINTFORMW 「あら、あまり乗り気じゃないのね？ もしかして、恥ずかしいとか？」
				PRINTFORMW 「でもそんなの…、これでシコシコされたら、どうでもよくなっちゃう癖に。ね…」
				PRINTFORMDL %CALLNAME%は悪戯っぽい笑みを浮かべると、%CALLNAME:MASTER%に挑発的な視線を投げかけながら
				PRINTFORMDW 小刻みな動きで、オナホールごと%CALLNAME:MASTER%のペニスを扱き始めた…
			CASE "許しを乞う"
				PRINTFORMW 「なによその反応。白けちゃうわね…」
				PRINTFORMW 「いいわ、そういうことなら…私にも考えがあるから」
				PRINTFORMDL %CALLNAME%はまるで罰を与えるかのように、強めに、大きなストロークで、
				PRINTFORMDW オナホールごと%CALLNAME:MASTER%のペニスを扱き始めた…
		ENDSELECT
		;調教対象がイきそう
		IF TCVAR:MASTER:イきそう
			PRINTL
			PRINTFORMW 「ん、なに？ もう駄目なの！？ イっちゃう？」
			;焦らし度で派生
			SELECTCASE TCVAR:MASTER:焦らし度
				;焦らしあり
				CASE IS > 1
					PRINTFORMW 「でも、簡単にイかせたんじゃつまらないのよね」
					PRINTFORMW 「思いっきり焦らした方が、その…、いっぱい出るし。%LOCALS%…」
				;焦らしなし
				CASEELSE
					PRINTFORMW 「ふふ、そうねぇ。このままイかせ%RAND_SPLIT("てみ/")%るのも一興かも…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT24_2, オナホール解除
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 3
	LOCALS = \@ AFFECTION("溺愛") ? ふふ # フフ \@
	;=================================================
	;REACT分類派生
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("消極的従う/受け入れる/積極的従う/平然と従う/許しを乞う") && FLAG:地の文制御 == 2
		;精液汚れあり
		IF GET_STAIN("ペニス", "精液", MASTER)
			;RANDREACTのみで分ける
			SELECTCASE RANDREACT(4, 300)
				CASE 0
					PRINTFORMW 「ま、当然…こうなるわよねぇ。%LOCALS%」
				CASE 1
					PRINTFORMW 「判ってはいたけど、酷い有様ねぇ？ %LOCALS%」
				CASE 2
					PRINTFORMW 「こんなに汚してくれちゃって…。洗浄は自分でするのよ。いい？」
				CASE 3
					PRINTFORMW 「あらあら…、これだけ愉しんで貰えれば、オナホも本望なんじゃないかしら？ %LOCALS%…」
			ENDSELECT
		;その他
		ELSE
			;REACT分類派生
			SELECTCASE COMGRONAME(TFLAG:REACT分類)
				CASE "積極的従う", "平然と従う"
					PRINTFORMW 「ま、何事にも前向きってのはいい事だと思うわよ。%LOCALS%」
				CASE "受け入れる"
					PRINTFORMW 「%LOCALS%、ちょっと名残惜しそうね？」
					PRINTFORMW 「ま、コレばっかりって訳にもいかないし…大人しく諦めるのね」
				CASE "消極的従う"
					PRINTFORMW 「ひょとしてコレ…、外される時も気持ちよかったりするのかしらね？」
				CASE "許しを乞う"
					PRINTFORMW 「は？ どういう事？ オナホ外されるのが嫌って訳なの？」
					PRINTFORMW 「よく判らないけど…、とりあえずもうお終いよ」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
RETURN 0
ENDIF
