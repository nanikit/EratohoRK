IF 구상기입체크(100) && !TFLAG:REACT파생
	;=================================================
	;무리를 한 행동		성적봉사시는 부하 2 이하의 선택지가 되는 것이 대부분 없기 때문에 연주해 둔다. 봉사계라면 그렇지도 않지만
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동" && !TEQUIP:성교봉사중
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「저기%CNAME(MASTER, TARGET)%…, 뭔가 무리하고 있지 않아?」
				PRINTFORMW 「그러한 것은, 미묘하게 전해지는거야. 조금 불쾌하구나」
			CASE 1
				PRINTFORMW 「… 응. 저기, 나쁘지 않지만…」
				PRINTFORMW 「어딘지 모르게, 조금 틀리다는 느낌이 드는거네요. 나에게는 알아」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%, 사실은 무리하고 있겠지요…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 성교(승마위)
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성교") && TEQUIP:성교중 == 3
		IF CFLAG:TARGET:의존도 > 4000
			PRINTFORMW 「자기보다, 내 쪽을 우선해 줄래? 사랑스러워요…」
			PRINTFORMDW %CALLNAME%는%CALLNAME:MASTER%를 끌어 들이면, 상냥하게 껴안으면서 살그머니 입맞춤을 했다…
			PRINTFORM 「푸하… 아…, 후후, 부드러워서 좋은 맛 해%RAND_SPLIT("있고/")%
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), @"  이봐요, 이번은%CNAME(MASTER, TARGET)%의 차례야」")
			PRINTFORMW 「분명하게…, 나에게 키스, 해……」
			CALL SET_KISS
		ELSE
			PRINTFORMW 「후후, 나를 기분 좋게 시키려고 노력해 버려. 기특하고 사랑스럽다」
			PRINTFORMDW %CALLNAME%는%CALLNAME:MASTER%의 손을 잡으면 제대로 꽉 쥐었다…
			PRINTFORMW 「자, 좋아요.%CNAME(MASTER, TARGET)%의 사랑스러운 곳, 나에게 보이게 해 줘」
			PRINTFORMW 「좀 더%CNAME(MASTER, TARGET)%의 소리, 들려줘……」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "봉사")
		;-------------------------------------------------
		;ACT 다리빨고강제
		;-------------------------------------------------
		IF 구상기입체크(100) && IS_NOWACTNAME("다리빨고강제")
			;BASE:8, 만족 파생
			SELECTCASE TENSION(TARGET, 8)
				;50% 이상
				CASE "높다"
					SELECTCASE RANDCOM(2, 10)
						CASE 0
							PRINTFORM 「%RAND_SPLIT("/응/아")%…
							CALL AUTOHEARTCB_K14
							PRINTFORMW   간사해요…, 그런 곳까지 빨다니」
							PRINTFORM 「%RAND_SPLIT("히/후")%
							CALL AUTOHEARTCB_K14
							PRINTFORMW   이제(벌써)! 그러니까…, 안된다고, 말하고 있는데……」
						CASE 1
							PRINTFORMW 「저기, 그러니까…. 빨아도 좋은 것은 허벅지까지라고 말하고 있겠지요」
							PRINTFORM 「%RAND_SPLIT("구/응")%
							CALL AUTOHEARTCB_K14
							PRINTFORMW   그렇게 상냥하게 여겨지면, 화낼 수 없게 되지 않아」
							PRINTFORMW 「간사해요,%CNAME(MASTER, TARGET)%…」
					ENDSELECT
				;25% 이상
				CASE "일반"
					SELECTCASE RANDCOM(2, 11)
						CASE 0
							PRINTFORMW 「저기, 조금. 올라 오는 것이 너무 빠르지 않아?」
							PRINTFORMW 「빨아도 좋은 것은 허벅지까지니까. 그렇지 않으면 나…」
							PRINTFORMW 「인내 할 수 없게 되어 버리는 것. 후후」
						CASE 1
							PRINTFORMW 「%RAND_SPLIT("응응―/어머나")%? 즐은 좋지 않아요. 허가한 것은 허벅지까지니까」
							PRINTFORM 「왜냐하면, 저기. 너무 위까지 올 수 있으면, 나, 다 억제할 수 있지 않게 되어 버리는 것…
							CALL PRINT_SYMBOL_K14
					ENDSELECT
				;25% 미만
				CASE "낮다"
					SELECTCASE RANDCOM(3, 12)
						CASE 0
							PRINTFORMW 「아라, 상당히 정중하게 빨아 주는 거네. 후후」
							PRINTFORM 「이런 식으로 되는 것도, 나쁘지 않다%RAND_SPLIT("것/와")%군요…
							CALL PRINT_SYMBOL_K14
						CASE 1
							PRINTFORMW 「뭐야, 상냥하지 않아. 어떤 바람의 불어 돌리기?」
							PRINTFORMW 「후후,%CNAME(MASTER, TARGET)%…. 뭔가 포상을 기대하기도 하고, 라든지?」
						CASE 2
							PRINTFORMW 「후후, 위의 (분)편이 신경이 쓰여? 그렇지만 안 돼요. 허벅지까지 해 둬」
							PRINTFORM 「거기에서 앞은, 후의 즐거움은…
							CALL PRINT_SYMBOL_K14
					ENDSELECT
			ENDSELECT
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		LOCALS = %RAND_SPLIT("/하/아/응/후~/아/후아")%
		;조교자가 갈 것 같다
		IF TCVAR:갈것같다 || PALAM:쾌V > 8500 || PALAM:쾌A > 8500 || PALAM:쾌B > 8500
			SELECTCASE RANDCOM(2, 13)
				CASE 0
					PRINTFORM 「%RAND_SPLIT("/하")%…… 아
					CALL AUTOHEARTCB_K14
					PRINTFORMW   그런, 상냥하게 여겨지면… 나, 나!」
					PRINTFORM 「\@ !AFFECTION("상사상애") ? %CNAME(MASTER, TARGET)%의 일…, 좋아하게 # 안타까워…, 억제할 수 없고 \@되어 버린닷…
					CALL PRINT_SYMBOL2_K14
				CASE 1
					PRINTFORM 「%LOCALS%…
					CALL AUTOHEARTCB_K14
					PRINTFORMW   응…, 그렇게 상냥하게 하지 말아요. 나, 인내 할 수 없게 되어 버려요…」
					PRINTFORM 「아″
					CALL AUTOHEARTCB_K14
					PRINTFORM   다, 그러니까! 안된다고, 말하고 있는데
					CALL PRINT_SYMBOL2_K14
			ENDSELECT
		;그 외
		ELSE
			SELECTCASE RANDCOM(2, 14)
				CASE 0
					PRINTFORMW 「뭐, 뭐야. 상당히 상냥하지 않아」
					PRINTFORMW 「그런 식으로 되면 나…, 스윗치 들어가 버린다\@ AFFECTION("상사상애") ? 하지만, 그런데도 좋은거야?  # 원이야… \@」
				CASE 1
					PRINTFORM 「%LOCALS%…
					CALL AUTOHEARTCB_K14
					PRINTFORMW   완전히%CNAME(MASTER, TARGET)%의 페이스에 빠져 버려, 조금 적이군요」
					PRINTFORMW 「그렇지만, 이렇게 상냥하게 여겨진다면, 그것도 좋은 것인지도…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 성적봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성적봉사")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "정상위시킨다"
				PRINTFORMW 「응…… 상냥한거네. 무슨 바람이 불었는지 모르지만. 후후」
				PRINTFORMW 「저기, 키스 해. 좀 더%CNAME(MASTER, TARGET)%의 체온, 느낌\@ AFFECTION("상사상애") ? 싶은거야 # 시켜 \@……」
				;지문
				PRINTFORMDW 두 명은 서로를 서로 확인하는것 같이 제대로 얼싸안아, 입맞춤을 주고 받으면서 서로 흔들렸다…
				;일행 비운다
				PRINTL
				;페이드 아웃
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				;콘피그를 참조
				IF ANIME_CONFIG("FADE")
					;진이라면 행 삭제한다
