;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM51, 道具を外す
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	[SKIPSTART]
	;=================================================
	;Ａ系道具を外す
	;=================================================
	IF 口上記入チェック(0) && GROUPMATCH(TFLAG:REACT派生, 25, 26, 27)
	ENDIF
	;=================================================
	;Ｃ系道具を外す
	;=================================================
	IF 口上記入チェック(0) && GROUPMATCH(TFLAG:REACT派生, 30, 31)
	ENDIF
	[SKIPEND]
	
	;-------------------------
	;汎用処理
	;-------------------------
	SELECTCASE RANDCOM(4, 0)
		CASE 0
			PRINTFORMW 「外していいなんて、言った覚えはないわよ…」
		CASE 1
			PRINTFORMW 「勝手なマネされると困るのよね」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%、お仕置きされたくてわざとやってるのかしら？」
		CASE 2
			PRINTFORMW 「なにそれ、私に逆らおうっていうワケ？」
			PRINTFORMW 「それなりの覚悟は、あるんでしょうね\@ TENSION(TARGET, 7) == "高い" ? ぇ # …… \@」
		CASE 3
			PRINTFORMW 「なに勝手な事してるのよ\@ TENSION(TARGET, 7) == "高い" ? っ！ # ? \@」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%…、ふざけるのも大概にしなさい」
	ENDSELECT
	RETURN 1
ENDIF

