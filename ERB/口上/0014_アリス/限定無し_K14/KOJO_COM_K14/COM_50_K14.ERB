;-------------------------------------------------
;・縄で縛られていると駄目
;・すでに自慰してる、性交中、性交奉仕中は弾かれる
;-------------------------------------------------
;TFLAG:REACT派生 == 0, 勝手にオナニーしてるけどいいか, 依存度変化(-1, 0)
;TFLAG:REACT派生 == 1, 勝手にオナニーするな, 依存度変化(-3, +2)
;-------------------------------------------------
;トリガーする行動や状況…
;ACT分類:0:会話
;	ACT:今の気分を聞く/自分への気持ちを聞く/性についての話をする
;ACT分類:1:愛撫
;	ACT:キス
;ACT分類:2:道具
;	ACT:
;ACT分類:3:性交
;	ACT:
;ACT分類:4:羞恥
;	ACT:自慰みせつけ/羞恥プレイ/野外プレイ
;ACT分類:5:奉仕
;	ACT:手で愛撫を強制/口で愛撫を強制/足舐め強制/胸愛撫強制
;ACT分類:6:加虐
;	ACT:罵倒
;ACT分類:7:異常
;	ACT:放置プレイ
;ACT分類:9:休憩
;	ACT:休ませる
;ACT分類:10:性交奉仕
;	ACT:
;-------------------------------------------------
;=================================================
@KOJO_COM_K14_50
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM50, 自慰し始める
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	;=================================================
	;派生：勝手にオナニーするな or オナ禁()を破った
	;=================================================
	IF 口上記入チェック(100) && (TFLAG:REACT派生 == 1 || GETBIT(KFLAG:TARGET:201, 4))
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「は？ 何してんのよ。そんなもの、許可した覚えはないけど…」
				PRINTFORMW 「一体どういうつもりなのか、説明してもらおうじゃない」
			CASE 1
				PRINTFORMW 「なにそれ…、ふざけてるの？」
				PRINTFORMW 「勝手なマネをしたらどうなるか、教えてあげる必要がありそうね…」
			CASE 2
				PRINTFORMW 「あらあら、サル並みの知能の持ち主には相応しい姿ね？」
				PRINTFORMW 「と、言ったところで…、%CNAME(MASTER, TARGET)%の勝手にさせるつもりはないのよ」
				PRINTFORMW 「悪いけど……」
		ENDSELECT
		;オナ禁ビットが立っていればお仕置きする！
		SIF GETBIT(KFLAG:TARGET:201, 4)
			;行動予約・スパンキング
			TCVAR:行動予約 = 60
		RETURN 1
	ENDIF
	
	;=================================================
	;派生：勝手にオナニーしてるけどいいか
	;=================================================
	;IF 口上記入チェック(0) && TFLAG:REACT派生 == 0
	;ENDIF
	
	;=================================================
	;会話系行動から実行
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 1)
			CASE 0
				PRINTFORMW 「こらこら…」
				PRINTFORMW 「人が話してる時に、自慰をするのはやめなさい…」
			CASE 1
				PRINTFORMW 「……やれやれ、ね」
				PRINTFORMW 「頭痛くなってきたわ……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;不要になった処理を回収してちょっと改変
	;-------------------------
	;その他
	;-------------------------
	;調教者BASE:満足派生
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORMW 「…やれやれ。%CNAME(MASTER, TARGET)%のオナニー好きにも困ったものね」
					IF AFFECTION("相思相愛")
						PRINTFORM 「まぁ、私も人の事言えないけど
						;1/2でPS
						IF RAND:2
							PRINTFORMW 。 ふふっ…」
						ELSE
							CALL PRINT_SYMBOL_K14
						ENDIF
					ENDIF
				CASE 1
					PRINTFORMW 「なんていうのか、こう…」
					PRINTFORMW 「欲望に忠実すぎるのも考え物ね？」
			ENDSELECT
		CASE "一般"
			SELECTCASE RANDCOM(2, 3)
				CASE 1
					PRINTFORMW 「うん、まぁ別にしてもいいけど…何も今じゃなくてもいいでしょう？」
				CASE 2
					PRINTFORMW 「ねぇ、一体何をしているのよ…」
					PRINTFORMW 「私そんなの許可した覚えはないけど？」
			ENDSELECT
		CASE "低い"
			SELECTCASE RANDCOM(2, 4)
				CASE 0
					PRINTFORMW 「……まったく」
					PRINTFORMW 「どうしようもない、オナニー好きの変態ね…\@ T_COND("怒り") ? ！ # \@」
				CASE 1
					PRINTFORMW 「……サルと一緒ね」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

