;=====================================================================
;특수한 반응 COM70～
;=====================================================================

;──────────────────────────────────
;COM70, 가게해！
;──────────────────────────────────
;·REACT분류:받아들인다
;·REACT인상:좋은인상
;·의존도 변화(+10, +6)
;-------------------------------------------------
;선택 가능한 조건…
;·애태움중은 아니다
;·TCVAR:MASTER:갈것같다가 서 있다
;·애무계 ACT(키스/쓰러트리기 이외) 한편 각 부위의 PALAM가 5000이상이다
;※상기 조건에 들어맞는 경우는 가능
;-------------------------------------------------
;ACT에 있어서 TCVAR:MASTER:애태움수 == 1(가게 한다)에서 만나도 COM70의 선택으로 TCVAR:MASTER:애태움수 > 1(초조하게 한다)(이)가 되는 경우 있어
;현재의 사양에서는 이 손의 변화를 참조할 방법은 없다. 본체측의 대응이 바람직한다!
;-------------------------------------------------
@KOJO_COM_K14_70
;-------------------------------------------------
;·TFLAG:REACT파생(0=들은체 만체 한다/1=가게해 준다/2=가게해 주지 않는다)
;-------------------------------------------------

LOCALS = \@ AFFECTION("상사상애") ? 킥킥 # 후후후 \@

;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 2, 가게해 주지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 2
	;TEQIP도 참조한다
	;=================================================
	;애널섹스
	;=================================================
	IF 구상기입체크(100) && (IS_NOWACTNAME("애널섹스시킨다") || A_SEX(MASTER))
		;우선 테두리를 단락지어 범용 처리에 떨어지도록(듯이)
		;-------------------------------------------------
		;페니스밴드장착 안
		;-------------------------------------------------
		;IF 구상 기입 체크(0) && TEQUIP:페니스밴드
		;ENDIF
		
		;장대 첨부의 경우만 떨어지도록(듯이)
		;-------------------------------------------------
		;통상의 성교 봉사
		;-------------------------------------------------
		IF 구상기입체크(100) && PENIS(MASTER)
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			;오늘의방침가 아이 만들기
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			IF 구상기입체크(100) && IS_POLICY("오늘", "자 만들기")
				PRINTFORMW 「저기… 자 만들어 한다 라고 결정한 날에 엉덩이에 내고 싶다고, 조금 어때?」
				PRINTFORMW 「보통, 조금은 참으려고 생각하지 않아? 뭐, 그런 (뜻)이유이니까…」
				PRINTFORM 「간단하게는가게해 주지 않으, 니까
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				RETURN 1
			ENDIF
			
			;-----------------------
			;그 외
			;-----------------------
			IF AFFECTION("상사상애")
				PRINTFORMW 「엉덩이에 내고 싶다고 말한다면 별로 상관없고, 싫어 하고 것도 아니에요. 다만…」
				PRINTFORMW 「어딘지 모르게 심술쟁이 하고 싶은 기분인 것이군요, 후훗」
			ELSE
				PRINTFORMW 「읏! 엉덩이안에 내는 것은 좋지만…」
				PRINTFORMW 「좀 더 노력해 받아요, 나쁘지만, 말야…」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(100) && (IS_NOWACTNAME("역강간/정상위시킨다/후배위시킨다/대면좌위시킨다/배면좌위시킨다") || V_SEX(MASTER))
		;현상, 전용 구상을 쓸 여유가 없어서 체크 떨어뜨려 뒷전
		;-------------------------------------------------
		;페니스밴드장착 안
		;-------------------------------------------------
		;IF 구상 기입 체크(0) && TEQUIP:페니스밴드
		;ENDIF
		
		;장대 첨부의 경우만 떨어지도록(듯이)
		;-------------------------------------------------
		;통상의 성교 봉사
		;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
		IF 구상기입체크(100) && PENIS(MASTER)
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			;오늘의방침가 아이 만들기
			;기입 체크
			LOCAL:1 = 1
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			IF 구상기입체크(100) && IS_POLICY("오늘", "자 만들기")
				PRINTFORMW 「안 돼요. 아무리 오늘은 아이 구조이니까 라고, 아직 너무 빠른 것」
				PRINTFORM 「좀 더 즐겁게 해 주면, 생각해도 좋지만… 말야
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				RETURN 1
			ENDIF
			
			;-----------------------
			;그 외
			;-----------------------
			IF AFFECTION("상사상애")
				PRINTFORMW 「그래요? 나의 안에 냈네요? 그렇지만, 후후후」
				PRINTFORMD 앨리스는
				;후배위 이외
				IF IS_NOWACTNAME("역강간/정상위시킨다/대면좌위시킨다/배면좌위시킨다") || GROUPMATCH(TEQUIP:성교봉사중, 1, 3, 4, 5) > 0
					PRINTFORMDW %CALLNAME:MASTER%의 귓전에 입을 대고 속삭였다…
				;후배위
				ELSEIF IS_NOWACTNAME("후배위시킨다") || TEQUIP:성교봉사중 == 2
					PRINTFORMDW 되돌아 보면서%CALLNAME:MASTER%에 향했다…
				ENDIF
				PRINTFORM 「아직 가게 하지 않아요  나·쁘·지·만
				CALL PRINT_SYMBOL_K14
			ELSE
				PRINTFORMW 「… 응. 나의 안에 털어 놓고 싶은 것은 알지만……」
				PRINTFORMW 「아직 안 돼요. 좀 더 즐겁게 해…」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;-------------------------------
	;샌 것은 여기서 처리
	;-------------------------------
	SELECTCASE RANDCOM(3, 0)
		CASE 0
			PRINTFORM 「그런 말을 들어도, 곧바로가게해는 재미있지 않네요
		CASE 1
			PRINTFORMW 「그런 일 말했기 때문에 라고, 이 내가 간단하게가게해 준다고 생각해?」
			PRINTFORM 「후후… 오히려 더욱 더 심술쟁이 하고 싶어져 버리, 지만
		CASE 2
			PRINTFORMW 「그런 일 말해지면, 조금 심술쟁이 하고 싶어지는거네요…」
			PRINTFORM 「갈 수 있을 것 같으면서 갈 수 없는 안타까움, 충분히 즐겨 줘
	ENDSELECT
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 1, 가게해 준다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	;=================================================
	;애널섹스
	;=================================================
	IF 구상기입체크(100) && (IS_NOWACTNAME("애널섹스시킨다") || A_SEX(MASTER))
		;범용 처리에 돌린다
		;-------------------------------------------------
		;페니스밴드장착 안
		;-------------------------------------------------
		;IF 구상 기입 체크(0) && TEQUIP:페니스밴드
		;ENDIF
		
		;장대 첨부의 경우만 떨어지도록(듯이)
		;-------------------------------------------------
		;통상의 성교 봉사
		;-------------------------------------------------
		IF 구상기입체크(100) && PENIS(MASTER)
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			;오늘의방침가 아이 만들기
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			IF 구상기입체크(100) && IS_POLICY("오늘", "자 만들기")
				PRINTFORMW 「저기, 그것은 좋지만… 오늘은 아이 만들기일 것이었네요」
				PRINTFORMW 「그러니까 적당히 해, 분명하게 남겨 두세요?」
				PRINTFORMW 「그렇지 않으면, 다음에 벌 하기 때문에……%LOCALS%」
				RETURN 1
			ENDIF
			
			;-----------------------
			;그 외
			;-----------------------
			IF AFFECTION("상사상애")
				PRINTFORMW 「좋아요, 나의 엉덩이로가게해 올 수 있다」
				PRINTFORM 「말라 비틀어질 때까지 짜내 주기 때문에, 각오 해
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ELSE
				PRINTFORMW 「좋아요, 사정해…」
				PRINTFORMW 「나의 엉덩이에 마음껏 털어 놓아…」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;Ｖ성교봉사중
	;=================================================
	IF 구상기입체크(100) && (IS_NOWACTNAME("역강간/정상위시킨다/후배위시킨다/대면좌위시킨다/배면좌위시킨다") || V_SEX(MASTER))
		;우선 범용 처리까지 떨어져 받는다
		;-------------------------------------------------
		;페니스밴드장착 안
		;-------------------------------------------------
		;IF 구상 기입 체크(0) && TEQUIP:페니스밴드
		;ENDIF
		
		;장대 첨부의 경우만 떨어지도록(듯이)
		;-------------------------------------------------
		;통상의 성교 봉사
		;-------------------------------------------------
		IF 구상기입체크(100) && PENIS(MASTER)
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			;오늘의방침가 아이 만들기
			;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
			IF 구상기입체크(100) && IS_POLICY("오늘", "자 만들기")
				PRINTFORMW 「… 응, 알았다」
				PRINTFORMD 앨리스는 질내의 귀두를 자궁에 밀착시키면%CALLNAME:MASTER%
				;정상위/역강간/대면좌위
				IF IS_NOWACTNAME("역강간/정상위시킨다/대면좌위시킨다") || GROUPMATCH(TEQUIP:성교봉사중, 1, 3, 4) > 0
					PRINTFORMDW 의 귓전으로 속삭였다…
				;후배위/배면좌위
				ELSEIF IS_NOWACTNAME("후배위시킨다/배면좌위시킨다") || GROUPMATCH(TEQUIP:성교봉사중, 2, 5) > 0
					PRINTFORMDW 에 되돌아 보면서 말했다…
				ENDIF
				PRINTFORMW 「나를 마마로 해…,%CNAME(MASTER, TARGET)%의 아이를 임신하게 해…」
				RETURN 1
			ENDIF
			
			;-----------------------
			;그 외
			;-----------------------
			IF AFFECTION("상사상애")
				PRINTFORMW 「좋아요, 가게해 준다……」
				PRINTFORMW 「이봐요, 나의 안에 쏟아…」
			ELSE
				PRINTFORMW 「지금 그것을 말한다는 일은, 나의 안에 내고 싶다는 일이군요?」
				PRINTFORMW 「좋아요……%CNAME(MASTER, TARGET)%에 책임을 질 각오가 있다, 면요…」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;성교, 성교봉사중 이외
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(!TEQUIP:성교봉사중, !TEQUIP:성교중, !TEQUIP:조수성교봉사중, !TEQUIP:조수성교중) > 0
		;-------------------------------------------------
		;오늘의방침가 아이 만들기
		;-------------------------------------------------
		IF 구상기입체크(100) && IS_POLICY("오늘", "자 만들기")
			PRINTFORMW 「가게해 주는 것은 좋지만, 오늘은 아이 만들기라는 일을 잊어서는 안 돼요?」
			PRINTFORMW 「분명하게 남겨 두지 않으면, 벌 하기 때문에…… 후훗」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;상사상애
		;-------------------------------------------------
		IF 구상기입체크(100) && AFFECTION("상사상애")
			SELECTCASE RANDCOM(3, 1)
				CASE 0
					PRINTFORMW 「후후…%CNAME(MASTER, TARGET)%의 부탁이라면 거절할 수 없네요」
					PRINTFORM 「좋아요, 소망 대로가게해 준다…
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「\@ !TEQUIP:아이마스크 ? 눈물고인 눈으로 # \@그런 일 말해지면 과연,」
					PRINTFORM 「차이 있고 원이야.%CNAME(MASTER, TARGET)%가 가는 곳, 나에게 보이게 해 줘
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CASE 2
					PRINTFORMW 「그렇구나? 과연 한계같고. 다만, 오늘은…」
					PRINTFORM 「\@ PENIS(MASTER) ? 철저하게 짜내 줄 생각이니까 # 마음껏 가 미치게 해 줄 생각이니까 \@, 각오 해
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		SELECTCASE RANDCOM(3, 2)
			CASE 0
				PRINTFORMW 「좋아요, 가세요…」
			CASE 1
				PRINTFORMW 「그렇구나, 슬슬 가게해 주어도 괜찮아요」
				PRINTFORM 「\@ PENIS(MASTER) ? 철저하게 짜내 주기 때문에 # 마음껏 가 미치게 해 주기 때문에 \@, 각오 해 줘…
				CALL PRINT_SYMBOL_K14
			CASE 2
				PRINTFORMW 「좋아요, 가게해 준다」
				PRINTFORM 「%CNAME(MASTER, TARGET)%가 보기 흉하게 끝나는 곳, 나에게 보이게 해 줘
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;요구를 들은체 만체 하는 시추에이션
;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 0, 「흐음」
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 0
	;2/3의 확률로 표시
	IF RAND:3
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「아라, 그렇게 가고 싶은거야?」
				PRINTFORMW 「그렇지만…… 그런 말을 들어도, 저기?  %LOCALS%…」
			CASE "일반"
				PRINTFORMW 「그래, 그래. 그래서…, 그것이 무엇인가?」
				PRINTFORMW 「%LOCALS%…」
			CASE "낮다"
				PRINTFORMW 「응? 뭔가 말했을까. 자주(잘) 들리지 않았어요」
				PRINTFORMW 「%LOCALS%…」
		ENDSELECT
	;1/3의 확률로 표시
	ELSE
		SELECTCASE RANDCOM(3, 3)
			CASE 0
				PRINTFORMW 「『가게해 주세요』, 이지요?」
				PRINTFORMW 「사람에게 물건을 부탁할 때는 자주(잘) 생각해 주었으면 하는 거네?」
			CASE 1
				PRINTFORMW 「후후, 그렇게 울 것 같은 얼굴 해도 안 돼요」
				PRINTFORMW 「어떻게 할까는 나나름. 내가 그럴 기분이 들 때까지 얌전하게 기다리는 일이군요?」
			CASE 2
				PRINTFORMW 「… 완전히. 야비한에도 정도가 있어요」
				PRINTFORMW 「머리의 안 가는 일 밖에 생각할 수 없는 추악하고 보기 흉한\@ TALENT:MASTER:남자 ? 마조돼지 # 암퇘지 \@, 라는 곳일까」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF