;──────────────────────────────────
;COM30, 痛みを我慢する
;──────────────────────────────────
;・REACT分類:消極的従う
;・REACT印象:好印象小
;・SOURCE:痛みが無い場合は弾かれる
;-------------------------------------------------
;・トリガーとなる行動、ソース、パラメーター
;・SOURCE:痛み/拘束
;-------------------------------------------------
;選択可能な条件…
;ACT分類:愛撫
;	ACT:胸愛撫/口で愛撫/アナル愛撫/アナル舐め/キス/パイズリする
;ACT分類:道具
;	ACT:バイブ/アナルバイブ/アナルビーズ
;ACT分類:性交
;	ACT:正常位/後背位/騎乗位/対面座位/背面座位/アナルセックス
;ACT分類:奉仕
;	ACT:イマラチオ
;ACT分類:加虐
;	ACT:スパンキング/鞭/針/縄/アイマスク/ボールギャグ/三角木馬/浣腸セット
;ACT分類:異常
;	ACT:フィストファック/アナルフィスト/両穴フィスト
;-------------------------------------------------
;・TFLAG:REACT派生(0=痛みを我慢/1=痛いけど気持ち良い！)
;-------------------------------------------------
;=================================================
@KOJO_COM_K14_30
;=================================================

LOCALS = \@ AFFECTION("相思相愛") ? %RAND_SPLIT("くすっ/クスクス/ふふ/ふふっ/くすくす/")% # %RAND_SPLIT("ウフフ/フフ/フフっ/フフフ")% \@
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM30_0, 派生：痛みを我慢する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「――%RANDTEXT_K14("意外と", "思ったより")%頑張るわね。でも本当は…」
					PRINTFORMW 「今にも泣き叫びたいくらいなんじゃないかしら。ねぇ？」
				CASE 1
					PRINTFORMW 「へぇ。頑張るじゃない。ちょっと見直したわ。まぁこれなら…」
					PRINTFORMW 「――もっと酷い事にも耐えられるかも。なんて、ね」
			ENDSELECT
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 1
					PRINTFORMW 「あらあら、大丈夫かしら？」
					PRINTFORMW 「この程度で音を上げられたら、興ざめもいいとこなんだけど」
				CASE 2
					PRINTFORMW 「この位なら平気かと思ったけど、そうでもなさそうね」
					PRINTFORMW 「まぁそれでも…、簡単に悲鳴を上げたりしないのは、褒めてあげてもいいわ\@ TENSION(TARGET, 8) == "高い" ? よ？ # \@」
			ENDSELECT
		ENDIF
		;調教者のBASE:満足が50%以上
		IF TENSION(TARGET, 8) == "高い"
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			;コンフィグを参照
			SIF ANIME_CONFIG("FADE")
				;真なら行削除する
				CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;無理のない行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理のない行動"
		;2/3の確立で真
		IF RAND:3
			SELECTCASE RANDCOM(3, 2)
				CASE 0
					PRINTFORMW 「ま、この程度のことに耐えるのは当然。って言ったら…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%は怒るかしら。ねぇ？」
				CASE 1
					PRINTFORMW 「そう、この位なら耐えられるって訳ね？ なるほど…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%に対しては、遠慮は無用なのかも%RAND_SPLIT("知れないわね/ね/ってね")%\@ TENSION(TARGET, 8) == "高い" ? ？ # \@」
				CASE 2
					PRINTFORMW 「あら、悲鳴も上げずに良く頑張るものね、と言うか、むしろ…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%にとっては物足りないのかしら？」
			ENDSELECT
			;調教者のBASE:満足が50%以上
			IF TENSION(TARGET, 8) == "高い"
				;フェードアウト
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				;コンフィグを参照
				SIF ANIME_CONFIG("FADE")
					;真なら行削除する
					CLEARLINE 1
			ENDIF
		;1/3の確立で真
		ELSE
			IF 0
			ELSEIF AFFECTION("相思相愛") && PERCENT(80)
				PRINTFORMW 「ふふっ、いい表情ね…」
				PRINTFORM 「痛みに耐える姿も可愛いわ
				CALL PRINT_SYMBOL_K14
			ELSEIF AFFECTION("溺愛") && PERCENT(70)
				PRINTFORMW 「うふふっ、いい表情ね…」
				PRINTFORM 「ちゃんと頑張れたら、御褒美をあげるわ
				CALL PRINT_SYMBOL_K14
			ELSEIF AFFECTION("眷愛") && PERCENT(60)
				PRINTFORM 「ふふっ、いい表情ね…
				CALL PRINT_SYMBOL_K14
			ELSEIF AFFECTION("嫌悪") || (!ALI(0) && !RAND:3) || T_COND("怒り")
				PRINTFORMW 「あらあら、我慢強いのねぇ」
				PRINTFORMW 「なら、まだまだ耐えられる筈よね？ あははっ…」
			ELSE
				PRINTFORMW 「あら、頑張るわね%RAND_SPLIT("ねぇ/…？")%」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%、見込みがあるわよ」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;自然な行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "自然な行動"
		;-------------------------------------------------
		;ACTがイラマチオ
		;-------------------------------------------------
		IF 口上記入チェック(100) && IS_NOWACTNAME("イラマチオ")
			PRINTFORMW 「玩具のように扱われてるのに、%CNAME(MASTER, TARGET)%は受け入れるのね」
			PRINTFORM 「その苦しそうな表情…、とっても良いわよ\@ TENSION(TARGET, 8) == "高い" ? ？ # 。 \@もっと酷い事シたくなっちゃうわ
			CALL PRINT_SYMBOL_K14
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		SELECTCASE RANDCOM(3, 10)
			CASE 0
				PRINTFORMW 「耳障りな悲鳴も上げず、良く頑張るものね\@ TENSION(TARGET, 8) == "高い" ? ？ # \@」
				PRINTFORMW 「ま、その位じゃないと…、私も張り合いがない%RAND_SPLIT("のよねぇ/んだけど")%」
			CASE 1
				PRINTFORMW 「あら、良い表情ね？ とっても健気な感じで悪くないわよ」
				PRINTFORMW 「と言っても…、この程度のことには耐えて貰わないと、お話にならないんだけど。ね」
			CASE 2
				PRINTFORMW 「これって…、歯を食い縛るほどの事なのかしら？」
				PRINTFORMW 「私にはちょっと疑問なんだけど。な～んて。ね」
		ENDSELECT
		;調教者のBASE:満足が50%以上
		IF TENSION(TARGET, 8) == "高い"
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			;コンフィグを参照
			SIF ANIME_CONFIG("FADE")
				;真なら行削除する
				CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM30_1, 派生：痛いけど気持ち良い！
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:REACT派生 == 1
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 20)
				CASE 0
					PRINTFORMW 「あら、その様子……、ただ痛みに耐えるってだけじゃなさそうね？」
					PRINTFORMW 「ほら…どうなの？ 正直に言ってみなさいよ」
				CASE 1
					PRINTFORMW 「んー？ 苦痛を堪えるだけなら、そんな甘い吐息は漏れないわよねぇ」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%、本当は…、ちょっと良くなってきちゃったんじゃない？」
			ENDSELECT
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 21)
				CASE 1
					PRINTFORMW 「痛み%RAND_SPLIT("に耐/を堪")%える風を装っても、なんとなく判っちゃうのよねぇ」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%…、ただ痛いって訳じゃないんでしょう？」
					PRINTFORMW 「無駄に足掻いたところで、自分自身が辛くなるだけよ」
				CASE 2
					PRINTFORMW 「こんな事でキモチ良くなっちゃう事は受け入れ難い、って感じ？ ふふ」
					PRINTFORMW 「ま、自分の意外な一面に戸惑う気持ちは、私にも理解できるけど」
					PRINTFORMW 「ほら、怯える事なんてないわ。あるがままの自分を受け入れるのよ」
					SIF TENSION(TARGET, 8) == "高い"
						PRINTFORMW 「その方が、きっと%CNAME(MASTER, TARGET)%の為になると思うし。ね…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;消費気力による派生
	SELECTCASE GET_COM_RANK()
		CASE "自然な行動"
			SELECTCASE RANDCOM(2,20)
				CASE 0
					PRINTFORMW 「いま%CNAME(MASTER, TARGET)%が感じてるのは苦痛？ 快楽？」
					PRINTFORMW 「それとも%CNAME(MASTER, TARGET)%自身…、良く判らなくなっちゃってるのかしら。ねぇ？」
				CASE 1
					PRINTFORMW 「%CNAME(MASTER, TARGET)%にとっては、苦痛も快楽の内なのかしら？」
					PRINTFORMW 「本当に、度し難い変態ね……。ま、それを責めるつもりは無いけれど」
				CASE 2
					PRINTFORMW 「そうやって、苦痛を快楽に転化できるのは、稀有な才能だと思うわよ？」
					PRINTFORMW 「ただ…、そんな%CNAME(MASTER, TARGET)%を羨みはしないけど。ね」
			ENDSELECT
		CASE "無理のない行動"
			SELECTCASE RANDCOM(3,21)
				CASE 0
					PRINTFORMW 「ん？ その声……。%CNAME(MASTER, TARGET)%、もしかして…」
					PRINTFORMW 「ちょっと良くなってきちゃったとか？」
					PRINTFORMW 「残念だけど、今更否定したって%RAND_SPLIT("遅いわ/手遅れ")%よ」
				CASE 1
					PRINTFORMW 「苦痛を堪えるにしては、随分甘い声が出ちゃってるわね？」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%自身、気がついていないのかも%RAND_SPLIT("だ/知れない")%けど」
				CASE 2
					PRINTFORMW 「ただ痛いってだけじゃ、そういう声は出ないわよねぇ？」
					PRINTFORMW 「ま、誤魔化したいって気持ちは判らないでもないけれど」
					PRINTFORMW 「こんな事で良くなっちゃう変態って思われるのは、流石の%CNAME(MASTER, TARGET)%でも嫌なんだろうし。ね」
			ENDSELECT
	ENDSELECT
	;調教者のBASE:満足が50%以上
	IF TENSION(TARGET, 8) == "高い"
		;フェードアウト
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
		;コンフィグを参照
		SIF ANIME_CONFIG("FADE")
			;真なら行削除する
			CLEARLINE 1
	ENDIF
	RETURN 1
ENDIF

;──────────────────────────────────
;COM31, 悲鳴を上げる
;──────────────────────────────────
;・REACT分類:消極的従う
;・REACT印象:0から遠ざかる
;-------------------------------------------------
;選択可能な条件…
;ACT分類:会話
;	ACT:厳しく脅す/お仕置きと宣言する/怒鳴る/不気味に笑う
;ACT分類:愛撫
;	ACT:胸愛撫/口で愛撫/アナル愛撫/アナル舐め/キス/パイズリする/足コキする
;ACT分類:道具
;	ACT:バイブ/アナルバイブ/アナルビーズ
;ACT分類:性交
;	ACT:正常位/後背位/騎乗位/対面座位/背面座位/アナルセックス
;ACT分類:奉仕
;	ACT:イラマチオ
;ACT分類:加虐
;	ACT:スパンキング/鞭/針/縄/アイマスク/ボールギャグ/罵倒/三角木馬/浣腸セット
;ACT分類:異常
;	ACT:フィストファック/アナルフィスト/両穴フィスト/公衆肉便器プレイ
;-------------------------------------------------
;・TFLAG:REACT派生(0=痛みによる悲鳴/1=恐怖による悲鳴)
;-------------------------------------------------
;=================================================
@KOJO_COM_K14_31
;=================================================

LOCALS = %RAND_SPLIT("ウフフ/フフ/フフフ")%

;派生で別けるのが面倒なので、どちらでも違和感でない程度にしてまとめてしまう
;━━━━━━━━━━━━━━━━━━━━━━━━
;無理をした行動
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
	;実はまんざらでもないかつ、調教者が怒りではない
	IF COMCOR_POSI() && !T_COND("怒り")
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「悪くないけど、ちょっとワザとらしいわね？」
				PRINTFORMW 「ま、今だけは…、ちょっと乗せられてもいいかもって思うけど」
			CASE 1
				PRINTFORMW 「どうせなら、もっと自然に出来ないのかしら」
				PRINTFORMW 「わざとらしさが鼻について、ちょっと白けるのよね」
			CASE 2
				PRINTFORMW 「なかなかの演技だとは思うけど、ちょっとあからさま過ぎるのよ」
				PRINTFORMW 「どうせなら、もっと上手く騙して貰いたいものね」
		ENDSELECT
	;その他
	ELSE
		SELECTCASE RANDCOM(3, 1)
			CASE 0
				PRINTFORMW 「…ウルサイわね。悲鳴を上げればこの私が喜ぶとでも思っているワケ？」
				PRINTFORMW 「わざとらしいのよ、ハッキリ言って…」
			CASE 1
				PRINTFORMW 「別に無理して悲鳴上げる事なんて無いと思うけどね？」
				PRINTFORMW 「その程度で、この私に媚びようとしても無駄だから…」
			CASE 2
				PRINTFORMW 「悲鳴なんて、どれも同じと思ったら大間違いなのよ。悪いけど」
				PRINTFORMW 「聞けば判るのよ。それが本物の悲鳴なのか、無理して媚を売る為の悲鳴なのか、ね」
				PRINTFORMW 「判ったらのならその耳障りな悲鳴、いい加減に%RANDTEXT_K14("して頂戴", "やめなさい")%…」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM31_0, 派生：痛みによる悲鳴
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;================================================
	;ACT分類が会話（ACT:罵倒がここで処理される場合あり）
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 9)
			CASE 0
				PRINTFORMW 「%RAND_SPLIT("騒がしい/うるさい/ウルサイ")%わねぇ。もっと静かにできないのかしら」
				PRINTFORMW 「その耳障りな悲鳴…、聞きたくもないわ！」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…、悲鳴を上げればどうにかなるとでも思ってるなら大間違いよ」
				PRINTFORMW 「黙らないのなら、その口、塞いでやるから……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が愛撫
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "愛撫")
		;=================================================
		;調教者の状態が怒り
		;=================================================
		IF 口上記入チェック(100) && T_COND("怒り")
			SELECTCASE RANDCOM(3, 10)
				CASE 0
					PRINTFORMW 「ふん、いい気味ね。私を怒らせるのがいけないのよ」
					PRINTFORMW 「これ以上痛い思いをしたくないのなら、自分の立場を弁えることね」
				CASE 1
					PRINTFORMW 「あらあら、そんなに痛かったかしら？ って……」
					PRINTFORMW 「そうなるようにしてるんだから、当たり前なんだけどねぇ」
				CASE 2
					PRINTFORMW 「気に入って貰えたかしら？ %CNAME(MASTER, TARGET)%にはこういう方が相応しいと思ってね」
					PRINTFORMW 「この私を怒らせたらどうなるか、よく覚えておきなさい……」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;=================================================
		;ACT18, 足コキするの場合
		;=================================================
		IF 口上記入チェック(100) && IS_NOWACTNAME("足コキする")
			SELECTCASE RANDCOM(3, 11)
				CASE 0
					PRINTFORMW 「ま、踏みつけられて痛いのは当然よね？」
					PRINTFORMW 「これで良くなっちゃうようなら、もう救いようが無いって感じだし」
				CASE 1
					PRINTFORMW 「いいわねぇ。ほら、もっと鳴いてみせなさいよ」
					PRINTFORMW 「こんな粗末なものでも、この私に踏んで貰える分、むしろ感謝して欲しいくらいだわ」
				CASE 2
					PRINTFORMW 「悪くないわねぇ。惨めで無様で、%CNAME(MASTER, TARGET)%に相応しい姿だわ」
					PRINTFORMW 「ほら、遠慮すること無いわ。つまらないプライドなんか捨てて、もっとマゾ豚らしく鳴いてみせなさいよ」
			ENDSELECT
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ TENSION(TARGET, 8) == "高い" ? あははっ！ # ウフフ… \@」")
				;コンフィグを参照
				SIF ANIME_CONFIG("FADE")
					;真なら行削除する
					CLEARLINE 1
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		SELECTCASE ABL:TARGET:愛撫
			CASE IS > 4
				SELECTCASE TENSION(TARGET, 8)
					CASE "高い"
						;かわいい
						PRINTFORMW 「は？ なによそれ。悲鳴を上げる程痛かった、私が下手だったって言いたい訳？」
						PRINTFORMW 「冗談にしてはツマラナイし、そうでないのなら…、侮辱以外の何物でもないわ！」
					CASE "一般"
						PRINTFORMW 「悲鳴をあげる程痛いとか…、あり得ないと思うんだけど？」
						PRINTFORMW 「それってある意味、私に対する侮辱と同じだって事、覚えておいた方がいいわよ…」
					CASE "低い"
						PRINTFORMW 「正直、そんな反応される程下手じゃないと思うんだけど」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%…、それ、私に対する当てこすりか何かな訳？」
				ENDSELECT
			CASE IS > 2
				SELECTCASE TENSION(TARGET, 8)
					CASE "高い"
						PRINTFORMW 「ウルサイわね、わかったわよ！ 痛かったのよね？ 痛くないようにすればいいのよね？」
						PRINTFORMW 「注意したつもりだったけど、上手くいかないものね。まったく……」
					CASE "一般"
						PRINTFORMW 「これでも気を使ったつもりなんだけど、上手くいかないものね…」
						PRINTFORMW 「それでも、悲鳴を上げる程のものかしら？ 私にはちょっと疑問だけどね」
					CASE "低い"
						PRINTFORMW 「なによ、これが悲鳴を上げる程痛かったワケ？」
