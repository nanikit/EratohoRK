;=====================================================================
;快楽系反応
;=====================================================================
;・快SOURCEや特定のPALAMがトリガーとなって発生する
;・COMによってはACT分類がトリガーとなる
;-------------------------------------------------

;──────────────────────────────────
;COM10, 恥ずかしがる
;──────────────────────────────────
;・REACT分類:消極的従う
;・お仕置きポイント加算
;・快SOURCEが一定以上の場合は派生1,快感に恥ずかしがるになる
;・依存度変化(+1, 0)
;-------------------------------------------------
;選択可能な条件…
;ACT分類:会話
;	ACT:性についての話をする/衣装の変更を要求する
;ACT分類:愛撫
;	ACT:手で愛撫/胸愛撫/口で愛撫/アナル愛撫/アナル舐め/キス/性器を擦り合う
;		パイズリする/足コキする
;ACT分類:道具
;	ACT:ローター/バイブ/アナルバイブ/アナルビーズ/Ｃ愛撫道具/ニプルキャップ
;ACT分類:羞恥
;	ACT:自慰/秘貝開帳/自慰みせつけ/羞恥プレイ/野外プレイ/放尿
;ACT分類:奉仕
;	ACT:手で愛撫を強制/口で愛撫を強制/パイズリ/素股/胸愛撫強制
;ACT分類:異常
;	ACT:放置プレイ
;
;※SOURCE:快Ｃ/快Ｖ/快Ａ/快Ｂ, PALAM:恥情がトリガーとなる
;-------------------------------------------------
;・TFLAG:REACT派生(0=状況に恥ずかしがる/1=快感に恥ずかしがる)
;-------------------------------------------------
;=================================================
@KOJO_COM_K14_10
;=================================================

;派生毎に切り分ける
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM10_0, 派生：状況に恥ずかしがる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			IF EXP:MASTER:恥辱快楽経験 > 30
				PRINTFORMW 「未だに%CNAME(MASTER, TARGET)%が羞恥心を持ち合わせてたなんて、ね」
				PRINTFORMW 「ちょっと驚き、って言ったら失礼かしら？ ふふ…」
			ELSE
				PRINTFORMW 「わざわざ恥ずかしがって見せる必要が、何かある訳？」
				PRINTFORMW 「何を考えてるのか、よく判らないわね」
			ENDIF
		;実は嫌
		ELSE
			IF EXP:MASTER:恥辱快楽経験 > 30
				PRINTFORMW 「あら、意外な反応ね。こんな事、もうとっくに慣れてると思ってたけど」
				PRINTFORMW 「ただそうやって、『%RAND_SPLIT("ウブ/うぶ/初心")%なとこ見せてればどうにかなる』なんて考えてるなら…」
				CALL FADEIN(10, "「大きな間違い、よ……」")
				WAIT
			ELSE
				PRINTFORMW 「無理してまで恥ずかしがる事ないと思うけどね？」
				PRINTFORMW 「ま、もしかすると……、本当に恥ずかしいのかもだけど」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;アイマスク装着中
	;================================================
	IF 口上記入チェック(100) && TEQUIP:アイマスク
		PRINTFORMW 「どうせ何も見えないんだから、恥ずかしがる事ないと思うけどね」
		PRINTFORMW 「まぁでも……、自分がどんな状態か判らないってのは、ちょっと恥ずかしいものなのかしら？」
		RETURN 1
	ENDIF
	
	;================================================
	;調教対象が自慰中
	;================================================
	IF 口上記入チェック(100) && GETBIT(TEQUIP:自慰中, 0)
		PRINTFORMW 「恥ずかしいのが嫌なら止めればいいと思うけど…」
		PRINTFORMW 「ホントは私に見られながらオナニーするのが癖になってきたんじゃないかしら？」
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が愛撫 RANDCOM(3)
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "愛撫")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「この程度で恥ずかしがってるようじゃ困るわ」
				PRINTFORM 「だってそうでしょう？ これからもっと凄い事するんだから、ね
				CALL PRINT_HEARTCBW_K14(COLOR_K14("pink"), , , COLOR2_K14("ALICE"))
			CASE 1
				PRINTFORMW 「そういう反応するのってなんか新鮮、って思ったけど…」
				PRINTFORMW 「意外とそれが%CNAME(MASTER, TARGET)%の本質なのかもね？ ふふ」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が道具 RANDCOM(4)
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "道具")
		SELECTCASE RANDCOM(2, 4)
			CASE 0
				PRINTFORMW 「ま、そうよね。恥ずかしいわよね？」
				PRINTFORMW 「こんな玩具で身体弄られてよくなっちゃうなんて、ね」
			CASE 1
				PRINTFORMW 「ま、その気持ちは判らないでもないわ」
				PRINTFORMW 「こんな姿、他の人には見せられないものね？」
				PRINTFORM 「ってまぁ、見せるつもりもないけれど…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が羞恥 RANDCOM(5)
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "羞恥")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "自慰"
				SELECTCASE RANDCOM(2, 5)
					CASE 0
						PRINTFORMW 「ふふ、私に見られるのは恥ずかしいかしら」
						PRINTFORMW 「ま、これはこれで悪くない眺めね。そんな訳で、じっくりと見物させて貰うわ」
					CASE 1
						PRINTFORMW 「恥ずかしいからって手抜きはダメよ。普段通りにやりなさい」
						PRINTFORMW 「私が気になるなら目を瞑ればいいと思うわ。それがどれだけ貴女の慰めになるかは知らないけど、ね」
				ENDSELECT
			CASE "秘貝開帳"
				;濡れてる
				IF PALAM:MASTER:潤滑 > 500
					PRINTFORMW 「恥ずかしがってる割にはすっかり濡れてるみたいだけど…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%って意外とそういう人なのかしら、ね」
				;濡れてない
				ELSE
					PRINTFORMW 「まだ濡れてないみたいだけど、イヤラシイ雌の匂いがするわ」
					PRINTFORMW 「恥ずかしがってみせても、これは隠しようが無いわね？ ふふ…」
				ENDIF
			CASE "自慰みせつけ"
				PRINTFORMW 「必死に\@ !TEQUIP:アイマスク ? 見ないように # 誤魔化そうと \@してるみたいだけど、顔が赤いわよ」
				PRINTFORMW 「\@ !TEQUIP:アイマスク ? 目を背けても # 見えなくても \@音は聞こえるものね？ ちょっと興奮してきたんじゃないかしら」
			CASEELSE
				PRINTFORMW 「何がそんなに恥ずかしいのか良く判らないけど…」
				PRINTFORMW 「そんな%CNAME(MASTER, TARGET)%の姿を見るのは、悪くない感じね。ふふっ」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が奉仕
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "奉仕")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "情欲", "鬱屈", "朦朧"
				PRINTFORMW 「……もどかしいわね。焦れったくておかしくなりそうよっ！」
				PRINTFORMW 「本当…、イライラするわ。ほら、もっとちゃんとやりなさいよ」
			CASE "怒り"
				PRINTFORMW 「普段ならそんな姿も可愛いとか思うんでしょうけど…」
				PRINTFORMW 「今はとてもそんな気分になれないのよ、悪いけど」
			CASE "退屈"
				PRINTFORMW 「ホント、信じられないくらい退屈だわ。もっとちゃんとやりなさいよっ！」
			CASEELSE
				PRINTFORMW 「一体何がそんなに恥ずかしいのか知らないけど…」
				PRINTFORMW 「取り敢えず、そんなのじゃ不満なのよ。ほら、もっとしっかりして頂戴」
		ENDSELECT
		
		;調教者がイきそうな場合に表示。条件は関数内
		CALL PRINT_HALFHEARTED_K14
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が異常
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "異常")
		IF IS_NOWACTNAME("放置プレイ")
			PRINTFORMW （何一人でモジモジしてるのかしら、この人……）
			PRINTFORMW 「あ、別に%CNAME(MASTER, TARGET)%は気にしなくていいわ。そこで大人しくしてて頂戴」
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「ふふっ……」")
			SIF ANIME_CONFIG("FADE")
				CLEARLINE 1
		;ELSE
		ENDIF
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;快感に対して恥ずかしがる
	;=================================================
	IF 口上記入チェック(0) && TFLAG:REACT派生 == 1
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「キモチ良いからって恥ずかしがる事なんてないのに…」
				PRINTFORM 「でも%CNAME(MASTER, TARGET)%のそんなところ、\@ AFFECTION("相思相愛") ? 可愛いくて好きよ # 嫌いじゃないわ \@…
				CALL PRINT_SYMBOL_K14
			CASE 1
				PRINTFORMW 「恥ずかしがる事はないわ…」
				PRINTFORMW 「もっと素直に、快楽を受け入れなさい\@ AFFECTION("相思相愛") ? よ # \@」
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		IF AFFECTION("相思相愛")
			SELECTCASE RANDCOM(2, 10)
				CASE 0
					PRINTFORMW 「うふっ、そんなに恥ずかしいのかしら？」
					PRINTFORM 「可愛らしいわ
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「何がそんなに恥ずかしいのかしら？」
					PRINTFORMW 「ま、%CNAME(MASTER, TARGET)%のそういうとこは嫌いじゃないわよ。ふふ」
			ENDSELECT
		ELSE
			SELECTCASE RANDCOM(2, 11)
				CASE 0
					PRINTFORMW 「そういう%RAND_SPLIT("ウブ/初心")%なところは、%RAND_SPLIT("嫌いじゃないわよ…？/ちょっと可愛いかも、って気がしないでもないわね…")%」
				CASE 1
					PRINTFORMW 「そういう態度見せられると、わたし…」
					PRINTFORMW 「もっと恥ずかしがらせたいって思っちゃうのよね？」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE GET_CONDITION(TARGET)
			CASE "情欲", "鬱屈", "朦朧"
				PRINTFORMW 「ねぇ、へたれるのもいい加減にしてくらない？」
				PRINTFORMW 「こっちまで恥ずかしくなるわよ…」
			CASE "怒り"
				PRINTFORMW 「恥ずかしがる%CNAME(MASTER, TARGET)%も嫌いじゃないけど……」
				PRINTFORMW 「正直、今はもどかしくてイライラするだけなの。いい加減にして頂戴」
			CASE "退屈"
				PRINTFORMW 「…ツマラナイわね、退屈してる時にそんな態度見せられるとがっかりするわ」
				PRINTFORMW 「退屈……ホントに退屈よ」
			CASEELSE
				PRINTFORML 「嫌いじゃないわよ？ そういうの。でも…」
				PRINTFORMW 「私としては、ちょっともどかしいのよね」
		ENDSELECT
	ENDIF
	
	;ACT分類が奉仕/性奉かつ調教者がイきそうな場合に表示。条件は関数内
	CALL PRINT_HALFHEARTED_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;派生：快感に対して恥ずかしがる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:REACT派生 == 1
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			;30日経過で100に満たない程度の変動と思われる。場合によってはもっと低い
			IF EXP:MASTER:恥辱快楽経験 > 30
				PRINTFORMW 「ふふ、いまさらそんな可愛い反応が見られるなんてね」
				PRINTFORMW 「もうとっくに慣れてるものだと思ってたけど」
				PRINTFORMW 「ま、%CNAME(MASTER, TARGET)%のそんな一面を見られた事を、喜ぶべきなのかしら。ね」
			ELSE
				PRINTFORMW 「何もそこまで恥ずかしがる必要なんて無いと思うけどね？」
				PRINTFORMW 「『恥ずかしい』ってだけじゃないのかも知れないけど」
			ENDIF
		;実は嫌
		ELSE
			;本来であれば、20, 40, 60くらいで刻むのが良いように思える
			IF EXP:MASTER:恥辱快楽経験 > 30
				PRINTFORMW 「いまさら快楽に対して恥ずかしがるなんてね？」
				PRINTFORMW 「もうそんな段階じゃないと思うんだけど。でも、まぁ…」
				;RAND_SPLITは空文字列も返す
				PRINTFORMW 「%CNAME(MASTER, TARGET)%が恥ずかしがる姿…、悪くないわ%RAND_SPLIT("よ/ね/")%。ふふ」
			ELSE
				PRINTFORMW 「キモチ良くなっちゃう事が恥ずかしい、って訳なの？」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%にもそんな%RAND_SPLIT("ウブ/うぶ/初心")%なとこがあるのね」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;アイマスク装着中
	;=================================================
	IF 口上記入チェック(100) && TEQUIP:アイマスク
		PRINTFORMW 「何も見えないはずなのに恥ずかしいとか、何かの冗談なの？」
		PRINTFORMW 「それとも……むしろ見えないからこそ、なのかしらね」
		RETURN 1
	ENDIF
	
	;================================================
	;野外プレイ中
	;================================================
	IF 口上記入チェック(100) && TEQUIP:野外プレイ
		PRINTFORML 「ま、恥ずかしいのも当然よね？ 人に見られるかも知れないもの。でも…」
		PRINTFORMW 「だからと言って、%CNAME(MASTER, TARGET)%の意思を尊重する気はないのよ。悪いけど…」
		RETURN 1
	ENDIF
	
	;================================================
	;調教対象が自慰中
	;================================================
	IF 口上記入チェック(100) && GETBIT(TEQUIP:自慰中, 0)
		PRINTFORMW 「恥ずかしがっても、オナニーを%RAND_SPLIT("止める気は/する手は止まら")%ないのね？」
		PRINTFORMW 「ま、私は別にいいけど…。気が済むまでしてればいいわ」
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が愛撫 RANDCOM(14)
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "愛撫")
		SELECTCASE RANDCOM(2, 25)
			CASE 0
				PRINTFORMW 「本当は気持ち良くて堪らない癖に、素直じゃないわね」
				PRINTFORMW 「恥ずかしがるのも結構だけど、いい気分しないのよね。そういう態度…」
			CASE 1
				PRINTFORMW 「時にはそういうのもいいけど……」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%に拒絶されてるみたいで、今は見たくない感じがするわね…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が道具 RANDCOM(15)
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "道具")
		SELECTCASE RANDCOM(2, 26)
			CASE 0
				PRINTFORMW 「恥ずかしがるのは結構だけど、もっと素直に受け入れられないのかしら？」
				PRINTFORMW 「ま、勝手すぎる話だってのは判ってるつもりよ。ふふ…」
			CASE 1
				PRINTFORMW 「別に恥ずかしがるような事じゃないと思うけど、その辺どうなのかしら？」
				PRINTFORMW 「って、今そんなこと聞かれると、余計に恥ずかしいかしら。ねぇ？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が羞恥
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "羞恥")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "自慰"
				PRINTFORMW 「ダメよそんなの。ほら、恥ずかしがってないで、ちゃんとやりなさいよ」
				PRINTFORMW 「言われた事が出来ないなら考えなくちゃいけないけど、%CNAME(MASTER, TARGET)%、それでもいいのかしら…？」
			CASE "秘貝開帳"
				PRINTFORMW 「その気持ちは判らないでもないわ。ただ、一つ教えてくれないかしら？」
				PRINTFORMW 「単に辱められて恥ずかしいのか、この私に見られてるから恥ずかしいのか」
				PRINTFORMW 「ほら、恥ずかしがってないで。言ってみなさいよ……」
			CASEELSE
				PRINTFORMW 「恥ずかしがって見せたところで、もうすっかり興奮しちゃってるのは隠せないわよ」
				PRINTFORMW 「自分から素直になれないなら、私が矯正するしかないけれど」
				CALL FADEIN(8, @"「%CNAME(MASTER, TARGET)%、それでもいいのかしら…」")
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が奉仕
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "奉仕")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "情欲", "鬱屈", "朦朧"
				PRINTFORMW 「ダメよ。そんなやり方で誤魔化そうったって駄目」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%、私に触れるのがそんなに嫌なのかしら」
				PRINTFORMW 「ま、何れにしても…。それじゃ足りないのよ、私。もっとちゃんとシて頂戴」
			CASE "怒り"
				PRINTFORMW 「そう……、私に触れるのが嫌なのね」
				PRINTFORMW 「先に言っておくけど、恥ずかしいとか聞くつもりはないわよ。悪いけど…」
			CASE "退屈"
				SELECTCASE TFLAG:経過時間
					CASE IS >= 60 ;長い
						PRINTFORMW 「ねぇ…そんなやりかたじゃ退屈過ぎて欠伸が出るわ」
						PRINTFORMW 「この際はっきり言わせて貰うとね、恥ずかしいからどうのとか聞く気はないのよ」
						PRINTFORMW 「言われた事が出来ないなら、それなりの覚悟はして貰うから…」
					CASE IS >= 30 ;普通
						PRINTFORMW 「恥ずかしいのはいいけど、もっとちゃんとして欲しいわね」
						PRINTFORMW 「なんとかする気がないなら、私も考えさせて貰うから…」
					CASEELSE ;その他
						PRINTFORMW 「お粗末過ぎて泣けてくるわね？ もう少しまともに出来ないのかしら」
						PRINTFORML 「ま、どうせ恥ずかしいとかなんでしょうけど…」
						PRINTFORMW 「退屈よ、%CNAME(MASTER, TARGET)%…、ホント退屈」
				ENDSELECT
			CASEELSE
				PRINTFORMW 「ねぇ……、%CNAME(MASTER, TARGET)%が恥ずかしがってどうするのよ」
				PRINTFORMW 「言った事はキチンとやって貰わないと困るのよ。悪いけど」
		ENDSELECT
		
		;調教者がイきそうな場合に表示。条件は関数内
		CALL PRINT_HALFHEARTED_K14
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が異常
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "異常")
		PRINTFORMW 「この状況で恥ずかしがるって、ちょっと興味深いわね」
		PRINTFORMW 「どういう心境なのか聞いてみたい\@ IS_NOWACTNAME("放置プレイ") ? けど # ところね \@……\@ IS_NOWACTNAME("放置プレイ") ? 、今はダメよね。ふふ # \@」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	SELECTCASE RANDCOM(2, 30)
		CASE 0
			PRINTFORMW 「キモチ良いからって恥ずかしがる事なんてないのに…」
			PRINTFORM 「でも%CNAME(MASTER, TARGET)%のそんなところ、\@ AFFECTION("相思相愛") ? 可愛いくて好きよ # 嫌いじゃないわ \@…
			CALL PRINT_SYMBOL_K14
