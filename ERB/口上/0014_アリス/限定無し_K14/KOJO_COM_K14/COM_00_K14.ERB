IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			PRINTFORMW 「何でもないとか言う割りに、随分余裕がないわねぇ」
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「その無愛想な態度も、%CNAME(MASTER, TARGET)%の本意じゃないんでしょ？」
				CASE 1
					PRINTFORMW 「この場を取り繕うなら、もっと上手くやればいいのにね？」
			ENDSELECT
			PRINTFORM 「まぁでも…、そんな%CNAME(MASTER, TARGET)%の不器用なところ、嫌いじゃないわよ
			CALL PRINT_SYMBOL_K14
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「あまり褒められた態度じゃないわね、そういうの」
					PRINTFORMW 「でもそうね？ どことなく余裕なさ気で、微笑ましくもあるわ」
				CASE 1
					PRINTFORMW 「そうやって虚勢を張ったところで…」
					PRINTFORMW 「無理してるのはバレバレなんだけど、ね」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が会話
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 2)
			CASE 0
				PRINTFORMW 「可愛くないわね…」
			CASE 1
				PRINTFORML 「ねぇ、一つ言っておくけど…」
				PRINTFORMW 「そういう態度をしても、何も良い事はないわよ」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が加虐
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "加虐")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「可愛くないわね、そういうの…」
				PRINTFORMW 「ま、当然の反応と言えない事もないけど、ね」
			CASE 1
				PRINTFORMW 「ねぇ、もう少し可愛げのある反応出来ないのかしら？」
				PRINTFORMW 「そういう態度されるの好きじゃないのよ。私…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF AFFECTION("相思相愛")
		PRINTFORMW 「機嫌悪そうね…」
		PRINTFORMW 「私、何か余計な事言っちゃったかしら…？」
	ELSEIF AFFECTION("溺愛")
		PRINTFORMW 「なによ、拗ねてるの…？」
	ELSEIF AFFECTION("眷愛")
		PRINTFORMW 「機嫌悪そうね…」
	ELSEIF AFFECTION("嫌悪")
		PRINTFORMW 「今更%CNAME(MASTER, TARGET)%に愛想の良さなんて期待していないけど…」
		PRINTFORMW 「それでも、いい気分はしないわね…」
	ELSE
		PRINTFORMW 「可愛くないわね…」
