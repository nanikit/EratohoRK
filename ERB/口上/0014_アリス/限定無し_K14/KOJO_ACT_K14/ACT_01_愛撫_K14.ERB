·TFLAG:벌플래그가 서 있으면 발생하지 않는다
·턴방침이 "하드", "이상"이라면 발생하지 않는다
·조교자에게 TALENT:벽통과가 없는 경우, TEQUIP:볼개그 장착중은 튕겨진다
[SKIPEND]
@KOJO_ACT_K14_15
#DIMS DYNAMIC KISS
#DIMS DYNAMIC STRING
#DIMS DYNAMIC FIRSTPERSON
#DIMS DYNAMIC RAND_HEART
#DIMS DYNAMIC MSTR
#DIMS DYNAMIC TGT

KISS   '= RAND_SPLIT("/응…///읏…")
STRING '= AFFECTION("몹시 사랑함", TARGET) ? "후후" # "후후" 
FIRSTPERSON '= TALENT:MASTER:작은체형 ? "누나" # "나" 
RAND_HEART  '= RAND:3 ? HTML_HEARTB(, COLOR("PINK")) # HTML_HEART(, COLOR("PINK"))
MSTR '= CALLNAME:MASTER
TGT  '= CALLNAME:TARGET

;ACT15, 첫회(퍼스트 키스)
;============
;개별의 테두리로부터 분리해 상층에서 처리
;앨리스씨가 주인님이 아니면 다양하게 이상해지므로 한정한다
IF TALENT:TARGET:주인님
	IF FIRSTACT(0)
		;//미약, 로션계의 bit가 서 있는 경우는 리셋트 한다
		IF GETBITOR(TFLAG:미약등사용, 0, 1)
			TFLAG:미약등사용구상 = CLEARBITS(TFLAG:미약등사용구상, "0/1")
			TFLAG:미약등사용지문스킵 = CLEARBITS(TFLAG:미약등사용지문스킵, "0/1")
		ENDIF

		;//조교 대상으로 키스경험 없음의 경우
		IF EXP:MASTER:키스경험 == 0 && !GETBIT(CFLAG:TARGET:각인, 11) && FIRSTACT(1)
			PRINTFORMW 「처음의 키스의 상대가 나이라니, 영광으로 생각해도 좋아요?」
			;조교자도 처음
			IF EXP:TARGET:키스경험 == 0
				PRINTFORMW 「게다가…, 나에 있어서도%CNAME(MASTER, TARGET)%가 처음의 상대야」
				PRINTFORMW 「그 의미를 알아차리지 않을 정도(수록),%CNAME(MASTER, TARGET)%가 바보가 아니면 좋지만, 말야…」
			;그 외
			ELSE
				;최초로 ACT15에 떨어지기 전에, 조수 소환해 조수와 키스에서도 하지 않는 한 여기에 오는 일은 없을까적인
				;다만, 대조수의 EXP라든지 거동이 이상한 듯해서 뭐라고곳
				PRINTFORMW 「후후…, 긴장하지 않아도 괜찮아요. 나에게 맡겨」
				PRINTFORMW 「키스라고 하는 것이 어떤 것인가,%CNAME(MASTER, TARGET)%에 가르쳐 주어요…」
			ENDIF
			RETURN 1
		ENDIF

		;//조교 대상으로 키스경험 유의 경우
		IF EXP:MASTER:키스경험 > 0 && !GETBIT(CFLAG:TARGET:각인, 11) && FIRSTACT(2)
			PRINTFORML 「이만큼 얼굴을 접근해도, 놀라지도 않으면 긴장도 하지 않는다…」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 키스의 경험…… 있는 거네」
			;심을 읽을 수 있다
			SIF TALENT:MASTER:독심술 == 2		;kawaii
				CALL FADEIN(10, "(분한, 분한……, 부러운…, 아니꼽게 거슬려요……!)")
			;공기를 읽을 수 있다
			SIF TALENT:MASTER:독심술 == 1
				PRINTFORMDL %CALLNAME%의 눈동자는 희미하게 흔들리고 있는 것 같다…
			WAIT
			PRINTFORML 「…… 좋아요, 별로. 나는 나의 방식으로…」
			;어떻게 할까 고민했을 때는@RAND_SPLIT로 전부 돌진해 버린다. 하는 김에 공문자열도w
			CALL FADEIN(15, @"「%CNAME(MASTER, TARGET)%의, 입술을…, 빼앗아 준다%RAND_SPLIT("원/로부터/")%……」")
			WAIT
			RETURN 1
		ENDIF
	ENDIF
ENDIF


;ACT15_0, 키스
;==============
IF !GETBIT(TFLAG:미약등사용, 1)
	;//전회 턴으로 입안사정
	IF PENIS(MASTER) && TCVAR:MASTER:전회턴사정 && GET_STAIN("구", "정액", TARGET) && FLAG:지문제어 == 2
		SELECTCASE RANDACT(8, 0)
			CASE 0
				PRINTFORMW 「이봐요, 이것이%CNAME(MASTER, TARGET)%의 정액의 맛이야. 자주(잘) 맛보세요」
			CASE 1
				PRINTFORMW 「%FIRSTPERSON%의 입의 안에 내 두어 자신은 할 수 없는, 뭐라고 하는… 설마 말하지 않네요?」
			CASE 2
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…%FIRSTPERSON%의 입의 안에 털어 놓아 두어 자신은 싫어, 라든지 없네요?」
			CASE 3
				PRINTFORMW 「이봐요, 이익 분배야.%FIRSTPERSON%만 맛보고 있는 것도 불공평하니까.%STRING%…」
			CASE 4
				PRINTFORMW 「자신이 낸 것이 어떤 맛인 것인가…%CNAME(MASTER, TARGET)%도 알아 두는 것이 좋다고 생각해요」
			CASE 5
				PRINTFORMW 「네,%FIRSTPERSON%로부터의 선물. 사양말고 받아 줘」
			CASE 6
				{
				PRINTFORMW 「\@ ABL:TARGET:정액중독 > 3 
				? 조금 아깝지만…# 
				\@전부%CNAME(MASTER, TARGET)%의 입에 흘려 넣어 준다. 받으세요」
				}
			CASE 7
				PRINTFORML 「예라면 필요하지 않아요. 사양말고 받으세요. 왜냐하면 원래…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 낸 것이고,.%STRING%」
		ENDSELECT
		;더러움의 이동
		CALL MOVE_STAIN("구", TARGET, "구", MASTER)
		;경험의 부여
		EXP:MASTER:정액경험 += 1
		SIF TALENT:MASTER:남자 ;조교 대상이 남자라면 이상경험
			EXP:MASTER:이상경험 += 1
		RETURN 1
	ENDIF

	;//전회 ACT가 입으로애무
	{
	IF (GET_STAIN("구", "정액", TARGET) || GET_STAIN("구", "애액", TARGET) || GET_STAIN("구", "애널", TARGET)) && 
	IS_PREVACTNAME("입으로애무/애널빨고") && FLAG:지문제어 == 3
	}
		SELECTCASE RANDACT(7, 1)
			CASE 0
				PRINTFORMW 「이봐요, 이것이%CNAME(MASTER, TARGET)%의 맛이야. 자주(잘) 맛보세요」
			CASE 1
				PRINTFORMW 「%FIRSTPERSON%에 빨게 해 두어 자신은 할 수 없는, 뭐라고 하는%CNAME(MASTER, TARGET)%… 설마 말하지 않네요?」
			CASE 2
				PRINTFORMW 「%CNAME(MASTER, TARGET)%도 자신의 맛을 알아 두는 것이 좋다고 생각해요.%STRING%…」
			CASE 3
				PRINTFORMW 「네, 그러세요. 자신의 맛을 마음껏 만끽하면 좋아요」
			CASE 4
				PRINTFORMW 「%CNAME(MASTER, TARGET)%, 해 받아 두어 자신은 싫다던가 없네요?」
			CASE 5
				PRINTFORMW 「이봐요, 사양하지 않아도 괜찮아요? 자신의 맛과 냄새를 충분히 만끽해 줘」
			CASE 6
				PRINTFORMW 「네, 이것이%CNAME(MASTER, TARGET)%의 맛. 잘 기억해 두세요」
		ENDSELECT
		;더러움의 이동
		CALL MOVE_STAIN("구", TARGET, "구", MASTER)
		RETURN 1
	ENDIF

	;//성교중
	IF TEQUIP:성교중
		CALL FADEOUT(20, @"　　%KOJO_RAND_K14(("키스"), 1)%")
		CALL FADE_CLINE(1)
		SELECTCASE TEQUIP:성교중
			;정상위
			CASE 1
				CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 이봐요, 좀 더 입을 열어……」")
			;후배위/배면좌위
			CASE 2, 5
				CALL HTMLPRINTW(@"「저기…, 좀 더 여기를 향해……%KISS%%RAND_HEART%」")
			;승마위
			CASE 3
				CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 좋아요… 좀 더%FIRSTPERSON%에 신체를 맡겨……」")
			;대면좌위
			CASE 4
				CALL HTMLPRINTW(@"「이봐요, 키스 해 주기 때문에%CNAME(MASTER, TARGET)%도, 좀 더…%KISS%%RAND_HEART%」")
			;애널섹스
			CASE 6
				;조교 대상이 남자
				IF TALENT:MASTER:남자
					PRINTFORML 「엉덩이 찔려 앙앙 울고 있다\@ TALENT:MASTER:작은체형 ? 사내 아이# 남자\@라든지…」
					CALL HTMLPRINTW(@"「%FIRSTPERSON%, 견딜 수 없게 되어 버려요…%KISS%%RAND_HEART%」")
				;그 외
				ELSE
					CALL HTMLPRINTW(@"「기특하게 노력해 버려…. 지금의%CNAME(MASTER, TARGET)%…, 매우 사랑스러워요.%KISS%%RAND_HEART%」")
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF

	;//성교봉사중
	IF TEQUIP:성교봉사중
		CALL FADEOUT(20, @"　　%KOJO_RAND_K14(("키스"), 1)%")
		CALL FADE_CLINE(1)
		SELECTCASE TCVAR:MASTER:초조하게해번
			;초조하게함 안
			CASE IS > 1
				SELECTCASE PALAM:쾌V
					CASE IS >= 7000
						{
						CALL HTMLPRINTW(@"「안 돼요…,%CNAME(MASTER, TARGET)%만 먼저는." + 
						@"%FIRSTPERSON%도, 좀 더이니까…%KISS%%RAND_HEART%」")
						}
					CASE IS >= 4000
						CALL HTMLPRINTW(@"「안돼, 아직 가지 마. 조금만 더, 이대로…%KISS%%RAND_HEART%」")
					CASEELSE
						{
						CALL HTMLPRINTW(@"「안 돼요…, 아직. 좀 더%FIRSTPERSON%를%RAND_SPLIT("락섬/기분 좋고")%" + 
						@"키라고…%KISS%%RAND_HEART%」")
						}
				ENDSELECT
			;초조하게함 종료
			CASE 1
				;페니스밴드 사용중
				IF TEQUIP:페니스밴드 && !PENIS(MASTER)
					CALL HTMLPRINTW(@"「좋아요… 키스 하면서가게해 준다.%KISS%%RAND_HEART%」")
				;오늘의방침가 아이 만들기
				ELSEIF IS_POLICY("오늘", "자 만들기")
					SELECTCASE RANDACT(3, 4)
						CASE 0
							PRINTFORMW 「저기, 알까. 자궁이 내려 와 버리고 있어…」
							CALL HTMLPRINTW(@"「%CNAME(MASTER, TARGET)%의 자식을 갖고 싶어서, 이제 기다릴 수 없어요…%KISS%%RAND_HEART%」")
						CASE 1
							{
							CALL HTMLPRINTW(@"「좋아요, 안에 내…%CNAME(MASTER, TARGET)%의" + 
							@"%RAND_SPLIT("자식/아기의 소")%、%FIRSTPERSON%안에 쏟아…%KISS%%RAND_HEART%」")
							}
						CASE 2
							PRINTFORMW 「저기, 나의 안이 얕아지고 있는 것 알아? 벌써 준비 가능하게 된 것 같다%RAND_SPLIT("예요//")%」
							CALL HTMLPRINTW(@"「이봐요, 좋아요…%FIRSTPERSON%의 가장 안쪽에서 사정해.%KISS%%RAND_HEART%」")
					ENDSELECT
				;상사상애
				ELSEIF GET_KFLAG_K14("상사상애")
					SELECTCASE RANDACT(3, 5)
						CASE 0
							CALL HTMLPRINTW(@"「저기, 이대로 안에 내. 이봐요, 키스 하면서…%KISS%%RAND_HEART%」")
						CASE 1
							CALL HTMLPRINTW(@"「안에 갖고 싶어요…, 이대로%FIRSTPERSON%안에 쏟아.%KISS%%RAND_HEART%」")
						CASE 2
							{
							CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 이봐요, 좋아요… 가운데에 내." + 
							@"%FIRSTPERSON%도, 뭐라고 할까… 그 쪽이 기쁘고…」")
							}
					ENDSELECT
				ELSE
					SELECTCASE RANDACT(3, 6)
						CASE 0
							CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 좋아요,%FIRSTPERSON%안에 내…」")
						CASE 1
							CALL HTMLPRINTW(@"「이봐요, 좋아요.%FIRSTPERSON%중(안)에서 가세요…%KISS%%RAND_HEART%」")
						CASE 2
							CALL HTMLPRINTW(@"「안에 내도 좋아요. 짜내 주어요…%KISS%%RAND_HEART%」")
					ENDSELECT
				ENDIF
			;초조하게함 없음
			CASEELSE
				SELECTCASE TEQUIP:성교봉사중
					;정상위
					CASE 1
						{
						CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 응, 좀 더\@ PALAM:쾌V >= 5000 
						? …… 죽어 버릴 정도로 격렬하게 해# 구를 열어\@……」")
						}
					;후배위
					CASE 2
						{
						CALL HTMLPRINTW(@"「저기, 좀 더…,\@ PALAM:쾌V >= 5000 
						? 짐승같이 범해# 여기에 와\@……%KISS%%RAND_HEART%」")
						}
					;역강간
					CASE 3
						{
						CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% \@ PALAM:쾌V >= 5000 
						? 이봐요 좀 더…, 좀 더 아래로부터 밀어올려# 좋은 아이, 그대로 얌전하게 하고 있어\@……」")
						}
					;대면좌위
					CASE 4
						{
						CALL HTMLPRINTW(@"「저기… 좀 더 꽉 해.\@ PALAM:쾌V >= 5000 
						? 망가져 버릴 정도# %CNAME(MASTER, TARGET)%의 팔로\@강하게 안아…%KISS%%RAND_HEART%」")
						}
					;배면좌위
					CASE 5
						{
						CALL HTMLPRINTW(@"「%KISS%%RAND_HEART% 좀 더 근처에 와…\@ PALAM:쾌V >= 5000 
						? %FIRSTPERSON%의 신체에 좀 더 접해# 뒤로부터 꽉 해\@……」")
						}
					;애널섹스
					CASE 6
						{
						CALL HTMLPRINTW(@"「\@ PALAM:쾌A >= 5000 
						? 좀 더, 좀 더 격렬하게 찔러
						# 좀 더 격렬하게 시라고도 좋아요
						\@…、%FIRSTPERSON%는 괜찮기 때문에…%KISS%%RAND_HEART%」")
						}
				ENDSELECT
		ENDSELECT
		PRINTFORMD %TGT%는
		SELECTCASE TEQUIP:성교봉사중
			CASE 1
				PRINTFORMDL %MSTR%에 제대로 손발을 걸면서
			CASE 2
				PRINTFORMDL 그 신체를 요염하게 구불거리게 되어지면서 되돌아 보면서
			CASE 3
				PRINTFORMDL %MSTR%에 덮어씌워, 신체를 겹치면서
			CASE 4
				PRINTFORMDL %MSTR%에 신체를 딱 대면서
			CASE 5
				PRINTFORMDL %MSTR%의 목덜미에 팔을 돌리면서
			CASE 6
				PRINTFORMDL %ANUS_DESCRIPTION(TARGET)%를%MSTR%에 관철해지면서
		ENDSELECT
		{
		PRINTFORMDW \@ GET_KFLAG_K14("상사상애")  
		? 지나 갈 때를 막는것 같이 심한, 정열적인
		# 부드럽고, 휘감기는 것 같은
		\@키스를 했다…
		}
		RETURN 1
	ENDIF

	[SKIPSTART]
	이 개소는 어떻게 하지…
	;//조교자 삽입중
	IF TEQUIP:성교중
		;상사상애
		IF GET_KFLAG_K14("상사상애", TARGET) && TALENT:TARGET:주인님
			PRINTFORMDW 앨리스는 %CALLNAME:MASTER%를 찌르면서 입술을 거듭해 왔다…
			PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
			PRINTFORMW 「응풋… 좋아… 너무 좋아……%KOJO_RAND_K14(("키스"), 1)%」
			PRINTFORMDW 연결된 채로의 두 명은 정열적으로 서로의 혀를 서로 걸었다…
		;그 이외
		ELSE
			PRINTFORMW 「응쿳! 안이 꼭 죄여 왔군요…, 사랑스럽다…」
			PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
		ENDIF
		RETURN 1
	ENDIF

	;//조교 대상 삽입중
	IF TEQUIP:성교봉사중
		IF PENIS(MASTER)
			;상사상애
			IF 구상 기입 체크(100) && AFFECTION("상사상애")
				PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
				PRINTFORMDW 서로의 입안을 유린해 허리를 서로 쳐박는 동안 에, %CALLNAME:MASTER%는 앨리스의\@ V_SEX(MASTER) ? 질 # 항내 \@가 꼭 죄여 오는 것을 느꼈다…
				PRINTFORMW 「응읏! …… 푸핫! 저기, 좀 더! 좀 더……!」
				PRINTFORMDW 미칠 듯한까지의 정동에 자극을 받으면서 두 명은 서로를 서로 탐냈다…
			;그 이외
			ELSE
				PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
				PRINTFORMW 「응읏! \@ V_SEX(MASTER) ? 질내 # 안 \@로, 크고……」
			ENDIF
		ELSE
			SELECTCASE RANDACT(3, 1)
				CASE 0
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
					PRINTFORMW 「이봐요, 좀 더 입 여세요…」
				CASE 1
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
					CALL HTMLPRINTW(@" 「응%HTML_HEARTB(, COLOR2_K14("pink"))% 시 비칠 때에 키스 하는 것이라고 해 원이군요…」")
				CASE 2
					CALL HTMLPRINTW(@" 「후~%HTML_HEARTB(, COLOR2_K14("pink"))% 하%HTML_HEARTB(, COLOR2_K14("pink"))%」")
					PRINTFORMDW 일단얼굴을 떼어 놓아 %CALLNAME:MASTER%를 응시한 후,%CALLNAME%는 정열적인 키스를 세례를 퍼부을 수 있었다…
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
