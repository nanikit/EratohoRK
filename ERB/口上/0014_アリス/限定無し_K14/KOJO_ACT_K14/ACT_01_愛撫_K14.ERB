					{
					PRINTFORMW 「\@ TCVAR:TARGET:갈것같다 
					? 그런 일이라면, 나라고…
					# 가장…,%CNAME(MASTER, TARGET)%는 이제 한계같지만.%STRING% \@」
					}
				;//그 외
				ELSE
					PRINTFORMW 「%STRING%, 슬슬 때일까요?」
					CALL HTMLPRINTW(@"「그러면, 조금 격렬하게 해 주어요…%HTML_PS_K14()%」")
				ENDIF
		ENDSELECT
		CALL FADEOUT(20, @"　　%KOJO_RAND_K14(("페라"), 1)%")
		CALL FADE_CLINE(1)
		RETURN 1
	ENDIF

	;//연속 실행
	IF TFLAG:ACT == TFLAG:전ACT
		SELECTCASE RANDACT(3, 0)
			CASE 0
				PRINTFORMW 「안쪽으로부터 넘쳐 나오는 것이 멈추지 않네요」
				PRINTFORMW 「이것은 마개를 하지 않으면 안될까…?%STRING%」
