					SIF TCVAR:TARGET:갈것같다
						CALL HTMLPRINTW(@"「라고 할까 이것…, 응%HTML_AH_K14()% 내 쪽이 가게 될 것 같다%RAND_SPLIT("입니다만/그런데/")%…」")
				;//그 외
				ELSE
					PRINTFORMW 「%STRING%, 이렇게 부풀려 버려 이제 한계일까?」
					CALL HTMLPRINTW(@"「슬슬 결정타를 찔러 주어요…%HTML_PS_K14()%」")
				ENDIF
			;초조하게함 없음
			CASEELSE
				;//안면승마 안
				IF TEQUIP:안면승마 || TEQUIP:안면승마애널
					{
					CALL HTMLPRINTW(@"「%RAND_SPLIT("아/응")%응%HTML_AH_K14()% \@ TCVAR:TARGET:갈것같다 
