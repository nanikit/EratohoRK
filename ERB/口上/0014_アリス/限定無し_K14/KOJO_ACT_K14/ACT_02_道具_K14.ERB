	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		PRINTFORMW 「こんなもの、誰が考えたのかしらね？」
		PRINTFORMW 「これはこれで、色々愉しめそうではあるけど…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%は気に入ってくれるかしら。ねぇ？ ふふ」
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;その他
	;-----------------------------------
	SELECTCASE RANDACT(2, 0)
		CASE 0
			PRINTFORMW 「さあ、楽にして。でないと辛いわよ」
			PRINTFORMW 「変に力んだりしたら、きっと大変な事になると思うわ」
			PRINTFORMW 「%LOCALS%…」
		CASE 1
			PRINTFORMW 「さて、%CNAME(MASTER, TARGET)%に可愛い尻尾を生やしてあげましょうか」
			PRINTFORM 「\@ TALENT:MASTER:オトコ ? 男としては惨めな事この上ない # 女の子のアクセサリーとしては、下品な事この上ない \@、とは思うけど…、ね
			CALL PRINT_SYMBOL_K14
		CASE 2
			PRINTFORMW 「さぁお待ちかね。尻尾をつけて犬になる時間よ」
			PRINTFORMW 「私の、私だけの、従順な犬にしてあげるわ……」
	ENDSELECT
	RETURN 1
ENDIF

