;────────────────────────────────────
;ACT22, アナルバイブ
;────────────────────────────────────
;・ターン方針が"ソフト", "異常"だと発生しない
;・TEQUIP(アナルビーズ/浣腸器＋プラグ/三角木馬/ペニスバンド)装着中は発生しない
;・アナルセックス中(TEQUIP:性交中 == 6, もしくはA_SEX())には発生しない
;-------------------------------------------------
@KOJO_ACT_K14_22
;-------------------------------------------------
;TFLAG:ACT派生(0=アナルバイブ装着/1=解除)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT22_0, アナルバイブ装着
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		PRINTFORMW 「こんなもの、おしりに挿入れて大丈夫なのかしら？」
		PRINTFORMW 「素材自体は柔らかくて、問題なさそうに思えるけど……」
		PRINTFORMW 「ま、何事もまずは試してみないと。ねぇ？ ふふ」
		RETURN 1
	ENDIF
	
	;=================================================
	;バイブ装着中(二穴責め)
	;=================================================
	IF 口上記入チェック(100) && TEQUIP:バイブ
		PRINTFORMW 「お尻の穴ヒクヒクさせちゃって…」
		PRINTFORM 「いいわ…、こっちも思いっきり掻き回してあげる
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF

	;-------------------------
	;その他
	;-------------------------
	IF ABL:MASTER:Ａ感覚 > 4
		SELECTCASE RANDACT(3, 10)
			CASE 0
				PRINTFORMW 「%CNAME(MASTER, TARGET)%の場合、もうほぐしてあげる必要もなさそうね？」
				PRINTFORMW 「お望みなら一気に奥まで……って、それじゃ風情がないのよね。ふふ」
			CASE 1
				PRINTFORMW 「ヒクヒクさせちゃって厭らしいわね…。もう欲しくて待ちきれないのかしら？」
				PRINTFORM 「さあ言うのよ。
				CALL PRINTFONTSTYLE(COLOR_K14("white"), @"\@ TALENT:MASTER:オトコ ? 僕 # 私 \@のおしりに下さいアリス様お願いします", 1)
				PRINTFORMW って」
				CALL FADEIN(10, "「ちゃんと言えたなら――」")
				WAIT
				PRINTFORMW 「%CNAME(MASTER, TARGET)%に慈悲を与えてあげるわ…」
			CASE 2
				PRINTFORMW 「この手の事もすっかり当たり前になっちゃって、ちょっと新鮮味に欠けるわね」
				PRINTFORMW 「まぁその分…、%CNAME(MASTER, TARGET)%がいい声で鳴いてくれればいいんだけど」
				PRINTFORM 「ほら、入れるわよ。
				IF TALENT:MASTER:小柄
					PRINTFORM \@ TALENT:MASTER:オトコ ? 女の子 # 子猫 \@
				ELSE
					PRINTFORM \@ TALENT:MASTER:オトコ ? 女 # メス豚 \@
				ENDIF
				PRINTFORMW みたいに鳴かせてあげるわ……」
		ENDSELECT
	ELSE
		SELECTCASE RANDACT(3, 11)
			CASE 0
				PRINTFORMW 「さぁ、力を抜いて楽にして頂戴」
				PRINTFORMW 「でないと…、どうなっても知らないから。 フフ…」
			CASE 1
				PRINTFORMW 「ほら、もっと力を抜きなさいよ。これじゃ入らないわ」
				PRINTFORMW 「柔らかいから抵抗があると曲がっちゃうのよね」
			CASE 2
				PRINTFORMW 「先端の方を持って曲がらないようにすると入れやすいみたいね…」
				PRINTFORMW 「ん、いい感じね。これなら……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

