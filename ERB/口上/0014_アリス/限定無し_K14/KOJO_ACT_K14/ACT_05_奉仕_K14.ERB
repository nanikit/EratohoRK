;========================================================================
;奉仕系 ACT50～57
;========================================================================
;・TFLAG:ACT(50=手で愛撫を強制/51=口で愛撫を強制/52=パイズリ/53=素股/55=足舐め強制/56=イラマチオ/57=胸愛撫強制)
;・TFLAG:ACT分類 == 5に該当
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "奉仕")で拾う事が可能
;・調教対象に、調教者の身体を愛撫するように命令する
;・基本的には命令なので、この段階では従うかどうか確定していない(ACT56,イラマチオは拒否できない)
;-------------------------------------------------
;・ACT54,足コキは12/04/08版で粛清
;-------------------------------------------------

;────────────────────────────────────
;ACT50, 手で愛撫を強制
;────────────────────────────────────
;・愛撫強制/手淫強制/ペニスバンド手淫強制
;・調教対象(MASTER)に、手による愛撫を強制する
;・調教者にペニスがあると必ず手淫強制になる(ペニスがあると通常の愛撫強制は起こらない)
;・調教者がペニスバンド装着中はペニスバンド手淫強制になる
;・TFLAG:お仕置きフラグが立っていると発生しない
;・ターン方針が"ハード", "異常"だと発生しない
;・押し倒し中には発生しない
;・TEQUIP(縄/三角木馬)装着中には発生しない
;・性交中/性交奉仕中には発生しない
;・調教対象が従うか否かはこの時点では未定
;・REACT分類が拒否/暴れる/逃げるの場合は拒絶扱いとなる
;-------------------------------------------------
@KOJO_ACT_K14_50
;-------------------------------------------------
;・TFLAG:ACT派生(0=愛撫強制/1=手淫強制/2=ペニスバンド手淫強制)
;-------------------------------------------------
;・地の文はACT派生毎に分岐。各ACT派生内では分岐なし
;・ACT派生0,愛撫の時のみ快Ｂ/快ＡSOURCE増加
;・その他の場合は快Ｃ限定
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT50_0, 愛撫強制
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;ACT50, 初回
	;=================================================
	IF 口上記入チェック(100) && !PENIS(TARGET) && FIRSTACT(0)
		;調教対象がオトコ
		IF TALENT:MASTER:オトコ
			PRINTFORMW 「\@ TALENT:MASTER:小柄 ? ほら…、君 # さぁ…、%CNAME(MASTER, TARGET)% \@がするのよ」
			;調教対象が童貞
			IF TALENT:MASTER:童貞
				PRINTFORMW 「女の子にとって大切で、デリケートなとこなんだから丁寧にお願いするわ」
			ELSE
				PRINTFORMW 「多少女の扱いは心得てるみたいだから、大丈夫だとは思うけど」
			ENDIF
		;調教対象がオトコでない
		ELSE
			PRINTFORMW 「ほら、%CNAME(MASTER, TARGET)%がするのよ」
			PRINTFORMW 「同じ女同士…、どこをどうすればいいかなんて心得たものでしょう？」
			;調教対象がふたなりかつ小柄でない
			SIF TALENT:MASTER:ふたなり && !TALENT:MASTER:小柄
				PRINTFORMW 「ま、%CNAME(MASTER, TARGET)%の場合はちょっと違うみたいだけど。ふふ」
			;調教対象が小柄
			SIF TALENT:MASTER:小柄
				PRINTFORMW 「って、小さな女の子に言うのは無茶すぎるかもしれないけど。クスクス」
		ENDIF
		PRINTFORMW 「じゃ、いいかしら。まずは私が言うところから触って頂戴……」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者がイキそう
	;=================================================
	IF 口上記入チェック(100) && TCVAR:TARGET:イきそう
		PRINTFORMW 「ねぇ、私もうイきそうなの…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%の手でイかせて頂戴……」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		;汎用処理
		CALL PRINT_ACT_50_0_K14
	ELSE
		IF AFFECTION("相思相愛")
			PRINTFORMW 「ねぇ、分かるかしら。　私の体、触って欲しくて熱くなってるの…」
			PRINTFORMW 「はやく私に触って……好きなように弄り回して…」
		ELSE
			PRINTFORMW 「さぁ、手で気持ちよくさせてもらおうかしら」
			PRINTFORMW 「好きなように弄っていいわよ…」
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT50_1, 手淫強制
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 1
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(1)
		IF PENIS(MASTER)
			PRINTFORMW 「ほら、どうすればいいかなんて%CNAME(MASTER, TARGET)%の方が良く知ってるでしょう？」
			PRINTFORMW 「\@ TALENT:MASTER:小柄 ? って、小さな男の子に言うのもどうかって気もするけど # ま、人のモノだと勝手が違うかも知れないけど \@」
		ELSE
			PRINTFORMW 「%CNAME(MASTER, TARGET)%の場合、\@ TALENT:MASTER:処女 ? こういう事するのは、これが初めてかしら？ # これが初めてって訳でもないでしょう？ \@」
			PRINTFORMW 「ってまぁ、憶測で勝手なこと言ってみた訳だけど。ふふ」
		ENDIF
		PRINTFORMW 「私もこういうのされる側ってのは経験がないからね。興味半分、不安半分ってとこなんだけど」
		PRINTFORMW 「ま、後は%CNAME(MASTER, TARGET)%次第って訳…」
		RETURN 1
	ENDIF
	
	;================================================
	;調教者がイキそう
	;=================================================
	IF 口上記入チェック(100) && TCVAR:TARGET:イきそう
		PRINTFORMW 「んっ、は…ッ！ もう限界…」
		PRINTFORMW 「ねぇ、もっと強くしごいて……壊れるくらい、思いっきり…」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		;汎用処理
		CALL PRINT_ACT_50_0_K14
	ELSE
		PRINTFORMW 「ねぇ、分かるかしら…？ %CNAME(MASTER, TARGET)%にされるの期待して、こんなになってるのよ…」
		PRINTFORMW 「期待してるわ…♪　ふふふ…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT50_2, ペニスバンド手淫強制
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 2
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(2)
		PRINTFORMW 「正直、これに意味があるのかどうか。ちょっと疑問に思うわね？」
		PRINTFORMW 「まぁでも…、やってみれば何か新しい発見があるかも。ふふ」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		;汎用処理
		CALL PRINT_ACT_50_0_K14
	ELSE
		{
		CALL HTMLPRINTW(@"「さて、%HTMLFONT("<b>コレ</b>", , COLOR2_K14("白"))%" + 
                         "をしごいて貰おうかしら？」")
		}
		IF TENSION(TARGET, 8) == "高い"
			CALL FADEOUT(10, "「ふふ…」")
			SIF ANIME_CONFIG("FADE")
				CLEARLINE 1
		ENDIF
		PRINTFORMW 「でもいいこと？ 作り物だからって、手抜きは許さないから…」
	ENDIF
	RETURN 1
ENDIF

;────────────────────────────────────
;ACT51, 口で愛撫を強制
;────────────────────────────────────
;・クンニ/フェラチオ/顔面騎乗/顔面騎乗アナル/
;・調教対象に、口による愛撫を強制する
;・調教者がふたなりの場合、調教対象が仰向けなら1/2で顔面騎乗、そうでなければフェラ強制になる
;・調教者にペニスがなく、調教対象が仰向けなら顔面騎乗になる
;・顔面騎乗が選ばれていたとき、顔面騎乗アナルに変化することがある
;・全ての条件から漏れた場合は通常のクンニとなる
;・TFLAG:お仕置きフラグが立っていると発生しない
;・押し倒し中には発生しない
;・TEQUIP(ボールギャグ/三角木馬)装着中は発生しない
;・調教者がペニスバンドを装着している場合は発生しない
;・性交中/性交奉仕中には発生しない
;・REACT分類が拒否/暴れる/逃げるの場合は拒絶扱いになる
;-------------------------------------------------
@KOJO_ACT_K14_51
;-------------------------------------------------
;・TFLAG:ACT派生(0=クンニ/1=フェラ/2=顔面騎乗/3=顔面騎乗アナル)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT51_0, クンニ FIRST/RANDACT(0)-(10)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0) && !TEQUIP:調教対象Ｃ使用
		PRINTFORMW 「さぁ…跪いて、口と舌を使って私にご奉仕するのよ」
		PRINTFORMW 「まだ判らない？ 舐めなさいって言ってるのよ」
		PRINTFORMW 「ご主人様の大事なとこに口で奉仕できるなんて、犬にとっては幸せでしょう？」
		RETURN 1
	ENDIF
	
	;=================================================
	;連続実行
	;=================================================
	IF 口上記入チェック(100) && TFLAG:ACT == TFLAG:前ACT
		IF AFFECTION("相思相愛")
			PRINTFORMW 「あっ…んあぁっ……はっ…あ…っふあ…」
			PRINTFORMW 「もっと…っ…もっと舌で掻き回して…ッ…！」
		ELSE
			PRINTFORMW 「はっ…あッ……そうじゃなくて…もっと…こう…」
			PRINTFORMW 「まったく、もどかしいわね……」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;精液舐め強制
	;=================================================
	IF 口上記入チェック(100) && INRANGE(TCVAR:MASTER:膣内射精後経過ターン, 1, 2)
		SELECTCASE RANDACT(3, 3)
			CASE 0
				PRINTFORMW 「随分注ぎ込んでくれたわね？」
				PRINTFORMW 「さ、自分の精液を舐めて綺麗にして頂戴」
			CASE 1
				PRINTFORMW 「ほら、精液があふれてきちゃってるの…判るかしら」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%が汚したんだから、ちゃんと綺麗にしてもらうわよ」
			CASE 2
				PRINTFORMW 「\@ !TEQUIP:アイマスク ? 見て # ほら \@…、%CNAME(MASTER, TARGET)%の精液があふれちゃってるの…」
				PRINTFORMW 「これをどうすればいいか。言わなくても判るわよねぇ？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;精液舐め強制2
	;=================================================
	IF 口上記入チェック(100) && IS_PREVACTNAME("足コキする") && GET_STAIN("ヴァギナ", "精液", TARGET)
		PRINTFORMDL アリスは精液に塗れた自らの秘所を%CALLNAME:MASTER%に舐めろと命じた…
		PRINTFORMW 「さ、責任をもって綺麗にして貰おうかしら？」
		PRINTFORMW 「一適残らず綺麗に舐めなさい。少しでも残したら承知しないから…」
		RETURN 1
	ENDIF
	
	;-------------------------
	;漏れたものはここで処理
	;-------------------------
	IF RAND:3
		SELECTCASE RANDACT(3, 10)
			CASE 0
				PRINTFORMW 「さぁ、跪いて舐めるのよ」
				PRINTFORMW 「ちゃんとご奉仕できたなら、何かご褒美を考えてあげない事もないわ。ふふ」
			CASE 1
				PRINTFORMW 「いつまでも眺めているのは結構だけど、さっさと始めてくれないかしら」
				PRINTFORMW 「じれったいのよ。%CNAME(MASTER, TARGET)%…」
			CASE 2
				PRINTFORMW 「舐めなさい。この『アリス様』に、口でご奉仕できるなんて幸せでしょう？」
				PRINTFORMW 「コレ、%CNAME(MASTER, TARGET)%だけの特権なんだから…、その意味を良く噛み締めて欲しいものね」
		ENDSELECT
	ELSE
		IF AFFECTION("相思相愛")
			PRINTFORMW 「ねぇ…私、もう我慢できないの……」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%の舌で……掻き回して…」
		ELSE
			PRINTFORMW 「さぁ、私のココを舐めなさい」
			PRINTFORMW 「丁寧に、優しくお願いするわ…」
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT51_1, フェラチオ FIRST/RANDACT(11)-(19)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 1
	;=================================================
	;調教者がイキそう
	;=================================================
	IF 口上記入チェック(100) && TCVAR:TARGET:イきそう
		PRINTFORMW 「%CNAME(MASTER, TARGET)%の舌使いがいやらしくて……もう限界…」
		PRINTFORMW 「んんっ…そうよ、もっと口を窄めて…っ」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象が仰向け
	;=================================================
	IF 口上記入チェック(100) && TFLAG:仰向け
		PRINTFORMW 「この体勢で咥えてもらうの、何か興奮するわね……」
		PRINTFORMW 「ほら、もっと舌使って。裏筋もお願いするわ…♪」
		RETURN 1
	ENDIF
	
	IF AFFECTION("相思相愛")
		PRINTFORMW 「%CNAME(MASTER, TARGET)%にしてもらうの……なんか凄い幸せな気分になるわ…」
		PRINTFORMW 「お願い、早く%CNAME(MASTER, TARGET)%の口で気持ちよくして…」
	ELSE
		PRINTFORMW 「さぁ、私の咥えて頂戴」
		PRINTFORMW 「歯を立てては駄目よ？」
 	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT51_2, 顔面騎乗 FIRST/RANDACT(20)-(29)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 2
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(20)
		;-------------------------------------------------
		;調教対象Ｃ使用中は正対していないとみなす
		;-------------------------------------------------
		IF 口上記入チェック(100) && GROUPMATCH(TEQUIP:調教対象Ｃ使用, 0, 1) > 0
			PRINTFORMW 「さぁ、今度は%CNAME(MASTER, TARGET)%の番よ」
			PRINTFORMW 「ちゃんとできないのなら、窒息するまで押し付けて、勝手に愉しませてもらう事になるけど」
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		PRINTFORMW 「あら、良い眺めねぇ。%CNAME(MASTER, TARGET)%にこんな事を強制するのって、興奮しちゃうわ…」
		PRINTFORMW 「嫌がるなら、息ができないくらい押し付けて…好きなように愉しめばいいし。ねぇ？」
		RETURN 1
	ENDIF
	
	;=================================================
	;連続実行
	;=================================================
	IF 口上記入チェック(100) && TFLAG:ACT == TFLAG:前ACT
		IF AFFECTION("相思相愛")
			PRINTFORMW 「んんっ…はぅっ…ぁぁ……あんッ…」
			PRINTFORMW 「\@ IS_PREVCOMNAME("積極的やる/愛情を込めてやる") ? 舌の動きがいやらしくて…ッ！ 蕩けちゃうっ！ # もっと、もっとちゃんとやりなさいよっ！ \@」
		ELSE
			PRINTFORMW 「ふぅんっ……んんっ……あっ…」
			PRINTFORMW 「んはっ……\@ IS_PREVCOMNAME("積極的やる/愛情を込めてやる") ? なかなか上手、よ。 # もどかしいわね。 \@ほら、もっと舌で穿るように…ッ！」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;精液舐め強制
	;=================================================
	IF 口上記入チェック(100) && INRANGE(TCVAR:MASTER:膣内射精後経過ターン, 1, 2)
		SELECTCASE RANDACT(3, 22)
			CASE 0
				PRINTFORMW 「%CNAME(MASTER, TARGET)%が汚したところ、舌で綺麗にしてもらおうかしら」
				PRINTFORMW 「ほら、残さず綺麗に舐めないと駄目よ？ ウフフ…」
			CASE 1
				PRINTFORMW 「ほら、自分が出した精液を味わいなさい」
				PRINTFORMW 「全部綺麗に舐め取ってもらうわよ？」
			CASE 2
				PRINTFORMW 「あふれた精液が%CNAME(MASTER, TARGET)%の顔に垂れちゃってるわ。ふふ」
				PRINTFORMW 「ほら、%CNAME(MASTER, TARGET)%の口で綺麗にして頂戴…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;精液舐め強制2
	;=================================================
	IF 口上記入チェック(100) && IS_PREVACTNAME("足コキする") && GET_STAIN("ヴァギナ", "精液", TARGET)
		PRINTFORMDL アリスは精液に塗れた自らの秘所を%CALLNAME:MASTER%に舐めろと命じた…
		PRINTFORMW 「さ、自分のした事の責任はとってもらうわよ」
		PRINTFORMW 「一適残らず綺麗に舐めなさい」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者が情欲
	;=================================================
	IF 口上記入チェック(100) && T_COND("情欲")
		PRINTFORMDW %CALLNAME%は興奮した様子で%CALLNAME:MASTER%の顔面に跨ると、
		PRINTFORMDW 昂ぶりきった官能の赴くまま、濡れそぼった秘唇を%CALLNAME:MASTER%の顔に押し付けた…
		SELECTCASE RANDACT(3, 24)
			CASE 0
				PRINTFORMW 「もう…、我慢できないわ。欲しくて、欲しくて…、変になっちゃいそう！」
				PRINTFORMW 「んもぅっ、じれったいのよ！ ほらっ、はやく！ 早くっ！」
			CASE 1
				PRINTFORMW 「%RAND_SPLIT("はぁ、はぁ、はぁ/ハァ、ハァ、ハァ")%……。もう駄目。我慢…、できない！」
			CASE 2
				PRINTFORMW 「疼いちゃって…、もうおかしくなりそう……。ほらはやく！ 早くしなさいよっ！」
		ENDSELECT
		PRINTL
		PRINTFORMD %CALLNAME%は
		;調教対象Ｃ使用につき正対してはいない状況
		IF GROUPMATCH(TEQUIP:調教対象Ｃ使用, 0, 1) > 0
			PRINTFORMD %CALLNAME:MASTER%の
			IF TEQUIP:ペニスバンド
				PRINTFORMD 張り型
			ELSEIF PENIS(MASTER)
				PRINTFORMD ペニス
			ELSE
				PRINTFORMD クリトリス
			ENDIF
			PRINTFORMDL を愛撫しながら腰を蠢かし、
			PRINTFORMDW 息ができない程にその%VAGINA_DESCRIPTION2(TARGET)%を擦り付けてきた…
		;正対し、跨っている状況
		ELSE
			PRINTFORMDW %CALLNAME:MASTER%の頭を掴むと腰をくねらせ、
			PRINTFORMDW まるで玩具を扱うかのように、その%VAGINA_DESCRIPTION2(TARGET)%を擦り付けている…
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象Ｃ使用中
	;=================================================
	IF 口上記入チェック(100) && GROUPMATCH(TEQUIP:調教対象Ｃ使用, 0, 1) > 0
		IF RAND:3
