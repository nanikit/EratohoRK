;-------------------------------------------------
;KFLAGを定義
;-------------------------------------------------
@GET_KFLAG_K14(ARGS, ARG = -2)
#FUNCTION
ARG = ARG != -2 ? ARG # TARGET
SELECTCASE ARGS
	CASE "相思相愛"
		RETURNF KFLAG:ARG:100 == 1
	CASEELSE
		CALLF PRINT_ERRORMESSAGE(@"@GET_KFLAG_K14に無効な定義(%ARGS%)が渡されました")
		RETURNF -1
ENDSELECT
