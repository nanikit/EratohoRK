@TC_EX_K14
#DIMS DYNAMIC STRING
STRING = %RAND_SPLIT("아/응///아″/응″/우아")%

SELECTCASE CFLAG:TARGET:C절회수
	CASE IS >= 5
		PRINTFORML 「%RAND_SPLIT("하, 하…… 하……/하아, 하아…… 하아……")%」
		CALL HTMLPRINTW(@"「이제(벌써)… 정말 안됏%HTML_AH_K14()% 죽어 버린다! 이렇게 가면 죽어 버린닷%EXCLAM_K14()%」")
		CALL HTMLPRINTW(@"「%RAND_SPLIT("아//응")%″… 아″…… 아″! 간닷%HTML_AH_K14()% 가고 있을 뿐에%EXCLAM_K14()%」")
		CALL HTMLPRINTL(@"「%STRING%아아″!! 간닷%HTML_AH_K14()% 간다아앗%EXCLAM_K14()%」")
	CASE 4
		CALL HTMLPRINTW(@"「%STRING%…… 아아… 아″%HTML_AH_K14()% 안됏! 와 버린다%HTML_AH_K14()% 또 와 버린닷%HTML_PS2_K14()%」")
		CALL HTMLPRINTL(@"「%STRING%…… 아아%HTML_AH_K14()% 히%HTML_AH_K14()% 간닷%HTML_AH_K14()% 이, 쿠우웃%EXCLAM_K14()%」")
	CASE 3
		CALL HTMLPRINTW(@"「%STRING%, 싫다%HTML_AH_K14()% 가는 것… 멈추는거야, 쿳%HTML_PS2_K14()%」")
		CALL HTMLPRINTL(@"「아앗…%HTML_AH_K14()% 이제(벌써), 이제 안돼… 가 버린다%HTML_AH_K14()% 가 버린다앗%EXCLAM_K14()%」")
	CASE 2
		CALL HTMLPRINTW(@"「%STRING%%HTML_AH_K14()% 가 버린다%HTML_AH_K14()% 또 가 버린다앗%HTML_PS2_K14()%」")
		CALL HTMLPRINTL(@"「%STRING%…… 아아아%HTML_AH_K14()% 익, 쿠웃%EXCLAM_K14()%」")
	CASE 1
		CALL HTMLPRINTW(@"「%RAND_SPLIT("아/하//응")%응%HTML_AH_K14()% %RAND_SPLIT("응구/는/아우")%……」")
		CALL HTMLPRINTL(@"「간닷%HTML_AH_K14()% 있고……%HTML_AH_K14()% ……%EXCLAM_K14()%」")
ENDSELECT
CALL QUAKE(4)
WAIT

