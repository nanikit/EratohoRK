;-------------------------------------------------
;함수명:GET_EX_K14
;개  요점:캐릭터 라인으로 EX관련을 참조한다
;인  수:ARGS… 절정의 종류
;반환값:지정한 캐릭터 라인에 준한 NOWEX
;사용예:GET_EX_K14("MC절정")
;-------------------------------------------------
@GET_EX_K14(ARGS)
#FUNCTION
#LOCALSIZE 30
SPLIT ARGS, "/", LOCALS

LOCAL:0 = NOWEX:MASTER:C절정
LOCAL:1 = NOWEX:MASTER:V절정
LOCAL:2 = NOWEX:MASTER:A절정
LOCAL:3 = NOWEX:MASTER:B절정
LOCAL:4 = NOWEX:MASTER:분유
LOCAL:5 = NOWEX:MASTER:사정
LOCAL:6 = NOWEX:MASTER:방뇨
LOCAL:7 = NOWEX:C절정
LOCAL:8 = NOWEX:V절정
LOCAL:9 = NOWEX:A절정
LOCAL:10 = NOWEX:B절정
LOCAL:11 = NOWEX:분유
LOCAL:12 = NOWEX:사정
LOCAL:13 = NOWEX:MASTER:물총

;물총이 추가되었지만 번호 바꿔 넣는 것 귀찮아서 우선 LOCAL:13으로
SELECTCASE ARGS
	CASE "MC절정"
		RETURNF LOCAL:0
	CASE "MV절정"
		RETURNF LOCAL:1
	CASE "MA절정"
		RETURNF LOCAL:2
	CASE "MB절정"
		RETURNF LOCAL:3
	CASE "M분유"
		RETURNF LOCAL:4
	CASE "M사정"
		RETURNF LOCAL:5
	CASE "M 드라이사정"
		RETURNF LOCAL:5 == 1
	CASE "M 대량사정"
		RETURNF LOCAL:5 == 3
	CASE "M 조루사정"
		RETURNF LOCAL:5 == 4
	CASE "MA고문 사정"
		RETURNF LOCAL:5 == 5
	CASE "M통상 사정"
		RETURNF LOCAL:5 == 2
	CASE "M방뇨"
		RETURNF LOCAL:6
	CASE "TC절정"
		RETURNF LOCAL:7
	CASE "TV절정"
		RETURNF LOCAL:8
	CASE "TA절정"
		RETURNF LOCAL:9
	CASE "TB절정"
		RETURNF LOCAL:10
	CASE "T분유"
		RETURNF LOCAL:11
	CASE "T사정"
		RETURNF LOCAL:12
	CASE "T 드라이사정"
		RETURNF LOCAL:12 == 1
	CASE "T 대량사정"
		RETURNF LOCAL:12 == 3
	CASE "T통상 사정"
		RETURNF LOCAL:12 == 2
	CASE "M물총"
		RETURNF LOCAL:13
	CASE "M_CVAB 절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:2 > 0 && LOCAL:3 > 0
	CASE "M_CVA 절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:2 > 0
	CASE "M_VAB 절정"
		RETURNF LOCAL:1 > 0 && LOCAL:2 > 0 && LOCAL:3 > 0
	CASE "M_CVB 절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:3 > 0
	CASE "M_CV절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0
	CASE "M_CA절정"
		RETURNF LOCAL:0 > 0 && LOCAL:2 > 0
	CASE "M_CB절정"
		RETURNF LOCAL:0 > 0 && LOCAL:3 > 0
	CASE "M_VA절정"
		RETURNF LOCAL:1 > 0 && LOCAL:2 > 0
	CASE "M_VB절정"
		RETURNF LOCAL:1 > 0 && LOCAL:3 > 0
	CASE "M_AB절정"
		RETURNF LOCAL:2 > 0 && LOCAL:3 > 0
	CASE "T_CVAB 절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:9 > 0 && LOCAL:10 > 0
	CASE "T_CVA 절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:9 > 0
	CASE "T_CVB 절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:10 > 0
	CASE "T_VAB 절정"
		RETURNF LOCAL:8 > 0 && LOCAL:9 > 0 && LOCAL:10 > 0
	CASE "T_CV절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0
	CASE "T_CA절정"
		RETURNF LOCAL:7 > 0 && LOCAL:9 > 0
	CASE "T_CB절정"
		RETURNF LOCAL:7 > 0 && LOCAL:10 > 0
	CASE "T_VA절정"
		RETURNF LOCAL:8 > 0 && LOCAL:9 > 0
	CASE "T_VB절정"
		RETURNF LOCAL:8 > 0 && LOCAL:10 > 0
	CASE "T_AB절정"
		RETURNF LOCAL:9 > 0 && LOCAL:10 > 0
	CASEELSE
		CALLF PRINT_ERRORMESSAGE(@"@GET_EX_K14에 무효인 정의가 건네받았습니다")
		RETURNF -1
ENDSELECT
RETURNF 0

