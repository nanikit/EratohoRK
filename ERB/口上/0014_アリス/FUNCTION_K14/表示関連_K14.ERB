;-------------------------------------------------
;CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K14
;引　数：ARG :0 = FADEの速度(ms)
;引　数：ARG :1 = 1でTARGET、 2でMASTERの状態変化を参照する
;引　数：ARG :2 = -2以外ならFADEがOFFである場合にRESETCOLORでPRINTを出力した後、口上主の文字色に戻す
;引　数：ARGS:0 = @KOJO_RAND_K14の行為の分類を指定する。文字列引数
;
;ARG:0の速度でSETCOLORされた文字色から背景色へARGS:0の分類のKOJO_RAND_K14をフェードアウト
;-------------------------------------------------
@CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K14(ARG:0, ARGS:0, ARG:1 = 1, ARG:2 = -2)
IF ANIME_CONFIG("FADE")
	CALL FADE(ARG:0, GETCOLOR(), GETBGCOLOR(), @"　　　　　%KOJO_RAND_K14((ARGS:0), ARG:1)%")
ELSE
	SIF ARG:2 != -2
		RESETCOLOR
	PRINTFORMW \@ ARG:2 == -2 ? 「 # （ \@%KOJO_RAND_K14((ARGS:0), ARG:1)%\@ ARG:2 == -2 ? 」 # ） \@
	SIF ARG:2 != -2
		CALL KOJO_COLOR(TARGET)
ENDIF
CALL FADE_CLINE(1)


;PRINT_SYMBOLのHTMLFONT版
;========================
@HTML_PS_K14(ARG:0 = -2) ;//引数を省略するとTARGET
#FUNCTIONS
#LOCALSSIZE 1
VARSET LOCALS
ARG:0 = ARG:0 != -2 ? ARG:0 # TARGET
;//基本的に行末での使用を想定

[SKIPSTART]
好意、好感度で派生する
ハート系の出力は2/3でHEARTB, 1/3でHEARTを出力する
音符系の出力は2/3で8分音符, 1/3で連8分音符を出力する
[SKIPEND]
IF GET_KFLAG_K14("相思相愛", ARG:0)
	;BASE:満足で派生する
	SELECTCASE TENSION(ARG:0, 8)
		;BASE:満足が50%以上
		CASE "高い"
			;1/4でハート2つを出力
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK"), 2) # HTML_HEART(, COLOR2_K14("PINK"), 2)
			;その他ならハート1つ
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK")) # HTML_HEART(, COLOR2_K14("PINK"))
			ENDIF
		;BASE:満足が25%以上
		CASE "一般"
			LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		;BASE:満足が25%未満
		CASE "低い"
			;なにも出力しない
			LOCALS:0 '= ""
	ENDSELECT
ELSEIF AFFECTION("溺愛", ARG:0)
	SELECTCASE TENSION(ARG:0, 8)
		CASE "高い"
			;1/4でハート系を出力
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK")) # HTML_HEART(, COLOR2_K14("PINK"))
			;その他なら音符系を出力
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
			ENDIF
		CASE "一般"
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		CASE "低い"
			LOCALS:0 '= ""
	ENDSELECT
ELSE
	SELECTCASE TENSION(ARG:0, 8)
		CASE "高い"
			LOCALS:0 '= HTML_EIGHTHNOTE()
		CASEELSE
			LOCALS:0 '= ""
	ENDSELECT
ENDIF
RETURNF LOCALS:0


