@PRINT_COMGRO_4_K14
SELECTCASE GET_COM_RANK()
	CASE "自然な行動"
		PRINTFORMW 「半端な事されても、じれったいだけなんだけど？ でもまぁ…」
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORM 「これはこれで、ちょっと微笑ましくもあるかしら、ね
			CASE "一般"
				PRINTFORM 「その様子じゃ仕方ないかしら、ね
			CASE "低い"
				PRINTFORM 「今の%CNAME(MASTER, TARGET)%には、何言っても無駄みたいね
		ENDSELECT
		;PSで閉じる
		CALL PRINT_SYMBOL_K14
	CASE "無理のない行動"
		PRINTFORMW 「あのねぇ…、言った事はちゃんとしてもらわないと困るんだけど」
		PRINTFORM 「ま、今集中するのが難しいってのは、判らないでもないけど、ね…
		;PSで閉じる
		CALL PRINT_SYMBOL_K14
	CASE "無理をした行動"
		PRINTFORMW 「\@ TENSION(TARGET, 8) == "高い" ? は？ ふざけてるの？　 # なにそれ…、 \@自分ばっかりよくなっちゃって、どういうつもり\@ GET_CONDITION(TARGET) == "怒り" ? よっ！ # なのかしら…… \@」
		PRINTFORMW 「ホント、\@ AFFECTION("溺愛") ? 呆れちゃうわね # 使えない犬ね \@…」
ENDSELECT
