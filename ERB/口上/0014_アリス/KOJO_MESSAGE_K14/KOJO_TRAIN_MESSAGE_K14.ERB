ELSEIF IS_COMGRONAME("積極的従う/平然と従う")
	;REACT分類が積極的従うの場合
	PRINTFORM %CALLNAME:MASTER%は
	;膝立ち
	SIF TCVAR:MASTER:姿勢 == 10
