;========================================================================
;NO9_EV21, 大満足口上
;========================================================================
;・調教者の大満足ボーナスを取得した場合に表示
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_21(ARG)
;-------------------------------------------------
;今の仕様だとREACT負荷で別けないと不味い気がする
;取りあえず、場合によってCOM口上との整合性がとれなくなりそうな箇所はコメントアウトする
;負荷2の場合、アリスさまがご機嫌斜めな場合があるのでｗ
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;アリスさま大満足
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	;=================================================
	;愛想よく応答する
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("愛想よく応答する")
		PRINTFORM 「ふふ、こういう咲夜も悪くないわね
		CALL PRINT_SYMBOL_K14
		SIF GET_COM_RANK() == "無理をした行動"
			PRINTFORMW 「ちょっとワザとらしい気もするけど、ね」
		PRINTFORMDW アリスは\@ TFLAG:REACT負荷 > 2 ? 満面の # \@笑みを湛え、%CALLNAME:MASTER%を見つめている
		RETURN 1
	ENDIF
	
	;=================================================
	;快感を受け入れる
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("快感を受け入れる/気持ち良い！")
		PRINTFORM 「ふふ、素直で可愛いわね
		CALL PRINT_SYMBOL_K14
		PRINTFORMDW アリスは満面の笑みを湛え、%CALLNAME:MASTER%を見つめている
		RETURN 1
	ENDIF
	
	;=================================================
	;中に出して！
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("中に出して！")
		PRINTFORMW 「ふふっ、中出しをおねだりだなんて……」
		PRINTFORM 「なんて可愛いのかしら…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;悲鳴を上げる
	;=================================================
	IF 口上記入チェック(0) && IS_NOWCOMNAME("悲鳴を上げる")
		IF AFFECTION("相思相愛")
			PRINTFORMW 「なんて甘い声で、鳴くのかしら……」
			PRINTFORMW 「ねぇ、もっと……、もっと聞かせて」
		ELSE
			PRINTFORMW 「いい声で鳴くわね……」
			PRINTFORMW 「…たまらないわ。その情けない顔も素敵よ？」
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「フフ…」")
			CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;愛情を込めてやる
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("愛情を込めてやる")
		IF AFFECTION("相思相愛")
			PRINTFORMW 「%CNAME(MASTER, TARGET)%の心が伝わってくるわ…」
			PRINTFORMDL アリスは蕩けきった表情を浮かべ、体を摺り寄せてきた…
			PRINTL
			PRINTFORM 「好きよ…、大好き…\@ CFLAG:TARGET:依存度 > CFLAG:MASTER:依存度 ? っ！ # \@
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			PRINTFORMDW アリスは潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		ELSE
			PRINTFORMW 「こんなになって…、私で、わたしなんかに…」
			PRINTFORMDW アリスは蕩けきった表情を浮かべ、体を摺り寄せてきた…
			PRINTFORM 「ふふ、 悪くないわ。こういうの…
			CALL PRINT_SYMBOL_K14
		ENDIF
		RETURN 1
	ENDIF
	
	;SELECTCOM == 15は上層で処理
	;=================================================
	;REACT分類が積極的従う(積極的やる、なすがまま等)
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("積極的従う/受け入れる")
		PRINTFORMW 「いいわよ、とってもいい感じ……」
		PRINTFORMDW アリスは潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;その他を処理
	;---------------------------------------------
	PRINTFORMW 「ふふ、いい感じね」
	PRINTFORM 「悪くないわよ、咲夜…
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;========================================================================
;NO9_EV22, 状態変化口上
;========================================================================
;・状態異常が付いたときに表示
;・全て派生で処理される
;・TCVAR:状態変化が調教者、TCVAR:MASTER:状態変化が調教対象
;・状態変化が起こると3ターン持続する
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_22(ARG)
;-------------------------------------------------
;────────────────────────────────────
;NO9_EV22_0, 調教者状態変化
;────────────────────────────────────
;T_COND TARGETが指定した状態なら真を返す
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;アリスさま[疲弊]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && T_COND("疲弊")
	TCVAR:状態変化持続 = 2
	IF RAND:3
		RETURN 0
	ELSE
		;アリスさま満足度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「疲れたけど…心地よい疲れって感じね…♪」
			CASE "一般"
				PRINTFORMW 「ちょっと疲れたわね…」
			CASE "低い"
				PRINTFORMW 「ふぅ……疲れたわね……」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;アリスさま[衰弱]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && T_COND("衰弱") && !TFLAG:一回休み
	TCVAR:状態変化持続 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORML 「はぁ……はぁ……」
		PRINTFORMDL アリスは肩で息をしながら、疲れきった表情を見せた
		PRINTFORMW 「ちょっと……ヤりすぎちゃったわね…」
		RETURN 1
	ENDIF
ENDIF

