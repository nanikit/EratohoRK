		TCVAR:状態変化持続 = 1
	ELSEIF AFFECTION("溺愛")
		IF RAND:3
			TCVAR:状態変化持続 = 2
		ELSE
			TCVAR:状態変化持続 = 1
		ENDIF
	ELSEIF AFFECTION("眷愛")
		IF RAND:3
			TCVAR:状態変化持続 = 3
		ELSE
			TCVAR:状態変化持続 = 2
		ENDIF
	ELSE
		TCVAR:状態変化持続 = 3
	ENDIF
	IF RAND:3
		RETURN 0
	ELSE
		;アリスさま満足度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORML 「悪くない気分だったけど…すっかり台無しにしてくれたわね？」
				PRINTFORMW 「その代償は高くつくから、覚悟して…」
			CASE "一般"
				PRINTFORML 「お願いだから、あまり怒らせないで頂戴」
				PRINTFORMW 「どこまで自分を押さえられるか、あまり自信が無いわ…」
			CASE "低い"
				PRINTFORMW 「私を怒らせても良い事は無いわよ…」
		ENDSELECT
		RETURN 1
	ENDIF
