				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「읏후앗! 아앗")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  간닷! 간다앗!!」")
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;기교 6이상
		;-------------------------------------------------
		IF 구상기입체크(100) && ABL:MASTER:기교 >= 6
			;2/3의 확률로 범용 처리
			IF RAND:3
				CALL TB_EX_K14
			;1/3의 확률로 표시
			ELSE
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "「아앗! 그, 그렇게 되면! 하우, 응읏!」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋은 기분이 들어 왔군요♪이봐요, 유두도 만지작거려 주어요」")
				PRINTFORMDW 사쿠야는 앨리스의 딱딱하게 응어리진 유두를 집으면 이끄는, 되돌리는 것 움직임을 반복했다…
				PRINTL
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「히웃, 응읏! 핫, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아프게 여겨지는 것, 기분 좋닷!」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후, 아직 가서는 안 돼요? 좀 더 즐겁게 해」")
				PRINTFORMDL 사쿠야는 유두를 집은 손가락끝을 교대로 움직여, 동글동글 비틀기 시작했다
				PRINTFORMDW 처음은 소프트에, 그리고 서서히 힘을 집중하고 앨리스의 유두를 계속 비틀었다…
				PRINTL
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "「아아웃, 아앙, 하앗! 안됏, 이런 것 견딜 수 없닷!!」")
				CALL QUAKE(4)
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「후앗")
				CALL AUTOHEARTCB_K14
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "  이, 쿠웃! 아아앗!!」")
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;기교 3이상
		;-------------------------------------------------
		IF 구상기입체크(100) && ABL:MASTER:기교 >= 3
			;2/3의 확률로 범용 처리
			IF RAND:3
				CALL TB_EX_K14
			;1/3의 확률로 표시
			ELSE
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "「응읏, 쿠웃! 핫, 앗! 응응읏!」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후, 상당히 흥분해 온 것 같구나? 슬슬 여기에서도 갈 수 있는 것이 아닐까」")
				PRINTFORMDL 사쿠야는 양손으로 앨리스의 유방을 감싸면서, 집게 손가락으로 유륜을 애무하기 시작했다
				PRINTFORMDW 절묘한 압력으로 피부 감각이 높아진 유륜을 몇번이나, 몇번이나, 열심히 훑었다…
				PRINTL
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "「이, 이나다 이것! 안타까워서… 이상해질 것 같다!」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지만 서서히 올까요? 이봐요, 슬슬 좋아져 왔지 않을까」")
				PRINTFORMDW 사쿠야는 튈듯이 부풀어 오른 첨단의 꽃봉오리를 피하도록(듯이), 한층 더 유륜의 애무를 계속했다…
				PRINTL
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「아아웃, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  유두, 손대어지지 않았는데… 지릿지릿 해 버렷!」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후, 이제(벌써) 준비 좋은 것 같구나. 자… 가세요, 앨리스」")
				PRINTFORMDW 사쿠야는 앨리스의 유두를 집으면, 손가락끝을 교대로 움직여 동글동글 비틀었다…
				PRINTL
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("앨리스"), "「응앗! 간다앗! 가 버린다앗!」")
				CALL QUAKE(4)
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「읏후앗! 아″아″")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  간다앗! 간다앗!!」")
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;ABL 조건 없음
		;---------------------
		;2/3의 확률로 범용 처리
		IF RAND:3
			CALL TB_EX_K14
		;1/3의 확률로 표시
		ELSE
			CALL PRINTCOLOR(COLOR_K14("앨리스"), "「아웃, 앗")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  하, 읏! 쿠웃, 응응읏!」")
			CALL PRINTCOLORW(COLOR_K14("앨리스"), "  하, 읏! 쿠웃, 응응읏!」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「고동이 전해져 와요… 굉장히 두근두근 해 버리고 있는 거네, 앨리스」")
			PRINTL
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 알고 있어? 심박이라고 동조시키고 있으면 매우 좋은 것 같아요」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이봐요, 이런 느낌으로… 맥일, 2회로 한번 주무름이 요령이야」")
			PRINTFORMDW 사쿠야는 앨리스의 유방을 그 고동에 맞추어, 크고 원을 그리듯이 주물렀다…
			PRINTL
			CALL PRINTCOLOR(COLOR_K14("앨리스"), "「아읏, 앗")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  나닷, 이것 기분 좋다…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇겠지요? 이봐요, 신경을 집중시켜. 좀 더 자주(잘) 해 주어요…♪」")
			PRINTFORMDW 사쿠야는 손바닥으로 앨리스의 유두를 굴리면서, 유방을 들어 올리도록(듯이), 아래에서 위에의 큰 움직임으로 주물렀다…
			PRINTL
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("앨리스"), "「앗")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  응응! 젖가슴 기분 좋아서…」")
			CALL QUAKE(4)
			CALL PRINTCOLORW(COLOR_K14("앨리스"), "「읏, 쿠웃! 간닷, 간다앗!!」")
		ENDIF
		RETURN 1
	ENDIF
ENDIF
