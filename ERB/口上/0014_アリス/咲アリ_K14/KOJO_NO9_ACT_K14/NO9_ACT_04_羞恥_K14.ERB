		PRINTL
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「시, 싫닷! 앨리스, 나, 아직…」")
		
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「사양이 없는 사람이네…")
