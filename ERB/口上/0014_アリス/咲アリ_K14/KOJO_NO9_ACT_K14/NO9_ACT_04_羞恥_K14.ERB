;
;	앨리스 구상(대상 한정) 수치계 액션 구상 파일
;
;		수치계 액션 구상을 정리한 파일
;
;	EXTRA 모드로 NO:9 사쿠야를 MASTER에 선택했을 경우만 표시
;
;Version		Reason
;1. 70			@구상 기입 체크 사양에. 그 외 섬세한 수정
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;수치계 ACT40～45
;========================================================================
;·TFLAG:ACT(40=자위/41=비패개장/42=자위과시하고/43=수치플레이/44=야외플레이/45=방뇨/46=코스프레(미실장))
;·TFLAG:ACT분류 == 4에 해당
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "수치")로 줍는 것이 가능
;·ACT에 따라서는 거부가 가능하고 있거나 않기도 하고
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는 구상측의 RESULT(종단의 RETURN 1 등)에 의해 구상이 표시되었는지 아닌지,
;·또 개소 마다의 특유의 거동(미약등 구상을 스킵 따위)을 제어하고 있습니다
;·@구상 기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는 구상측에서 결정하고 쳐 할 필요가 있습니다
;-------------------------------------------------
;관련 변수…
;TFLAG:벌플래그
;TCVAR:MASTER:상태변화(조교 대상 상태)
;TCVAR:상태변화(조교자 상태)
;TFLAG:ACT
;TFLAG:전ACT
;PREVCOM 전회 COM
;-------------------------------------------------
;관련 함수…
;FIRSTACT
;첫회 ACT 판정 함수
;TFLAG:ACT(TFLAG:90)를 자동으로 참조, 현재 실행되고 있는 ACT 마다 번호가 독립해 보존된다
;
;ONCEACT
;조교내미독판정 함수  조교전에 초기화된다
;그 외의 사양은 거의 FIRSTACT와 같이
;-------------------------------------------------

;─
;NO9_ACT40, 자위
;─
;·자위 해 보여라라고 명령한다
;·이 단계에서 명령에 따르는지 아닌지는 미정
;·REACT분류가 소극적 따르는/태연하게 따른다/적극적 따른다/받아들이는 이외의 경우는, 모두 REACT분류 "거부"취급
;·턴방침이 "이상"이라면 발생하지 않는다
;·밀어넘어뜨리기중인 경우는 발생하지 않는다
;·벌써 자위중인 경우는 발생하지 않는다
;·TEQUIP(줄/삼각목마) 장착중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·이 ACT에 파생은 없다
;-------------------------------------------------
;옷을 모두 입은 상태에서도 일어날 수 있다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_40
;-------------------------------------------------
;·조교 대상으로 페니스가 없는 경우
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT40, 자위
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !PENIS(MASTER)
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORMW 「그런데, 사쿠야에게는 여기서 자위를 해 받아요」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫!? 여기서 해라고 말하는 거야? 당신의 눈앞에서?」")
		PRINTFORMW 「그래요? 그 이외의 무언가에 들린 것이라면, 나의 말투가 나빴다라는 일이군요」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런, 남의 앞에서는…」")
		PRINTL
		PRINTFORMW 「후후, 과연 사쿠야도 처음의 일같구나?」
		PRINTFORMW 「그렇지만 신경쓰는 일 없어요. 언제나 하고 있는 것 같게, 평상시 대로시라고 주면 그것으로 좋은거야」
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「간단하게 말해 주어요…」")
		PRINTFORMW 「왜냐하면 간단한 일이겠지? 아무것도 어려운 일 해라고 하고 있을 생각은 없는 것」
		PRINTFORMW 「자, 수다는 여기까지. 사쿠야가 어떤 식으로 하는지, 나에게 보이게 해 줘…」
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가가게해！
	;=================================================
	IF 구상기입체크(100) && IS_PREVCOMNAME("가게해！")
		[SKIPSTART]
		;-------------------------------------------------
		;전ACT가 자위(현재의 사양이라고 죽어 분기)
		;-------------------------------------------------
		IF 구상 기입 체크(0) && IS_PREVACTNAME("자위")
			PRINTFORMDW 앨리스는가게해！와 조르는 사쿠야에, 한층 더 자위를 계속하도록(듯이) 명했다…
			PRINTL
			PRINTFORMW 「이봐요, 스스로 시 비치기 때문에, 사람을 믿지 않고 스스로 가면 어때?」
			PRINTFORMDW 앨리스의 말은 닿지 않고, 사쿠야는 열중해 자신을 위로하고 있다…
			PRINTL
			;페이드 아웃
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
			CLEARLINE 1
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("사쿠야"), " 「응읏, 앗")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  나닷, 멈추지 않는다! 멈추지 않는거야!」")
			PRINTFORMW 「지금은 무슨 말을 해도 헛됨같구나…」
			RETURN 1
		ENDIF
		[SKIPEND]
		
		;---------------------
		;그 외
		;---------------------
		PRINTFORMDW 앨리스는가게해！와 조른다%CALLNAME:MASTER%에, 자위를 하도록(듯이) 명했다…
		PRINTL
		;2/3의 확률로 표시
		IF RAND:3
			;조교자 BASE 만족 파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「좋아요? 마음대로 가세요」
					PRINTFORMW 「우선, 나는 아무것도 해 줄 생각은 없으니까…」
					CALL FADEIN(13, "「스스로 어떻게든 하는 일이군요?」")
				CASE "일반"
					PRINTFORMW 「그렇게 가고 싶으면, 마음대로 하면?」
					PRINTFORMW 「나는 여기서 감상시켜 받기 때문에」
				CASE "낮다"
					PRINTFORMW 「좋아요. 좋을대로 가세요. 다만… 스스로, 말야?」
			ENDSELECT
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			CLEARLINE 1
		;1/3의 확률로 표시
		ELSE
			PRINTFORMW 「이봐요, 언제라고도 가도 좋은거야?」
			PRINTFORMW 「사쿠야가 스스로 위로해 가 버리는 곳, 나에게 보이게 해 줘」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「싫닷! 말하지 말아요, 그런 일…」")
			PRINTFORMW 「후후, 사랑스러운 일 말하는 거네」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가기분좋게해줘
	;=================================================
	IF 구상기입체크(100) && IS_PREVCOMNAME("기분좋게해줘")
		PRINTFORMDW 앨리스는기분좋게해줘와 조르는 사쿠야에, 자위를 하도록(듯이) 명했다…
		PRINTL
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「기분 좋아지고 싶으면, 스스로 어떻게든 해」
				PRINTFORMW 「지금은 아무것도 할 생각 없네요. 나쁘지만…」
			CASE 1
				PRINTFORMW 「… 기분좋게해줘, 저기. 뭐 좋지만」
				PRINTFORMW 「우선, 스스로 어떻게든 해 줘. 후후」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가자위하기시작한다
	;=================================================
	IF 구상기입체크(100) && IS_PREVCOMNAME("자위하기시작한다")
		PRINTFORMW 「당신이 이렇게 자위 정말 좋아했던이라니? 사쿠야」
		;페이드 아웃
		CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
		CLEARLINE 1
		
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응읏, 앗")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  나, 멈추지 않게 되어 버린다앗!」")
		
		PRINTFORMW 「사양이 없는 사람… 후훗. 좋아요, 기분이 풀릴 때까지 계속해 줘」
		
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「나는 차분히 감상시켜 받는 일로 해요…")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		RETURN 1
	ENDIF
	
	;=================================================
	;연속 실행
	;기입 체크
	LOCAL:1 = 1
	;=================================================
	IF 구상기입체크() && TFLAG:ACT == TFLAG:전ACT
		PRINTFORMDW 앨리스는 자위에 빠지는 사쿠야에 한층 더 계속하도록(듯이) 명했다…
		;전COM 파생 처리
		SELECTCASE TRAINNAME:PREVCOM
			CASE "부끄러워한다"
				PRINTFORMW 「부끄러워도 결국은 그만둘 수 없는거네?」
				PRINTFORMW 「뭐, 당신이부끄러워한다일자체, 조금 깜짝이지만」
				PRINTFORMW 「… 읏, 조금 과언일까. 저기? 후후」
			CASE "소극적한다"
				PRINTFORMW 「뭐야, 그다지 시 싶지 않은거야?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「시, 시끄럽네요…」")
				PRINTFORMW 「안 돼요, 사쿠야… 분명하게 평상시 대로에 해 줘」
			CASE "적극적한다", "태연하게한다"
				;페이드 아웃
				CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
				CLEARLINE 1
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏! (이)나다 이것, 기분 좋닷! 응읏!」")
				PRINTFORMW 「좋은 느낌이야, 사쿠야. 그 상태로 계속하며♪」
			CASEELSE ;그 이외
				PRINTFORMW 「그대로 계속해도 좋아요, 사쿠야」
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「이제 와서 그만두어라고 해도, 제지당할 것 같지 않은 것 같고 죽어라,")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자도 자위중
	;=================================================
	IF 구상기입체크(100) && GETBIT(TEQUIP:자위중, 1)
		;원래는 SELECTCASE로 PALAM:쾌C파생을 보고 있었던 개소. TARGET에도갈것같다가 실장되었으므로 완화함
		IF TCVAR:TARGET:갈것같다 && !TEQUIP:아이마스크
			;페이드 아웃
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 1))
			CLEARLINE 1
			PRINTFORMDW 앨리스는%CLITORIS_DESCRIPTION(TARGET)%음핵에 스스로의 애액을 발라, 손가락끝으로 격렬하게 훑고 있다…
			CALL QUAKE(2)
			PRINTFORMW 「아″아″! 이, 이것! 굉장하닷! 굉장한거야!」
			PRINTFORMW 「자, 사쿠야… 나, 이, 이제(벌써)! 아개, 아, 아아읏!」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「쿳… 그런 것 보여지면, 나…」")
		ELSEIF PALAM:TARGET:쾌C >= 5000
			;페이드 아웃
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 1))
			CLEARLINE 1
			PRINTFORMW 「앗, 싫다… 여기 만지작거리면 조크는 해 버린다! 손가락, 멈추지 않아서… 응″응!」
			PRINTFORMW 「이봐요, 사쿠야도 하는거야… 어차피, 슬슬 인내 할 수 없게 되어 와 있겠지만♪」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 일!」")
			PRINTFORMW 「솔직하지 않은거네? 그러면, 무엇으로 조금 전부터 머뭇머뭇 하고 있을까, 저기 사쿠야? 후후후」
		ELSE
			PRINTFORML 「이봐요, 사쿠야도 함께 하는거야…」
			PRINTFORMW 「혼자서 소곤소곤 하는 것보다, 반드시 즐길 수 있다고 생각해요♪」
			PRINTL
			CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「낫!?」")
			PRINTFORMW 「후후, 내가 깨닫지 않다고 생각해?」
			PRINTL
			PRINTFORMW 「뭐,1개 가르쳐 두어 주지만… 그렇게 소리 내고 있으면, 싫어도 들려 버려요?」
			PRINTFORMW 「그렇게 말하면, 누군가의 이름을 외치고 있었군요? 엣또…」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아벌써, 알았다! 알았어요! 하면 되겠지요!」")
			PRINTFORMW 「후후, 최초부터 솔직하게 하면 좋았을텐데, 저기 사쿠야? 킥킥」
		ENDIF
		RETURN 1
	ENDIF
	
	;벌모드 분기는 "벌 해"를 선택했을 경우 끝없이 불리므로 확률을 짠다
	;이전에는 상층에 두었지만, 그것은 잘못인 감이 있으므로 하층에 가져왔다
	;내용적으로는 단순한 개시의 분기이고
	;=================================================
	;벌모드
	;=================================================
	IF 구상기입체크(45) && TFLAG:벌플래그
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「자, 자위를 하세요」
				PRINTFORMW 「사쿠야가 보기 흉하게 자신을 위로하는 곳, 차분히 봐 주어요♪」
			CASE 1
				PRINTFORMW 「벌로서 당신에게는 자위를 해 받아요, 사쿠야…」
				PRINTFORMW 「이렇게 말해도… 벌이라고 말하는 것보다, 포상과 같은 생각도 들지만♪」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;정욕/울적/몽롱
	;=================================================
	IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(MASTER), "정욕", "울적", "몽롱")
		PRINTFORMW 「이봐요, 사양하지 말아줘. 스스로 시라고 좋아요?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무, 무슨 이야기, …」")
		PRINTFORM 「후후, 시치미를 떼어도 안돼. 스스로 만지작거리고 싶어서 견딜 수 없는 주제에
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;분노
	;=================================================
	IF 구상기입체크(100) && M_COND("분노")
		PRINTFORMW 「기분 나쁜 것 같구나? 사쿠야. 그러면, 여기서 자위 해 보여」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 앨리스, 1개 (들)물어도 좋을까…」")
		PRINTFORMW 「뭐야?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「『기분이 나쁘다』와 『자위』에 도대체 무슨 연결이 있는거야!」")
	
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「별로 없지만? 다만 재미있을 것 같다고 생각해,")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
		
		;지금이라면 다른 기술로 갈 수 있는 감
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「무엇이 『")
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "군요")
		CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "』. 완전히…」")
		
		PRINTFORMW 「화내거나 기가 막히거나 사쿠야는 바쁘네요?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「누구의 탓이야, 누구의!」")
		PRINTFORMW 「후후… 그것에 대해서는 나의 잘못을 인정해도 괜찮지만. 뭐, 그건 그걸로하고…」
