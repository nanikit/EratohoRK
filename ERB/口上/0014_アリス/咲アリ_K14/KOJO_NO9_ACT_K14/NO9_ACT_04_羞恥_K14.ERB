	;=================================================
	;怒り
	;=================================================
	IF 口上記入チェック(100) && M_COND("怒り")
		PRINTFORMW 「機嫌悪そうね？ 咲夜。じゃ、ここでオナニーしてみせて」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ねぇアリス、一つ聞いていいかしら…」")
		PRINTFORMW 「なによ？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「『機嫌が悪い』と『自慰』に一体何の繋がりがあるのよ！」")
	
		CALL PRINTCOLOR(COLOR_K14("アリス"), "「別に無いけど？ ただ面白そうだと思って、ね")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
		
		;今なら違う記述でいける感
		CALL PRINTCOLOR(COLOR_K14("咲夜"), "「何が『")
		CALL PRINTCOLOR(COLOR_K14("アリス"), "ね")
		CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "』よ。まったく…」")
		
		PRINTFORMW 「怒ったり呆れたり、咲夜は忙しいわね？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「誰のせいよ、誰の！」")
		PRINTFORMW 「ふふ…それについては私の非を認めてもいいけど。ま、それはそれとして…」
		PRINTFORMW 「そろそろ始めて貰おうかしら、ねぇ咲夜？」
		RETURN 1
	ENDIF
	
	;=================================================
	;疲弊/衰弱/無気力
	;=================================================
	IF 口上記入チェック(100) && GROUPMATCH(GET_CONDITION(MASTER), "疲弊", "衰弱", "無気力")
		PRINTFORMW 「ちょっとお疲れのようだけど、自慰をして貰うわよ」
		PRINTFORM 「文句があるなら後で聞いてあげるから、さっさと初めて頂戴
		CALL PRINT_SYMBOL_K14
		RETURN 1
