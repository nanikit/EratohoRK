;NO9_ACT40, 自慰
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !PENIS(MASTER)
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		PRINTFORMW 「さて、咲夜にはここで自慰をして貰うわ」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「なっ！？ ここでしろって言うの？ 貴女の目の前で？」")
		PRINTFORMW 「そうよ？ それ以外の何かに聞こえたのなら、私の言い方が悪かったって事ね」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そんな、人前でなんて…」")
		PRINTL
		PRINTFORMW 「ふふ、流石の咲夜も初めての事みたいね？」
		PRINTFORMW 「でも気にする事ないわ。いつもやってるように、普段通りシてくれればそれでいいの」
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「簡単に言ってくれるわね…」")
		PRINTFORMW 「だって簡単な事でしょ？ 何も難しい事しろって言ってるつもりはないもの」
		PRINTFORMW 「さ、お喋りはここまでよ。咲夜がどんな風にするのか、私に見せて頂戴…」
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMがイかせて！
	;=================================================
	IF 口上記入チェック(100) && IS_PREVCOMNAME("イかせて！")
		[SKIPSTART]
		;-------------------------------------------------
		;前ACTが自慰（現在の仕様だと死に分岐）
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_PREVACTNAME("自慰")
			PRINTFORMDW アリスはイかせて！とせがむ咲夜に、さらに自慰を続けるよう命じた…
			PRINTL
			PRINTFORMW 「ほら、自分でシてるんだから、人を当てにしないで自分でイったらどうなの？」
			PRINTFORMDW アリスの言葉は届かず、咲夜は夢中で自分を慰めている…
			PRINTL
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("自慰"), 2))
			CLEARLINE 1
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("咲夜"), "「んんっ、あっ")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　やだっ、とまらない！ とまらないのっ！」")
			PRINTFORMW 「今は何を言っても無駄みたいね…」
			RETURN 1
		ENDIF
		[SKIPEND]
		
		;---------------------
		;その他
		;---------------------
		PRINTFORMDW アリスはイかせて！とせがむ%CALLNAME:MASTER%に、自慰をするよう命じた…
		PRINTL
		;2/3の確率で表示
		IF RAND:3
			;調教者BASE満足派生
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORMW 「いいわよ？ 勝手にイきなさいよ」
					PRINTFORMW 「とりあえず、私は何もしてあげるつもりはないから…」
					CALL FADEIN(13, "「自分でなんとかする事ね？」")
				CASE "一般"
					PRINTFORMW 「そんなにイきたいなら、好きにすれば？」
					PRINTFORMW 「私はここで鑑賞させてもらうから」
				CASE "低い"
					PRINTFORMW 「いいわ。好きなようにイきなさいよ。ただし…自分で、ね？」
			ENDSELECT
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			CLEARLINE 1
		;1/3の確率で表示
		ELSE
			PRINTFORMW 「ほら、いつてもイっていいのよ？」
			PRINTFORMW 「咲夜が自分で慰めてイっちゃうところ、私に見せて頂戴」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「やだっ！ 言わないでよ、そんな事…」")
			PRINTFORMW 「ふふ、可愛い事言うのね」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが気持ち良くして
	;=================================================
	IF 口上記入チェック(100) && IS_PREVCOMNAME("気持ち良くして")
		PRINTFORMDW アリスは気持ち良くしてとせがむ咲夜に、自慰をするよう命じた…
		PRINTL
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「気持ちよくなりたいなら、自分でなんとかして」
				PRINTFORMW 「今は何もする気ないのよねぇ。悪いけど…」
			CASE 1
				PRINTFORMW 「…気持ち良くして、ね。まぁいいけど」
				PRINTFORMW 「とりあえず、自分でなんとかして頂戴。ふふ」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが自慰し始める
	;=================================================
	IF 口上記入チェック(100) && IS_PREVCOMNAME("自慰し始める")
		PRINTFORMW 「貴女がこんなにオナニー大好きだったなんてね？ 咲夜」
		;フェードアウト
		CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("自慰"), 2))
		CLEARLINE 1
		
		CALL PRINTCOLOR(COLOR_K14("咲夜"), "「んんっ、あっ")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　や、とまらなくなっちゃうぅっ！」")
		
		PRINTFORMW 「仕様の無い人ね…ふふっ。いいわ、気の済むまで続けて頂戴」
		
		CALL PRINTCOLOR(COLOR_K14("アリス"), "「私はじっくり鑑賞させて貰う事にするわ…")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		RETURN 1
	ENDIF
	
	;=================================================
	;連続実行
	;記入チェック
	LOCAL:1 = 1
	;=================================================
	IF 口上記入チェック() && TFLAG:ACT == TFLAG:前ACT
		PRINTFORMDW アリスは自慰に耽る咲夜にさらに続けるよう命じた…
		;前COM派生処理
		SELECTCASE TRAINNAME:PREVCOM
			CASE "恥ずかしがる"
				PRINTFORMW 「恥ずかしくても結局はやめられないのね？」
				PRINTFORMW 「ま、貴女が恥ずかしがる事自体、ちょっとびっくりなんだけど」
				PRINTFORMW 「…って、ちょっと言いすぎかしら。ねぇ？ ふふ」
			CASE "消極的やる"
				PRINTFORMW 「なによ、あまりシたくないの？」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「う、五月蝿いわね…」")
				PRINTFORMW 「駄目よ、咲夜…ちゃんと普段通りにやって頂戴」
			CASE "積極的やる", "平然とやる"
				;フェードアウト
				CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("自慰"), 2))
				CLEARLINE 1
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んっ！ やだこれ、気持ち良いっ！ んんっ！」")
				PRINTFORMW 「良い感じよ、咲夜。その調子で続けて♪」
			CASEELSE ;それ以外
				PRINTFORMW 「そのまま続けていいわよ、咲夜」
				CALL PRINTCOLOR(COLOR_K14("アリス"), "「今更やめろって言っても、止められそうに無いみたいだしし、ね")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者も自慰中
	;=================================================
	IF 口上記入チェック(100) && GETBIT(TEQUIP:自慰中, 1)
		;元々はSELECTCASEでPALAM:快Ｃ派生を見てた箇所。TARGETにもイきそうが実装されたのでモディファイ
		IF TCVAR:TARGET:イきそう && !TEQUIP:アイマスク
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("自慰"), 1))
			CLEARLINE 1
			PRINTFORMDW アリスは%CLITORIS_DESCRIPTION(TARGET)%陰核に自らの愛液を塗りつけ、指先で激しくしごいている…
			CALL QUAKE(2)
			PRINTFORMW 「あ゛あ゛ぁっ！ こ、これっ！ 凄いっ！ 凄いのっ！」
			PRINTFORMW 「さ、咲夜ぁ…私ッ、も、もうっ！ ふあぁつ、あっ、あぁんっ！」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「くっ…そんなの見せられたら、私…」")
		ELSEIF PALAM:TARGET:快Ｃ >= 5000
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("自慰"), 1))
			CLEARLINE 1
			PRINTFORMW 「あっ、やだ…ここ弄るとゾクってしちゃう！ 指、とまらなくって…ん゛んっ！」
			PRINTFORMW 「ほら、咲夜もやるのよ…どうせ、そろそろ我慢出来なくなってきてるでしょうけど♪」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そ、そんな事っ！」")
			PRINTFORMW 「素直じゃないのね？ なら、なんでさっきからモジモジしてるのかしら、ねぇ咲夜？ ふふふ」
		ELSE
			PRINTFORML 「ほら、咲夜も一緒にやるのよ…」
			PRINTFORMW 「一人でこそこそやるより、きっと愉しめると思うわ♪」
			PRINTL
			CALL PRINTCOLORL(COLOR_K14("咲夜"), "「なっ！？」")
			PRINTFORMW 「ふふ、私が気がついてないと思って？」
			PRINTL
			PRINTFORMW 「ま、一つ教えておいてあげるけど…あんなに声出してたら、嫌でも聞こえちゃうわよ？」
			PRINTFORMW 「そういえば、誰かの名前を叫んでたわね？ えっと…」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あぁもう、わかった！ わかったわよ！ やればいいんでしょう！」")
			PRINTFORMW 「ふふ、最初から素直にやればよかったのに、ねぇ咲夜？ クスクス」
		ENDIF
		RETURN 1
	ENDIF
	
	;お仕置きモード分岐は"お仕置きして"を選択した場合延々と呼ばれるので確率を絞る
	;以前は上層に置いてたけど、それは誤りな感があるので下層に持ってきた
	;内容的には只の開始の分岐だし
	;=================================================
	;お仕置きモード
	;=================================================
	IF 口上記入チェック(45) && TFLAG:お仕置きフラグ
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「さぁ、自慰をしなさい」
				PRINTFORMW 「咲夜が無様に自分を慰めるところ、じっくり見てあげるわ♪」
			CASE 1
				PRINTFORMW 「お仕置きとして貴女には自慰をして貰うわ、咲夜…」
				PRINTFORMW 「と言っても…お仕置きと言うより、ご褒美のような気もするけど♪」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;情欲/鬱屈/朦朧
	;=================================================
	IF 口上記入チェック(100) && GROUPMATCH(GET_CONDITION(MASTER), "情欲", "鬱屈", "朦朧")
		PRINTFORMW 「ほら、遠慮しないで。自分でシていいわよ？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「な、なんの話、よ…」")
		PRINTFORM 「ふふ、とぼけたって駄目。自分で弄りたくて堪らない癖に
		CALL PRINT_SYMBOL_K14
