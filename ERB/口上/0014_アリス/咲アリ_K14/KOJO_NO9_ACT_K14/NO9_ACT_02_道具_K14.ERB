;
;	앨리스 구상(대상 한정) 도구계 액션 구상 파일
;
;		도구계 액션 구상을 정리한 파일
;
;	EXTRA 모드로 NO:9 사쿠야를 MASTER에 선택했을 경우만 표시
;
;
;Version		Reason
;1. 70			@구상 기입 체크 사양에. 그 외 섬세한 수정
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;도구계 ACT20~26
;========================================================================
;·TFLAG:ACT(20=로터/21=바이브/22=애널 바이브/23=애널 비즈/24=C애무 도구/25=니플캡/26=페니스 밴드)
;·TFLAG:ACT 분류 == 2에 해당
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "도구")로 줍는 것이 가능
;·조교자가 조교 대상으로 도구를 사용한다. 이 구상의 시점에서 조교자는 벌써 손을 대고 있다
;·미약·로션을 사용하는 케이스가 있다
;·도구류의 장착, 해제등은 TFLAG:ACT 파생으로 정의된다
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는 구상측의 RESULT(종단의 RETURN 1 등)에 의해 구상이 표시되었는지 아닌지,
;·또 개소 마다의 특유의 거동(미약등 구상을 스킵 따위)을 제어하고 있습니다
;·@구상 기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는 구상측에서 결정하고 쳐 할 필요가 있습니다
;-------------------------------------------------
;도구계 실행 시에 강제 탈의가 들어가는 사양이 불만
;매우 재미있지 않다. 이것은 개선되어야 함
;-------------------------------------------------
;관련 함수…
;FIRSTACT
;첫회 ACT 판정 함수
;TFLAG:ACT(TFLAG:90)를 자동으로 참조, 현재 실행되고 있는 ACT 마다 번호가 독립해 보존된다
;
;ONCEACT
;조교내미독판정 함수  조교전에 초기화된다
;그 외의 사양은 거의 FIRSTACT와 같이
;-------------------------------------------------

;─
;ACT20, 로터
;─
;·C 중심으로 애무한다
;·애무계로부터 발생하는 경우도 있다
;·TFLAG:벌플래그가 서 있으면 발생하지 않는다
;·턴 방침이 "하드", "이상"이라면 발생하지 않는다
;·TEQUIP(클리캡/오나홀/페니스 밴드/삼각 목마) 장착중에는 발생하지 않는다
;·성교중/성교 봉사중에는 발생하지 않는다
;·조교 대상으로 페니스가 있는 경우는 발생하지 않는다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_20
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT20, 로터
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크() && FIRSTACT(0)
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐, 뭐야! 그것!」")
		PRINTFORMW 「아라, 사쿠야도 모르는거네? 우선 연성은 해 보았지만, 나도 자주(잘)은 모르는거야…」
		PRINTFORMW 「(와)과 아무튼 그러한 (뜻)이유, 그리고… 사쿠야로 시험하게 해 받는 일로 해요♪」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;ONCEACT
	;=================================================
	IF 구상 기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("오늘") ;오늘의 조교 방침 파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "애널"
				PRINTFORMW
			CASE "벌"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "러브러브"
				PRINTFORMW
			CASEELSE ;그 외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;이것은 필요하지 않을까 우선 테두리만
	;=================================================
	;갈 것 같다
	;=================================================
	IF 구상 기입 체크(0) && TCVAR:MASTER:갈 것 같다
		SELECTCASE TCVAR:MASTER:초조하게 해 번
			CASE IS > 1 ;초조하게 해 안
				PRINTFORMW
			CASE 1 ;초조하게 해 종료
				PRINTFORMW
			CASEELSE ;초조하게 해 없음
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;연속 실행
	;=================================================
	;이것이 성립할 가능성은 한없고 낮다. 어디선가 뭔가 가르칠 수 밖에
	;TEQUIP로 관리되고 있는 것이 아니기도 하고
	;-------------------------------------------------
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;하반신 속옷이 요점 밀치고
			IF LOWERUNDER(1) == 1
				;속옷을 비켜 놓아
				IF GETBIT(TEQUIP:착의노출, 2)
					PRINTFORMDW 앨리스는 비켜 놓았다%INNER_B(MASTER)%로부터 들여다 본 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%음핵을 로터로 계속 꾸짖고 있다…
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏… 야, 안돼엣, 싫어엇!」")
					;개발이 끝난 상태
					IF ABL:MASTER:C감각 > 5
						PRINTFORMW 「그런 일 말해도, 소리에 달콤한 것이 섞이는 것은 숨길 수 있지 않아요? 후훗」
					;개발중
					ELSE
						PRINTFORMW 「직접은 자극이 너무 강한일까요? 사쿠야의 일이니까 곧바로 익숙해지겠지만. 후후」
					ENDIF
				;천 너머로
				ELSE
					PRINTFORMDW 앨리스는%INNER_B(MASTER)% 위로부터 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%음핵을 로터로 계속 애무했다…
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏… 응쿠… 앗!」")
					;개발이 끝난 상태
					IF ABL:MASTER:C감각 > 5
						PRINTFORMW 「이제(벌써) 이래서야 사쿠야에게는 어딘지 부족한 느낌일, 까요♪」
					;개발 도상
					ELSE
						PRINTFORMW 「아직 그다지 익숙해지지 않는 느낌일까… 에서도 속옷 위로부터라면 나쁘지 않을 것입니다?」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;입지 않았다
		;---------------------
		PRINTFORMDW 앨리스는 사쿠야의 노출이 되었다%VAGINA_DESCRIPTION(MASTER)%를 로터로 계속 애무했다…
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏… 야앗! 싫어엇!」")
		;개발이 끝난 상태
		IF ABL:MASTER:C감각 > 5
			PRINTFORMW 「그런 일 말해도, 소리에 달콤한 것이 섞이는 것은 숨길 수 있지 않아요? 후훗」
		;개발 도상
		ELSE
			PRINTFORMW 「직접은 자극이 너무 강한일까요? 사쿠야의 일이니까 곧바로 익숙해지겠지만. 후후」
		ENDIF
		RETURN 1
	ENDIF
	
	;미묘한 점 테두리만 놓아둔다
	;=================================================
	;니플캡/클리캡 장착중
	;=================================================
	;IF 구상 기입 체크(0) && TRUECHECK(TEQUIP:니플캡, TEQUIP:클리캡) > 0
	;ENDIF
	
	;=================================================
	;하반신 복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(100) && PANTSCHECK(1, 1)
		;하반신 속옷이 요점 밀치고
		IF LOWERUNDER(1) == 1
			;속옷을 비켜 놓아
			IF GETBIT(TEQUIP:착의노출, 2)
				PRINTFORMDL 앨리스는%INNER_B(MASTER)%를 비켜 놓아 사쿠야의%VAGINA_DESCRIPTION(MASTER)%를 노출시키면,
				PRINTFORMDW 로터를 꽉 눌러,%CLITORIS_DESCRIPTION(MASTER)%음핵을 희롱하기 시작했다…
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후앗! 아앗!」")
				PRINTFORMW 「후후, 굉장히 안 되는 것 하고 있는 기분이 되어요, 이것」
				PRINTFORMW 「어때? 역시 직접 자극되는 것은 기분 좋은 것일까?」
				;개발이 끝난 상태
				IF ABL:MASTER:C감각 > 5
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「아, 앗")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  응응, 핫, 아앗!」")
					PRINTFORMW 「후후, (들)물을 것도 없었던 것 같구나♪」
				;개발중
				ELSE
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이, 이나다 앨리스! 이런 건… 응, 응읏!」")
					PRINTFORMW 「아직 익숙해지지 않는 것 같구나? 그렇지만 괜찮아, 곧바로 좋아져요♪」
				ENDIF
			;천 너머로
			ELSE
				PRINTFORMDW 앨리스는%INNER_B(MASTER)% 너머로 로터를 꽉 눌러,%CLITORIS_DESCRIPTION(MASTER)%사쿠야의 음핵을 희롱하기 시작했다…
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏! 앗!」")
				PRINTFORMW 「이런 자극이 강한 것은, 속옷 넘어(분)편이 좋은 것 같아요?」
				;개발이 끝난 상태
				IF ABL:MASTER:C감각 > 5
					PRINTFORMW 「보통은, 말야. 사쿠야에게는 조금 어딘지 부족할지도이지만♪」
				;개발중
				ELSE
					PRINTFORMW 「조금씩 길들여 가 주어요, 사쿠야♪」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;입지 않았다
	;-------------------------
	PRINTFORMDW 앨리스는 노출이 된 사쿠야의%VAGINA_DESCRIPTION(MASTER)%에 로터를 꽉 누르고%CLITORIS_DESCRIPTION(MASTER)%음핵을 희롱하기 시작했다…
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후앗! 아앗!」")
	PRINTFORMW 「어때? 좀 더 강 시력이 좋아? 그렇지 않으면 약한 것이 좋은 것일까?」
	IF ABL:MASTER:C감각 > 5 ;개발이 끝난 상태
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「아, 앗")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  응응, 핫, 아앗!」")
		PRINTFORMW 「후후, 알았다. 좀 더 강하게 시라고 주어요♪」
	ELSE ;개발중
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이, 이나다 앨리스! 이런 건… 응, 응읏!」")
		PRINTFORMW 「그래… 그러면, 조금 상냥하게 시라고 주어요♪」
	ENDIF
	RETURN 1
ENDIF
;NO9_ACT20, 로터

;─
;ACT21, 바이브
;─
;·조교 대상이 남자의 경우는 발생하지 않는다
;·TFLAG:벌플래그가 끊고 있으면 발생하지 않는다
;·턴 방침이 "이상"이라면 발생하지 않는다
;·TEQUIP(삼각 목마/페니스 밴드) 장착중이라면 발생하지 않는다
;·성교중에는 발생하지 않는다
;·조교 대상이 처녀라도 발생할 가능성이 있다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_21
;-------------------------------------------------
;·TFLAG:ACT 파생(0=바이브 장착/1=해제)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT21_0, 바이브 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
;삽입에 이르지 않는, 바이브에 의한 애무라든지 있어도 괜찮다
;-------------------------------------------------
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		IF TEQUIP:아이마스크
			PRINTFORMDW %CALLNAME%는 살그머니%CALLNAME:MASTER%의 눈가리개를 제외해 주었다…
			TEQUIP:아이마스크 = 0
			PRINTL
		ENDIF
		PRINTFORMW 「저기, 사쿠야는 이것 본 적 있을까?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「-본 일 없지만. 무엇에 사용하는지, 상상은 붙어요…」")
		PRINTFORMW 「후후, 과연… 소쇄인 가정부는 짐작이 좋네요. 뭐, 이 형태, 그 밖에 잘못볼 길도 없겠지만♪」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 그래서. 그래서 무엇을 하자고 말하는거야」")
		
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「사쿠야… 전언 철회해요. 아직 모르는거야? 당신에게 사용하는 것으로 정해져 있지 않아")
		;하트를 표시해 괄호를 닫아 WAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「조, 조금 기다려! 과연 나도 그것만은 싫어!」")
		PRINTFORMW 「어째서?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아무래도!」")
		PRINTFORMW 「구별하고가 없는 것, 말하지 말아줘. 모처럼 사쿠야를 위해서(때문에) 마력으로 연성한 것이니까」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무엇이 모처럼! 그런 것, 부탁한 기억은 없어요!」")
		
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「괜찮아, 괜찮아♪사쿠야라면 반드시 곧 익숙해져요. 푹 빠지게 되어 버릴지도")
		;하트를 표시해 괄호를 닫아 WAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTFORMW 「자, 체념해. 얌전하게 하세요, 사쿠야. 후후…」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;가설
	;=================================================
	;ONCEACT
	;=================================================
	IF 구상 기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("오늘") ;오늘의 조교 방침 파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "애널"
				PRINTFORMW
			CASE "벌"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "러브러브"
				PRINTFORMW
			CASEELSE ;그 외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;우선 테두리만 설치
	;=================================================
	;갈 것 같다
	;=================================================
	IF 구상 기입 체크(0) && TCVAR:MASTER:갈 것 같다
		SELECTCASE TCVAR:MASTER:초조하게 해 번
			CASE IS > 1 ;초조하게 해 안
				PRINTFORMW
			CASE 1 ;초조하게 해 종료
				PRINTFORMW
			CASEELSE ;초조하게 해 없음
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;연속 실행
	;=================================================
	;※이것은 성립하지 않기 때문에 REACT든지로 샐비지 할 필요 있음
	;---------------------------------------------
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		PRINTFORMDW 꿈틀거리는 바이브를%VAGINA_DESCRIPTION(MASTER)%에 파묻힌 사쿠야는,\@ ABL:MASTER:V감각 < 5 ? 질내의 이물감에# 쾌락에\@몸부림쳐, 허덕이고 있다…
		CALL QUAKE(1)
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「아, 앗")
		;하트를 표시해 괄호를 닫아 WAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("SAKUYA"), "  응응, 핫, 아앗!」")
		;V개발이 끝난 상태
		IF ABL:MASTER:V감각 > 5
			PRINTFORMW 「완전히 마음에 드는 것 같구나? 뭔가 보고 있는 내가 흥분해 버릴지도, 이것…」
		;개발 도상
