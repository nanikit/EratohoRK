;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT25_0, 니플캡 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「또 도무지 알 수 없는 것 끌어내 왔군요…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…… 완전히. 자주(잘) 생각이 떠올라요, 그런 것」")
		PRINTFORMW 「아라, 별로 내가 생각해 낸 것이 아니에요?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그것은 그렇겠지요하지만, 앨리스가 연성하고 있는 시점에서 같은 것이야」")
		PRINTFORMW 「뭐, 그렇게 생각하고 싶으면, 나는 그런데도 상관없지만, 말야♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;갈 것 같다
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:쾌B > 8500
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「얏, 안됏! 지, 지금 그런 것 붙일 수 있으면…」")
		PRINTFORMW 「후후, 좋지 않아. 그렇지 않으면 이런 완구로 가 버리는 것은 싫어?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「기, 기다려! 정말 가 버리기 때문에!」")
		PRINTFORMW 「그런 일 말해지면 더욱 더 하고 싶어지는거네요♪자, 얌전하게 해 줘」
		PRINTFORMDW 앨리스는%NIPPLE_DESCRIPTION(MASTER)%사쿠야의 유두에, 클립 로터를 설치했다…
		RETURN 1
	ENDIF
	
	;=================================================
	;클리캡 장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:클리캡
		PRINTFORMW 「사쿠야, 위의 (분)편이 외로운 것 같구나?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「벼, 별로 그런 일 없어요!」")
		PRINTFORMW 「후후, 무리하지 않아도 괜찮은거야. 이봐요, 움직이지 마…」
		PRINTFORMDW 앨리스는%NIPPLE_DESCRIPTION(MASTER)%사쿠야의 유두를 가볍게 혀로 굴리면, 클립 로터를 설치했다…
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗! 시, 싫닷! 찌릿찌릿, 떨렷!」")
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;용도 없는 것 같지만 일단
	;=================================================
	;상반신 복장이 요점 밀치고
	;=================================================
	IF 구상 기입 체크(0) && BRACHECK(1, 1)
		IF BRACHECK(1) ;상반신 복장이 방해
			PRINTW
		ELSEIF UPPERJACKET(1) == 1 ;상반신 윗도리가 요점 밀치고
			PRINTW
		ELSEIF UPPERUNDER(1) == 1 ;상반신 속옷이 요점 밀치고
			PRINTW
		ENDIF
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMDW 앨리스는%NIPPLE_DESCRIPTION(MASTER)%사쿠야의 유두를 가볍게 혀로 굴리면, 클립 로터를 설치했다…
	;조교 대상의 마조끼 5이상
	IF ABL:MASTER:마조끼 > 5
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아얏! 아″아″! 이, 이런 건! 우앗!」")
		PRINTFORMW 「후후, 사쿠야는 아픈 위가 아니면 만족할 수 없을 것입니다?」
		PRINTFORMW 「일부러 압력을 조금 강하게 해 준거야? 마음껏 즐겨 주세요♪」
	;그 외
	ELSE
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아웃! 싫다, 이것… 떨려…」")
		PRINTFORMW 「분명하게 조정했기 때문에 아프지 않을 것이야? 그 중 서서히 오기 때문에, 차분히 즐겨 줘♪」
	ENDIF
	RETURN 1
ENDIF
