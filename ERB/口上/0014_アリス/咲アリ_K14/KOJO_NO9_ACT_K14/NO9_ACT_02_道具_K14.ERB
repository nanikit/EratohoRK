;========================================================================
;道具系 ACT20～26
;========================================================================
;・TFLAG:ACT(20=ローター/21=バイブ/22=アナルバイブ/23=アナルビーズ/24=Ｃ愛撫道具/25=ニプルキャップ/26=ペニスバンド)
;・TFLAG:ACT分類 == 2に該当
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "道具")で拾う事が可能
;・調教者が調教対象に道具を使う。この口上の時点で調教者はすでに手を出している
;・媚薬・ローションを使用するケースがある
;・道具類の装着、解除等はTFLAG:ACT派生で定義される
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では口上側のRESULT(終端のRETURN 1など)によって口上が表示されたのか否か、
;・また箇所毎の特有の挙動(媚薬等口上をスキップなど)を制御しています
;・@口上記入チェックの場合代入処理を省略できますが、RESULT値は口上側で決めうちする必要があります
;-------------------------------------------------
;道具系実行の際に強制脱衣が入る仕様が不満
;非常に面白くない。これは改善されるべき
;-------------------------------------------------
;関連関数…
;FIRSTACT
;初回ACT判定関数
;TFLAG:ACT（TFLAG:90）を自動で参照、現在実行されているACTごとに番号が独立して保存される
;
;ONCEACT
;調教内未読判定関数　調教前に初期化される
;その他の仕様はほぼFIRSTACTと同様
;-------------------------------------------------

;────────────────────────────────────
;ACT20, ローター
;────────────────────────────────────
;・Ｃ中心に愛撫する
;・愛撫系から発生する場合もある
;・TFLAG:お仕置きフラグが立っていると発生しない
;・ターン方針が"ハード", "異常"だと発生しない
;・TEQUIP(クリキャップ/オナホール/ペニスバンド/三角木馬)装着中には発生しない
;・性交中/性交奉仕中には発生しない
;・調教対象にペニスがある場合は発生しない
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_20
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT20, ローター
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック() && FIRSTACT(0)
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「な、なによ！ それ！」")
		PRINTFORMW 「あら、咲夜も知らないのね？ 取りあえず練成はしてみたけれど、私も良くは分からないの…」
		PRINTFORMW 「とまぁそういう訳、で…咲夜で試させてもらう事にするわ♪」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;ONCEACT
	;=================================================
	IF 口上記入チェック(0) && ONCEACT(0)
		SELECTCASE POLICY("今日") ;今日の調教方針派生処理
			CASE "通常"
				PRINTFORMW
			CASE "アナル"
				PRINTFORMW
			CASE "お仕置き"
				PRINTFORMW
			CASE "ハード"
				PRINTFORMW
			CASE "イチャイチャ"
				PRINTFORMW
			CASEELSE ;その他
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;これは要らないかな とりあえず枠だけ
	;=================================================
	;イきそう
	;=================================================
	IF 口上記入チェック(0) && TCVAR:MASTER:イきそう
		SELECTCASE TCVAR:MASTER:焦らし度
			CASE IS > 1 ;焦らし中
				PRINTFORMW
			CASE 1 ;焦らし終了
				PRINTFORMW
			CASEELSE ;焦らし無し
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;連続実行
	;=================================================
	;これが成立する可能性は限りなく低い。何処かで何か仕込むしか
	;TEQUIPで管理されてる訳じゃないしねぇ
	;-------------------------------------------------
	IF 口上記入チェック(100) && TFLAG:ACT == TFLAG:前ACT
		;-------------------------------------------------
		;下半身服装が要押し退け
		;-------------------------------------------------
		IF 口上記入チェック(100) && PANTSCHECK(1, 1)
			;下半身下着が要押し退け
			IF LOWERUNDER(1) == 1
				;下着をずらして
				IF GETBIT(TEQUIP:着衣露出, 2)
					PRINTFORMDW アリスはずらした%INNER_B(MASTER)%から覗いた咲夜の%CLITORIS_DESCRIPTION(MASTER)%陰核をローターで責め続けている…
					CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んぅっ、んっ…や、だめぇっ、いやぁぁっ！」")
					;開発済み
					IF ABL:MASTER:Ｃ感覚 > 5
						PRINTFORMW 「そんな事言っても、声に甘いものが混じるのは隠せてないわよ？ ふふっ」
					;開発中
					ELSE
						PRINTFORMW 「直接は刺激が強すぎるのかしらね？ 咲夜の事だからすぐに慣れるんでしょうけど。ふふ」
					ENDIF
				;布地越しに
				ELSE
					PRINTFORMDW アリスは%INNER_B(MASTER)%の上から咲夜の%CLITORIS_DESCRIPTION(MASTER)%陰核をローターで愛撫し続けた…
					CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んぅっ、んっ…んく…あっ！」")
					;開発済み
					IF ABL:MASTER:Ｃ感覚 > 5
						PRINTFORMW 「もうこれじゃ咲夜には物足りない感じかしら、ね♪」
					;開発途上
					ELSE
						PRINTFORMW 「まだあまり慣れない感じかしら…でも下着の上からだと悪くないでしょう？」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;穿いてない
		;---------------------
		PRINTFORMDW アリスは咲夜の剥き出しになった%VAGINA_DESCRIPTION(MASTER)%をローターで愛撫し続けた…
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んぅっ、んっ…やぁっ！ いやぁぁっ！」")
		;開発済み
		IF ABL:MASTER:Ｃ感覚 > 5
			PRINTFORMW 「そんな事言っても、声に甘いものが混じるのは隠せてないわよ？ ふふっ」
		;開発途上
		ELSE
			PRINTFORMW 「直接は刺激が強すぎるのかしらね？ 咲夜の事だからすぐに慣れるんでしょうけど。ふふ」
		ENDIF
		RETURN 1
	ENDIF
	
	;微妙なところ 枠だけ置いておく
	;=================================================
	;ニプルキャップ/クリキャップ装着中
	;=================================================
	;IF 口上記入チェック(0) && TRUECHECK(TEQUIP:ニプルキャップ, TEQUIP:クリキャップ) > 0
	;ENDIF
	
	;=================================================
	;下半身服装が要押し退け
	;=================================================
	IF 口上記入チェック(100) && PANTSCHECK(1, 1)
		;下半身下着が要押し退け
		IF LOWERUNDER(1) == 1
			;下着をずらして
			IF GETBIT(TEQUIP:着衣露出, 2)
				PRINTFORMDL アリスは%INNER_B(MASTER)%をずらし咲夜の%VAGINA_DESCRIPTION(MASTER)%を露出させると、
				PRINTFORMDW ローターを押し当て、%CLITORIS_DESCRIPTION(MASTER)%陰核を嬲り始めた…
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ふぁっ！ あぁっ！」")
				PRINTFORMW 「ふふ、凄くイケナイ事してる気分になるわ、これ」
				PRINTFORMW 「どう？ やっぱり直接刺激されるのは気持ち良いのかしら？」
				;開発済み
				IF ABL:MASTER:Ｃ感覚 > 5
					CALL PRINTCOLOR(COLOR_K14("咲夜"), "「あっ、あっ")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
					PRINTFORMW 「ふふ、聞くまでもなかったみたいね♪」
				;開発中
				ELSE
					CALL PRINTCOLORW(COLOR_K14("咲夜"), "「や、やだっアリス！ こんなの…ん、んんっ！」")
					PRINTFORMW 「まだ慣れないみたいね？ でも大丈夫、すぐに良くなるわ♪」
				ENDIF
			;布地越しに
			ELSE
				PRINTFORMDW アリスは%INNER_B(MASTER)%越しにローターを押し当て、%CLITORIS_DESCRIPTION(MASTER)%咲夜の陰核を嬲り始めた…
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んっ！ あっ！」")
				PRINTFORMW 「こういう刺激が強いのって、下着越しの方がいいらしいわよ？」
				;開発済み
				IF ABL:MASTER:Ｃ感覚 > 5
					PRINTFORMW 「普通は、ね。咲夜にはちょっと物足りないかもだけど♪」
				;開発中
				ELSE
					PRINTFORMW 「少しずつ馴らしていってあげるわ、咲夜♪」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;穿いてない
	;-------------------------
	PRINTFORMDW アリスは剥き出しになった咲夜の%VAGINA_DESCRIPTION(MASTER)%にローターを押し当て%CLITORIS_DESCRIPTION(MASTER)%陰核を嬲り始めた…
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ふぁっ！ あぁっ！」")
	PRINTFORMW 「どう？ もう少し強目がいい？ それとも弱い方がいいのかしら？」
	IF ABL:MASTER:Ｃ感覚 > 5 ;開発済み
		CALL PRINTCOLOR(COLOR_K14("咲夜"), "「あっ、あっ")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
		PRINTFORMW 「ふふ、わかった。もっと強くシてあげるわ♪」
	ELSE ;開発中
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「や、やだっアリス！ こんなの…ん、んんっ！」")
		PRINTFORMW 「そう…じゃ、少し優しくシてあげるわ♪」
	ENDIF
	RETURN 1
ENDIF
