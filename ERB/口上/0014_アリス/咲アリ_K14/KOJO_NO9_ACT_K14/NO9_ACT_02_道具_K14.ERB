;────────────────────────────────────
;ACT21, バイブ
;────────────────────────────────────
;・調教対象がオトコの場合は発生しない
;・TFLAG:お仕置きフラグがたっていると発生しない
;・ターン方針が"異常"だと発生しない
;・TEQUIP(三角木馬/ペニスバンド)装着中だと発生しない
;・性交中には発生しない
;・調教対象が処女でも発生する可能性がある
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_21
;-------------------------------------------------
;・TFLAG:ACT派生(0=バイブ装着/1=解除)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT21_0, バイブ装着
;━━━━━━━━━━━━━━━━━━━━━━━━
;挿入に至らない、バイブによる愛撫とかあってもいい
;-------------------------------------------------
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		IF TEQUIP:アイマスク
			PRINTFORMDW %CALLNAME%はそっと%CALLNAME:MASTER%の目隠しを外してやった…
			TEQUIP:アイマスク = 0
			PRINTL
		ENDIF
		PRINTFORMW 「ねぇ、咲夜はこれ見たことあるかしら？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「――見た事ないけど。何に使うのか、想像はつくわ…」")
		PRINTFORMW 「ふふ、流石…瀟洒なメイドさんは察しがいいわね。ま、この形、他に見間違えようもないでしょうけど♪」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「…で。それで何をしようって言うのよ」")
		
		CALL PRINTCOLOR(COLOR_K14("アリス"), "「咲夜…前言撤回するわ。まだわからないの？ 貴女に使うに決まってるじゃない")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ちょ、ちょっと待って！ さすがに私もそれだけは嫌！」")
		PRINTFORMW 「どうして？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「どうしても！」")
		PRINTFORMW 「聞き分けのない事、言わないで頂戴。折角咲夜の為に魔力で練成したんだから」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「何が折角よ！ そんなもの、頼んだ覚えはないわよ！」")
		
		CALL PRINTCOLOR(COLOR_K14("アリス"), "「大丈夫、大丈夫♪ 咲夜ならきっとすぐ慣れるわ。病み付きになっちゃうかも")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTFORMW 「さ、観念して。大人しくなさい、咲夜。フフ…」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;仮設
	;=================================================
	;ONCEACT
	;=================================================
	IF 口上記入チェック(0) && ONCEACT(0)
		SELECTCASE POLICY("今日") ;今日の調教方針派生処理
			CASE "通常"
				PRINTFORMW
			CASE "アナル"
				PRINTFORMW
			CASE "お仕置き"
				PRINTFORMW
			CASE "ハード"
				PRINTFORMW
			CASE "イチャイチャ"
				PRINTFORMW
			CASEELSE ;その他
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;とりあえず枠だけ設置
	;=================================================
	;イきそう
	;=================================================
	IF 口上記入チェック(0) && TCVAR:MASTER:イきそう
		SELECTCASE TCVAR:MASTER:焦らし度
			CASE IS > 1 ;焦らし中
				PRINTFORMW
			CASE 1 ;焦らし終了
				PRINTFORMW
			CASEELSE ;焦らし無し
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;連続実行
	;=================================================
	;※これは成立しないのでREACTなりでサルベージする必要アリ
	;---------------------------------------------
	IF 口上記入チェック(100) && TFLAG:ACT == TFLAG:前ACT
		PRINTFORMDW 蠢くバイブを%VAGINA_DESCRIPTION(MASTER)%に埋め込まれた咲夜は、\@ ABL:MASTER:Ｖ感覚 < 5 ? 膣内の異物感に # 快楽に \@身悶え、喘いでいる…
		CALL QUAKE(1)
		CALL PRINTCOLOR(COLOR_K14("咲夜"), "「あっ、あっ")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
		;Ｖ開発済み
		IF ABL:MASTER:Ｖ感覚 > 5
			PRINTFORMW 「すっかりお気に入りみたいね？ なんか見てる私が興奮しちゃうかも、これ…」
		;開発途上
		ELSE
			PRINTFORMW 「思ったより気に入ってもらえてるみたいね。ふふ…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;アナルバイブ/アナルビーズ装着中(二穴責め)
	;=================================================
	IF 口上記入チェック(100) && TRUECHECK(TEQUIP:アナルバイブ, TEQUIP:アナルビーズ) > 0
		PRINTFORMW 「前が寂しそうね、咲夜。こっちにも欲しいのかしら？ フフ…」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ダメっ！ いま、そんなの入れられたら…」")
		PRINTL
		PRINTFORMDL アリスは\@ TEQUIP:アナルバイブ ? アナルバイブ # アナルビーズ \@を埋め込まれ、快楽に身悶える
		;これは口上側のVAGINA_DESCRIPTION		紛らわしいからリネームもありかも
		PRINTFORMDW 咲夜の%VAGINA_DESCRIPTION2(MASTER)%にバイブをあてがい、徐々に馴染ませながらズブズブと埋め込んでいった…
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あ゛あ゛ぁぁぁっ！ 壊れるっ！ 壊れるうぅっ！！」")
		PRINTFORMW 「大丈夫よ、咲夜。貴女はこんなもので壊れはしない」
		PRINTFORMW 「だってそうでしょう？ 貴女を壊すのは、この私なんだから…」
		RETURN 1
	ENDIF
	
	;仮設
	;=================================================
	;下半身服装が要押し退け(現在死に口上)
	;=================================================
	IF 口上記入チェック(100) && PANTSCHECK(1, 1)
		;下半身下着が要押し退け
		IF LOWERUNDER(1) == 1
			;下着をずらして
			IF GETBIT(TEQUIP:着衣露出, 2)
				PRINTFORMDL アリスは咲夜の下着をずらし、バイブに愛液をたっぷりと絡めると
				PRINTFORMDW 入り口で軽く馴染ませた後、ズブズブと咲夜の膣内に埋め込んでいった…
					CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あぅっ！ は、入って…」")
				IF ABL:MASTER:Ｖ感覚 > 5
					PRINTFORMW 「ふふ、すんなり入っていくわね。さぁ、愉しんで頂戴、咲夜♪」
				ELSE
					PRINTFORMW 「ちょっと抵抗感があるかしら？ もう少し、時間をかけて馴らす必要があるみたいね」
				ENDIF
			;布地越しに
			;ELSE
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMDW アリスは剥き出しになった咲夜の%VAGINA_DESCRIPTION2(MASTER)%をバイブで撫で、愛液を絡めると
	PRINTFORMDW 咲夜の反応を伺いつつ、徐々に膣内に埋め込んでいった…
	CALL QUAKE(1)
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はぅっ！ は、入ってくる…」")
	;Ｖ開発済み
	IF ABL:MASTER:Ｖ感覚 > 5
		PRINTFORMW 「ふふ、しっかり咥えこんじゃって、イヤラシイわね。さぁ、愉しんで頂戴、咲夜♪」
	;開発途上
	ELSE
		PRINTFORMW 「ん、まだ慣れない感じかしらね？ でも大丈夫よ、少しずつ馴染ませていってあげるから」
	ENDIF
	RETURN 1
ENDIF
