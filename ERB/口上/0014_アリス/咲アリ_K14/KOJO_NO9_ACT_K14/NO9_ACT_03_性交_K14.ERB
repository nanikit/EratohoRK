;━━━━━━━━━━━━━━━━━━━━━━━━
;·REACT 분류가 날뛰는이었던 경우는 캔슬된다
;·받아들였을 경우, MASTER가 처녀라면 처녀 상실, TARGET가 동정이라면 동정 상실이 일어난다
;·v1. 010 현재, 거절했을 경우의 SOURCE 리셋트 처리가 없다
;-------------------------------------------------
IF 구상기입체크(100)
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORMDW 앨리스는 배후로부터 사쿠야를 껴안고 있다…
		PRINTL
		PRINTFORMW 「사쿠야의 냄새가 난다…」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무, 무엇 당연한 일 말하고 있을까……」")
		PRINTFORMW 「그렇구나, 그렇지만… 매우 좋은 향기. 내가 좋아하는 향기야」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 갑자기 그런 일 말하기 시작해」")
		PRINTL
		PRINTFORMW 「후후, 어째서일까요? 스스로도 잘 몰라요. 그렇지만…」
		PRINTFORMW 「이렇게 해 당신의 향기에 휩싸여지고 있으면, 왜일까 매우 침착해. 사실이야?」
		PRINTL
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 앨리스. 너무 그런 일, 말하지 마…」")
		PRINTFORMW 「어째서?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「핫」")
		PRINTFORMW 「하?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…… 부끄럽기 때문에,」")
		PRINTL
		PRINTFORMW 「후후, 완전해 소쇄인 수행원은 부끄럽게 여기기인 거네? 그렇지만…」
		PRINTFORMW 「사쿠야의 그러한 곳, 나는 좋아해요?」
		PRINTL
		
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「")
		CALL PRINTFONTSTYLE(COLOR2_K14("SAKUYA"), "다  인가  등", 1)
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), ", 그런 일 말해지는 것이, 부끄럽다고 말하고 있겠지요!」")
		
		CALL PRINTCOLOR(COLOR_K14("앨리스"), "「괜찮아, 괜찮아")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  나는 전혀 아무렇지도 않기 때문에♪」")
		
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「사람의 이야기 (듣)묻고 있어? 읏, 조금!」")
		PRINTFORMW 「후후, 부끄러워하고 있는 사쿠야 보고 있으면 인내 할 수 없게 되었어. 그대로 얌전하게 하고 있어 줘?」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;ONCE
	;=================================================
	IF 구상 기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("오늘") ;오늘의 조교 방침 파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "애널"
				PRINTFORMW
			CASE "벌"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "러브러브"
				PRINTFORMW
			CASEELSE ;그 이외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;전회 질내사정
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질내사정 && TFLAG:전턴성교
		;범용 처리
		CALL NO9_PRINTTEXT_CATEGORY_3_ACT_1_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;연속(배면좌위로부터)
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 1
		;-------------------------------------------------
		;조교자가 갈 것 같다
		;-------------------------------------------------
		IF 구상기입체크(100) && TCVAR:TARGET:갈것같다
			;범용 처리(한정 없음 구상측에서 부른다)
			CALL PRINTTEXT_CATEGORY_3_ACT_2_K14
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;조교자가 정욕
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("정욕")
			;범용 처리(한정 없음 구상측에서 부른다)
			CALL PRINTTEXT_CATEGORY_3_ACT_3_K14
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				CALL QUAKE(4)
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「아, 아읏! 핫, 아앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				
				PRINTFORMW 「입은 채로의 사쿠야로 하는 것은 굉장히 흥분해요…」
				PRINTFORMDW 앨리스는 배후로부터 사쿠야의%OUTER_B(MASTER, 0, 1)%에 손을 꽂아 넣어 전신을 애무하고 있다…
			;속옷을 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				CALL QUAKE(4)
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응아앗! 핫, 아앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  거기, 만지작거려!」")
				
				SELECTCASE RANDACT(2, 0)
					CASE 0
						PRINTFORMW 「여기가 외로울까? 이봐요, 직접 만지작거려 주어요」
						PRINTFORMDW 앨리스는 사쿠야의%INNER_B(MASTER)%에 손을 넣어%CLITORIS_DESCRIPTION(MASTER)%음핵을 그 손가락으로 희롱하고 있다…
					CASE 1
						PRINTFORMW 「후후, 젖가슴 기분 좋아? 좀 더 만지작거려 주어요」
						PRINTFORMDW 사쿠야는 배후로부터 가슴을 주물러져%NIPPLE_DESCRIPTION(MASTER)%유두에게 줄 수 있는 자극에 격렬하게 허덕이고 있다…
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리
		;-----------------------------------------
		CALL QUAKE(4)
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「얏, 아앗")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  응응, 쿠웃!」")
		
		PRINTFORMW 「이봐요, 여기를 향해, 사쿠야. 키스 해 준다…」
		PRINTFORMDW 앨리스는 사쿠야를 뒤돌아 보게 해 키스를 하면서 사쿠야를 계속 범했다…
		RETURN 1
	ENDIF
	
	;=================================================
	;체위 변경(V성교로부터)
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 2
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				PRINTFORMW 「이번은 뒤로부터 괴롭혀 주어요, 사쿠야…」
				PRINTFORMDW 앨리스는 배후로부터%OUTER_B(MASTER, 0, 1)% 너머로 사쿠야의 전신을 애무하면서, 신체를 계속 흔들었다…
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗, 그, 그렇게 되면!」")
				PRINTFORMW 「당신도 즐기고 있는 것 같구나? 이봐요, 귀와 목덜미도 귀여워해 주어요…」
			;속옷을 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				PRINTFORMW 「이봐요, 좀 더 나에게 신체를 맡겨. 그래, 좋아요. 그렇다 치더라도…」
				PRINTFORMW 「입은 채로 하는 것은, 뭔가 굉장히 싫은 것 같네요…」
				PRINTFORMDW 사쿠야의%VAGINA_DESCRIPTION2(MASTER)%로부터는 끝 없게 애액이 흘러넘치고이고,%INNER_B(MASTER)%를 더럽히고 있다…
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리
		;-----------------------------------------
		;쾌V파생
		SELECTCASE PALAM:MASTER:쾌V
			;조교 대상이 갈 것 같다
			CASE IS >= 8500
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앨리스, 나… 이제(벌써), 갈 것 같다…」")
				
				PRINTFORM 「알았다
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  조금 격렬하게 시라고 주어요」")
				
				PRINTFORMDW 앨리스는 배후로부터 사쿠야의 전신을 애무하면서, 격렬하게 신체를 흔들기 시작했다…
				PRINTL
				CALL QUAKE(4)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아웃, 앗! 얏, 아앗!」")
				PRINTFORMW 「이대로 가게 해 주어요, 사쿠야…」
			;그 외
			CASEELSE
				PRINTFORMDL 앨리스는 한 손으로 사쿠야의%NIPPLE_DESCRIPTION(MASTER)%유두를 꾸짖으면서,
				PRINTFORMDW 남은 손으로%CLITORIS_DESCRIPTION(MASTER)%음핵을 손가락끝으로 희롱하고 있다…
				PRINTL
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗, 얏! 그렇게, 만지작거리지 맛!」")
				PRINTFORMW 「후후, 이 몸의 자세라면 여러가지 할 수 있어요♪」
				PRINTFORMW 「이봐요, 좀 더 나에게 몸을 맡겨. 즐겁게 해 주어요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;A성교로부터
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 3
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				;쾌A7000 이상
				IF PALAM:MASTER:쾌A >= 7000
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아…… 얏! 어째서 그만두어 버리는거야!」")
					PRINTFORMW 「아라, 엉덩이가 좋았어?」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우… 그, 그것은…」")
					PRINTFORMW 「후후, 미안해요. 이번은 이쪽에서 즐겁게 해 받아요」
				;그 외
				ELSE
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아…… 끝나, 야…?」")
					PRINTFORMW 「유감이지만 하지 않아요. 이번은 이쪽에서 즐겁게 해 받을 뿐. 후후…」
				ENDIF
		
				PRINTFORMDL 앨리스는 사쿠야에게%OUTER_B(MASTER, 0, 1)%의 옷자락을 걷어 붙이게 해%PENIS_DESCRIPTION(TARGET)%페니스 위에 앉도록(듯이) 명했다
				PRINTFORMDW 사쿠야는 질구를 벌려 페니스의 첨단을 붙잡아, 서서히 허리를 가라앉혀 간다…
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아아웃! 아아, 들어가…」")
				PRINTFORMW 「뒤로부터 귀여워해 주어요, 사쿠야… 이봐요, 좀 더 신체를 맡겨」
			;속옷을 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				;쾌A7000 이상
				IF PALAM:MASTER:쾌A >= 7000
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗…… 싫닷! 어째서! 어째서 뽑아 버리는거야!」")
					PRINTFORMW 「응, 조금, 기분이 바뀌었어. 그렇지만 안심해? 아직, 끝이 아니니까♪」
				;그 외
				ELSE
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아…… 끝나, 야…?」")
					PRINTFORMW 「유감이지만 당신의 기대에는 응할 수 없어요, 이번은 이쪽에서 즐겁게 해 줘♪」
				ENDIF
				
				PRINTFORMDW 앨리스는 사쿠야를 위에 걸치게 하면%INNER_B(MASTER)%를 비켜 놓아 스스로 삽입하도록(듯이) 명했다…
				PRINTFORMW 「후후, 그렇게… 좋아요. 그대로 느리, 구나♪」
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏! 아아…」")
				PRINTFORMW 「자신으로부터 해 버리다는… 정말, 싫은 것 같네요? 사쿠야…」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그것을 해라고 말한 것은 어디의 누구야…」")
				PRINTFORMW 「후후, 그랬네요」
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리 AUTORAND(130)
		;-----------------------------------------
		;범용 처리
		CALL NO9_PRINTTEXT_CATEGORY_3_ACT_2_K14
		RETURN 1
	ENDIF
	
	;부실이다 이것
	;=================================================
	;V성교 봉사/A성교 봉사로부터
	;=================================================
	IF 구상기입체크(100) && GROUPMATCH(TFLAG:ACT파생, 4, 5)
		;조교자가 초조하고 있다
		IF TENSION(TARGET, 7) == "높다"
			;범용 처리(한정 없음으로부터 부른다)
			CALL PRINTTEXT_CATEGORY_3_ACT_4_K14
		;그 외
		ELSE
			SELECTCASE RANDACT(2, 1)
				CASE 0
					PRINTFORMW 「%CNAME(MASTER, TARGET)%보고 있으면……, 나부터 시 싶어져 버렸어요. 후후」
					
					PRINTFORM 「자, 뒤로부터 껴안으면서 찔러 준다
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「이번은 나부터 시라고 준다……」
					PRINTFORMW 「자, 여기에 몸을 맡겨. 그래, 좋아요…… 후후」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;페니스 밴드 사용시
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(!PENIS(TARGET), TEQUIP:조교자페니스밴드) > 0
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1 && !TEQUIP:아이마스크
				PRINTFORMW 「자, 이것을 사용해요!」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앨리스, 당신…… 즐거운 것 같구나…」")
				PRINTFORMW 「당연하겠지요? 이것으로 사쿠야의 안되게 되어 버리는 곳이 볼 수 있기 때문에」
				PRINTL
				PRINTFORMW 「응, 혹시 사쿠야, 당신… 이런 유사품이 싫은 것일까?」
				PRINTFORMW 「진짜가 좋으면, 그렇게 말해 준다면 좋은데」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아무도 그런 일 말하지 않았다!」")
				PRINTFORMW 「아라, 그랬어. 이것은 실례했군요, 후후후」
				PRINTL
				PRINTFORMW 「자, 장난은 끝이야. 슬슬 시작해 받을까? 저기 사쿠야」
				PRINTFORMDL 앨리스는 사쿠야에게%OUTER_B(MASTER, 0, 1)%의 옷자락을 걷어 붙이게 해%DHARNESS_DESCRIPTION(TARGET)% 위에 앉도록(듯이) 명했다
				PRINTFORMDW 사쿠야는 질구를 벌려 첨단을 붙잡으면, 서서히 길들이면서 허리를 가라앉혀 갔다…
				PRINTL
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응앗! 읏, 쿠웃!」")
				
				PRINTFORM 「자, 뒤로부터 껴안으면서 찔러 주어요. 마음껏 귀여워해 준다…
				CALL PRINT_SYMBOL_K14
			;속옷을 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				PRINTFORMW 「사쿠야는 그대로 좋아요, 가만히 하고 있어 줘…」
				PRINTFORMDL 앨리스는 사쿠야의%INNER_B(MASTER)%를 비켜 놓아%WET_DESCRIPTION(MASTER)%%VAGINA_DESCRIPTION2(MASTER)%에%DHARNESS_DESCRIPTION(TARGET)%를 주면,
				PRINTFORMDW 질구로 애액을 관련되면서, 서서히 안쪽으로 꿰뚫어 갔다…
				PRINTL
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「하웃! 아아…… 들어가, 들어 온다…」")
				PRINTFORMW 「전부 들어갔어요, 사쿠야…… 망가져 버리는 위 찔러 주기 때문에, 각오 해…」
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리
		;-----------------------------------------
		;윤활 파생 처리
		SELECTCASE PALAM:MASTER:윤활
			CASE IS >= 1000 ;너무 젖고
