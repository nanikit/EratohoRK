			PRINTL
			PRINTFORMW 「ちょっと！ 何するのよ！」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「いいじゃない。どうせ私の為に作ってくれたんだから」")
			PRINTFORMW 「それはまぁ、そう、なんだけど…って、何言わせるのよっ」
			
			CALL PRINTCOLOR(COLOR_K14("咲夜"), "「アリスのそういう素直じゃないとこ、私は好きよ…")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
			
			PRINTFORM 「…
