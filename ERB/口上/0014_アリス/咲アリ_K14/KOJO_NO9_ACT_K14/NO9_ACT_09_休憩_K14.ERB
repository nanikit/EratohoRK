	;=================================================
	;調教対象BASE:体力25%以上
	;=================================================
	IF 口上記入チェック(100) && TENSION(MASTER, 0) == "一般"
		;好意、好感度派生
		IF AFFECTION("相思相愛")
			PRINTFORMW 「ねぇ咲夜、少し休憩する？」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そうね、そうして貰えると有り難いわ」")
			PRINTFORMW 「了解～♪ でも、そうね…どうしても自分でシたくなったら、それは自己責任でお願いね？」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はぁ？ 何で私がそんな事」")
			PRINTFORMW 「…何でって。咲夜、時々自分で勝手に始めるじゃない」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「記憶に無いわね、そんな事…」")
			PRINTFORMW 「やだ咲夜ったら…ボケるにはまだちょっと早いんじゃないかしら」
			
			CALL PRINTCOLOR(COLOR_K14("咲夜"), "「")
			CALL PRINTFONTSTYLE(COLOR2_K14("SAKUYA"), "張り倒すわよ", 1)
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "」")
		ELSEIF AFFECTION("眷愛")
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ねぇアリス。私、ちょっと疲れたわ…」")
			PRINTFORMW 「そう…あれだけアンアン鳴いてたんだから、さぞかし疲れたんでしょうね」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「不思議ね？ 貴女が何を言ってるのか、全然判らないんだけど」")
			PRINTFORMW 「あら、とぼける気？ 何なら今すぐ思い出させてあげてもいいのよ？」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あぁもう、わかった、わかったから！ お願いだから少し休ませて」")
			
			PRINTFORM 「ふふ、最初から素直にそう言えばいいのに、ね…
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ELSEIF AFFECTION("嫌悪")
			PRINTFORMW 「少し疲れてるみたいね？ だからと言って、これで終わりにするつもりはないんだけど」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ええ、そうでしょうね。期待はしていないわ…」")
			PRINTFORMW 「殊勝な心掛けね？ まぁ休憩くらいなら構わないわ。精々しっかりと休む事ね」
		ELSE
			PRINTFORMW 「あら、ちょっとお疲れな感じね？」
			CALL PRINTCOLORL(COLOR_K14("咲夜"), "「そうね、確かに少し疲れたわ」")
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「と言っても、貴女は止めるつもりなんて無いんでしょうけど…」")
			PRINTFORMW 「そうね？ でも、休憩を挟む程度の気配りはするつもりよ」
			PRINTFORMW 「少しの間だけどゆっくり休んで頂戴、咲夜」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;好意、好感度派生
	IF AFFECTION("相思相愛")
		PRINTFORMW 「ねぇ咲夜…疲れてるなら言ってくれればいいのに、って思うんだけど？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はぁ、はぁ…よく言うわね。どうせ言ったところで聞かない癖に」")
		PRINTFORMW 「流石瀟洒なメイド長は良く判ってらっしゃる。ってとこかしら？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そこまで開き直られると何も言う気がしないわ。ホント度し難いわね、アリス…」")
		PRINTFORMW 「ふふ…お互いに、ね？ ま、それは置いといて休憩にしましょう」
		
		PRINTFORM 「\@ TIME ? 夜はまだまだこれからだし # まだまだ時間はあるし \@、ね…
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
	ELSEIF AFFECTION("眷愛")
		CALL PRINTCOLORL(COLOR_K14("咲夜"), "「ハァ、ハァ……まったく。少しは加減して欲しいものね」")
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「流石にこれじゃ身体が持たないわ…」")
		PRINTFORMW 「そうは言うけど仕方無いでしょ？ 咲夜がもっと、もっとっておねだりするんだから…♪」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「貴女が何を言ってるのか良く判らないわね、アリス…」")
		PRINTFORMW 「ふふ、まぁいいわ。とりあえずちょっと休憩にしましょう」
		
		PRINTFORM 「\@ TIME ? 夜はまだまだこれからだし # まだまだ時間はあるし \@、ね…
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
	ELSEIF AFFECTION("嫌悪")
		PRINTFORMW 「あらあら、大分お疲れの様子ね？」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はぁ、はぁ…誰のせいでこうなったと思ってるのよ」")
		PRINTFORMW 「そんな事言われるまでも無いけど…まぁいいわ。ちょっと休みましょう」
		PRINTFORMW 「でも、これで終わりって訳じゃないから…」
	ELSE
		PRINTFORMW 「ねぇ咲夜、随分疲れてるみたいだけど」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ハァ、ハァ…見ての通りよ…」")
		PRINTFORMW 「わかった、少しの間休憩にするわ。でも、まだ終わりじゃないからね？」
	ENDIF
	RETURN 1
ENDIF
