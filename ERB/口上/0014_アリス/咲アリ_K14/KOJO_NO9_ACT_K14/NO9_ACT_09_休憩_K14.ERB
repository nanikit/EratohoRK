			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「どういう意味よ」")
			PRINTFORMW 「そのままの意味だけど？ 何の捻りもなくて悪かったわね」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ホント可愛くないわね、アリス…」")
			
			PRINTFORM 「ふふっ、お互いに、ね…
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ELSEIF AFFECTION("溺愛")
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「折角出してくれたのに悪いけど、私、疲れてないわよ」")
