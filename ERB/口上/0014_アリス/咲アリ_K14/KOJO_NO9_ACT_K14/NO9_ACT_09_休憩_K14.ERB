			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そう、それなら…って、何ジロジロ見てるのよ」")
			PRINTFORMW 「ふふふ…」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「な、何よ！ なに笑ってるのよ！」")
			
			PRINTFORM 「だって、ねぇ？ くすくす…ホント、貴女といると飽きないわ、咲夜…
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ELSEIF AFFECTION("眷愛")
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「なによ。もう終わり、なの？」")
			PRINTFORMW 「ふふ、そんなにがっかりしなくていいわよ？ ちょっと休むだけだから」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「なっ！？ がっかりなんてしてないわよ！」")
			
			PRINTFORM 「はいはい…♪ そういう事にしておいてあげるわ
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ELSEIF AFFECTION("嫌悪")
			PRINTFORMW 「少し早目に休憩しましょう。精々しっかりと休んで頂戴」
			PRINTFORMW 「貴女にはまだまだ頑張って貰わないと、ねぇ咲夜？ フフ…」
		ELSE
			PRINTFORMW 「ちょっと早いけど休憩にするわ。今のうちにしっかりと休んでおいて」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そう、まぁそれでもいいけど」")
			PRINTFORMW 「何よ、少し不満そうね？ 続きをしたかったのかしら」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「誰もそんな事言ってないわよ…」")
			
			PRINTFORM 「あら、これは失礼したわね…
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
