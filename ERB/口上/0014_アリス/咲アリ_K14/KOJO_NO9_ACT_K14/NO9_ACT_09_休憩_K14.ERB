			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「その時はアリスにお世話になるわ」")
			
			PRINTFORM 「
			CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "お断りよ", 1)
			PRINTFORMW 」
			
