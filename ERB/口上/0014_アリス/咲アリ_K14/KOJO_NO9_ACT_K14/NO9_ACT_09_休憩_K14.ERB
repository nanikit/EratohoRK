			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「叩いていい？」")
		ELSEIF AFFECTION("嫌悪")
			PRINTFORMW 「ほら、飲みたければ飲めば？」
			PRINTFORMDW アリスは咲夜に栄養剤を投げて渡した…
		ELSE
			PRINTFORMW 「はい、これを飲んで頂戴」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「まだそんなに疲れてないけど」")
			PRINTFORMW 「あら、何事も早目早目が肝心よ？」
			CALL PRINTCOLORL(COLOR_K14("咲夜"), "「まぁそれはそうなんでしょうけど、でも…」")
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「美味しくないのよね、コレ」")
			PRINTFORMW 「ウルサイわね！ つべこべ言ってないでさっさと飲みなさいよっ」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「……やれやれ、ね」")
		ENDIF
		RETURN 2
	ENDIF
	
	;=================================================
	;調教対象BASE:体力25%以上
	;=================================================
	IF 口上記入チェック(100) && TENSION(MASTER, 0) == "一般"
		;ここは地の文捏造する必要ありかな
		IF AFFECTION("相思相愛")
			CALL PRINTCOLORL(COLOR_K14("咲夜"), "「…………」")
			PRINTFORMW 「…？」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「それ、私にはくれないの？」")
			PRINTFORMW 「美味しくない、マズイって言ってた癖に、どういう風の吹き回し？」
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「覚えてない」")
