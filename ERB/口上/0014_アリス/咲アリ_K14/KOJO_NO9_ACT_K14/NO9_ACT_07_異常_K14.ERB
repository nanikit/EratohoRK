;
;	앨리스 구상(대상 한정) 이상계 액션 구상 파일
;
;		이상계 액션 구상을 정리한 파일
;
;	EXTRA 모드로 NO:9 사쿠야를 MASTER에 선택했을 경우만 표시
;
;Version		Reason
;1. 70			@구상 기입 체크 사양에. 그 외 섬세한 수정＆가필
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;이상계 ACT70～75
;========================================================================
;·TFLAG:ACT(70=피스트퍽/71=애널피스트/72=양구멍피스트/73=방치플레이/74=공중육변기플레이/75=페니스밴드장착)
;·TFLAG:ACT분류 == 7에 해당
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "이상")로 참조하는 것이 가능
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는 구상측의 RESULT(종단의 RETURN 1 등)에 의해 구상이 표시되었는지 아닌지,
;·또 개소 마다의 특유의 거동(미약등 구상을 스킵 따위)을 제어하고 있습니다
;·@구상 기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는 구상측에서 결정하고 쳐 할 필요가 있습니다
;-------------------------------------------------

;────────────────────────────────────
;NO9_ACT70, 피스트퍽 ※봉인이 끝난 상태
;────────────────────────────────────
;@KOJO_NO9_MS_ACT_K14_70

;────────────────────────────────────
;NO9_ACT71, 애널피스트 ※봉인이 끝난 상태
;────────────────────────────────────
;@KOJO_NO9_MS_ACT_K14_71

;────────────────────────────────────
;NO9_ACT72, 양구멍피스트 ※봉인이 끝난 상태
;────────────────────────────────────
;@KOJO_NO9_MS_ACT_K14_72

;────────────────────────────────────
;NO9_ACT73, 방치플레이 FIRSTACT(0) RANDACT(0)
;────────────────────────────────────
;·조교자가 조교 대상을 방치한다
;·턴방침이 "소프트", "노멀", "이상"이라면 발생하지 않는다
;·(이상계 ACT로 턴방침 "이상"으로 연주하고 있는 것은 미스라고 생각되고)
;·오늘의방침가 "러브러브"의 경우는 발생하지 않는다
;·쓰러트리기중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·수치계로부터 발생하는 경우도 있다(v1. 010 현재, 수치계로부터 파생했을 경우에게만 발생이라고 생각되고)
;·안면승마계는 해제된다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_73
;-------------------------------------------------
;·ACT의 파생 없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT73, 방치플레이
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 이것. 도대체 어떤 생각이야」")
		PRINTFORML 「어떤 생각이 뭐고…, 단순한」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「단순한?」")
		
		PRINTFORM 「
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "방치플레이", 1)
		PRINTFORMW 라는 녀석」
		
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「하?」")
		PRINTFORMW 「아니, 그러니까 방치플레이라고 말하고 있겠죠?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지 않아서……, 이것에 무슨 의미가 있는지라고 (듣)묻고 있는거야!」")
		PRINTFORMW 「의미는…, 그런 것이 있다면 내가 (듣)묻고 싶을 정도」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…… 아휴. 머리 아파져 왔어요」")
		RETURN 1
	ENDIF
	
	;성실하게 생각하면, 다양하게 가능성을 느낀다
	;-------------------------
	;그 외
	;-------------------------
	SELECTCASE RANDACT(3, 0)
		CASE 0
			PRINTFORMW 「심심한 듯한 사쿠야의 모습을 보는 것도 나쁘지 않네요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「악취미군요, 앨리스…」")
			PRINTFORMW 「덕분에,」
		CASE 1
			PRINTFORMW 「아라, 아무것도 되지 않으면 오히려 침착하지 않은 것 같구나?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「덕분에. 당신이 무엇을 기도하고 있는지, 제정신이 아닌 것뿐」")
			PRINTFORMW 「『기도하고 있다』는…, 남듣기 안 좋은 일 말하지 않아 받을 수 있어?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇구나, 당신 이외에는 말하지 않게 조심해요」")
			PRINTFORMW 「헤에, 이거 정말. 신경을 쓰게 해 버려 나쁘네요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「천만에요」")
			PRINTFORMW 「…………」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…………」")
		CASE 2
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그다지 빤히 보지 않아 받을 수 있을까…」")
			PRINTFORMW 「어째서?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「침착하지 않기 때문에」")
			PRINTFORMW 「…… 알았다」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「에?」")
			PRINTFORM 「좀 더 침착하지 않은 기분에 시켜 주어요, 사쿠야
			CALL PRINT_SYMBOL_K14
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일일거라고 생각했어요……」")
			PRINTFORMW 「후후후…」
	ENDSELECT
	RETURN 1
ENDIF

;────────────────────────────────────
;NO9_ACT74, 공중육변기플레이 ※봉인이 끝난 상태
;────────────────────────────────────
;@KOJO_NO9_MS_ACT_K14_74

;────────────────────────────────────
;NO9_ACT75, 페니스밴드장착
;────────────────────────────────────
;·조교자가 쌍두 페니스밴드를 몸에 대어, 조교 대상으로 과시한다
;·조교자가 처녀인 경우 처녀상실이 된다
;·성교중/성교봉사중에는 발생하지 않는다
;·조교자에게 페니스가 있는 경우에는 발생하지 않는다
;·이미 장착하고 있었을 경우는 해제가 된다
;·안면승마계는 해제된다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_75
;-------------------------------------------------
;·TFLAG:ACT파생(0=장착/1=해제)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT75_0, 페니스밴드장착
;━━━━━━━━━━━━━━━━━━━━━━━━
;·페니스밴드 장비중은 쾌Ｃ가 쾌Ｖ에 변환
;·변환율… 쾌Ｃ를 20％로 해, 나머지의 값에Ｖ감각+1/Ｃ감각+1을 걸어 2로 나누어 쾌Ｖ에 가산한다
;·지문은 ACT파생, 및 처녀인지 아닌지로 분기 한다
;-------------------------------------------------
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;조교자가 처녀
	;=================================================
	IF 구상기입체크(100) && !TEQUIP:아이마스크 && TALENT:TARGET:처녀
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아, 앨리스! 어째서, 그런……」")
		PRINTFORMW 「읏! 괴…… 굉장한 일 없어요, 이런 것. 한 번 해 버리면, 어떻게라는 일, 없다……」
		PRINTFORML 「후훗, 아하하! 사실, 굉장한 일 없어요!」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앨리스, 당신……」")
		PRINTFORMW 「뭐야, 내가 무리하고 있다고라도?」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「나에게는 그렇게 보여요」")
		PRINTFORMW 「그렇구나, 확실히 아픔은 느낀다. 심상치 않을 정도로」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇다면…」")
		PRINTFORMW 「별로 좋은 것, 그런 것은 문제가 되지 않는거야. 왜냐하면 그렇겠지요?」
		PRINTFORMW 「이것으로 당신과 하나가 될 수 있기 때문에……」
		PRINTFORMDW 그렇게 단언하는 앨리스의 눈동자는, 요염한 빛을 띠고 있었다…
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「그럼……, 이것으로 좋네요」
	PRINTFORMW 「자, 각오는 좋을까? 이것 마음껏 즐겁게 해 받기 때문에, 각오 해 줘」
	CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후…」")
	CLEARLINE 1
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT75_1, 페니스밴드 해제
;━━━━━━━━━━━━━━━━━━━━━━━━
;·몸에 대고 있는 페니스밴드를 떼어낸다
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 1
	PRINTFORMW 「응, 뭐야? 유감스러운 얼굴 해 버려」
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「누, 누가 그런!」")
	PRINTFORM 「후후, 뭐 좋아요. 그런 일로 해 두어 준다
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF