@KOJO_NO9_MS_ACT_K14_60
;-------------------------------------------------
;このACTはいずれ派生を増やしたいところ
;という事情もあるのでACT派生0で決めうち
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT60, スパンキング
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTACT(0)
		PRINTFORML 「さぁ、おしりをこっちに向けなさい、咲夜」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「な、何をするつもりよ」")
		PRINTFORML 「お仕置き…」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「は？ どういう事よ、それ…」")
		PRINTL
		PRINTFORMW 「だから言ったでしょう？ 貴女にお仕置きをしてあげるのよ、咲夜」
		PRINTFORMW 「でも安心して、手先の感覚は私の命、そんなに無茶は出来ないわ」
		PRINTFORMW 「と言っても、魔法で部分的に強化する事は可能なんだけど、ね…」
		RETURN 1
	ENDIF
	
	;=================================================
	;連続実行
	;=================================================
	IF 口上記入チェック(100) && TFLAG:ACT == TFLAG:前ACT
		;地の文の処理は本体側を殺してKOJO_TRAIN_MESSAGEでおこなう
		;KOJO_TRAIN_MESSAGEのRESULTはFLAG:地の文制御に代入されている
		;-------------------------------------------------
		;下半身服装が要押し退け
		;-------------------------------------------------
		IF 口上記入チェック(100) && PANTSCHECK(1, 1) && FLAG:地の文制御 == 2
			;服の上から
			IF LOWERJACKET(1) == 1
				;PRINTFORMDW アリスは%OUTER_B(MASTER, 0, 1)%の上から咲夜の尻を叩き続けた…
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「…っ！ くぅっ！」")
				PRINTFORMW 「悲鳴も上げずに良く頑張るわね？ 流石と言ったところかしら」
				PRINTFORMW 「貴女がどこまで耐えられるか試してあげるわ、咲夜…」
			;下着の上から
			ELSEIF LOWERUNDER(1) == 1
				;PRINTFORMDL アリスは%PANTY_DESCRIPTION(MASTER)%%INNER_B(MASTER)%の上から咲夜の尻を叩き続けた
				;PRINTFORMDW 灼けるような鋭痛は次第に快感へと変わり、咲夜の身体を蝕んでいく…
				;濡れてる
				IF PALAM:MASTER:潤滑 >= 500
					CALL QUAKE(1)
					CALL PRINTCOLOR(COLOR_K14("咲夜"), "「…っ！ くぅ…")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　…っ ふぁっ、んッ！」")
					PRINTFORML 「こんな事されてるのに、随分甘い声が出るのね、咲夜？」
					PRINTFORMW 「下着もそんなに汚しちゃって、なんてイヤラシイのかしら…」
				;その他
				ELSE
					;PRINTFORMDW アリスは%PANTY_DESCRIPTION(MASTER)%%INNER_B(MASTER)%の上から咲夜の尻を叩き続けている…
					CALL QUAKE(1)
					CALL PRINTCOLORW(COLOR_K14("咲夜"), "「…っ！ くぅっ！」")
					PRINTFORML 「なかなか頑張るわね、咲夜？」
					PRINTFORMW 「少し下着が濡れてきたみたいだけど、私の気のせいかしら。ふふっ」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		
		;地の文の処理は本体側を殺してKOJO_TRAIN_MESSAGEでRESULTを正の値にし、この箇所で行う
		;って、なぜこっちでやる事にしたのか、当時どのような思惑があったのか、良く判らないのでそのまま
		;---------------------
		;その他
		;---------------------
		IF PALAM:MASTER:潤滑 >= 500 && FLAG:地の文制御 == 1
			PRINTFORMDL アリスは剥き出しになった咲夜の尻を叩き続けている…
			PRINTFORMDL 真っ赤に腫れ上がった尻を叩かれる度、その身に刻みつけられる、
			PRINTFORMDW 苦痛と快楽が綯い交ぜとなった感覚に、咲夜は身体を震わせながら喘いでいる…
			PRINTL
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("咲夜"), "「…っ！ くぅ…")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　…っ ふぁっ、んッ！」")
			PRINTFORMW 「そう、いいわよ。もっと良い声で鳴いて頂戴…」
		ELSEIF FLAG:地の文制御 == 1
			PRINTFORMDL アリスは剥き出しになった咲夜の尻を叩き続けている
			PRINTFORMDW 咲夜は眉根を寄せ、必死に苦痛に耐えている…
			PRINTL
			CALL QUAKE(1)
			CALL PRINTCOLORW(COLOR_K14("咲夜"), "「…っ！ くぅっ！」")
			PRINTFORMW 「流石、この程度ではなんともないみたいね？」
			PRINTFORMW 「でも、おしりはもう真っ赤になっちゃったけど♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;性交中
	;=================================================
	IF 口上記入チェック(100) && TEQUIP:性交中
		CALL QUAKE(4)
		CALL PRINTCOLOR(COLOR_K14("咲夜"), "「あんっ、あっ、あっ")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　はぅっ！ あぁ…」")
		PRINTFORMW 「気持ち良さそうね、咲夜。ほら、もっとよくしてあげる…」
		PRINTFORMW 「貴女…、嫌いじゃないわよね？ こういうの。ふふ…」
		PRINTFORMDW アリスは身をくねらせ甘い声をあげる咲夜の双臀を強く叩き始めた…
		RETURN 1
	ENDIF
	
	;そういえば、KOJO_TRAIN_MESSAGEが実装されたのかこのあたりを書いている時だったか
	;=================================================
	;アイマスク装着
	;=================================================
	IF 口上記入チェック(100) && TEQUIP:アイマスク && FLAG:地の文制御 == 2
		;調教対象マゾっ気派生
		SELECTCASE ABL:MASTER:マゾっ気
			CASE IS >= 4
				;PRINTFORMDW 視界を奪われた咲夜は、被虐への妖しい期待にその身体を火照らせている…
				PRINTFORML 「ほら、言わなくても何をするか判るわよね？」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「わ、わかったわ…こう、で、いいかしら…」")
				PRINTFORMW 「ふふ、それでいいわ。さぁ、思いっきり愉しませてあげるわ！」
				PRINTFORMW
			CASEELSE
				;PRINTFORMDW 咲夜は視界を奪われ、不安感に苛まれている…
				CALL PRINTCOLORL(COLOR_K14("咲夜"), "「ねぇ、アリス…そこに居るんでしょう？」")
				PRINTFORMW 「大丈夫、ちゃんと居るわよ。じゃ、ちょっとおしりを向けて貰えるかしら？」
				CALL PRINTCOLORL(COLOR_K14("咲夜"), "「何をするつもりよ…」")
				PRINTFORMW 「ふふ、すぐに判るわよ咲夜。さぁ、いくわよ！」
		ENDSELECT
		RETURN 1
