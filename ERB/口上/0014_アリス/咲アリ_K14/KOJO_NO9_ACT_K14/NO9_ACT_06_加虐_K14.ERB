		;調教者満足度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				SELECTCASE ABL:MASTER:マゾっ気 ;調教対象マゾっ気派生
					CASE IS >= 4 ;マゾっ気4以上
						PRINTFORM 「さぁ、
						CALL SWEARWORDS_NO9(1)
						PRINTFORML にお仕置きの時間よっ！ って、咲夜？」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「なによ」")
						PRINTFORMW 「…なによって。何も言い返さないの？ ちょっと気味が悪いんだけど」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「…！？ ちょ、ちょっとぼんやりしてただけよ…」")
						CALL PRINTCOLOR(COLOR_K14("アリス"), "「ふふ、本当かしら？ ま、その答えはすぐに判るでしょうけど、ね…")
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					CASEELSE ;それ以外
						PRINTFORML 「この期に及んでじたばたするのは見苦しいわよ、咲夜」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「だ、誰がじたばたなんてしてるのよ！」")
						PRINTFORML 「そうだったかしら？ ふふ、まぁいいけど。でも…」
						PRINTFORMW 「悪いけど今、貴女を思いっきり鳴かせたい気分なの。覚悟して頂戴…」
				ENDSELECT
			CASE "一般"
				SELECTCASE ABL:MASTER:マゾっ気 ;調教対象マゾっ気派生
					CASE IS >= 4 ;マゾっ気4以上
						PRINTFORM 「じゃ、良い声で鳴いて貰うわよ。
						CALL SWEARWORDS_NO9(1)
						PRINTFORML さん…♪」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ねぇ、なんか凄い侮辱された気がするんだけど」")
						PRINTFORML 「気のせいじゃないかしら…？」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そうよね、アリスがそんな事…とか、私が言うとでも思った？」")
						PRINTL
						PRINTFORML 「別に？ 普通に怒ると思ってたわ」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「素でそう言われると、何も言う気が無くなるわ…」")
						CALL PRINTCOLOR(COLOR_K14("アリス"), "「それは結構な事ね…")
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
						PRINTFORMW 「じゃ、お遊びはここまで。そろそろいかせて貰うわ、咲夜…♪」
					CASEELSE ;それ以外
						PRINTFORML 「ほら、大人しくして。メス豚みたいに鳴かせてあげるわ！ って、違ったわね」
						PRINTFORMW 「……豚じゃなくて犬じゃないの。まったく私ったら、くすくす…」
						CALL PRINTCOLORL(COLOR_K14("咲夜"), "「ねぇ…なに一人で愉しそうにしてる訳？ 人をダシにして…」")
						PRINTFORMW 「なによ、怒ったの？」
						CALL PRINTCOLORL(COLOR_K14("咲夜"), "「あれだけ言われて、怒らない方がどうかしてるわよ！」")
						PRINTFORMW 「そう…冗談の通じない女ってホント嫌ね。つまらないわ、咲夜…」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「殴って良い？」")
				ENDSELECT
			CASE "低い"
				SELECTCASE ABL:MASTER:マゾっ気 ;調教対象マゾっ気派生
					CASE IS >= 4 ;マゾっ気4以上
						PRINTFORMW 「まだ何も言ってないのに自分からなんて、準備がいいわね、咲夜？」
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ち、違っ…！ これは…」")
						CALL PRINTCOLOR(COLOR_K14("アリス"), "「ふふ、すっかり取り乱しちゃって、可愛らしい事…")
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
