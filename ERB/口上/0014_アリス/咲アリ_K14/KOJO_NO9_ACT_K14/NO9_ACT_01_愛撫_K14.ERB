;
;		애무계 액션 구상을 정리한 파일
;
;	EXTRA 모드로 NO:9 사쿠야를 MASTER에 선택했을 경우만 표시
;
;Version		Reason
;1. 70			@구상 기입 체크 사양에. 그 외 섬세한 수정＆가필
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;애무계 ACT10～19 & 27
;========================================================================
;·TFLAG:ACT(10=손으로애무/11=가슴애무/12=입으로애무/13=애널애무/14=애널핥기/15=키스/16=성기를서로켠다/17=파이즈리한다/18=풋잡한다/19=손가락삽입해라/27=쓰러트린다)
;·TFLAG:ACT분류 == 1에 해당
;·ACT18, 풋잡은 이상계로부터 파생하는 경우 있어
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "애무")로 줍는 것이 가능
;·조교자가 조교 대상을 애무한다. 이 구상의 시점에서 조교자는 벌써 손을 대고 있다
;·미약을 사용하는 케이스가 있다
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는 구상측의 RESULT(종단의 RETURN 1 등)에 의해 구상이 표시되었는지 아닌지,
;·또 개소 마다의 특유의 거동(미약등 구상을 스킵 따위)을 제어하고 있습니다
;·@구상 기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는 구상측에서 결정하고 쳐 할 필요가 있습니다
;·특별한 이유가 없는 한, 텐프레로서는 RETURN 1을 기술해 둡니다
;-------------------------------------------------
;관련 함수…
;FIRSTACT
;첫회 ACT 판정 함수
;TFLAG:ACT(TFLAG:90)를 자동으로 참조, 현재 실행되고 있는 ACT 마다 번호가 독립해 보존된다
;-------------------------------------------------

;────────────────────────────────────
;NO9_ACT10, 손으로애무(ACT10_0, 애무하는/ACT10_1, 자위 하는/ACT10_2, 페니스밴드 자위)
;────────────────────────────────────
;·Ｃ중심으로 애무한다
;·조교 대상으로 페니스가 있는 경우, 및 페니스밴드장착 때에는 파생 커맨드가 된다
;·조교 대상이 자위중의 경우는 해제된다
;·조교자의 손에 더러움이 있는 경우 불쾌 SOURCE가 변동한다
;·조교자 기교, 애무, 기묘한손가락소질에 응해 발기도 상승
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_10
;-------------------------------------------------
;·TFLAG:ACT파생(0=애무/1=자위/2=페니스밴드 자위)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT10_0, 애무한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	[SKIPSTART]
	;=================================================
	;첫회
	;=================================================
	IF 구상 기입 체크(0) && FIRSTACT(0)
	ENDIF
	
	;=================================================
	;조교내 첫회
	;=================================================
	IF 구상 기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("오늘") ;오늘의 조교 방침 파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "벌"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "러브러브"
				PRINTFORMW
			CASEELSE ;그 외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;쓰러트리기 상태
	;=================================================
	IF 구상기입체크(100) && TEQUIP:쓰러트리기중
		IF TFLAG:ACT == TFLAG:전ACT ;연속 실행
			PRINTFORMW 「이대로 손으로 가게 해 버릴까♪」
			PRINTFORMW 「이봐요, 좋을 것입니다? 좀 더 기분 좋아져…」
		ELSE ;통상
			PRINTFORMW 「자, 나의 손가락으로 즐겨 줘」
			PRINTFORMW 「민감한 곳도 만져 주어요…  후후…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이갈것같다
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:갈것같다
		;전부터
		IF TCVAR:MASTER:위치전후 == 1
			SELECTCASE TCVAR:MASTER:애태움수 ;애태움수 파생 처리
				CASE IS > 1 ;애태움중
					PRINTFORMW 「간단하게 가 받아서는 곤란해요? 사쿠야」
					PRINTFORM 「좀 더 나를 즐겁게 해 줘
					CALL PRINT_SYMBOL_K14
				CASE 1 ;애태움 종료
					PRINTFORMW 「후후, 이렇게 부풀려 버려 이제 한계일까?」
					CALL PRINTCOLOR(COLOR_K14("앨리스"), "「슬슬 결정타를 찔러 주어요…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CASEELSE ;애태움 없음
					PRINTFORMW 「슬슬 때일까요?」
					CALL PRINTCOLOR(COLOR_K14("앨리스"), "「그러면, 조금 격렬하게 해 주어요…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		;뒤로부터
		ELSE
			SELECTCASE TCVAR:MASTER:애태움수 ;애태움수 파생 처리
				CASE IS > 1
					PRINTFORMW 「후후, 슬슬 갈것같다인 거네? 얼굴이 안보여도 알아요」
					PRINTFORM 「그렇지만 간단하게 가게 할 생각은인 것이군요. 좀 더 애태움이라고 준다
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「좋아요, 가세요 사쿠야. 나의 팔 중(안)에서가게해 주기 때문에…」
					PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 사쿠야를 절정으로 막상 되었다…
				CASEELSE
					PRINTFORMW 「후후, 이것으로는 도망칠 수 없네요?」
					PRINTFORMW 「이대로가게해 주기 때문에, 얌전하게 하고 있어…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;PALAM를 물게 해야 하는 것이라고 생각한다
	;=================================================
	;연속 실행
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신 복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			PRINTFORMW 「저기 사쿠야… 속옷 위로부터라도 아는 위 부풀어 올라 왔어요?」
			IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0 ;안면승마중
				PRINTFORMW 「아라, 미안해요. 이 상태는 말할 수 없네요? 후훗」
			ELSE ;그 이외
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요! 그런 것…」")
				CALL PRINTCOLOR(COLOR_K14("앨리스"), "「후후, 속여도 안돼")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  체는 정직한 것이니까」")
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;입지 않았다
		;---------------------
		PRINTFORMW 「대단히 오돌오돌 해 왔군요. 그렇게 기분 좋은 것일까, 저기 사쿠야?」
		IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0 ;안면승마중
			PRINTFORMW 「그렇다고 해도, 이 상태는 대답은 할 수 없을까요. 후후…」
		ELSE ;그 이외
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「지금 그런 일 (듣)묻지 않으면 좋네요…」")
			PRINTFORMW 「후후, 그렇구나. 생각이 잘 미치지 않아서 미안해요♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;전회Ｖ질내사정
	;=================================================
	;IF 구상 기입 체크(0) && TFLAG:전회질내사정 == 1
	;ENDIF
	
	;=================================================
	;아이마스크 장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:아이마스크
		PRINTFORMDL 앨리스는 시야를 빼앗긴 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%음핵을 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「호흡이 거치네요? 사쿠야. 눈가리개 되어 흥분하고 있을까」
		PRINTFORMW 「후후, 몇시, 어디를 손대어지는지 모른다는 것도 좋은 것」
		RETURN 1
	ENDIF
	
	;=================================================
	;클리캡 장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:클리캡
		PRINTFORMDL 앨리스는 클리캡 너머로 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%클리토리스를 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「이대로 만지작거리면, 당신 어떻게 되어 버릴까? 저기 사쿠야」
		PRINTFORMW 「후후후…」
		RETURN 1
	ENDIF
	
	;eraWIZ K12 나아가씨 구상에 인스파이어 된 개소
	;아무튼 Rev의 경우는 임의에 커맨드 선택할 수 있는 것이 아니고 이것이 성립하는 확률은 꽤 낮은일까w
	;=================================================
	;전ACT가 스팽킹
	;=================================================
	IF 구상기입체크(100) && PREVACTNAME() == "스팽킹"
		PRINTFORMDW 앨리스는 유카리 색의 타척의 자국을 소프트에, 그리고 열심히 더듬었다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼 파생
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응읏, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「당신에 있어서는 아픔도 쾌락인 거네? 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗! 얏, 싫어엇!」")
				PRINTFORMW 「아픈 것은 지금 가운데만… 곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전ACT가 채찍
	;=================================================
	IF 구상기입체크(100) && PREVACTNAME() == "채찍"
		PRINTFORMDW 채찍에 의해 사쿠야의 신체에 새겨진 상흔을, 앨리스는 섬세한 손가락 사용으로 살그머니 훑고 있다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼 파생
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응읏, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「당신에 있어서는 아픔도 쾌락인 거네? 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗! 얏, 싫어엇!」")
				PRINTFORMW 「아픈 것은 지금 가운데만… 곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;가설
	;=================================================
	;전ACT가 바늘
	;=================================================
	IF 구상 기입 체크(0) && PREVACTNAME() == "바늘"
	ENDIF
	
	;가설
	;줄은 해제의 경우도 있기 때문에 뭐라고곳
	;=================================================
	;전ACT가 줄
	;=================================================
	IF 구상 기입 체크(0) && PREVACTNAME() == "줄"
	ENDIF
	[SKIPEND]
	
	;=================================================
	;하반신 복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(100) && PANTSCHECK(1, 1)
		IF PALAM:MASTER:윤활 >= 500 ;윤활 충분히
			PRINTFORMW 「이제 이렇게 속옷을 더럽혀 버려, 기다릴 수 없었을까?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「달랏! 이, 이것은…」")
			PRINTFORMW 「후후, 부끄러워하지 않아도 괜찮아요. 젖고 있는 (분)편이 다양하게 적당한 거, 네♪」
		ELSE ;윤활 부족
			PRINTFORMW 「그다지 젖지 않네요?」
			PRINTFORMW 「좋아요, 곧바로 자신으로부터 속옷을 벗고 싶어질 정도로 자주(잘) 해 주기 때문에♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;위치전후 == 2가 되는 확립, 및 수고의 관점으로부터 이러한 모양에
	;RETURN 하지 않고 하층에 흘리면 상황에 따라서는 위화감이 나오므로 여기서 완결시킨다
	;=================================================
	;배후로부터
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:위치전후 == 2
		SELECTCASE TENSION(TARGET, 8) ;조교자 만족도 파생 처리
			CASE "높다"
				PRINTFORMW 「이봐요, 움직이지 마. 뒤로부터 귀여워해 주기 때문에」
				PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 애무하기 시작했다…
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏! 싫다, 앨리스… 이, 이런 건…」")
				PRINTFORMW 「그렇지만 싫지 않겠지요? 후후, 나에게는 알아요」
			CASE "일반"
				PRINTFORMW 「이봐요 어때? 뒤로부터 만지작거려지는 것은 좋을 것입니다?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요!」")
				PRINTFORMW 「그렇게는 말하지만…… 반응이 조금 다른거네요. 후훗」
			CASE "낮다"
				PRINTFORMW 「뒤로부터 껴안을 수 있으면 얌전해져 버리는 거네?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무슨 일인가, 나에게는 형편없구나…」")
				PRINTFORM 「후후, 별로 좋아요. 다만, 좋은 소리로 울어 주기만 하면, 말야
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자 상태변화 파생 처리
	;=================================================
	IF 구상기입체크(100)
		;-------------------------------------------------
		;정욕/울적/몽롱
		;-------------------------------------------------
		IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "정욕", "울적", "몽롱")
			SELECTCASE TENSION(TARGET, 8) ;조교자 만족도 파생 처리
				CASE "높다"
					IF PALAM:MASTER:윤활 >= 500 ;젖고 있다
						PRINTFORMW 「만지작거리기를 원했으면, 솔직하게 말하면 좋은데는 나는 생각하지만. 저기 사쿠야?」
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫!? 무슨 말을 하고 있는지 전혀 알지 않아요…」")
						PRINTFORM 「후후… 당신은 알지 않아도, 여기는 갖고 싶고 이제 기다릴 수 없는 것 같지만, 말야…
						CALL PRINT_SYMBOL_K14
					ELSE ;젖지 않았다
						PRINTFORM 「자, 색정광의 변태 메이드에게 포상을 주어요! 마음껏 즐겨 줘
						CALL PRINT_SYMBOL_K14
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그 발언의 내용은 차치하고… 뭐 그 음울한 하이 텐션은…」")
						CALL PRINTCOLOR(COLOR_K14("앨리스"), "「별로 좋지 않아. 내가 즐기고 있는 것은 당신에 있어서도 나쁜 일이 아닐 것이야, 사쿠야…")
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					ENDIF
				CASE "일반"
					IF PALAM:MASTER:윤활 >= 500 ;젖고 있다
						PRINTFORMW 「이미 이렇게 적시고 있는 거네, 사쿠야. 추잡한 것일까…」
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일, 하나 하나 말하지 않으면 좋네요…」")
						PRINTFORM 「후후, 방법 없지요. 정말의 일인 것이니까
						CALL PRINT_SYMBOL_K14
					ELSE ;젖지 않았다
