;-------------------------------------------------
;・TFLAG:ACT派生(0=指挿入れ/1=Ｇスポット刺激/2=バイブ出し入れ)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT19_1, Ｇスポット刺激
;━━━━━━━━━━━━━━━━━━━━━━━━
;イきそうor快Ｃが5000以上の場合、調教者の愛撫スキルに応じてＧスポット刺激
;Ｇスポット責めに派生すると前立腺刺激扱いになる
;-------------------------------------------------
IF 口上記入チェック(100) && TFLAG:ACT派生 == 1 && FLAG:地の文制御 == 2
	;=================================================
	;後ろから
	;=================================================
	IF 口上記入チェック(100) && TCVAR:MASTER:位置前後 == 2
		;調教者満足度派生
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「後ろから抱かれると大人しくなっちゃうのね？」
				PRINTFORM 「期待してるのかしら……、ふふ、可愛らしいわ
			CASE "一般"
				PRINTFORMW 「さて、これで動けないわよね？ ふふ」
				PRINTFORM 「後ろから弄ってあげるから、大人しくして頂戴
			CASE "低い"
				PRINTFORMW 「動かないで……別に痛めつけようって訳じゃないから」
				PRINTFORM 「大人しくしてれば可愛がってあげるわ
		ENDSELECT
		;状態に応じたシンボルを表示して括弧を閉じる
		CALL PRINT_SYMBOL_K14
		;一行空ける
		PRINTL
		;潤滑派生
		SELECTCASE PALAM:MASTER:潤滑
			CASE IS > 1000
				PRINTFORMW 「これだけ濡れてると指もあっさり飲み込んじゃうわね？」
				PRINTFORMW 「…ん、ここね。ふふ、指先でじっくり嬲ってあげる」
			CASE IS > 500
				PRINTFORMW 「ん、ザラザラしてる……此処ね。ふふふ…」
				PRINTFORMW 「ほらどう？ 擦るよりちょっと圧迫する位がいいのよね？」
			CASEELSE
				PRINTFORMW 「あまり濡れてないからちょっと痛いかしら？」
				PRINTFORMW 「でも、ふふ……ここ弄られると堪らないのよね？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;前から
	;=================================================
	IF 口上記入チェック(100) && TCVAR:MASTER:位置前後 == 1
		;調教者満足度派生
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「さ～て、今更嫌なんて言わせないわよ？ ふふ、そう……、私に良く見えるように」
			CASE "一般"
				PRINTFORMW 「さぁ、大人しく足を開いて。そう、それでいいわ…」
			CASE "低い"
				PRINTFORMW 「ほら、もっと足を開きなさい。手間をかけさせないで頂戴…」
		ENDSELECT
		;潤滑派生
		SELECTCASE PALAM:MASTER:潤滑
			CASE IS > 1000
				PRINTFORMW 「あら、はしたないわね？ こんなに濡らしちゃって。でも…」
				PRINTFORMW 「中弄るならこの位がいいのよねぇ。ふふ」
				PRINTFORMW 「ほらどう？ 円を描くように圧迫されると堪らないわよね？」
			CASE IS > 500
				PRINTFORMW 「…ん、ここね。プックリ膨らんじゃって判りやすいわ」
				PRINTFORMW 「ねぇどうかしら？ おしっこ漏れそうな感じがするでしょう？」
			CASEELSE
				PRINTFORMW 「ちょっとお湿りが足りないみたい……、少し痛いかもしれないわ」
				PRINTFORMW 「でも少しずつ慣らせば……。ふふっ、やっぱりここは弱いみたいね」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT19_2, バイブ出し入れ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 2
	;Ｖ感覚派生
	SELECTCASE ABL:MASTER:Ｖ感覚
		CASE IS > 6
			PRINTFORMW 「ほらここ？ ここがいいのよね？」
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), @"%RANDTEXT_K14("ぐちゅっ、ぐちゅっ", "じゅぷ、じゅぷっ")%…")
			CLEARLINE 1
			PRINTFORMW 「こんなはしたない音させちゃって……、聞くまでもなかったかしら」
			PRINTFORM 「いいのよ？ 思いっきり乱れちゃって。発情したメス豚みたいに鳴かせてあげるから…
			CALL PRINT_SYMBOL_K14
		CASE IS > 3
			PRINTFORMW 「ほら、聞こえるかしら……この音」
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), @"%RANDTEXT_K14("ぐちゅっ、ぐちゅっ", "じゅぷ、じゅぷっ")%…")
			CLEARLINE 1
			PRINTFORMW 「フフ……本当、イヤラシイ音ね？ 私まで変な気持ちになっちゃうわ」
			PRINTFORMW 「さぁて、少し激しくシてあげようかしら？ いい声で鳴いて頂戴……」
		CASEELSE
			PRINTFORMW 「どう？ まだ慣れない感じかしら」
			PRINTFORM 「ふふ、大丈夫……すぐに慣れるわ。そのうち自分からおねだりしちゃうくらいに、ね
			CALL PRINT_SYMBOL_K14
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT19_0, 指挿入れ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生 && FLAG:地の文制御 == 2
	;=================================================
	;顔面騎乗中
	;=================================================
	IF 口上記入チェック(100) && TRUECHECK(TEQUIP:顔面騎乗, TEQUIP:顔面騎乗アナル) > 0
		IF GROUPMATCH(PREVCOM, 6, 8, 15, 42, 43)
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("クンニ"), 2))
			CLEARLINE 1
		ENDIF
		;前回COM派生
		SELECTCASE TRAINNAME:PREVCOM
			CASE "快感を受け入れる", "積極的やる", "愛情を込めてやる"
				PRINTFORMW 「%RANDTEXT_K14("んッ", "んくっ")%！ そう……それでいいわよ」
				PRINTFORMW 「ちゃんと言う事を聞いてれば可愛がってあげる。私の指で、ね」
			CASE "快感を我慢する", "消極的やる"
				PRINTFORMW 「上辺だけ従った振りしてどうにかなると思ってるなら大間違いよ…」
				PRINTFORMW 「ま、それはそれとして……、私は私で愉しませて貰うから…」
			CASE "もっとハードにして", "気持ち良くして"
				PRINTFORMW 「そう……悪くないわよ。素直にしてれば可愛がってあげるわ」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%の望む通りに、ね」
			CASE "拒否する", "暴れる"
				PRINTFORMW 「本当……可愛くないわ。逆らったところで無駄なのが判らないのかしら」
				PRINTFORMW 「まぁ勝手にすれば？ 私も好きなようにさせて貰うから……」
			;その他
			CASEELSE
				PRINTFORMW 「悪くないわね……、そのまま続けて頂戴」
				PRINTFORMW 「大人しくしてれば、ちゃんと可愛がってあげるから…」
		ENDSELECT 
		RETURN 1
	ENDIF
	
	;箇所によってPRINTをFORM展開するか否か、時々考えないではないけど効果の程が知れてるのでなんとも
	;手間の割りに報われないすぎるんであまり気は進まない。置換で大量にエラー吐いたりするしｗ
	;=================================================
	;後ろから RANDACT(0)
	;=================================================
	IF 口上記入チェック(100) && TCVAR:MASTER:位置前後 == 2
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORM 「こうすればもう逃げられないわよね？
			CASE 1
				PRINTFORM 「ほら、大人しくして。このまま弄ってあげる……
		ENDSELECT
		PRINTFORM 　って、
		;潤滑派生
		SELECTCASE PALAM:MASTER:潤滑
			CASE IS > 1000
				PRINTFORMW こんなに濡らしちゃって……」
				PRINTFORMW 「ほらどう？ これが欲しかったんでしょう？ 待ちきれなかったんでしょう？」
				PRINTFORMW 「正直に言いなさい……」
			CASE IS > 500
				PRINTFORMW こんなにしっとりさせちゃって……」
				PRINTFORMW 「私に弄られるのが待ちきれなかったのかしらね？」
			CASEELSE
				PRINTFORMW まだ濡れてないようだけど……」
				PRINTFORM 「大丈夫……、しっかり濡れるまで優しくシてあげるわ
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		;相思相愛
		IF AFFECTION("相思相愛")
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("ふふふ", "くすくす", "クスクス")%……」")
		;その他
		ELSE
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("フフフ", "ウフフ")%……」")
		ENDIF
		CLEARLINE 1
		RETURN 1
	ENDIF
	
	;PANTSCHECKについてはあとから考える事に
	;=================================================
	;前から
	;=================================================
	IF 口上記入チェック(100) && TCVAR:MASTER:位置前後 == 1
		;調教者満足度派生
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「さて、ご対面といったところかしら？ ふふ…」
			CASE "一般"
				PRINTFORMW 「ほら、もっと足を開いて。そう……、いいわよ」
			CASE "低い"
				PRINTFORMW 「もっと足を開きなさい。それじゃ弄り難いわ」
		ENDSELECT
		;潤滑派生
		SELECTCASE PALAM:MASTER:潤滑
			CASE IS > 1000
				PRINTFORMW 「！？ なによ、もうビショビショじゃない。イヤラシイわね……」
				PRINTFORMW 「わかったわ……お望み通り、思いっきり掻き回してあげる…」
			CASE IS > 500
				PRINTFORMW 「あら？ もう濡れてるじゃない。お待たせしちゃったかしら、ね」
				PRINTFORMW 「そうよ、力を抜いて……私の指を受け入れて…」
			CASEELSE
				PRINTFORMW 「ん、まだ濡れてないようね？ ま、いいわ」
				PRINTFORMW 「入り口から少しずつ慣らしていってあげるから……」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

