					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「벼, 별로 그런 일! 응읏, 응쿠……」")
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%, 후후, 좋으니까 얌전하게 하고 있어…」
					;페이드 아웃
					CALL FADE(22, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
					CLEARLINE 1
			ENDSELECT
			CALL PRINT_REACT_15_01
			RETURN 1
