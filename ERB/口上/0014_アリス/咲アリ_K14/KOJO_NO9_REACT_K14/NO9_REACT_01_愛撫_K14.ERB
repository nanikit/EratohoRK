;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;V1. 70의 손질도 이 근처에서 뭔가 괴로워져 왔으므로 이후, 부실
;REACT계는 머지않아 기회를 봐, 라는 것으로

;===================================================
;파일 개요…
;NO9_REACT10, "손으로애무"
;	PREVCOM:스팽킹 or 채찍 PERCENT(100)
;		SELECTCOM:쾌감을참는다
;		SELECTCOM:쾌감을받아들인다
;	SELECTCOM:부끄러워하는 PERCENT(65)
;	SELECTCOM:서투르다고매도한다 PERCENT(65)
;	SELECTCOM:쾌감을참는다 PERCENT(65)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT11, "가슴애무"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	ACT파생:유두 들이마셔
;		SELECTCOM:쾌감을참는다 PERCENT(100)
;		SELECTCOM:쾌감을받아들인다 PERCENT(100)
;	ACT파생:몸싸움
;		SELECTCOM:쾌감을참는다 PERCENT(100)
;		SELECTCOM:쾌감을받아들인다 PERCENT(100)
;	ACT파생:유두 고문
;		SELECTCOM:쾌감을참는다 PERCENT(100)
;		SELECTCOM:쾌감을받아들인다 PERCENT(100)
;		TFLAG:REACT분류:허가를청한다 PERCENT(100)
;	ACT파생:통상의 가슴애무
;		SELECTCOM:쾌감을참는다 PERCENT(100)
;		SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT12, "입으로애무"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	SELECTCOM:쾌감을참는다 PERCENT(100)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT13, "애널애무"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	SELECTCOM:쾌감을참는다 PERCENT(100)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT14, "애널빨고"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	SELECTCOM:쾌감을참는다 PERCENT(100)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT15, "키스"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	SELECTCOM:쾌감을참는다 PERCENT(100)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT16, "성기를서로켠다"
;	SELECTCOM:부끄러워하는 PERCENT(100)
;	SELECTCOM:서투르다고매도한다 PERCENT(100)
;	SELECTCOM:쾌감을참는다 PERCENT(100)
;	SELECTCOM:쾌감을받아들인다 PERCENT(100)
;NO9_REACT17, "파이즈리한다" ※미사용
;NO9_REACT18, "풋잡한다" ※미사용
;NO9_REACT27, "밀어넘어뜨린다"
;COM 파일이 완성하면 개소에 따라서는 확률을 변경해, REACT와 COM로 서로 보완하는 형태로 할 예정
;이 근처의 조정 작업은 귀찮지만 싫지 않아. 오히려 즐겁다
;===================================================

;========================================================================
;애무계 REACT10～19 & REACT27
;========================================================================
;· 각 REACT내의 나누는 방법으로서 TFLAG:REACT분류(IS_COMGRONAME)나 SELECTCOM(IS_NOWCOMNAME)가 생각됩니다
;·다만, 범위의 넓은 조건으로 주워 버리는 것은 기본적으로 추천하지 않습니다
;·ACT분류:애무계의 경우, 기본적으로 애무계 COM(COM10~COM16)가 선택지가 됩니다만
;·상황에 따라서는 그 회화계, 고통계, 그 외의 것이 선택지에 포함될 가능성도 있습니다
;·리액션(REACT)으로 처리되지 않았던 것은 범용 리액션(COM)으로 보완되므로
;·무리하게 모두 묻을 필요는 없습니다
;·필요라고 생각하는 것을 좁혀, COM와의 제휴를 생각해 효율 좋게 구축해야 합니다
;-------------------------------------------------
;· 각 REACT 및 ACT파생내는,&& IS_NOWCOMNAME든지,&& IS_COMGRONAME등의 조건으로 결정하고 쳐 합시다
;·여기에 조건 없음 분기가 있으면, 리액션에 관련되지 않고, 모두 주워 버려 범용 리액션에 흐르지 않습니다
;·요컨데, 상정된 시스템의 파괴에 연결됩니다
;-------------------------------------------------
;  기본적으로 지문은 TFLAG:REACT분류(받아들인다/적극적 따른다/태연하게 따른다/허가를청한다/소극적 따른다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운 행동/무리가 없는 행동/무리를 한 행동)으로 파생
;  ACT_MESSAGE.ERB를 참조하면서의 작업을 추천합니다
;-------------------------------------------------
;=================================================
;관련 변수/함수등…
;TFLAG:REACT인상, 각 SELECTCOM의 상대에게로의 심상(0=영향없음/1=좋은인상/2=약간좋은인상/3=나쁜인상/4=약간나쁜인상/5=0으로부터 원 자리인가
;TFLAG:ACT파생 해당 ACT에서의 패턴 변화
;
;TFLAG:REACT분류 (0=없음/1=소극적 따른다/2=태연하게 따른다/3=적극적 따른다/4=받아들인다/5=허가를청한다/6=날뛴다/7=요구에 응하지 않는, 거부/8=도망치는, 발버둥 친다)
;IS_COMGRONAME("없음/소극적 따른다/태연하게 따른다/적극적 따른다/받아들인다/허가를청한다/날뛴다/거부/도망친다")
;
;FIRSTREACT
;첫회 ACT 판정 함수
;TFLAG:ACT(TFLAG:90)를 자동으로 참조, 현재 실행되고 있는 REACT 마다 번호가 독립해 보존된다
;
;ONCEREACT
;조교내미독판정 함수  조교전에 초기화된다
;그 외의 사양은 거의 FIRSTREACT와 같이
;=================================================

;─
;NO9_REACT10, 손으로애무·개별 리액션
;─
;·TFLAG:ACT파생(0=애무/1=자위/2=페니스밴드 자위)
;-------------------------------------------------
;상정되는 COM…
;회화계 반응
;	COM:좀더하드하게해
;애무계 반응
;	COM:부끄러워한다/싫어한다/서투르다고매도한다/쾌감을참는다/쾌감을받아들인다/기분좋다！
;고통계 반응
;	COM:날뛴다
;-------------------------------------------------
;애무의 지문은 ACT_MESSAGE에는 존재하지 않고, REACTION_MESSAGE로 COM마다 처리된다
;자위의 지문은 ACT_MESSAGE에 존재하지만, 대상 한정에는 무관계
;(IS_NOWCOMNAME("쾌감을참는다") /SELECTCOM == 14)는 기본적으로 REACT분류 == 1("소극적 따른다")이지만
;확률로 REACT분류 == 3("허가를청한다")에 파생하는 경우 있어. 지문도 거기에 맞추어 변화한다
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_10
;-------------------------------------------------
;TFLAG:ACT파생 == 2의 테두리를 마련할 필요가…
;우선! TFLAG:ACT파생를 설정해 범용에 던진다
;-------------------------------------------------

;TFLAG:REACT부하 == 2의 무리를 한 행동은 연주한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;전ACT가 스팽킹 or 채찍
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_PREVACTNAME("스팽킹/채찍")
	;REACT분류가 "허가를청한다"에 파생하면 COM에 날리는 상정
	;=================================================
	;쾌감을참는다
	;=================================================
	IF 구상기입체크(100) && (IS_NOWCOMNAME("쾌감을참는다") && TFLAG:REACT부하 < 2) && IS_COMGRONAME("소극적 따른다")
		PRINTFORMDL 앨리스는 사쿠야의 반응을 물으면서\@ IS_PREVACTNAME("스팽킹") ? 타척의 자국을 # 채찍의 상흔을 \@정중하게 더듬었다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼 파생
			CASE IS > 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  응″응! 아웃, 아아…」")
				PRINTFORMW 「참아도 달콤한 소리가 나오는 것은 억제할 수 없는 것 같구나? 후후」
			CASEELSE
				PRINTFORMW 「이봐요, 어때? 아직 아플까?」
				CALL QUAKE(1)
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응″! 우웃, 아아…」")
				PRINTFORMW 「그 반응, 아픔 뿐이 아닌 것 같구나? 참아도, 나에게는 알기 때문에…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을받아들인다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을받아들인다") && TFLAG:REACT부하 < 2
		PRINTFORMDW 앨리스는 사쿠야의 반응을 물으면서\@ IS_PREVACTNAME("스팽킹") ? 타척의 자국을 # 채찍의 상흔을 \@섬세한 손가락 사용으로 더듬었다…
		CALL QUAKE(1)
		SELECTCASE ABL:MASTER:마조끼 ;마조끼 파생
			CASE IS > 4
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응″")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  나닷! 어째서, 기분 좋고!」")
				PRINTFORMW 「후후, 신경쓰는 일 없어요. 솔직하게 쾌락을 받아들이세요, 사쿠야…」
			CASEELSE
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  만나는, 아아…」")
				PRINTFORMW 「사쿠야에게는 아픈 것도 기분 좋은 것도 함께 같구나. 이봐요, 좀 더 자주(잘) 해 주어요…」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;NO9_REACT10, 손으로애무

;TEQUIP 나름으로 COM에 흘린다. ACT파생 0로 결정하고 쳐 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;부끄러워하는 75%
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(75) && (!TFLAG:ACT파생 && IS_NOWCOMNAME("부끄러워한다") && (!TEQUIP:아이마스크 || !TEQUIP:대거울 || !TEQUIP:야외플레이))
	;=================================================
	;쾌감에 대해서 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && TFLAG:REACT파생 == 1
		PRINTFORMDW 앨리스는 수치에 몸부림치는 사쿠야를 응시하면서, 섬세한 손가락 사용으로 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%쾌락의 원천을 애무하고 있다…
		PRINTL
		;페이드 아웃
		CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("애무"), 1)%")
		CLEARLINE 1
		CALL QUAKE(1)
		;조교자 만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아, 아읏! 야… 부탁, 앨리스. 보지 말고…」")
				PRINTFORMW 「싫어요. 이런 사랑스러운 사쿠야, 좀처럼 볼 수 없는 것…」
				PRINTFORMDW 앨리스는 수치에 눈동자를 물기를 띠게 하는 사쿠야의 머리를 어루만지면, 얼굴을 대고 상냥하게 입맞추고 했다…
				CALL SET_KISS
			CASE "일반"
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「핫, 아앗! 싫다, 앨리스! 이, 이런 건…」")
				PRINTFORM 「후후, 아직도 시초야. 좀 더 부끄러운 일 해 주기 때문에…
				CALL PRINT_SYMBOL_K14
			CASE "낮다"
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗, 응읏! 얏! 앨리스, 그런 곳…」")
				PRINTFORMW 「부끄러워하는 일 같은거 없는 것, 사쿠야. 좀 더 솔직하게 받아들여…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;지금의 상황에 대해서 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && !TFLAG:REACT파생
		PRINTFORMDW 앨리스는 수치에 몸부림치는 사쿠야를 응시하면서, 섬세한 손가락 사용으로 사쿠야의%CLITORIS_DESCRIPTION(MASTER)%쾌락의 원천을 애무하고 있다…
		;조교자 만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「사쿠야라는거 의외로 신선한 거네? 적합하지 않은 생각도 들지만, 그러한 것도 나쁘지 않아요」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄럽네요…」")
				PRINTFORM 「후후, 사랑스럽다. 자, 이 앨리스씨가 좀 더 부끄러운 일 해 주어요…
				CALL PRINT_SYMBOL_K14
			CASE "일반"
				PRINTFORMW 「저기 사쿠야, 가르쳐 줘. 도대체 무엇이 그렇게 부끄러운거야?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 일 말할 수 있는 것…」")
				PRINTFORM 「그래, 그렇지만… 지금부터 좀 더 부끄러운 일 해 주기 때문에, 각오 해 줘…
				CALL PRINT_SYMBOL_K14
			CASE "낮다"
				PRINTFORMW 「저기 사쿠야, 나에게 손대어지는 것이 그렇게 부끄러워?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「드, 듣지 말아요. 그런 일…」")
				PRINTFORM 「후후, 좋아요. 더 이상은 (듣)묻지 않고 두어 준다. 지금은, 말야…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
RETURN 0
ENDIF
;NO9_REACT10, 손으로애무

;부하 2는 연주한다. ACT파생 0로 결정하고 쳐 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;서투르다고매도한다 85%
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(85) && !TFLAG:ACT파생 && (IS_NOWCOMNAME("서투르다고매도한다") && TFLAG:REACT부하 < 2)
	;=================================================
	;미숙한 테크닉에 대해서
	;=================================================
	IF 구상기입체크(100) && TFLAG:REACT파생 == 0
		;조교자 만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「… 아휴,. 당신을 만족시키는 것은 편하지 않아요」
