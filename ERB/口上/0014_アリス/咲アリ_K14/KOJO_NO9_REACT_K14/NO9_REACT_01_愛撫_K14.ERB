	;=================================================
	;調教対象がイきそう RANDREACT(0)(1)(2)
	;=================================================
	;確率で下位の分岐に流す。書いた分は漏れなく、無駄なく機能してもらう構え
	;-------------------------------------------------
	IF 口上記入チェック(75) && TCVAR:MASTER:イきそう
		CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("愛撫"), 1)%")
		CLEARLINE 1
		CALL QUAKE(2)
		;焦らし度派生処理
		SELECTCASE TCVAR:MASTER:焦らし度
			;焦らし中
			CASE IS > 1
				SELECTCASE RANDREACT(2, 0)
					CASE 0
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「んんっ！ はっ、ん…んくっ…」")
						PRINTFORMW 「ふふ、必死に我慢しちゃって。イきたくないのかしら？」
