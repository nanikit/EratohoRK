			;페이드 아웃
			CALL FADE(22, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
			CLEARLINE 1
			CALL PRINT_REACT_15_01
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;대면좌위
		;-------------------------------------------------
		IF 구상기입체크(100) && TRUECHECK(TEQUIP:성교중 == 4, TEQUIP:성교봉사중 == 4) > 0
