						CALL PRINT_SYMBOL_K14
					CASE 1
						CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あぅっ、んんっ！ ふっ、くぅっ…」")
						PRINTFORMW 「腰まで浮いちゃってるのに頑張るわね、咲夜？」
						PRINTFORM 「それなら貴女がどこまで耐えられるか、じっくり試してあげるわ…
						CALL PRINT_SYMBOL_K14
				ENDSELECT
			;焦らし終了
			CASE 1
