		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「자주(잘) 말해요… 어차피 그것이라도 억지 쓰고인 것이지요」")
		PRINTFORMW 「잘 알고 있지 않아. 뭐, 그런 (뜻)이유로……, 결론짓고 생각해 줘」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「정말, 귀찮구나……」")
		RETURN 1
	ENDIF
	
	;지루함은 조교 종료 직전에 자주(잘) 보는 인상
	;조금 바리에이션을 늘려도 좋은 것인지도 모른다
	;귀찮지만
	;=================================================
	;지루함
	;=================================================
	IF 구상기입체크(100) && T_COND("지루함")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), @"「%KOJO_RAND_K14(("키스"), 2)%」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 그것. 자신으로부터 키스 해 둬, 의지 없다든가……」")
		PRINTFORML 「왜냐하면……」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「왜냐하면, 뭐야」")
		PRINTFORMW 「사쿠야의 키스는 지루함 하고」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「맞고 싶은거야?」")
		RETURN 1
	ENDIF
	
	;TARGET에 관해서는 현상, 우선 보는 일은 없는 것이 뭐라고도
	;M_COND 씹게 하는 것도 좋은 것인지도. 귀찮지만
	;=================================================
	;피폐/쇠약/무기력
	;=================================================
	IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "피폐", "쇠약", "무기력") > 0
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 상당히 일시 모면적인 키스군요」")
		PRINTFORMW 「나 피곤해. 그 정도 아세요, 바보 사쿠야」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…… 말해 주어요. 사람을 바보 불러, 무료로 끝난다고 생각해?」")
		PRINTFORMW 「생각하고 있었어요」
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…… 이중의 의미로 바보 취급 당한 기분이예요. 사람이 흉내내는 것은 그만두어 받을 수 있을까」")
		PRINTFORMW 「싫어요. 왜냐하면 사쿠야의 반응이 재미있어서, 후훗…」
		RETURN 1
	ENDIF
	
	;적극적 반응이 나오는 무렵에는 BASE치도 그 나름대로 변동하는 것이라고 상정하고 있다
	;=================================================
	;통상
	;=================================================
	IF 구상기입체크(100) && T_COND("통상")
		;만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
