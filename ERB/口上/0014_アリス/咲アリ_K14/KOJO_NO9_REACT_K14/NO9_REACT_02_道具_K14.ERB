;
;Version		Reason
;1. 70			@구상 기입 체크 사양에. REACT20가 없었다(w 의 것으로 추가. 그 외 섬세한 수정＆가필등
;1. 65			GET_COM_RANK() == "무리를 한 행동"의 경우 COM에 흐르도록(듯이)
;1. 62			@IS_NOWCOMNAME의 신설에 수반해 SELECTCOM를 IS_NOWCOMNAME로 변경
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;도구계 REACT20～26
;========================================================================
;  REACT의 지문은 ACT_MESSAGE.ERB의 TRAIN_MESSAGE2_{TFLAG:ACT}로 처리됩니다
;  REACT에 따라서는 전용 처리가 존재하지 않고, @REACTION_MESSAGE에서의 범용 처리만되고 있습니다
;  기본적으로 지문은 TFLAG:REACT분류(받아들인다/적극적으로 한다/태연하게 한다/용서를빈다/소극적으로 한다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운 행동/무리가 없는 행동/무리를 한 행동)으로 파생
;-------------------------------------------------
;이렇게 해 되돌아보면, 도구계 REACT는 부실입니다
;-------------------------------------------------
;────────────────────────────────────
;NO9_REACT20, 로터
;────────────────────────────────────
;·ACT파생없음
;-------------------------------------------------
;상정되는 COM…
;애무계 반응
;	COM:부끄러워한다/싫어한다/서투르다고매도한다/쾌감을참는다/쾌감을받아들인다/기분좋다！
;고통계 반응
;	COM:날뛴다
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_20
;-------------------------------------------------
;·지문은 REACT분류(받아들인다/적극적으로 한다, 태연하게 한다/용서를빈다/소극적으로 한다)로 분기 한다
;·REACT분류:받아들이고는 TCVAR:MASTER:갈것같다인가 아닌가로 분기 한다
;-------------------------------------------------
;REACT20의 테두리가 존재하고 있지 않는 것에 지금쯤 깨닫는다
;그런 (뜻)이유로 우선 날조한다
;-------------------------------------------------

LOCALS = \@ AFFECTION("몹시 사랑함") ? 킥킥 # 후후후 \@

;━━━━━━━━━━━━━━━━━━━━━━━━
;부끄러워한다（REACT분류:소극적으로 한다）
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생로 나누지 않고 정리해 버린다
;TRAIN_MESSAGE2, REACTION_MESSAGE와의 균형으로 그러한 결론에 이른 나름
;ACT_MESSAGE도 정직, 좀 더 어떻게든 할 필요가 있도록(듯이) 생각한다. 뭐 우선도는 낮지만
;-------------------------------------------------
IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운 행동", "무리가 없는 행동"
			;쾌감에부끄러워한다		이 개소만 REACT파생를 본다
			IF TFLAG:REACT파생 == 1
				PRINTFORM 「후후,%CNAME(MASTER, TARGET)%가 그런%RAND_SPLIT("초심//우브")%인 반응하다니
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「그렇지만, 그렇구나. 나로서는 좀 더 사랑스러운 소리를 들려주었으면 좋은 곳이지만」
				PRINTFORMW 「이봐요, 사실은 기분 좋겠지요? 좀 더 자주(잘) 해 주어요」
			;상황에부끄러워한다
			ELSEIF !TFLAG:REACT파생
				PRINTFORMW 「아라, 사랑스럽네요. 나의 시선에부끄러워한다는」
				PRINTFORM 「그렇지만 안 돼요.%CNAME(MASTER, TARGET)%는부끄러워한다와 개, 좀 더%RAND_SPLIT("나에게/자주(잘)")%보여 줘
				CALL PRINT_SYMBOL_K14
			ENDIF
		CASE "무리를 한 행동"
			IF COMCOR_POSI()
				PRINTFORMW 「%CNAME(MASTER, TARGET)%, 그런 얼굴 하고 있지만…」
				PRINTFORMW 「사실은 좀 더 기분 좋아지고 싶은 것이 아닌거야?」
				PRINTFORMW 「후후, 거짓말 해도 안 돼요. 어딘지 모르게 알아 버리는거네요, 그러한 것」
			ELSE
				PRINTFORMW 「후훗, 싫지 않아요. 그러한 반응. 다만…」
				PRINTFORM 「그런%CNAME(MASTER, TARGET)%를 보고 있으면 나……, 조금 심술쟁이 하고 싶어져 버리, 지만
				CALL PRINT_SYMBOL_K14
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF

;는 REACT보다 COM 방향인 생각이 드는구나w
;━━━━━━━━━━━━━━━━━━━━━━━━
;싫어한다（싫어했지만 방해하지 않고）REACT분류:소극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 1, 싫어해 ACT 방해는 COM로
;-------------------------------------------------
IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다") && !TFLAG:REACT파생
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운 행동"
			PRINTFORMW 「그런 아이같은 일 말해져도 곤란한거네요」
			PRINTFORMW 「모처럼%CNAME(MASTER, TARGET)%를 위해서(때문에) 준비한 것이고. 뭐, 그런 (뜻)이유이니까…」
			PRINTFORMW 「얌전하게 하고 있어 줘」
		CASE "무리가 없는 행동"
			PRINTFORMW 「그런 식으로 싫어할 수 있으면, 더욱 더 하고 싶어지는거네요」
			PRINTFORMW 「혹시%CNAME(MASTER, TARGET)%, 그것을 기대하고 있다든가?」
			PRINTFORM 「뭐, 어쨌든……, 멈출 생각은 없어요. 나쁘지만\@ AFFECTION("몹시 사랑함") ? , 군요 # \@
			CALL PRINT_SYMBOL_K14
		CASE "무리를 한 행동"
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				PRINTFORMW 「정말로 싫으면, 좀 더 저항해 보이면 좋은데?」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%, 실은 상당히 즐기고 있는 것이 아닐까」
				PRINTFORM 「후훗, 거짓말해도 쓸데없어요. 왜냐하면%CNAME(MASTER, TARGET)%, 알기 쉽다 것이, 군요…
			;실은 싫어
			ELSE
				PRINTFORMW 「싫다던가 말하고 있는 것 치고는, 상당히 여유가 없는 것 같지만 말야」
				PRINTFORMW 「정말로 싫으면, 좀 더 분명히 말하면 어때?」
				PRINTFORM 「뭐, 어느 쪽으로 하든…, 나는 멈출 생각 같은거 없지만
			ENDIF
			CALL PRINT_SYMBOL_K14
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;서투르다고매도한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("서투르다고매도한다")
	;=================================================
	;강한척 해（REACT분류:소극적으로 한다）
	;=================================================
	IF 구상기입체크(100) && TFLAG:REACT파생 == 1
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「이런 것은 느끼지 않는, 저기? 그러면, 어떻게 하면 느낄까～응이, 라고」
				PRINTFORMW 「사실은 느끼고 있는 주제에, 솔직하지 않아요. 뭐 그렇지만……」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%의 그러한 곳, 나, 싫지 않아요?」
			CASE "무리가 없는 행동"
				PRINTFORMW 「아라, 서투르고 나빴지요. 그렇지만…」
				PRINTFORMW 「그 모양으로 강한척 한 곳에서, 설득력이 부족한거야. 유감스럽게,」
			CASE "무리를 한 행동"
				PRINTFORMW 「사랑스러운 소리로 울면서, 그런 일 말해져도. 저기?」
				PRINTFORMW 「정말로 서투르면, 그런 소리는 나오지 않을 것이라고 생각하지만」
		ENDSELECT
		;페이드 아웃
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
		CLEARLINE 1
		RETURN 1
	ENDIF
	
	;REACT라고 하는 것보다 COM전용기분이 들어 버렸다. 다만, REACT20로 한정하고 싶기 때문에 이대로
	;COM로 IS_NOWACTNAME 할 정도라면, 여기서 좋다
	;=================================================
	;실제로 서투름（RAECT 분류:태연하게 한다）
	;=================================================
	;TRAIN_MESSAGE2_20은 죽여 REACTTION_MESSAGE만 호출하는 사양
	;-------------------------------------------------
	IF 구상기입체크(100) && !TFLAG:REACT파생 && FLAG:지문제어 == 1
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「헤에, 말하지 않아. 그렇지만 말야……」
				PRINTFORMW 「행동이 수반하지 않는 말은, 아무 의미도 없는거야. 나쁘지만」
			CASE "무리가 없는 행동"
				PRINTFORMW 「아라, 그것은 나쁜 일을 했군요. 다만, 그렇구나…」
				PRINTFORMW 「미숙함을 극복하기 위해서도,%CNAME(MASTER, TARGET)%의 협력이 필요한 것이야」
				PRINTFORMW 「그러한 (뜻)이유이니까, 얌전하게 하세요……」
			CASE "무리를 한 행동"
				PRINTFORMW 「말과는 정반대로, 상당히 여유가 없는 것 같네? 게다가, 뭐……」
				PRINTFORMW 「그러한 허풍도, 가끔 번거롭고, 화가 나지는 일도 있는거야」
				PRINTFORMW 「우쭐해지고 있으면, 아픈 꼴을 당하게 하기 때문에……」
				;상태변화[분노]		REACT파생 0의 경우, COM 인상이 약간나쁜인상인 것으로 분노가 될 가능성도 그 나름대로 있지만, 뭐 치료는 해 두고 싶은 느낌
				TCVAR:상태변화 = 6
		ENDSELECT
		RETURN 1
	ENDIF
;파생이 추가되거나 등의 개변이 있었을 경우, RAECT 파생 결정 집이 살아 온다
;정합성이 이상해질 정도라면, 표시시키지 않다고 하는 사상. COM는 아무튼 범용적인 기술이 주된 것으로, 뭐 좋을까적인
;보완의 사상은 본래, 이 바리안트의 구상 시스템의 근간을 이루는 것이지만, REACT에 있어서는 반드시 들어맞지 않으면 생각한다
;후는, 시스템측 및 재야의 패치 작자에게로의 신뢰감 혹은 불신감이라든지 그러한 이야기가 되어 지는 것으로, 명언은 피하고 싶은 곳w
RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;쾌감을참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;기본적으로 TRAIN_MESSAGE2_20에 맞춘 기술. 바뀐 보람 없는 곳은 병렬처리로
;-------------------------------------------------
IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을참는다")
	;=================================================
	;파생, 애원（REACT분류:용서를빈다）
	;=================================================
	IF 구상기입체크(100) && TFLAG:REACT파생 == 1
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「그런 말을 들어도, 저기?  %CNAME(MASTER, TARGET)%, 그렇게 기분 좋은 것 같은 것에」
				PRINTFORM 「오히려, 후후…… 조금 괴롭히고 싶어져 버려요
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는 행동", "무리를 한 행동"
				PRINTFORMW 「아라,\@ GET_COM_RANK() == "무리를 한 행동" && !COMCOR_POSI() ? 그런 일 말하지 않고 # 무서워한다일 같은거 없어요.\@솔직하게 느끼면 좋은거야. 다만, 뭐…」
				PRINTFORMW 「이런 대용품으로 잘 되어 버리는 것은, 뭔가 납득 할 수 없는 것이 있을까. 저기?」
		ENDSELECT
		;페이드 아웃
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
		CLEARLINE 1
		RETURN 1
	ENDIF
	
	;=================================================
	;파생, 내구（REACT분류:소극적으로 한다）
	;=================================================
	IF 구상기입체크(100) && !TFLAG:REACT파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				PRINTFORMW 「후훗, 조금 낯간지러운 느낌일까. 그렇지만, 나쁘지 않겠지요?」
				PRINTFORMW 「이봐요, 인내 같은거 하지 않고…, 좀 더 솔직하게 느껴……」
			CASE "무리가 없는 행동"
				PRINTFORMW 「아라, 상당히 사랑스러워져 버렸군요. 뭐, 저항하지 않는다는 일은……」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%도 그 나름대로 즐기고 있다는 것으로 좋은 것일까. 저기?」
			CASE "무리를 한 행동"
				;실은 기분 좋다		COMCOR는 반응의 미묘한 강약을 참조할 수 있으므로 재미있지만, 귀찮기도 하다w
				IF COMCOR_POSI()
					PRINTFORMW 「후후, 그런 얼굴 되어도 곤란해요」
					PRINTFORMW 「원망한다면, 이런 완구로 용이하게 느껴 버리는, 자기 자신을 원망하는 거네」
				;실은 싫어
				ELSE
					PRINTFORMW 「무심코 소리가 나와 버려도 곳일까? 후후…」
					PRINTFORM 「이제 와서 평정을 가장한 곳에서, 이제(벌써) 뒤늦음이라고 생각하지만, 말야」
					CALL PRINT_SYMBOL_K14
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;쾌감을받아들인다/기분좋다！（REACT분류:받아들인다）
;━━━━━━━━━━━━━━━━━━━━━━━━
;현상쾌감을받아들인다/기분좋다！에 REACT파생는 존재하지 않지만 조건 결정 집에서 보험에 든다
;-------------------------------------------------
IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을받아들인다/기분좋다！") && !TFLAG:REACT파생
	;=================================================
	;조교 대상이갈것같다
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:갈것같다
		;애태움수에 의한 파생
		SELECTCASE TCVAR:MASTER:애태움수
			;애태움중
			CASE IS > 1
				PRINTFORMW 「후훗, 어때? 초조해 해지면 견딜 수 없게 될까요?」
				PRINTFORM 「…… 은. 지금의%CNAME(MASTER, TARGET)%에 말한 곳에서, 그럴 곳이 아닐지도이지만
				CALL PRINT_SYMBOL_K14
			;애태움 종료
			CASE 1
				PRINTFORMW 「이봐요, 좋아요. 초조하게 하는 것은 멈추어 준다」
				PRINTFORMW 「최대한, 좋은 소리로 울어 즐겁게 해 줘……」
			;애태움 없음
			CASEELSE
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 이런 완구로 가게 되는 것 일에는 저항이 없는거네?」
				PRINTFORMW 「뭐 좋아요. 이봐요, 가세요……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운 행동"
			PRINTFORMW 「상스럽네요? 그런 소리 내 버려……」
			PRINTFORMW 「그렇지만, 후후……, 이것은 그렇게 좋은 것일까」
		CASE "무리가 없는 행동"
