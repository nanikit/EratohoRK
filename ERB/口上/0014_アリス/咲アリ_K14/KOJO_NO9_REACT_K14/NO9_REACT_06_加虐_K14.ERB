;NO9_REACT63, 縄・個別リアクション
;────────────────────────────────────
;・TFLAG:ACT派生(0=拘束/1=解除)
;-------------------------------------------------
;想定されるCOM…
;会話系反応
;	COM:沈黙する/皮肉を言う/もっとハードにして/許しを乞う
;愛撫系反応
;	COM:不敵に笑う
;苦痛系反応
;	COM:痛みを我慢する/悲鳴を上げる/暴れる/怯える
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_63
;-------------------------------------------------
;・固有の地の文なし。調教対象の反応に応じてREACTION_MESSAGEで汎用処理
;-------------------------------------------------

LOCALS = \@ AFFECTION("溺愛") ? ふふふ # フフフ \@

;━━━━━━━━━━━━━━━━━━━━━━━━
;拒絶処理
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && IS_COMGRONAME("許しを乞う")
	CALL 加虐系解除処理_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT派生0, 拘束
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;ここでREACT派生を考慮する必要はない
	;=================================================
	;沈黙する
	;=================================================
	;状態変化は地の文で処理される訳でもないので微妙かもしれない
	;まぁ捏造する手段には事欠かないけど。可能であれば本体作者を煽ってTCVAR:状態変化を書かせたいところだが、無理っぽいなｗ
	;KOJO_REACTTION_MESSAGE辺りで地道に捏造するしか
	;-------------------------------------------------
	IF 口上記入チェック(100) && IS_NOWCOMNAME("沈黙する")
		IF M_COND("怒り")
			PRINTFORMW 「言いたいことがあるならハッキリ言ったら？」
			PRINTFORMW 「黙っていては何も伝わらないわよ。機嫌悪そうなのだけは判るけど、ね」
		ELSE
			PRINTFORMW 「ま、そうよねぇ。その状態で何か言っても滑稽なだけだし」
			PRINTFORMW 「黙っているのは賢明だと思うわよ。取り敢えずのところは。ね」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;皮肉を言う
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("皮肉を言う")
		PRINTFORMW 「縄で縛られた状態で皮肉、ねぇ？」
		PRINTFORMW 「正直、サマにならないと思うわ、そういうの。まぁ…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%にはお似合いかも知れないわね」
		RETURN 1
	ENDIF
	
	;=================================================
	;もっとハードにして
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("もっとハードにして")
		;派生:恥知らずの豚め！
		IF TFLAG:REACT派生 == 1
			PRINTFORMW 「なによ、それ。こんなものじゃ手緩いって言いたいわけ！？」
			PRINTFORMW 「そうであるなら、相応の仕打ちを何か考えてあげなくちゃ。ねぇ…」
		;「ふーん」
		ELSE
			PRINTFORMW 「あらあら、貪欲ね？ でも、何事にも限度ってものがあるのよ」
			PRINTFORMW 「尤も…、今の%CNAME(MASTER, TARGET)%に言ったところで、詮無いことかも知れないけど」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;不敵に笑う
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("不敵に笑う")
		;-------------------------------------------------
		;無理をした行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;実はまんざらでもない
			IF COMCOR_POSI()
				PRINTFORMW 「そんな顔して見せた割りには、随分余裕が無いわね」
				PRINTFORMW 「何かを誤魔化す為に無理してる、ってとこかしら。まぁ…」
				PRINTFORMW 「縛られるのが実は嫌じゃないとか、認めたくはないわよねぇ？」
			;実は嫌
			ELSE
				PRINTFORMW 「強がったところで、そのザマじゃ。ねぇ？」
				PRINTFORMW 「そういうのは、相手の目をしっかり見据えてやりなさいよ。でないと…」
				PRINTFORMW 「逆に惨めさが増すだけよ、残念だけど。ね…」
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;自然な行動/無理のない行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GROUPMATCH(TFLAG:REACT負荷, 0, 1) > 0
			PRINTFORMW 「そんな顔したところで、ねぇ？」
			PRINTFORMW 「今の%CNAME(MASTER, TARGET)%の境遇の惨めさは、如何ともしがたいと思うけど」
			PRINTFORMW 「ま、強がりたいなら、好きにすればいいわ。気の済むまで、ね……」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;痛みを我慢する
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("痛みを我慢する") && TFLAG:REACT負荷 < 2
		;-------------------------------------------------
		;痛いけど気持ち良い！
		;-------------------------------------------------
		IF 口上記入チェック(100) && TFLAG:REACT派生 == 1
			PRINTFORMW 「あら、そんなに痛かったかしら？ というか、%CNAME(MASTER, TARGET)%…」
			PRINTFORM 「それ、ただ痛いだけじゃないわよね。だって、そういう\@ GET_COM_RANK() == "無理をした行動" ? 顔し # 声が出 \@てるもの」
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			CLEARLINE 1
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;痛みを我慢
		;-------------------------------------------------
		IF 口上記入チェック(100) && !TFLAG:REACT派生
			IF AFFECTION("相思相愛") && T_COND("怒り")
				PRINTFORM 「%CNAME(MASTER, TARGET)%、とっても縄が似合ってるわよ…
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「ゾクゾクしちゃう… めちゃめちゃに虐めたくなるわね…」
			ELSE
				PRINTFORMW 「あら、ちょっときつ過ぎたかしら」
				PRINTFORMW 「%RAND_SPLIT("手加減し/余裕を持たせ")%たつもりなんだけど、って……」
				PRINTFORMW 「今の%CNAME(MASTER, TARGET)%には何の慰めにもならないかしら。ねぇ？」
				;1/2の確率でフェード
				IF !RAND:2
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
					CLEARLINE 1
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
RETURN 0
ENDIF

