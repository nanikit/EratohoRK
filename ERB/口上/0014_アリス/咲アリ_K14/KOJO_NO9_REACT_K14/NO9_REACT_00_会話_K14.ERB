				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「そうね、貴女が自分で思うよりは遥かにね。アリス」")
				PRINTFORMW 「ま、まぁ…今更否定はしないけど。ちょっと癪ね…」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「くすくす…」")
			ENDIF
