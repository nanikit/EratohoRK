;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생 2, 입으로애무 강제
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:ACT파생 == 2
	;=================================================
	;소극적 따른다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다")
		;싫어한다, 날뛴다 모두 ACT 방해에 이르지 않는 것을 처리
		;-------------------------------------------------
		;싫어한다/날뛴다 RANDREACT(1)
		;-------------------------------------------------
		IF 구상기입체크(100) && ((IS_NOWCOMNAME("싫어한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2) || (IS_NOWCOMNAME("날뛴다") && TFLAG:REACT파생 > 0))
			SELECTCASE RANDREACT(2, 1)
				CASE 0
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「조, 조금! 머리 억누르는 것 그만두세요!」")
					PRINTFORMW 「방법 없네요? 속상하고 안절부절 하는걸」
					PRINTFORMW 「사쿠야가 좀 더 솔직하게 해 주면, 이런 일도 할 필요없지만, 말야」
				CASE 1
					PRINTFORMW 「하나 하나 강요하지 않으면 안된다던가 , 정말 귀찮음…」
					PRINTFORMW 「다만, 그렇구나…, 사쿠야에 이런 일강요하는 것도 나쁘지 않다」
					PRINTFORMW 「그런 생각도 들어요. 조금이, 구나……」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;파생 「소극적인의가 마음에 들지 않는다」는 연주한다. 부하 2는 연주한다
		;-------------------------------------------------
		;소극적한다 RANDREACT(2)
		;-------------------------------------------------
		IF 구상기입체크(70) && IS_NOWCOMNAME("소극적한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2
			SELECTCASE PALAM:쾌B
				CASE IS >= 8500
					;페이드 아웃
					CALL FADE(20, COLOR2_K14("SAKUYA"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("유두 들이마셔"), 2)%")
					CLEARLINE 1
					SELECTCASE RANDREACT(2, 2)
						CASE 0
							PRINTFORMW 「아…… 야! 어, 어젯밤! 그, 그것…… 응읏!」
							CALL PRINTCOLORW(COLOR_K14("사쿠야"), "(… 이 정도라도 상당히 느끼고 있어요. 조금 부러울 정도 예요)")
						CASE 1
							PRINTFORMW 「응앗! 시, 싫다…… 더 이상, 되면……」
							CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 되면? 어떻게 되어 버릴까?」")
							PRINTFORMW 「우, 시끄럽네요…… 좋으니까 입다물어」
							CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐 좋지만. 그렇지만, 이 정도로 좋아져 버리다니저기 앨리스?」")
							PRINTFORMW 「입다물어, 라고 말했을 것이지만……」
							CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아라, 이것은 실례했군요. 후후…」")
					ENDSELECT
				CASE IS >= 4000
					PRINTFORMW 「저기 사쿠야, 좀 더 진지하게 해 주지 않을까」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그럴 생각이지만?」")
					PRINTFORMW 「…… 속이 빤하다. 그렇지만, 무엇인가, 이것…… 읏!」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아라, 시시하네요? 앨리스」")
					PRINTFORMW (…… 조금 화난다. 그렇지만 이 정도로 좋아져 버리는 나도 대개군요…)
				CASEELSE
					PRINTFORMW 「어차피라면 좀 더 적극적으로 해 주었으면 하는 거네…」
					PRINTFORMW 「어중간하게 되어도 속상한 것뿐, 사쿠야」
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;적극적한다/태연하게한다
	;=================================================
	IF 구상기입체크(70) && IS_NOWCOMNAME("적극적한다/태연하게한다") && TFLAG:REACT부하 < 2
		;페이드 아웃
		CALL FADE(20, COLOR2_K14("SAKUYA"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("유두 들이마셔"), 2)%")
		CLEARLINE 1
		SELECTCASE PALAM:쾌B
			CASE IS >= 8500
				PRINTFORMW 「하아, 하아…… 혀사용이 추잡해요, 사쿠야…」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지만, 그것이 좋네요?」")
				PRINTFORMW 「…… 모른다」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이제 와서 고집부려도 늦다고 생각하지만. 그렇지만, 그런 일 말한다면……」")
				PRINTL
				PRINTFORMDL 사쿠야가 그 날카롭게 할 수 있었던 혀끝에서%NIPPLE_DESCRIPTION(TARGET)%첨단의 꽃봉오리를 애무하면,
				PRINTFORMDW 앨리스는%PLEASURE_DESCRIPTION(TARGET)%, 저항하기 어려운 관능의 고조됨에 격렬하게 허덕여 몸부림쳤다…
			CASE IS >= 4000
				SELECTCASE ABL:B감각
					CASE IS > 3
						PRINTFORMW 「얏! 사쿠야…… 거, 거기! 응, 아앗!」
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "(지금의 반응, 조금 사랑스러웠지요……)")
						PRINTFORMW 「…… 응, 뭐야?」
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아무것도 아니에요, 신경쓰지 마」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이봐요, 좋으니까, 좀 더 소리를 들려줘. 후후」")
					CASEELSE
						PRINTFORMW 「… 응. 저기, 사쿠야. 조금 낯간지러워요」
						CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「그것은 저것이군요. 익숙해지고의 문제야」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그 만큼 민감하다는 일이니까, 오히려 부러울 정도 예요」")
						PRINTFORMW 「그러한 것일까…」
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그러한 것이야, 그러니까…… 조금의 사이는 참아 받을 수 있을까?」")
						PRINTFORMW 「능숙하게 말아넣어진 생각도 들어요… 뭐, 좋지만」
				ENDSELECT
			CASEELSE
				PRINTFORMW 「하, 하…… 응 사쿠야. 너무 이빨을 세우지 않고…」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일 말해지면 더욱 더 하고 싶어지지만?」")
				PRINTFORMW 「좋은 성격 하고 있어요…, 알았지만」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그러면 단념해. 괜찮아, 나쁘게는 하지 않아요. 후후」")
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
