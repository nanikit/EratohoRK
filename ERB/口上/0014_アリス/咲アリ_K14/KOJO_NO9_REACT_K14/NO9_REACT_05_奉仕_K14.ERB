					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「어머나 유감")
						CALL AUTOHEARTCB2_K14
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "  에서도, 그것이 무엇보다의 대답일까요?」")
					PRINTFORMW 「시끄럽네요. 불필요한 일 말하지 말아줘…」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋아요, 당신이 그렇게 말한다면. 후후」")
				ENDIF
		ENDSELECT
		;는 gdgd가 되었다. 좀 더 선명한 방식이 있을 것…
		PRINTL
