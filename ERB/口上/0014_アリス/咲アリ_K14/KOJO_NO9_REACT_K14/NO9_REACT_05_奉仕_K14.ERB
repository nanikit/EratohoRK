;-----------------------------
;ACT파생 마다의 처리
;-----------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생 1, 손으로애무 강제
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;REACT분류가 소극적 따른다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("소극적 따른다")
		;이하, COM 마다의 분기
		;COM날뛴다는 REACT파생 0가 아닌 경우, 날뛰고는 통하지 않다
		;COM로싫어한다ACT 방해가 되는 경우는 연주한다
		;-------------------------------------------------
		;싫어한다/날뛴다 RANDREACT(0)
		;-------------------------------------------------
		IF 구상기입체크(100) && (IS_NOWCOMNAME("싫어한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2) || (IS_NOWCOMNAME("날뛴다") && TFLAG:REACT파생 > 0)
			SELECTCASE RANDREACT(2, 0)
				CASE 0
					PRINTFORMW 「이봐요, 그렇지 않아서……」
					PRINTFORMW 「정말, 속상하고 이상해질 것 같아!」
				CASE 1
					PRINTFORMW 「이제 와서 싫어해도 어쩔 도리가 없다는 것에, 완전히…」
					PRINTFORMW 「이제 되기 때문에…, 나의 손의 움직임에 맞추세요」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;파생 「소극적인의가 마음에 들지 않는다」의 경우는 연주한다. 부하 2는 연주한다. 확률을 짠다
		;-------------------------------------------------
		;소극적한다
		;-------------------------------------------------
		IF 구상기입체크(50) && IS_NOWCOMNAME("소극적한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2
			IF AFFECTION("상사상애")
				PRINTFORMW 「아아읏…… 아읏…… 앗…」
				PRINTFORMW 「저기…… 좀 더 강하게 비비어요……」
			ELSEIF AFFECTION("몹시 사랑함")
				PRINTFORMW 「뭐야, 뭔가 부실 같네요…」
				PRINTFORMW 「이봐요, 좀 더 분명하게 해요…」
			ELSEIF AFFECTION("권애")
				PRINTFORMW 「뭐야, 나의 가슴에 손대고 싶지 않은거야…?」
			ELSEIF AFFECTION("혐오")
				PRINTFORMW 「이봐요,! 좀 더 정중하게 하세요!」
				PRINTFORMW 「정말 서투름대변…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%의 기억의 나쁨에는 기가 막혀요…」
			ELSE
				PRINTFORMW 「의지가 없는 것일까… 사용할 수 없네요」
			ENDIF
			RETURN 1
		ENDIF
		RETURN 1
	ENDIF
	
	;무리를 한 행동시는 연주한다. 확률을 짠다
	;=================================================
	;적극적한다
	;=================================================
	IF 구상기입체크(70) && IS_NOWCOMNAME("적극적한다/태연하게한다") && TFLAG:REACT부하 < 2
		;조교자쾌B파생
		SELECTCASE PALAM:쾌B
			CASE IS >= 8500
				;배후로부터
				IF TCVAR:MASTER:위치전후 == 3
					PRINTFORM 「응쿳! 응읏, 앗…
						CALL AUTOHEARTCB_K14
					PRINTFORMW   , 어젯밤! 나, 이제(벌써)…」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 알았어요. 이봐요 앨리스, 이렇게?」")
					PRINTFORM 「앗…
						CALL AUTOHEARTCB_K14
					PRINTFORMW   나, 기다려! 아앗, 안됏!」
					CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「… 은. 정말로 기다리면 화내는 주제에」")
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「뭐, 기다릴 생각도 없지만, 말야")
						CALL PRINT_SYMBOLM_K14
				;그 외
				ELSE
					PRINTFORM 「응앗! 야…… 앗
					CALL AUTOHEARTCB_K14
					PRINTFORMW   팥고물…… 후앗!」
					PRINTFORMW 「그렇게 되면…… 이, 이제(벌써)… 나!」
					CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「괜찮아, 알고 있어요」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋아요, 앨리스. 이대로……」")
				ENDIF
			CASE IS >= 4000
				;배후로부터
				IF TCVAR:MASTER:위치전후 == 3
					PRINTFORMW 「응앗! 자, 사쿠야…… 그렇게 되면, 소리가 나와 버린다」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후, 좋지 않아. 거기에 나도, 당신의 소리를 듣고 싶어요」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「라고 할까, 뒤로부터 되는 (분)편이 흥분할까?」")
					PRINTFORMW 「몰라요, 그런 것…」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐, 좋아요. 그런 일로 해 두어 주어도, 말야」")
				;그 외
				ELSE
					PRINTFORMW 「후아…… 아…… 앗! 야, 거, 거기! 아앗!」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「상당히 사랑스러워져 버렸군요, 앨리스?」")
					PRINTFORMW 「… 우, 시끄러워요. 게다가…, 그 우쭐해하는 얼굴, 조금 화난다…」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지만, 나쁘지 않겠지요? 뭐, 나도 불필요한 일은 말하지 않도록 하기 때문에」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「지금은 솔직하게 느껴……」")
				ENDIF
			CASEELSE
				;배후로부터
				IF TCVAR:MASTER:위치전후 == 3
					PRINTFORMW 「읏! 저기, 사쿠야… 가슴은 좋다고 해도, 귀 만지작거려지면 낯간지러워요」
					CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「그런 말을 들어도 말야…… 이 몸의 자세라고 하고 싶어지는거야」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「게다가, 말하는 만큼 싫어하고 있는 것도 아닌 것 같고」")
					PRINTFORMW 「확실히 그럴지도…… 는, 감쪽같이 실을 수 있는 것 조금 아니꼽게 거슬려요」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐, 좋으니까, 조금 얌전하게 하고 있어…… 후후」")
				;그 외
				ELSE
					PRINTFORMW 「손놀림이 추잡해요, 사쿠야…」
