		PRINT 「
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "すっっごく痛いわ", 1)
		PRINTW ！」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「……自業自得、ね」")
		RETURN 1
	ENDIF
	
	SELECTCASE TRAINNAME:SELECTCOM
		CASE "沈黙する"
			PRINTFORMW 「なによ、黙っちゃって。ツマラナイわね」
