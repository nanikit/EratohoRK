;────────────────────────────────────
;EV202, スキンシップ0
;────────────────────────────────────
;・台所でセクハラ
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_202(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_0 股間まさぐり
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 口上記入チェック(0) && ARG == 0
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_1 おっぱいまさぐり
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 1
	PRINTFORMW 「ねぇ咲夜…」
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はいはい、忙しいから後にして」")
	PRINTFORMW 「かまってくれないと、悪戯するわよ？」
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「はいはい、どうぞご自由に」")
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_2 無視したら諦めて出て行った
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 2
	IF AFFECTION("相思相愛")
		PRINTFORMW 「ねぇ、駄目なの？ 今はシたくないの…？」
		PRINTFORMW 「……もうっ、仕方ないわね。邪魔して悪かったわ、そのまま続けて頂戴」
	ELSE
		PRINTFORMW 「あら、つれないわね…」
		PRINTFORMW 「お邪魔だったようね。じゃ、そのまま続けて頂戴」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_3 身をゆだねて股間でイかされ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && ARG == 3
	PRINTW
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_4 身をゆだねておっぱいで
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 4
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ちょ、ちょっとアリス！ どこ触ってるのよ！」")
	PRINTFORMW 「咲夜がいいって言ったのよ？」
	PRINTL
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「いや、確かに、言いましたけど…」")
	PRINTFORMW 「でしょう？ ほら、大人しくして頂戴」
	PRINTL
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「ま、まだ、するの…？」")
	PRINTFORM 「もちろん
	CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
	PRINTFORMW 　咲夜をイかせるまで、やめないわよ？ ふふっ」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_5 ぱんつお着替えさせてくれました
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 5
	PRINTFORMW 「あらあら、こんなに下着を汚しちゃって…」
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「う…そ、それは…」")
	PRINTL
	PRINTFORMW 「イヤイヤ言ってても、結局よくなっちゃったのね？ ふふ」
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「う、ウルサイわねっ！ 仕方無いでしょう！」")
	PRINTL
	PRINTFORMW 「そうよね？ 仕方無いわよねぇ？ ふふふ」
	CALL PRINTCOLORW(COLOR_K14("咲夜"), "「あぁ…なんか凄く悔しいわ…」")
	PRINTFORM 「まぁまぁ
	CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
	PRINTFORMW 　着替えを持ってきてあげるから、機嫌直してよ♪」
	RETURN 1
ENDIF

