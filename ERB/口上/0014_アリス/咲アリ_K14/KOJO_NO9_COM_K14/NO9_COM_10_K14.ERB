			;만족도 파생 처리
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「읏, 쿠웃! 얏, 아앗")
					CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "  안돼엣!")
					SELECTCASE RANDCOM(2, 7)
						CASE 0
							PRINTFORMW 「슬슬 소리를 억제하는 것도 한계일까요? 후훗」
						CASE 1
							PRINTFORMW 「상당히 좋은 느낌에 녹아 왔을까요? 후후」
					ENDSELECT
				CASE "일반"
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응읏, 쿠웃! ·응응응!」")
					SELECTCASE RANDCOM(2, 8)
						CASE 0
							PRINTFORM 「꽤 노력하고 있지만, 몇시까지 인내 할 수 있을까, 저기 사쿠야…
							CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
