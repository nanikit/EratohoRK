;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「その態度の裏には、何か含みがありそうね」
					PRINTFORMW 「それが何なのかは、私にも判らないけど……」
				CASE 1
					PRINTFORMW 「単純ならざる心の機微……か。ああ、気にしなくていいわよ」
					PRINTFORMW 「今の%CNAME(MASTER, TARGET)%の姿を見て、何となく言葉に出ただけだから」
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("溺愛") ? ふふ # フフ \@…」")
					SIF ANIME_CONFIG("FADE")
						CLEARLINE 1
			ENDSELECT
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「なによそれ。わざわざ元気の無いふりして何の意味があるワケ？」
				CASE 1
					PRINTFORMW 「たまにはしおらしい態度でもしてみようって訳？」
					PRINTFORMW 「ホント、%CNAME(MASTER, TARGET)%の考える事は良く判らないわねぇ」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が会話
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 2)
			CASE 0
				PRINTFORML 「何を言ってるのか良く聞こえないわね」
				PRINTFORMW 「そんな弱気な態度、らしくないと思うけど？」
			CASE 1
				PRINTFORML 「珍しいわね、そういうの」
				PRINTFORMW 「どこか具合でも悪いのかしら？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が加虐
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "加虐")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「随分弱気ね？ そういうの、らしくないと思うけど」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「私の勝手でしょう、そんなの…」")
				PRINTFORMW 「そうね？ 私は別に構わないわよ」
			CASE 1
				PRINTFORML 「咲夜の事だから、てっきり何か言い返すと思ったけど…」
				PRINTFORMW 「ちょっと拍子抜けするわね？ 一体どうしたのよ」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「どうでもいいでしょう…」")
				PRINTFORMW 「ま、そうなんだけど……、何か引っかかるのよね」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が異常
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "異常")
		PRINTFORML 「放っておかれるのが、そんなに心細いのかしら？」
		PRINTFORMW 「咲夜にもこんな繊細な面があるのね…」
		CALL PRINTCOLORW(COLOR_K14("咲夜"), "「何よそれ、馬鹿にしてる？」")
		PRINTFORMW 「そんな事ないわ、ただの素直な感想よ。ふふっ」
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が休憩
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "休憩")
		SELECTCASE RANDCOM(2, 5)
			CASE 0
				PRINTFORMW 「なによ、随分元気がないわね？」
				PRINTFORMW 「もしかして、貴女……お腹が空いたとか？」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「人の事、何だと思ってるのかしら…」")
			CASE 1
				PRINTFORMW 「ねぇ、やめてくれないかしら。そういうの」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「どういう意味よ」")
				PRINTFORML 「判らないの？」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「判らないわね」")
				PRINTFORMW 「じゃあ言うけど…、そんなしおらしい態度、咲夜には似合わないわよ？」
				CALL PRINTCOLORW(COLOR_K14("咲夜"), "「叩いていい？」")
				PRINTFORM 「ふふ、そのくらいの方が咲夜らしいわ…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;漏れた分はここで処理
	;-------------------------
	SELECTCASE RANDCOM(2, 10)
		CASE 0
			PRINTFORML 「何を言ってるのか良く聞こえないわね」
			PRINTFORMW 「そんな弱気な態度、らしくないと思うけど？」
		CASE 1
			PRINTFORML 「珍しいわね、そういうの」
			PRINTFORMW 「どこか具合でも悪いのかしら？」
	ENDSELECT
	RETURN 1
ENDIF

