;────────────────────────────────────
;COM8, 기분좋게해줘
;────────────────────────────────────
;·REACT분류:받아들인다
;·조교자의 소질, BASE:이성, 아라이먼트에 의해 REACT파생 1, 「이 철면피의 암퇘지놈!」(이)가 된다
;·파생 1의 경우는 벌포인트 가산
;·REACT인상:0에서멀어짐
;·의존도 변화(0, +2)
;-------------------------------------------------
;트리거하는 행동이나 상황…
;ACT분류:0:회화
;	ACT:지금의기분을얘기한다/자신에게로의기분을얘기한다/성에대한이야기를한다/상냥하게위로한다/합의를요구한다
;ACT분류:1:애무
;	ACT:애널애무/애널핥기/쓰러트린다
;ACT분류:2:도구
;	ACT:페니스밴드
;ACT분류:3:성교
;	ACT:
;ACT분류:4:수치
;	ACT:자위과시/방뇨
;ACT분류:5:봉사
;	ACT:
;ACT분류:6:가학
;	ACT:매도
;ACT분류:7:이상
;	ACT:방치플레이/페니스밴드장착
;ACT분류:9:휴식
;	ACT:쉬게한다/영양제를먹인다/치료한다
;ACT분류:10:성교 봉사
;	ACT:역강간
;
;TEQUIP:볼개그는 튕겨진다
;-------------------------------------------------
@KOJO_NO9_MS_COM_K14_8
;-------------------------------------------------
;·TFLAG:REACT파생(0=-응/1=이 철면피의 암퇘지놈!)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM8_0, 기분좋게해줘
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;=================================================
	;무리를 한 행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
		;실은 그렇지만도 않다
		IF COMCOR_POSI()
			PRINTFORMW 「그것이 사쿠야의 본심으로부터 나온 말인 것인가, 조금 이상한 곳이네?」
			PRINTFORMW 「정직, 간단하게 꼬리를 흔드는 개는 좋아하지 않아. 나쁘지만」
		;실은 싫어
		ELSE
			PRINTFORMW 「임시 방편의 거짓말, 이라고 할 것도 아닌 것 같구나」
			PRINTFORMW 「뭐, 아픈 생각 하는 것보다 마시, 라는 것이%CNAME(MASTER, TARGET)%의 본심인 것일까. 저기?」
		ENDIF
		RETURN 1
	ENDIF
	
	;ACT분류:수치는 내용적으로 그다지 공통성이 없기 때문에, 정리해 처리하는 것은 주의가 필요할지도 알지 못하고
	;여기서 TFLAG:ACT나 TEQUIP로 분기 시켜도 좋지만, REACT로 하는 것과 함께지요 하고 말하는 느낌에
	;라든지 말하면서 현상 REACT까지 손길이 닿지 않기 때문에, 해 보거나 한다
	;================================================
	;ACT분류가 수치
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "수치")
		;ACT 마다의 처리
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "자위과시"
				PRINTFORMW 「… 응. 내가 시 비치는 곳 봐 인내 할 수 없게 되었어?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 것이 아니다, 하지만…」")
				PRINTFORMW 「거짓말한다면 좀 더 능숙하게 하지 않으면? 이제 와서 부정해도 설득력이 없어요」
			CASE "수치플레이"
				PRINTFORMW 「후훗, 거울\@ !TEQUIP:아이마스크 ? 에 비치는 모습을 봐 # 의 앞에서 \@흥분해 버리다니.\@ !TEQUIP:아이마스크 ? 의외로 사랑스러운 곳 있어요?  # 어차피 아무것도 안보이는데 \@」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「사람의 일 얼버무려 즐거운 것 같구나, 앨리스…」")
				PRINTFORMW 「즐거운 것은 부정하지 않지만, 따로 얼버무리고 있는 것이 아니에요. 오히려…」
				PRINTFORM 「지금의 사쿠야…, 매우 사랑스럽구나, 라는 느낌
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「바, 바봇! 그만두어 받을 수 없을까, 그러한 것…」")
				PRINTFORMW 「메이드장은 부끄럼쟁이네? 그것도 나쁘지는 않지만……」
				PRINTFORMW 「도가 지나치면 여기까지 부끄러워지기 때문에, 적당 산뜻하게 해 주, 세요?」
			CASE "야외플레이"
				PRINTFORMW 「후후, 밖에 나와 흥분하고 있을까?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「벼, 별로! 그런, (뜻)이유다…」")
				PRINTFORM 「라든지 말한 곳에서 이제(벌써) 뒤늦음이군요? 뭐, 별로 좋지만…
				CALL PRINT_SYMBOL_K14
			CASEELSE
				PRINTFORMW 「사쿠야가 그런 일 말하다니 조금 의외이구나?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「나빴지요」")
				PRINTFORMW 「별로 나쁘지 않아요? 단지 그, 뭐라고 말할까……」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 분명하게 말하세요」")
				PRINTFORMW 「그렇구나……, 아니, 역시 그만두어요. 후훗」
				;1/3의 확률로 표시
				IF !RAND:3
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「더욱 더 신경이 쓰여요…」")
					PRINTFORM 「그렇겠지요? 그렇지만, 가르쳐 주지 않으니까
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;ACT:매도때만. REACT로 해도 괜찮지만…
	;================================================
	;ACT분류가 가학 RANDCOM(2)
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "가학")
		SELECTCASE RANDCOM(2, 2)
			CASE 0
				PRINTFORML 「그것은 좋지만……」
				PRINTFORMW 「당신이 지금 말해야 할 말일까? 나는 의문으로 생각해요」
			CASE 1
				PRINTFORML 「그 밖에 말하는 것이 있을 것이라고 생각하지만…」
				PRINTFORMW 「뭐, 사쿠야가 그것으로 좋으면, 나는 상관없어요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가 이상 RANDCOM(3)
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "이상")
		IF IS_NOWACTNAME("방치플레이")
			SELECTCASE RANDCOM(2, 3)
				CASE 0
					PRINTFORMW 「네네, 알았어요. 그렇지만…」
					PRINTFORMW 「지금은 안 돼요? 거기서 얌전하게 하고 있어」
				CASE 1
					PRINTFORMW 「유감이구나? 지금은 아무것도 시라고 줄 생각은 없는거야」
					PRINTFORM 「분명하게 얌전하게 하고 있으면, 다음에 귀여워해 주어요
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		;ELSE
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가 휴식 RANDCOM(4)
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "휴식")
		IF IS_NOWACTNAME("쉬게한다")
			SELECTCASE RANDCOM(2, 4)
				CASE 0
					PRINTFORMW 「그것은 상관없지만, 지금 쉬어 두는 것이 좋다고 생각해요?」
					PRINTFORMW 「따로 당황하는 일 같은거 없는 것. 그렇겠지요? 사쿠야」
				CASE 1
					PRINTFORMW 「아라아라, 메이드장은 성급하구나?」
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄럽네요」")
					PRINTFORMW 「따로 수줍은 일은 없다고 생각하지만 말야? 뭐, 지금 쉬어 두는 것이 좋아요」
			ENDSELECT
		;ELSE
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;샌 것은 여기서 처리
	;-------------------------
	IF RAND:3
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울적", "몽롱"
				PRINTFORMW 「에에, 좋아요. 나도 그럴 생각이었던 것. 다만…」
				PRINTFORMW 「손대중 없음으로 가기 때문에, 각오는 해 두어요?」
				;페이드 아웃
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "분노"
				PRINTFORML 「최악의 기분때에 그런 일 말해져도 말야?」
				PRINTFORMW 「조금은 생각해 주었으면 하는 것이예요……」
			CASE "지루함"
				PRINTFORMW 「어차피라면, 좀 더 지루함을 감추는 것 같은 일 말하기를 원했지만…」
				PRINTFORMW 「거기까지 기대하는 것은 가혹할, 까요」
			CASE "피폐", "쇠약", "무기력"
				PRINTFORMW 「지쳐있는 때에 말해져도 곤란해요, 그런 일…」
			CASE "통상"
				SELECTCASE RANDCOM(2, 10)
					CASE 0
						PRINTFORMW 「아라, 미안해요. (듣)묻지 않았어요」
					CASE 1
						PRINTFORMW 「응? 뭔가 말해진 것 같지만…」
						PRINTFORMW 「나의 지나친 생각일까?」
					CASE 2
						PRINTFORMW 「… 아휴. 메이드장은 탐욕이군요?」
						PRINTFORM 「그렇지만, 나쁘지 않은 느낌이군요. 그러한 것도
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				ENDSELECT
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 킥킥 # 후후후 \@…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
		ENDSELECT
	ELSE
		IF AFFECTION("상사상애")
			SELECTCASE RANDCOM(2, 10)
				CASE 0
					PRINTFORMW 「에에, 좋아요…」
					PRINTFORM 「%CNAME(MASTER, TARGET)%가 바라는 일은, 뭐든지 해 준다…
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORM 「아라, 상스럽네요…
					CALL PRINT_SYMBOL_K14
					PRINTFORMW 「후훗…」
			ENDSELECT
		ELSE
			SELECTCASE RANDCOM(2, 11)
				CASE 0
					PRINTFORMW 「아라, 미안해요. (듣)묻지 않았어요」
				CASE 1
					PRINTFORMW 「응? 뭔가 말해진 것 같지만…」
					PRINTFORMW 「나의 지나친 생각일까?」
			ENDSELECT
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 킥킥 # 후후후 \@…」")
			CLEARLINE 1
		ENDIF
	ENDIF
	RETURN 1
ENDIF

