;-------------------------------------------------
@KOJO_NO9_MS_COM_K14_41
;-------------------------------------------------
;・TFLAG:REACT派生 == 0, 消極的だけどいいか
;・依存度変化(-3, +2)
;-------------------------------------------------
;・TFLAG:REACT派生 == 1, 消極的なのが気に入らない
;・依存度変化(-5, 0)
;-------------------------------------------------

;派生毎に処理を切り分けるのは作業量的に難しいので統合してしまう
;パッチでREACTION_MESSAGE内に派生1の処理を追加して、より派生1の記述が生きるように自作自演。システム側に黙殺されたらその時は何か考えるｗ
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_COM41_0 & 1, 消極的やる
;━━━━━━━━━━━━━━━━━━━━━━━━
;早い段階から選択可能になる可能性が高く、進展した後は負荷2で選択肢になる場合もあったり
;ある程度注力すべき箇所であるように思う。という訳でちょっと真面目にやってみる
;-------------------------------------------------
IF 口上記入チェック(100) && GROUPMATCH(TFLAG:REACT派生, 0, 1) > 0
	;=================================================
	;ACT分類が性交
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "性交") && SEX_INITIATIVE(MASTER)
		;-------------------------------------------------
		;無理をした行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;実はまんざらでもない
			IF COMCOR_POSI()
				SELECTCASE RANDCOM(3, 0)
					CASE 0
						PRINTFORMW 「なによそれ、%RAND_SPLIT("じれったい/もどかしい")%わね。少しは自分で動きなさいよ」
						PRINTFORMW 「ほんと、\@ TFLAG:REACT派生 ? 使えないったらないわね！ # 興ざめもいいとこね \@……」
					CASE 1
						PRINTFORMW 「退屈でツマラナイわね…。%CNAME(MASTER, TARGET)%みたいな人は"dead fish"って言うらしいわ」
						PRINTFORMW 「誰が考えたのか知らないけど、%CNAME(MASTER, TARGET)%にはピッタリの言葉ね…\@ TFLAG:REACT派生 ? ！ # \@」
					CASE 2
						PRINTFORMW 「は？ なんなのよ、それ。%CNAME(MASTER, TARGET)%、全部私にやらせるつもり！？」
						PRINTFORMW 「マグロ相手のセックスって、%RAND_SPLIT("最高にツマラナイわ/ホント退屈/あり得ないくらい最悪")%\@ TFLAG:REACT派生 ? ね！ # だわ… \@」
				ENDSELECT
			;実は嫌
			ELSE
				SELECTCASE RANDCOM(3, 1)
					CASE 0
						PRINTFORMW 「ふぅん。取りあえず、逆らうつもりはないみたいだけど」
						IF TFLAG:REACT負荷
							PRINTFORMW 「そのやる気のない態度、気に入らないRAND_SPLIT("わ/のよね/わね")……」
						ELSE
							PRINTFORMW 「単調でつまらないわね、%CNAME(MASTER, TARGET)%……」
						ENDIF
					CASE 1
						PRINTFORMW 「今更悔しそうな顔したって、滑稽なだけなのにね」
						PRINTFORMW 「もう色んな意味で手遅れなんだから、いい加減\@ TFLAG:REACT派生 ? 諦めなさいよ！ # 素直にしたらどうなの？ \@」
					CASE 2
						PRINTFORMW 「逆らうつもりはないけど、積極的に従うつもりも無いってわけ？」
						PRINTFORMW 「ホント退屈ね、%CNAME(MASTER, TARGET)%…。\@ TFLAG:REACT派生 ? 面白くないわ…… # もう少しなんとかならないのかしら \@」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		;無理のない行動
		IF TFLAG:REACT負荷
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORMW 「う～ん、まぁ…。大人しく、言う通りにするのは結構なんだけど…」
					IF TFLAG:REACT派生
						PRINTFORMW 「%CNAME(MASTER, TARGET)%がそんな様子じゃ、全然愉しくないのよ…」
					ELSE
						PRINTFORMW 「そんなんじゃ、退屈過ぎて欠伸がでるわ。勘弁してくれないかしら」
					ENDIF
				CASE 1
					PRINTFORMW 「気の無い動きねぇ。単調過ぎて嫌になってくるわ」
					PRINTFORMW 「特別なにかを期待してた訳じゃないけど、流石に\@ TFLAG:REACT派生 ? お粗末すぎるわよ！ # \@ちょっと。ねぇ…」
			ENDSELECT
		;自然な行動
		ELSE
			SELECTCASE RANDCOM(2, 3)
				CASE 0
					PRINTFORMW 「逆らえないから仕方なくって感じがアリアリで、ホント白けちゃうわ」
					PRINTFORMW 「『もっと情熱的に』なんて言うつもりはないけど、もう少しなんとかならないの\@ TFLAG:REACT派生 ? ！ # \@？」
				CASE 1
					PRINTFORMW 「取りあえず、私の機嫌を損ねない程度にって感じよね？」
					PRINTFORMW 「半端なことされても、じれったくて…、イライラする\@ TFLAG:REACT派生 ? のよ！ # わ… \@」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類が羞恥
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "羞恥")
		LOCALS = \@ AFFECTION("溺愛") ? %RAND_SPLIT("ふふ/くすくす/ふふっ")% # %RAND_SPLIT("ウフフ/フフフ/フフ")% \@
		;ACTを決めうちしてしまう
		IF IS_NOWACTNAME("自慰/放尿")
			;FADEの処理を纏めたかったので刻まずに派生処理する
			SELECTCASE GET_COM_RANK()
				CASE "自然な行動"
					;派生：消極的なのが気に入らない
					IF TFLAG:REACT派生
						PRINTFORMW 「大人しく従うのは結構だけど…」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%、消極的すぎるのよ。気に入らないわ…」
					;派生：消極的だけどいいか
					ELSE
						SELECTCASE RANDCOM(3, 10)
							CASE 0
								PRINTFORMW 「悪くない眺めね。その『強要されて仕方なく』って感じもとってもいいわよ」
								PRINTFORMW 「%CNAME(MASTER, TARGET)%には、もっと色んな事させたくなっちゃうわ」
							CASE 1
								PRINTFORMW 「『逆らえないから仕方なく』って感じも悪くないわね」
								PRINTFORMW 「どうせ恥ずかしがったところで手遅れなんだから。もっとよく見えるようにして頂戴」
							CASE 2
								PRINTFORMW 「流石の%CNAME(MASTER, TARGET)%でも、こういうのは気乗りしないのかしら？」
								PRINTFORMW 「ま、そういう事も含めて…。私はじっくり鑑賞させて貰うことにするわ」
						ENDSELECT
					ENDIF
				CASE "無理のない行動"
					;派生：消極的なのが気に入らない
					IF TFLAG:REACT派生
						PRINTFORMW 「ねぇ、言っとくけど…。言う通りにしてば良いってものじゃないのよ」
						PRINTFORMW 「その、言われた事だけ最低限って感じ…。気に食わないわ」
					;派生：消極的だけどいいか
					ELSE
						SELECTCASE RANDCOM(3, 11)
							CASE 0
								PRINTFORMW 「流石にあまり気乗りはしないみたいね」
								PRINTFORMW 「ま、そうよね。こんな恥ずかしい事、人前ではしたくないわよね？」
							CASE 1
								PRINTFORMW 「どうせなら、もっと堂々とすればいいのに。ってコレ」
								PRINTFORMW 「そういう問題じゃ、ないわよねぇ」
							CASE 2
								PRINTFORMW 「そんなんじゃ駄目よ。もっと私に良く見えるようにして頂戴」
								PRINTFORMW 「だって…、ねぇ？ 折角の機会なんだから」
						ENDSELECT
					ENDIF
				CASE "無理をした行動"
					;実はまんざらでもない
					IF COMCOR_POSI()
						;派生：消極的なのが気に入らない
						IF TFLAG:REACT派生
							PRINTFORMW 「……まったく。駄目ね、%CNAME(MASTER, TARGET)%…」
							PRINTFORMW 「半端なことされても、見てる方は面白くないのよ」
						;派生：消極的だけどいいか
						ELSE
							SELECTCASE RANDCOM(2, 12)
								CASE 0
									PRINTFORMW 「気が進まないようだけど、逆らうまでには至らない。って感じな訳？」
									PRINTFORMW 「でもまぁ…。結局、私に従うなら同じことよね」
								CASE 1
									PRINTFORMW 「あら、いいわねぇ。あり得ないくらい無様で、%CNAME(MASTER, TARGET)%には良くお似合いよ」
									PRINTFORMW 「ま、こんな恥ずかしい事…、積極的にはできないわよね」
							ENDSELECT
						ENDIF
					;実は嫌
					ELSE
						;派生：消極的なのが気に入らない
						IF TFLAG:REACT派生
							PRINTFORMW 「なによ……、悔しそうな顔しちゃって。文句があるなら逆らってみなさいよ」
							PRINTFORMW 「そんな半端なことされても、面白くもなんともないのよ！」
						;派生：消極的だけどいいか
						ELSE
							SELECTCASE RANDCOM(2, 13)
								CASE 0
									PRINTFORMW 「悔しそうな顔したところで、結局は私の言うこと聞くのね？」
									PRINTFORMW 「ふふ…本当に情けない姿。見てる方が恥ずかしくなるわ」
								CASE 1
									PRINTFORMW 「ふふ…、いい表情。こんな事を強要されるのは、さぞかし悔しいでしょうねぇ」
									PRINTFORMW 「こんな恥ずかしい事、本来…、人前ですることじゃないし。ね」
							ENDSELECT
						ENDIF
					ENDIF
			ENDSELECT
			;調教者のBASE:満足が50%以上の場合かつ派生0の場合
			IF TENSION(TARGET, 8) == "高い" && !TFLAG:REACT派生
				;フェードアウト
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				;コンフィグを参照
				SIF ANIME_CONFIG("FADE")
					;真なら行削除する
					CLEARLINE 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類が奉仕
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "奉仕")
		LOCALS = \@ TALENT:MASTER:オトコ ? 犬 # メス犬 \@
		;ACTが口による奉仕である場合		関数による処理を終端で纏めたかったので刻まない。というか刻んで記述したものをあとから統合した
		IF IS_NOWACTNAME("口で愛撫を強制/足舐め強制/イラマチオ") || (IS_NOWACTNAME("胸愛撫強制") && TFLAG:ACT派生 == 2)
			SELECTCASE GET_COM_RANK()
				CASE "自然な行動"
					;派生：消極的なのが気に入らない
					IF TFLAG:REACT派生
						PRINTFORMW 「これまた随分と%RAND_SPLIT("熱のこもってない/おざなりな")%奉仕だこと」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%…、%RAND_SPLIT("私を怒らせたいの？/その程度で誤魔化せると思ったら大間違いよ")%」
					;派生：消極的だけどいいか
					ELSE
						PRINTFORMW 「ふふ、そうそう。大人しく従ってればいいのよ」
						PRINTFORMW 「ご主人様に\@ IS_NOWACTNAME("足舐め強制") ? 舌で # 口を使って \@奉仕できるなんて、%CNAME(MASTER, TARGET)%のような%LOCALS%にとっては、この上ない幸せでしょう？」
						;BASE:8 満足
						PRINTFORMW 「例えどうしようもない駄犬であったとしても、ね\@ TENSION(TARGET, 8) == "高い" ? ぇ # … \@」
					ENDIF
				CASE "無理のない行動"
