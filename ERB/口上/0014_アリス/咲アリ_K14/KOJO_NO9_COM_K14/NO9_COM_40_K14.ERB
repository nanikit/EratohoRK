					;파생:소극적이지만 좋은가
					ELSE
						SELECTCASE RANDCOM(3, 11)
							CASE 0
								PRINTFORMW 「과연 그다지 마음에 당김은 하지 않는 것 같구나」
								PRINTFORMW 「뭐, 그래요. 이런 부끄러운 일, 남의 앞에서는 하고 싶지 않네요?」
							CASE 1
								PRINTFORMW 「어차피라면, 좀 더 당당히 하면 좋은데. 라는 이것」
								PRINTFORMW 「그러한 문제가 아니네요」
							CASE 2
								PRINTFORMW 「그런 것은 안 돼요. 좀 더 나에게 자주(잘) 보이도록(듯이)해 줘」
								PRINTFORMW 「왜냐하면…, 저기? 모처럼의 기회인 것이니까」
						ENDSELECT
					ENDIF
				CASE "무리를 한 행동"
					;실은 그렇지만도 않다
					IF COMCOR_POSI()
						;파생:소극적인의가 마음에 들지 않는다
						IF TFLAG:REACT파생
							PRINTFORMW 「…… 완전히. 안되구나,%CNAME(MASTER, TARGET)%…」
							PRINTFORMW 「어중간한 일 되어도, 보고 있는 (분)편은 재미있지 않은거야」
						;파생:소극적이지만 좋은가
						ELSE
							SELECTCASE RANDCOM(2, 12)
								CASE 0
									PRINTFORMW 「마음이 내키지 않는 것 같지만, 거역하기까지는 도달하지 않는다. 라는 느낌인 이유?」
									PRINTFORMW 「그렇지만 아무튼…. 결국, 나에 따른다면 같은 것이군요」
								CASE 1
									PRINTFORMW 「아라, 좋네요. 있을 수 없을 정도 보기 흉해,%CNAME(MASTER, TARGET)%에는 자주(잘) 어울려요」
									PRINTFORMW 「뭐, 이런 부끄러운 일…, 적극적으로는 할 수 없네요」
							ENDSELECT
						ENDIF
					;실은 싫어
					ELSE
						;파생:소극적인의가 마음에 들지 않는다
						IF TFLAG:REACT파생
							PRINTFORMW 「뭐야……, 분한 것 같은 얼굴 해 버려. 불평이 있다면 거역해 보세요」
							PRINTFORMW 「그런 어중간한 일 되어도, 재미있지도 어떻지도 않은거야!」
						;파생:소극적이지만 좋은가
						ELSE
							SELECTCASE RANDCOM(2, 13)
								CASE 0
									PRINTFORMW 「분한 것 같은 얼굴 한 곳에서, 결국은 나의 말하는 일 (듣)묻는 거네?」
									PRINTFORMW 「후후… 정말로 한심한 모습. 보고 있는 (분)편이 부끄러워져요」
								CASE 1
									PRINTFORMW 「후후…, 좋은 표정. 이런 일을 강요받는 것은, 필시 분하겠지요」
									PRINTFORMW 「이런 부끄러운 일, 본래…, 남의 앞에서 하는 것이 아니고. 저기」
							ENDSELECT
						ENDIF
					ENDIF
			ENDSELECT
			;조교자의 BASE:만족이 50%이상의 경우 또한 파생 0의 경우
			IF TENSION(TARGET, 8) == "높다" && !TFLAG:REACT파생
				;페이드 아웃
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				;콘피그를 참조
				SIF ANIME_CONFIG("FADE")
					;진이라면 행 삭제한다
					CLEARLINE 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "봉사")
		LOCALS = \@ TALENT:MASTER:남자 ? 개 # 암캐 \@
		;ACT가 입에 의한 봉사인 경우		함수에 의한 처리를 종단으로 모으고 싶었기 때문에 새기지 않는다. 라고 할까 새겨 기술한 것을 나중에 통합했다
		IF IS_NOWACTNAME("입으로애무를강제/밟핥기강제/이라마치오") || (IS_NOWACTNAME("가슴애무강제") && TFLAG:ACT파생 == 2)
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운 행동"
					;파생:소극적인의가 마음에 들지 않는다
					IF TFLAG:REACT파생
						PRINTFORMW 「이것 또 상당히%RAND_SPLIT("열이 가득차지 않았다/일시 모면적인")%봉사다 일」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%…、%RAND_SPLIT("나를 화나게 하고 싶은거야?/그 정도로 속일 수 있다고 생각하면 큰 실수야")%」
					;파생:소극적이지만 좋은가
					ELSE
						PRINTFORMW 「후후, 그래그래. 얌전하게 따라서 있으면 좋은거야」
						PRINTFORMW 「주인님에\@ IS_NOWACTNAME("밟핥기강제") ? 혀로 # 구를 사용해 \@봉사할 수 있다니%CNAME(MASTER, TARGET)%와 같은%LOCALS%에 있어서는, 이 이상 없는 행복하겠지요?」
						;BASE:8 만족
						PRINTFORMW 「비유하고 어쩔 수 없는 똥개였다고 해도, 말야\@ TENSION(TARGET, 8) == "높다" ? # … \@」
					ENDIF
				CASE "무리가 없는 행동"
					PRINTFORMW 「좀 더,%CNAME(MASTER, TARGET)%…」
					PRINTFORMW 「%LOCALS%의 주제에 혀사용도 되지 않고, 싫증 마지막 없어요\@ TFLAG:REACT파생 ? !  # 군요…… \@」
				CASE "무리를 한 행동"
					;실은 그렇지만도 않다
					IF COMCOR_POSI()
						PRINTFORMW 「기르는 개라면, 좀 더 기뻐해 주인님에 봉사해야 하지 않아\@ TFLAG:REACT파생 ? !  # \@?」
						;파생:소극적인의가 마음에 들지 않는다
						IF TFLAG:REACT파생
							PRINTFORMW 「%CNAME(MASTER, TARGET)%…、%LOCALS%로서의 자각이 부족한거야!」
						;파생:소극적이지만 좋은가
						ELSE
							PRINTFORMW 「혹시%CNAME(MASTER, TARGET)%…, 아직%LOCALS%로서의 자각이 부족한 것일까%RAND_SPLIT(". 저기?/")%」
						ENDIF
					;실은 싫어
					ELSE
						PRINTFORMW 「응? 뭐야. 아직 남들 수준의 프라이드가 남아 있을까」
						PRINTFORMW 「그런 것…, 이미 한마리의%LOCALS%에 지나지 않는다%CNAME(MASTER, TARGET)%에는 필요없을 것이지만」
						;파생:소극적인의가 마음에 들지 않는다
						IF TFLAG:REACT파생
							PRINTFORMW 「한번 더,%CNAME(MASTER, TARGET)%의 입장을 깨닫게 해 줄 필요가 있을 듯 하구나…」
						;파생:소극적이지만 좋은가
						ELSE
							PRINTFORMW 「… 은. 여기까지 말해져도 거역할 수 없는 시점에서, 이제(벌써) 어쩔 수 없다%RAND_SPLIT("느낌/와")%응?  \@ TENSION(TARGET, 8) == "높다" ? 아하하!  # 우후후… \@」
						ENDIF
					ENDIF
			ENDSELECT
		;그 외의 봉사계 ACT
		ELSE
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운 행동"
					SELECTCASE RANDCOM(2, 20)
						CASE 0
							PRINTFORMW 「거역할 수 없기 때문에 어쩔 수 없어서 느낌이군요.%RAND_SPLIT("시시하다/재미있지 않다")%원」
							;파생:소극적인의가 마음에 들지 않는다
							IF TFLAG:REACT파생
								PRINTFORMW 「한 번%CNAME(MASTER, TARGET)%를 다시 예의범절을 가르칠 필요가 있을 듯 하구나……」
							;파생:소극적이지만 좋은가
							ELSE
								PRINTFORMW 「좀 더 즐겁게 해 준다고 생각했지만」
							ENDIF
						CASE 1
							;BASE:7 초조
							PRINTFORMW 「\@ TENSION(TARGET, 7) == "높다" ? 는!?  # \@뭐야, 상당히 소극적이지 않아」
							;파생:소극적인의가 마음에 들지 않는다
							IF TFLAG:REACT파생
								PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 나를 화나게 할 생각?」
							;파생:소극적이지만 좋은가
							ELSE
								PRINTFORMW 「%RAND_SPLIT("특/구별")%에 기대하고 있었다는 것이 아니지만, 그렇다고 해서 너무 허술한 것도, 저기…」
							ENDIF
					ENDSELECT
				CASE "무리가 없는 행동"
					SELECTCASE RANDCOM(2, 21)
						CASE 0
							PRINTFORMW 「말해진 것만 최저한이라든지…,%RAND_SPLIT("바보/어리석은 자")%의 생각할 것 같은 일이군요」
							;파생:소극적인의가 마음에 들지 않는다
							IF TFLAG:REACT파생
								PRINTFORMW 「나, 정말 싫은 것이군요. 그러한 것……」
							;파생:소극적이지만 좋은가
							ELSE
								PRINTFORMW 「지루한 것이야,%CNAME(MASTER, TARGET)%……」
							ENDIF
						CASE 1
							PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 말해진 것만 하고 있으면 좋다고 생각하고 있어?」
							PRINTFORMW 「\@ TFLAG:REACT파생 ? 마음에 들지 않는다 # 그다지 감탄하지 않는다 \@원이군요, 그러한 것……」
					ENDSELECT
				CASE "무리를 한 행동"
					;실은 그렇지만도 않다
					IF COMCOR_POSI()
						PRINTFORMW 「사실…, 속상하네요. 좀 더 어떻게든 안 되는거야?」
						PRINTFORMW 「너무 지루해,\@ TFLAG:REACT파생 ? 화가 나는거야…!  # 싫게 되어 와요… \@」
					;실은 싫어
					ELSE
						PRINTFORMW 「그런 마지못해서면서 되어도 곤란함%RAND_SPLIT("의 것이군요/원/원이군요")%…」
						;파생:소극적인의가 마음에 들지 않는다
						IF TFLAG:REACT파생
							PRINTFORMW 「-전언 철회. 곤란하다고 하는 것보다, 마음에 들지 않네요. 그러한 태도…」
						;파생:소극적이지만 좋은가
						ELSE
							PRINTFORM 「뭐,%CNAME(MASTER, TARGET)%에 뭔가를 강요하는 일자체는, 나쁘지 않은 기분이지만
							CALL PRINT_SYMBOL_K14
