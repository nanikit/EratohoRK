@KOJO_NO9_MS_COM_K14_42
;-------------------------------------------------
;·REACT파생없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM42, 적극적한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;REACTION_MESSAGE내는 COMCOR_POSI()로 밝혀질 수 있거나 하지만, 아무튼 이것으로 조달할 수 있을까나와
	;=================================================
	;무리를 한 행동
	;=================================================
	IF 구상기입체크(100) && (GET_COM_RANK() == "무리를 한 행동" && (!TCVAR:갈것같다 || PALAM:쾌Ｖ < 8500 || PALAM:쾌Ａ < 8500 || PALAM:쾌Ｂ < 8500))
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「무리하고 있는 것이 보이고 보여라. 뭐 그렇지만…」
				PRINTFORM 「사쿠야의 그 자세는 평가해야 할까. 좋은 경향이예요
				CALL PRINT_SYMBOL_K14
			CASE "일반"
				PRINTFORMW 「아라아라, 노력해 버려 기특한 일. 뭐 기분은 나쁘지 않지만…」
				PRINTFORMW 「\@ AFFECTION("몹시 사랑함") ? 포상을 갖고 싶으면 # 그만한 취급을 받고 싶으면 \@, 좀 더 노력하는 일이군요」
				SIF AFFECTION("몹시 사랑함")
					PRINTFORMW 「기대하고 있어요. 후훗…」
			CASE "낮다"
				PRINTFORMW 「필사적으로 꼬리 흔들어 버려……,%CNAME(MASTER, TARGET)%에는 프라이드가 없는 것일까?」
				PRINTFORMW 「뭐, 나는 별로 상관없어요? 그만한,%CNAME(MASTER, TARGET)%에 적당한 취급을 해 주기 때문에」
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후…」")
				CLEARLINE 1
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "봉사")
		IF AFFECTION("상사상애")
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORM 「후앗
					CALL AUTOHEARTCB_K14
					PRINTFORMW   나, 싫닷! 거기, 약한데……」
					PRINTFORMW 「읏…, 아… 앗! 이렇게, 되면…!」
				CASE 1
					PRINTFORM 「아읏
					CALL AUTOHEARTCB_K14
					PRINTFORMW   응아, 하아읏…, 응쿳…!」 
					PRINTFORM 「좋아요……, 좀 더, 좀 더… 아아…
					CALL PRINT_SYMBOL2_K14
			ENDSELECT
		ELSEIF AFFECTION("몹시 사랑함")
			PRINTFORMW 「앗, 응읏! 꽤, 좋은 느낌이야……」
			PRINTFORMW 「… 응앗! 야…, 그런, 곳까지……」
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「응읏…! 아무튼 아무튼이라는 느낌이군요…」
					PRINTFORMW 「언제나 이 상태로 부탁하고 싶은 것이예요……」
				CASE 1
					PRINTFORMW 「응…, 응읏! 나쁘고, 없네요…」
					PRINTFORMW 「이봐요, 좀 더 부탁…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;성교의 기승위는 봉사계 반응이 나온다. 테두리 신설이 귀찮아서 여기에 떨어져 받는다
	;=================================================
	;ACT분류가 성적봉사 혹은 ACT 기승위
	;=================================================
	IF 구상기입체크(100) && (MENUMATCH(TFLAG:ACT, "성적봉사") || IS_NOWACTNAME("기승위") || TEQUIP:성교봉사중 || TEQUIP:성교중 == 3)
		IF TEQUIP:성교중 == 3 || IS_NOWACTNAME("기승위")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응읏, 아앗! 하아… 하웃, 응읏!」")
			PRINTFORMW 「좋은 느낌이야, 사쿠야……」
			PRINTFORMW 「이봐요, 좀 더 좋은 소리를 듣게 해 줘…」
		ELSE
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORM 「웃…, 아앗
					CALL AUTOHEARTCB_K14
					PRINTFORMW   다, 안돼엣! 좀 더, 천천히……」
					PRINTFORMW 「힛……, 앗! 아앗! 읏…, 응쿳!」
				CASE 1
					PRINTFORM 「아읏
					CALL AUTOHEARTCB_K14
					PRINTFORMW   응아! 하아읏……, 읏!」
					PRINTFORM 「좋아요……, 좀 더, 좀 더! 아아…
					CALL PRINT_SYMBOL2_K14
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;범용 처리
	;-------------------------
	IF AFFECTION("상사상애")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「뭐, 그래요. 그 정도는 해 받지 않으면」
				PRINTFORMW 「따로 칭찬하고 있는 것이 아니에요? 이 정도의 일, 당연하다고 생각하고…?」
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「킥킥…」")
				CLEARLINE 1
			CASE 1
				PRINTFORMW 「완전히 길러 말해져 버려……, 라고 말하는 것은 금구일까? 후훗」
				PRINTFORMW 「그렇지만 그렇구나.%CNAME(MASTER, TARGET)%가 솔직하면, 나도 기쁜거야?」
				PRINTFORM 「이렇게 보여도,…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ELSE
		SELECTCASE RANDCOM(2, 4)
			CASE 0
				PRINTFORMW 「아라, 상당히 적극적이 아니야?」
				PRINTFORMW 「뭐, 이것은 이것대로…, 나쁘지 않아요.%CNAME(MASTER, TARGET)%의 본심은 모르지만, 말야」
			CASE 1
				PRINTFORMW 「우선, 솔직하게 나에 따르는 것은 좋은 경향이라고 말할 수 있을까?」
				PRINTFORM 「언제나 이런 느낌이라고, 나도 락 할 수 있…, 지만
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

