@KOJO_NO9_MS_COM_K14_50
;-------------------------------------------------
;TFLAG:REACT派生 == 0, 勝手にオナニーしてるけどいいか, 依存度変化(-1, 0)
;TFLAG:REACT派生 == 1, 勝手にオナニーするな, 依存度変化(-3, +2)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_COM50_0, 派生：勝手にオナニーしてるけどいいか
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;ACT分類が会話
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 1)
			CASE 0
				PRINTFORMW 「こらこら…」
				PRINTFORMW 「人が話してる時に、自慰をするのはやめなさい…」
			CASE 1
				PRINTFORMW 「……やれやれ、ね」
				PRINTFORMW 「頭痛くなってきたわ……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類が奉仕
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "奉仕")
		;調教者満足度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「…やれやれ。咲夜のオナニー好きにも困ったものね」
			CASE "一般"
				PRINTFORMW 「うん、まぁ別にしてもいいけど…何も今じゃなくてもいいでしょう？」
			CASE "低い"
				PRINTFORMW 「ねぇ、一体何をしているのよ…」
				PRINTFORMW 「私そんなの許可した覚えはないけど？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;調教者BASE:満足派生
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORMW 「…やれやれ。%CNAME(MASTER, TARGET)%のオナニー好きにも困ったものね」
					IF AFFECTION("相思相愛")
						PRINTFORM 「まぁ、私も人の事言えないけど
						;1/2でPS
						IF RAND:2
							PRINTFORMW 。 ふふっ…」
						ELSE
							CALL PRINT_SYMBOL_K14
						ENDIF
					ENDIF
				CASE 1
					PRINTFORMW 「なんていうのか、こう…」
					PRINTFORMW 「欲望に忠実すぎるのも考え物ね？」
			ENDSELECT
		CASE "一般"
			SELECTCASE RANDCOM(2, 3)
				CASE 1
					PRINTFORMW 「うん、まぁ別にしてもいいけど…何も今じゃなくてもいいでしょう？」
				CASE 2
					PRINTFORMW 「ねぇ、一体何をしているのよ…」
					PRINTFORMW 「私そんなの許可した覚えはないけど？」
			ENDSELECT
		CASE "低い"
			SELECTCASE RANDCOM(2, 4)
				CASE 0
					PRINTFORMW 「……まったく」
					PRINTFORMW 「どうしようもない、オナニー好きの変態ね…\@ T_COND("怒り") ? ！ # \@」
				CASE 1
					PRINTFORMW 「……盛りのついたメス犬って、見るに耐えないわね」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

