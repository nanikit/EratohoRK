;-------------------------------------------------
;관련 변수…
;FIRSTEVENT
;첫회 EVENT 판정 함수  TFLAG:이벤트번호(TFLAG:10)를 자동으로 참조해,
;현재 발생하고 있는 EVENT 마다 번호가 독립해 보존된다
;-------------------------------------------------
;EV12 FIRSTTIME(0) ～ (11)
;	조교 중지
;		포상에 의한 중지
;			루트 A FIRSTTIME(0)(1)(2)(3)
;				TRUE END 해제용 KFLAG:(FLAG:주인님):10
;			루트 B
;		조교 대상의 피로에 의한 중지
;	조교 개시 직후 FIRSTTIME(10)
;	벌개시 FIRSTTIME(11)
;-------------------------------------------------

;========================================================================
;EV12, 조교자 행동 서두 FIRSTEVENT(0) ～
;========================================================================
;·TRAIN_PROCCES.ERB내, @TRAIN_CANCEL로부터 불려 간다
;·구상주의 파라미터 표시의 직후에 표시. 조교 지시는 이미 결정되고 있다
;·조교 중지 선언의 교환이나, 조교 턴 개시전에 주인님으로부터 한 마디, 와 같이 쓰는 일도 가능
;·오전, 오후의 조교 개시는 아니고, 조교 턴의 개시전에는 매회 불려 가는 개소
;·그 때문에도 있어, 조교 중지시와 경과시간 0이외의 개소는 IF 구상 기입 체크(25) 등으로서 확률 표시하는 일을 추천
;·조교 중지시에게만 지문이 존재한다
;·문장 표시 후에 본체측의 DRAWLINE 처리 있어
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_12(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0. 조교가능/1. 구상주의 피로로 중지/2. 조교 대상의 피로로 중지/3. 포상에 의한 중지)
;-------------------------------------------------

;─
;조교 중지 FIRSTEVENT(0)(1)(2)(3) (9까지 확보)
;─
;·피로로 조교가 중지가 되는 경우의 구상
;·기입 체크를=2로 하면, 디폴트의 휴일 선언을 표시하지 않는다
;-------------------------------------------------
;BEFORE_ACT로부터 잘라 독립해 처리되고 있다
;조교 중지의 조건으로부터 빠졌을 경우는 CALL _BEFORE_ACT_SUB1로 조교 개시 직후 구상이 불리는 흐름
;상당히기를 중지해, ARG마다 테두리를 마련하고 새긴다
;-------------------------------------------------
;FIRSTEVENT는 이벤트번호 마다 관리되고 있으므로
;전체의 일련 번호를 고려할 필요는 없다
;따라 이벤트마다 FIRSTEVENT(0)로부터 사용해, 이벤트번호 안에서 입지 않으면 OK
;━━━━━━━━━━━━━━━━━━━━━━━━
;포상에 의한 중지
;RESULT치=(1, 구상의 뒤로 디폴트문을 표시(비추천), 2, 구상만 표시)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 3
	;=================================================
	;루트 A
	;=================================================
	IF 구상기입체크(100) && KFLAG:(FLAG:주인님):8 == 1
		;TRUE END에 연결되는 일련의 이벤트
		;사쿠야와 피에 관련되는 이야기, 첫회
		;-------------------------------------------------
		;루트 A FIRSTEVENT(0)
		;-------------------------------------------------
		IF 구상기입체크(100) && FIRSTEVENT(0)
			CALL BGM_CUTIN("인형이 있는 풍경")
			
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋은 냄새가 나요. 파이 옷감의 타는 냄새일까」")
			PRINTFORMW 「과연, 메이드장은 냄새를 잘 맡아요. 애플 파이를 구어 본거야」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「사람이 요리해 주는 것은 신선한 느낌이 든다. 나쁘지 않네요」")
			PRINTFORMW 「뭐, 그렇겠지요? 지금 준비하기 때문에 얌전하게 앉아 기다리고 있어」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그게 뭐야, 아이 취급이야?」")
			PRINTFORMW 「… 좋으니까, 얌전하게 하고 있어」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 알았다」")
			PRINTFORMW 「후훗, 솔직하고 좋다♪」
			PRINTFORMDW 앨리스는 막 구운 파이를 테이블에 두면, 나이프로 분리하려고 했다…
			PRINTL
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 앨리스, 그 정도는 내가…」")
			PRINTFORMW 「조금! 칼날 가지고 있을 때 가까워지지 말고, 는……!」
			PRINTFORMDW 일어서 걸친 사쿠야와 앨리스가 교착한 순간, 앨리스는 잘못해 손가락끝을 잘라 버렸다…
			PRINTL
			PRINTFORMW 「이제(벌써)! 그러니까 얌전하고 자리, 는…… 자, 사쿠야?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「미안해요…… 내가 책임져요」")
			PRINTFORMDW 사쿠야는 그렇게 말하면 앨리스의 손가락끝을 입에 넣어, 피를 빨기 시작했다…
			PRINTL
			PRINTFORMW 「저기… 그것이, 책임을 진다는 것이야?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 응. 좋으니까, 그대로 가만히 하고 있어」")
			PRINTFORMW 「… 그런 말을 들어도. 그래서, 언제까지 계속할 생각이야?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋으니까, 좀 더 이대로…」")
			PRINTFORMDW 그렇게 해서 사쿠야는 언제까지나 피를 계속 빨았다. 붉게 물든 눈동자에, 이상한 빛을 가득 채우면서…
			
			CALL TWAIT3_K14
			RETURN 2
		ENDIF
		
		;사쿠야와 피에 관련되는 이야기, 제 2회
		;-------------------------------------------------
		;루트 A FIRSTEVENT(1)
		;-------------------------------------------------
		IF 구상기입체크(100) && FIRSTEVENT(1)
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 앨리스, 손목 잘라도 괜찮아?」")
			PRINTFORMW 「그 만큼 (들)물으면 단순한 엽기 취미의 괴짜네. 어차피 또, 저것이지요?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이해가 빠르고 살아나요」")
			PRINTFORMW 「별로 좋지만…… 너무 깊게 자르지 마」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「에에, 알고 있다」")
			
			PRINTFORMDL 사쿠야는 그렇게 말하면, 꺼낸 나이프로 앨리스의 손목에 새겼다
			PRINTFORMDL 너무 얕지 않고, 너무 깊지 않고, 절묘하게 붙여진 그 상처으로 피가 서서히 스며나온다
			PRINTFORMDW 사쿠야는 상처에 입술을 대어, 살그머니 입맞춤한 뒤 피를 빨기 시작했다…
			PRINTL
			
			PRINTFORMW 「… 정말, 사쿠야는 변해요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일 없어요. 나는 있어도 보통이야」")
			PRINTFORMW 「그것, 괴짜의 상투구…… 아무튼 그것은 좋지만, 당신은 정말로 인간인 것일까?」
			PRINTFORMW 「정직, 사쿠야(정도)만큼 피에 집착 하는 인간을 몰라요. 그렇다고 해서, 흡혈귀화하고 있는 것도 아닌 것 같지만…」
			PRINTFORMW 「혹시, 사쿠야는 단피르가 아닌거야?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런데 어떨까요? 나는 인간의 생각이야, 거기에…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「단피르는 멸종했다고 듣고 있어요. 50년 정도 전에, 말야」")
			PRINTL
			PRINTFORMW 「단피르가 그것이라고 알지 못하고, 인간으로서 살아 있다는 것일 가능성도 버려지지 않네요?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「꽤 재미있는 고찰이군요? 그렇다고 해도 졸지에는 수긍 할 수 없지만」")
			PRINTFORMW 「후훗, 신경쓰지 마. 나로서도 더 이상 잔소리할 생각은 없으니까」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「에에, 알고 있다」")
			PRINTFORMDW 그렇게 해서, 때는 지나 갔다…
			
			CALL TWAIT3_K14
			RETURN 2
		ENDIF
		
		;사쿠야와 피에 관련되는 이야기, 제 3회
		;-------------------------------------------------
		;루트 A FIRSTEVENT(2)
		;-------------------------------------------------
		IF 구상기입체크(100) && FIRSTEVENT(2)
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기 앨리스. 또 평소의 저것… 부탁해도 좋을까」")
			PRINTFORMW 「네네, 알았어요. 나의 경우, 상처는 치유 마법으로 어떻게라도 되지만… 보통 손목이라든지 자르는 것이 아니네요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「다른 것으로 할 이유 없잖아. 내가 이런 일 하는 것은 앨리스만이야」")
			PRINTFORMW 「그 거 기뻐해도 괜찮은 것인지 미묘한 점…, 사쿠야는 어째서 나의 피를 빠는 거야?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 좋아하기 때문에」")
			PRINTFORMW 「피가?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앨리스가, 에 정해져 있을까요?」")
			PRINTFORMW 「… 자주(잘) 알지 않아요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그래, 그렇다면 당신도 시험해 봐?」")
			PRINTFORMDW 사쿠야는 그렇게 말하면, 꺼낸 나이프로 스스로의 손목에 새겼다…
			PRINTL
			
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「네, 좋아요」")
			PRINTFORMW 「하?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아니, 그러니까… 나의 피를 빨아도 좋아요, 라는 일」")
			PRINTFORMW 「나에 그렇게 말하는 취미는 없지만……, 아무튼 거기까지 말한다면」
			PRINTFORMDL 앨리스는 사쿠야의 손목에 입술을 대어, 그 피를 빨기 시작했다
			PRINTFORMDW 사쿠야는 그 상태를 봐 만족스럽게 미소지으면, 스스로도 앨리스의 피를 빨기 시작했다…
			
			PRINTL
			PRINTFORML 「… 응, 이상한 느낌이군요. 뭐라고 할까…, 하나에―」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「-될 수 있던 것 같은 생각이 든다」")
			PRINTFORMW 「… 씌우는 것은 그만두어 줘. 그렇지만, 그렇구나. 그런 생각이 들어요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이상하네요, 우리들」")
			PRINTFORMW 「당신이 그것을 말하는 것? 그렇지만 아무튼, 그래요. 후훗」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「킥킥」")
			PRINTFORMDW 그렇게 해서, 때는 지나 갔다…
			
			CALL TWAIT3_K14
			RETURN 2
		ENDIF
		
		;사쿠야와 피에 관련되는 이야기, 최종회.
		;-------------------------------------------------
		;루트 A FIRSTEVENT(3)
		;-------------------------------------------------
		IF 구상기입체크(100) && FIRSTEVENT(3)
			PRINTFORMDL 소파에 줄서 앉은 두 명은
			
			IF FLAG:계절 == 0 ;계절이 봄
				SIF TIME == 0 ;낮
					PRINTFORM 창으로부터 찔러넣는 봄의 햇볕을 즐기면서
				SIF TIME == 1 ;밤
					PRINTFORM 창으로부터 들여다 보는 롱월을 즐기면서
			ELSE ;봄 이외
				PRINTFORM 낙낙하게 지나가는 시간을 즐기면서
			ENDIF
			PRINTFORMDW , 이야기에 꽃을 피웠다…
			
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이렇게 해 한가로이 보낸다 라고 하는 것도 나쁘지 않네요」")
			PRINTFORMW 「정말로, 말야」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런데, 슬슬 저것을 하고 싶은 것이지만」")
			PRINTFORML 「말하고 있는 곁으로부터 그것이야? 뭐 좋지만…」
			CALL FADEIN(15, "(나의 흐르게 되기 쉬움도 대개군요…)")
			WAIT
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「오늘은 조금 취향을 바꾸어 볼 생각이야」")
			PRINTFORMW 「뭐야 그것?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후…… 뭐, 좋으니까 맡겨」")
			
			PRINTFORMDL 사쿠야는 꺼낸 나이프로 앨리스의 손목에 상처를 입히면, 스스로의 손목에도 새기고 상처를 붙였다
			PRINTFORMDW 그리고 앨리스의 손을 잡아, 두 명의 상처를 거듭해 맞춘다
			PRINTFORMDW 스며나오는 서로의 피가 용해되어, 하나의 물방울이 되어 타 떨어져 작은 얼룩을 만들어 간다…
			PRINTFORMDW 앨리스가 사쿠야를 바라보면 그 표정은 온화해, 채워지고 있어 그 모습에 어딘가 기시감을 기억했다…
			PRINTL
			
			CALL FADEIN(15, "(무엇일까, 이 느낌…… 우리들, 옛날 어디선가……)")
			WAIT
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응, 어떻게든 했어?」")
			PRINTFORMW 「아무것도 아니에요, 신경쓰지 마」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 말을 들으면 더욱 더 신경이 쓰이지만?」")
			
			PRINTFORM 「안 돼요  가·르·쳐·주·지·않·는·다
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「당신, 그렇게 말하면 이제 (듣)묻지 않는 거네? 얌전하게 단념해요…」")
			PRINTFORMW 「그것이 현명하구나. 후후」
			PRINTFORMDW 그렇게 해서, 때는 지나 갔다…
			
			;TRUE END용 플래그에 대입
			KFLAG:(FLAG:주인님):10 = 1
			
			CALL TWAIT3_K14
			RETURN 2
		ENDIF
		
		;그 외
		;PRINT
		RETURN 2
	ENDIF
[SKIPSTART]
	;=================================================
	;루트 B
	;=================================================
	IF 구상 기입 체크(0) && KFLAG:(FLAG:주인님):9 == 1
		;-------------------------------------------------
		;루트 B첫회 FIRSTEVENT(4)
		;-------------------------------------------------
		IF 구상 기입 체크(0) && FIRSTEVENT(4)
			PRINTFORMW
			RETURN 2
		ENDIF
		
		;그 외
		PRINTFORMW
		RETURN 2
	ENDIF
	
	;그 외 포상에 의한 중지
	SELECTCASE RAND:4
		CASE 0
			PRINTFORMW
		CASE 1
			PRINTFORMW
		CASE 2
			PRINTFORMW
		CASEELSE
			PRINTFORMW
	ENDSELECT
	RETURN 2
[SKIPEND]
ENDIF

[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자의 피로에 의한 중지
;RESULT치(1, 구상의 뒤로 디폴트문을 표시(비추천), 2, 구상만 표시)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상 기입 체크(0) && ARG == 1
	PRINTFORMW
	RETURN 2
ENDIF

