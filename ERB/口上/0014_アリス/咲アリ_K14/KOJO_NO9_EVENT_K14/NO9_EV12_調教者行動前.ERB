;─
;·조교 방침이 바뀌었을 때에 표시한다
;·어디까지나 방침인 것으로 행동을 세세하게 결정하는 것은 아니다
;-------------------------------------------------
;관련 변수…
;TFLAG:턴방침		이 턴의 조교 방침(1=휴게/2=소프트/3=노멀/4=하드/5=이상)
;TFLAG:전턴방침		전턴의 조교 방침(내용은 턴방침과 같음)
;
;관련 함수…
;IS_POLICY	조교 방침 명칭과 합치하면 진을 돌려준다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 방침 변경
;━━━━━━━━━━━━━━━━━━━━━━━━
;·25%
;-------------------------------------------------
IF 구상기입체크(25) && (TFLAG:턴방침 != TFLAG:전턴방침)
	;=================================================
	;턴방침 1:휴게(확실히 체력 회복 행동이 된다) 60
	;=================================================
	IF 구상기입체크(100) && IS_POLICY("지금", "휴게")
		SELECTCASE RANDEVENT(2, 60)
			CASE 0
				PRINTFORMW 「조금 지쳤군요…, 휴게로 합시다」
				PRINTFORM 「괜찮아…, 시간은 아직 있어요
				CALL PRINT_SYMBOL_K14
			CASE 1
				PRINTFORMW 「조금 휴게로 할까?」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;턴방침 2:소프트			61
	;=================================================
	IF 구상기입체크(100) && IS_POLICY("지금", "소프트")
		PRINTFORM 「자, 귀여워해 주어요. 여기에 계(오)세요
		CALL PRINT_SYMBOL_K14
		
		SELECTCASE RANDEVENT(2, 61)
			CASE 0
				PRINTFORMDW 앨리스는 자애로 가득 찬 표정으로%CALLNAME:MASTER%를 응시하고 있다…
			CASE 1
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아이 상대가 아닐 것이고, 그 거 어떨까」")
				PRINTFORMW 「아라, 별로 좋지 않아. 가끔 씩은 이런 것도 좋을 것입니다?」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐 좋아요. 좋을대로하세요」")
				PRINTFORMW 「그런 일 말하고 있지만, 사실은 그렇지만도 않겠지요?」
				IF AFFECTION("조교가 끝난 상태")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫!? 바보 같은 일 말하지 않아 받을 수 있을까…」")
					PRINTFORMW 「후후, 솔직하지 않은거네. 사쿠야」
				ELSE
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「농담도 쉬엄쉬엄 말했으면 좋네요」")
					PRINTFORMW 「그래? 그것은 유감이, 구나…」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;턴방침 3:노멀		62
	;=================================================
	IF 구상기입체크(100) && IS_POLICY("지금", "노멀")
		PRINTFORMW 「보통이 1번일까요…」
		RETURN 1
	ENDIF
	
	;=================================================
	;턴방침 4:하드			63
	;=================================================
	IF 구상기입체크(100) && IS_POLICY("지금", "하드")
		IF ALI(0)
			PRINTFORMW 「사쿠야, 조금 격렬하게 가요」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 것, 부탁한 기억은 없지만……」")
			
			SELECTCASE RANDEVENT(2, 63)
				CASE 0
					PRINTFORMW 「이상하네요? 당신이 바라고 있다고 생각했지만」
				CASE 1
					PRINTFORML 「당신의 잠재적인 소망을 실현해 주려고 생각해?」
					PRINTFORMW 「아, 별로 감사의 말은 필요하지 않아요」
			ENDSELECT
			
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「하? 농담도 대충 해두었으면 좋네요」")
			PRINTFORMW 「농담 같은거 말한 생각 없지만?」
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「-그렇게 온다고 생각하고 있었어요」")
			PRINTFORMW 「후후, 과연. 잘 알고 있지 않아. 사쿠야…」
		ELSE
			PRINTFORMW 「자, 격렬하게 가요!」
			PRINTFORMW 「각오 하세요」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;턴방침 5:이상			64
	;=================================================
	IF 구상기입체크(100) && IS_POLICY("지금", "이상")
		SELECTCASE RANDEVENT(2, 64)
			CASE 0
				PRINTFORMW 「…………」
				PRINTFORMDW 앨리스는 눈동자에 요염한 빛을 가득 채우면서,%CALLNAME:MASTER%에 다가왔다…
			CASE 1
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「지금의 당신, 조금 보통이 아니네요. 앨리스…」")
				PRINTFORMW 「아라? 나는 있어도 보통이야」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「보통이 아닌 사람은 모두 그렇게 말하는거야」")
				PRINTFORMW 「후후, 아무튼 그럴지도. 그렇지만 어떻든지 좋아요, 그런 일」
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아휴,……」")
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;─
;조건 없음 FIRSTEVENT(70)～
;─
;·여기까지의 조건에 히트 하지 않았던 경우에 표시되는 개소
;·BASE로 소분기를 마련해 있습니다만, 임의의 조건을 추가하는 일도 할 수 있습니다
;------------------------------------------------
;관련 변수…
;BASE		(0, 체력/1, 기력/2, 사정/3, 모유/4, 소변욕/5, 이성/6, 흥미/7, 초조해/8, 만족)
;
;관련 함수…
;TENSION	인수로 지정한 BASE치를 나타낸다(50%이상은 높은,25%이상은 일반,25% 미만은 낮다)
;------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;조건 없음
;━━━━━━━━━━━━━━━━━━━━━━━━
;·100% 소분기마다 확률을 바꾼다
;-------------------------------------------------
IF 구상기입체크(100)
	;=================================================
	;만족 50％이상
	;=================================================
	IF 구상기입체크(25) && TENSION(TARGET, 8) == "높다" ;BASERATIO(TARGET, 8, 80)
		PRINTFORMW 「좋아요… 꽤 좋은 느낌이야」
		PRINTFORM 「자, 계속해요
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;이성 25％이하
	;=================================================
	IF 구상기입체크(100) && TENSION(TARGET, 5) == "낮다" ;! BASERATIO(TARGET, 5, 20)
		PRINTFORMW 「나… 나… 이, 이제(벌써)……」
		RETURN 1
	ENDIF
	
	;=================================================
	;흥미 25％이하
	;=================================================
	IF 구상기입체크(30) && TENSION(TARGET, 6) == "낮다" ;! BASERATIO(TARGET, 6, 20)
		PRINTFORMW 「뭐라고 할까…%RANDTEXT_K14("흥을 깸이, 군요", "질려 왔군요")%」
		RETURN 1
	ENDIF
	
	;=================================================
	;초조 50％이상
	;=================================================
	IF 구상기입체크(25) && TENSION(TARGET, 7) == "높다" ;BASERATIO(TARGET, 7, 50)
		PRINTFORMW 「그렇게 나를 화나게 하고 싶은 것일까요?」
		PRINTFORMW 「좋아요, 깨닫게 해 준다……」
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(25)
		IF GET_KFLAG_K14("상사상애") && !RAND:3
			PRINTFORM 「후후…… 즐겁네요
			CALL PRINT_SYMBOL_K14
		ELSE
			PRINTFORMW 「그런데,%RAND_SPLIT("는 다음은/다음은 어떻게 하지")%……」
		ENDIF
		RETURN 1
	ENDIF
ENDIF