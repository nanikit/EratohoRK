;━━━━━━━━━━━━━━━━━━━━━━━━
;調教者が着替えた FIRSTEVENT(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;通常になった
		CASE 1
			PRINTFORMW 「これが１番私らしいかしら､ね…？」
			RETURN 1
		;半裸になった
		CASE 11
			PRINTFORMW 「%CNAME(MASTER, TARGET)%、こういう格好が趣味なのかしら…？」
			RETURN 1
		;全裸になった
		CASE 21
			IF TALENT:TARGET:妊娠 && TALENT:TARGET:ご主人様
				PRINTFORML 「家の中とは言っても……」
				PRINTFORMW 「妊婦が全裸でうろうろするのって、ちょっとどうかって気もするわ」
			ELSE
				PRINTFORMW 「…変態」
			ENDIF
			RETURN 1
		;……すーすーする						現在のR#ぽでは実装されていない。将来的には復活する可能性は割りとある
		;CASE 31
		;	PRINTFORMW 「私にスカートのたくし上げをして欲しいのかしら…？」
		;	RETURN 1
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

