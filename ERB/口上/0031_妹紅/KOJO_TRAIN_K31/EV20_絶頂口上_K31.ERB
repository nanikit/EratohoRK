		;アライメント負
		ELSE
			PRINTFORML 「んはぁ。いっぱい出しちゃった」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%の中、本当に気持ちよかったよ」
		ENDIF
		RETURN 2
	ENDIF
	
	;=================================================
	;COMが中に出さないで！
	;=================================================
	IF 口上記入チェック(100) && IS_NOWCOMNAME("中に出さないで！")
		;アライメント正
		IF ALI(0)
			PRINTFORML 「んはぁ。いっぱい出しちゃった」
			PRINTFORML 「ん、酷いって？でも気持ちよかったんでしょ、あんなに腰振っちゃって」
			PRINTFORMW 「それで、中は嫌だと言われてもね」
		;アライメント負
		ELSE
			PRINTFORML 「んはぁ。いっぱい出しちゃった」
			PRINTFORML 「ん、酷いって？でも気持ちよかったんでしょ、あんなに腰振っちゃって」
			PRINTFORMW 「それで、中は嫌だと言われてもね」
		ENDIF
		RETURN 2
	ENDIF
	
	;アライメント正
	IF ALI(0)
		PRINTFORML 「んはぁ。いっぱい出しちゃった」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%の中、本当に気持ちよかったよ」
	;アライメント負
	ELSE
		PRINTFORML 「んはぁ。いっぱい出しちゃった」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%の中、本当に気持ちよかったよ」
	ENDIF
	RETURN 2
ENDIF

