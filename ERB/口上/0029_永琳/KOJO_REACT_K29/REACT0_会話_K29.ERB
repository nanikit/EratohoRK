;────────────────────────────────────
;REACT5, 衣装の変更を要求する・個別リアクション
;────────────────────────────────────
;・調教対象のリアクションによって一枚脱ぐ、二枚脱ぐ、脱がないに分かれる
;・REACT分類が"消極的従う", "受け入れる"の場合は一枚脱ぐ
;・REACT分類が"積極的従う", "平然と従う"の場合は二枚脱ぐ
;・その他の場合は脱がない(PALAM恥情/反抗の数値によって、恥ずかしくて脱がない、嫌がって脱がないに分かれる)
;-------------------------------------------------
@KOJO_REACT_K29_5
;-------------------------------------------------
;・TFLAG:ACT派生(0=服を脱ぐように命令/1=服を着るように命令)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_0, 服を脱ぐように命令
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック() && TFLAG:ACT派生 == 0
	;=================================================
	;一枚脱いだ(消極的反応)
	;=================================================
	IF 口上記入チェック() && IS_COMGRONAME("消極的従う/受け入れる")
		;-------------------------------------------------
		;最後の一枚を脱いで全裸になった
		;-------------------------------------------------
		IF 口上記入チェック() && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			;REACT分類受け入れる			全裸になり、期待と興奮で肌が火照るのを感じた
			IF IS_COMGRONAME("受け入れる")
				PRINTFORMW 「あらあら、赤くなっちゃって、可愛いわ」
			;REACT分類消極的従う			仕方なく最後の一枚を脱ぎ、全裸になった
			ELSE
				PRINTFORMW 「いい体ね」
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;緩やかな動きで服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック() && IS_COMGRONAME("受け入れる")
			PRINTFORMW 「さっさと脱ぎなさい」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;仕方なく服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック() && IS_COMGRONAME("消極的従う")
			PRINTFORMW 「まぁいいわ」
			RETURN 1
		ENDIF
		
		;上層の分岐を全て埋めた場合は不要な箇所
		;逆に埋めない場合はこの箇所が補完の役目を果たします
		;-------------------------------------------------
		;その他・条件なし
		;-------------------------------------------------
		IF 口上記入チェック(0)
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;二枚脱いだ(積極的反応)
	;=================================================
	IF 口上記入チェック() && IS_COMGRONAME("積極的従う/平然と従う")
		;-------------------------------------------------
		;全裸になった
		;-------------------------------------------------
		IF 口上記入チェック() && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			PRINTFORMW 「人前でちゃんと脱げる子は手間がかからなくて良いわ」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;積極的に脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック() && IS_COMGRONAME("積極的従う")
			PRINTFORMW 「なかなかいいじゃない」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;手際よく脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック() && IS_COMGRONAME("平然と従う")
			PRINTFORMW 「ずいぶんと慣れた感じね」
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;脱がなかった
	;=================================================
	IF 口上記入チェック() && IS_COMGRONAME("拒否")
		;恥ずかしくて脱げなかった
		IF PALAM:MASTER:恥情 > PALAM:MASTER:反抗
			PRINTFORMW 「あら、恥ずかしがり屋さんね」
			PRINTFORMW 「私が脱がしてあげようかしら？」
		;嫌がって脱がなかった
		ELSE
			PRINTFORMW 「着衣プレイが好きなの？」
		ENDIF
		RETURN 1
	ENDIF
ENDIF


