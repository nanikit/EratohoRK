	;=================================================
	;조교 대상이갈것같다
	;=================================================
	IF 구상기입체크(0) && TCVAR:MASTER:갈것같다
		;애태움수 파생
		SELECTCASE TCVAR:MASTER:애태움수
			;애태움중
			CASE IS > 1
				PRINTFORMW 
			;애태움 종료
			CASE 1
				PRINTFORMW 
			;애태움 없음
			CASEELSE
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;연속 실행
	;=================================================
	IF 구상기입체크(0) && TFLAG:ACT == TFLAG:전ACT
		;위로돌리고
		IF TFLAG:위로돌리고
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;지문은 조교 대상의 하반신 복장 마다의 분기 있어. 필요에 따라서 확장해 주세요
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		;위로돌리고
		IF TFLAG:위로돌리고
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
ENDIF

