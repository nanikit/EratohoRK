IF 구상기입체크(0) && AFFECTION("상사상애")
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTEVENT(20)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

