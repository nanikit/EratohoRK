	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTDATUI(30)
	ENDIF
	
	;=================================================
	;응 이렇게 p
	;=================================================
	IF 구상기입체크(0) && PENIS(TARGET)
	ENDIF
	
	;=================================================
	;벌써 가슴이 보이고 있다
	;=================================================
	IF 구상기입체크(0) && B_DATUICHECK(0) == 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
