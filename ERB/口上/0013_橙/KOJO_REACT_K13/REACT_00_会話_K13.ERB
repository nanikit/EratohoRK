	;REACT부하 1, 뜻을 정해 바쳐도 괜찮다고 고했다
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "무리가 없는 행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;무리를 한 행동
	;=================================================
	;REACT부하 2, 이제 지킬 수 없다면 적어도 상냥하게 여겨지도록(듯이)와 동의 했다
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "무리를 한 행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_3, 기분좋게해줘 주는 것이라면 처음을 주어도 괜찮다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 받아들이는 것 경우
;-------------------------------------------------
IF 구상기입체크(0) && IS_COMGRONAME("받아들인다")
	;자연스러운 행동
	;=================================================
