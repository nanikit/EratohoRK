;·지문에 분기 없음@TARGET_DESCRIPTION에 의한 상황 묘사가 들어갈 가능성 있어
;-------------------------------------------------
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교중/성교 봉사중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:성교중, TEQUIP:성교봉사중) > 0
		;성교중
		IF TEQUIP:성교중
			PRINTFORMW
		;성교 봉사중
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;자위중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(GETBIT(TEQUIP:자위중, 0), GETBIT(TEQUIP:자위중, 1)) > 0
		;조교 대상이 자위중
		IF GETBIT(TEQUIP:자위중, 0)
			PRINTFORMW 
		;조교자가 자위중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;C애무 계속중
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TEQUIP:조교대상C사용, 1, 2)
		;자위·애무 계속중
		IF TEQUIP:조교대상C사용 == 1
			PRINTFORMW 
		;페라·쿤닐링구스 계속중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

