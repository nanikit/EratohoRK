;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 < 2
	;COMの派生を見る	COM毎にRESULTを返す
	SELECTCASE TRAINNAME:SELECTCOM
		CASE "もっとハードにして"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「ハードに、ねぇ…」
				PRINTFORMW 「そんな趣味があったのね、ちょっと対応を変えようかしら」
			;アライメント負
			ELSE
				IF TFLAG:ACT派生 == 1
					PRINTFORMW 「…血が出るほどに扱き上げて欲しいかったの？」
					PRINTFORMW 「やりすぎるのはあまり趣味じゃないのよねぇ…」
					PRINTFORMDW 困った子ねぇ、と紫はぼやきながら腕を組んだ
				ELSE
					PRINTFORMW 「……指じゃなくて拳が良かったの？」
					PRINTFORMW 「やりすぎるのはあまり趣味じゃないのよねぇ…」
					PRINTFORMDW 困った子ねぇ、と紫はぼやきながら腕を組んだ
				ENDIF
			ENDIF
			RETURN 1
		CASE "恥ずかしがる"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「あらあら可愛いわね」
				PRINTFORMW 「でもダメよ、ちゃんと素直にならなきゃ」
				PRINTFORMDW そう言って紫は激しく%CALLNAME:MASTER%を責め立てた
			;アライメント負
			ELSE
				PRINTFORMW 「恥ずかしがるのも悪くはないんだけど…」
				PRINTFORMW 「もうちょっと私を喜ばせてくれないかしら？　少々詰りませんの」
			ENDIF
			RETURN 1
		CASE "嫌がる"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「別に嫌がらなくてもいいじゃない、気持ちよくしてあげるんだから」
				PRINTFORMW 「それとも、お仕置きされるほうがお好みかしら？」
			;アライメント負
			ELSE
				PRINTFORMW 「別に嫌がらなくてもいいじゃない、気持ちよくしてあげるんだから」
				PRINTFORMW 「それとも、お仕置きされるほうがお好みかしら？」
			ENDIF
			RETURN 1
		CASE "下手だと罵る"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「やせ我慢のつもりかしらね」
				PRINTFORMW 「こんなにしておきながらそんなこと言っても、説得力ないわね」
			;アライメント負
			ELSE
				PRINTFORMW 「あら、ごめんなさい」
				PRINTFORMW 「それじゃあ、上手になるために練習をさせてもらうわね」
			ENDIF
			RETURN 1
		CASE "不敵に笑う"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「ふーん…その態度、あまりいい判断とは言えないわね」
				PRINTFORMDW 紫は冷たい笑みを浮かべている
			;アライメント負
			ELSE
				PRINTFORMW 「ふーん…その態度、あまりいい判断とは言えないわね」
				PRINTFORMDW 紫は冷たい笑みを浮かべている
			ENDIF
			RETURN 1
		CASE "快感を我慢する"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「あらあら…我慢してるのね、可愛いわよ」
				PRINTFORMW 「でも、気持ちがいいなら素直になってもいいのよ」
			;アライメント負
			ELSE
				PRINTFORMW 「我慢は体に毒よ」
				PRINTFORMW 「でも、我慢してる%CNAME(MASTER, TARGET)%の顔を見るのはいいわね」
				PRINTFORMW 「簡単に受け入れちゃだめよ、限界まで我慢して」
			ENDIF
			RETURN 1
		CASE "快感を受け入れる"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「気持ちよさそうな顔しちゃって…そんなにいいのかしら？」
				PRINTFORMW 「ここがいいの？　それともここ？」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%は受け入れたのね…いいわ、私も応えてあげる」
				PRINTFORMDW そう言って紫は妖しく微笑んだ
			;アライメント負
			ELSE
				PRINTFORMW 「あらら…もう落ちちゃったのかしら」
				PRINTFORMW 「まあ、これはこれで楽しみ方はあるわね、これからはちゃんと言うことを聞くのよ？」
				PRINTFORMDW 紫はまるでペットの躾でもするように%CALLNAME:MASTER%に接している
			ENDIF
			RETURN 1
		CASE "暴れる"
			;アライメント正
			IF ALI(0)
				PRINTFORMW 「あまりおいたが過ぎると…」
				PRINTFORMW 「わかってるわよねぇ？」
			;アライメント負
			ELSE
				PRINTFORMW 「あまりおいたが過ぎると…」
				PRINTFORMW 「わかってるわよねぇ？」
			ENDIF
			RETURN 1
		;ヒットしなければ汎用リアクションへ
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF
