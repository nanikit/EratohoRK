;────────────────────────────────────
;ACT51, 口で愛撫を強制
;────────────────────────────────────
;・クンニ/フェラチオ/顔面騎乗/顔面騎乗アナル/
;・調教対象に、口による愛撫を強制する
;・調教者がふたなりの場合、調教対象が仰向けなら1/2で顔面騎乗、そうでなければフェラ強制になる
;・調教者にペニスがなく、調教対象が仰向けなら顔面騎乗になる
;・顔面騎乗が選ばれていたとき、顔面騎乗アナルに変化することがある
;・全ての条件から漏れた場合は通常のクンニとなる
;・TFLAG:お仕置きフラグが立っていると発生しない
;・押し倒し中には発生しない
;・TEQUIP(ボールギャグ/三角木馬)装着中は発生しない
;・調教者がペニスバンドを装着している場合は発生しない
;・性交中/性交奉仕中には発生しない
;・REACT分類が拒否/暴れる/逃げるの場合は拒絶扱いになる
;-------------------------------------------------
@KOJO_FEMALEYOU_ACT_K22_51
;-------------------------------------------------
;・TFLAG:ACT派生(0=クンニ/1=フェラ/2=顔面騎乗/3=顔面騎乗アナル)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT51, 口で愛撫を強制
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GROUPMATCH(TFLAG:ACT派生, 0, 2, 3) > 0
	;アライメント正
	IF ALI(0)
		PRINTFORMW 「どう？　私の\@ TFLAG:ACT派生 == 3 ? おしり。ふふ # おまんこ \@」
		PRINTFORMW 「舐めたくなってこない？」
	;アライメント負
	ELSE
		PRINTFORMW 「ほら、舐めなさい」
		PRINTFORMW 「バター塗っておいてあげたわよ？」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT51_1, フェラチオ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 1
	;前回がフェラ
	IF IS_PREVACTNAME("口で愛撫を強制")
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「んっ……良いわ、上手よ」
			PRINTFORMW 「この調子で続けて……」
		;アライメント負
		ELSE
			PRINTFORMW 「誰が止めて良いなんて言ったかしら？」
			PRINTFORMW 「さっさと続けなさい」
			PRINTFORMW 「それとも無理矢理押し込まれるのがお好み？」
		ENDIF
		RETURN 1
	ENDIF
	
	;前回がイラマチオ
	IF IS_PREVACTNAME("イラマチオ")
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「ごめんなさいね、無理矢理で。苦しかったでしょ？」
			PRINTFORMW 「今度は自分のペースでやってみて」
		;アライメント負
		ELSE
			PRINTFORMW 「ふぅ……無理矢理犯すの疲れちゃったわ」
			PRINTFORMW 「貴方からヤってくださらない？」
		ENDIF
		RETURN 1
	ENDIF
	
	;前回がパイズリ
	IF IS_PREVACTNAME("パイズリ")
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「胸の間から出てるさきっぽが寂しそうなの」
			PRINTFORMW 「口でお相手してくださらない？」
		;アライメント負
		ELSE
			PRINTFORMW 「ちゃんと全体に奉仕なさい」
			PRINTFORMW 「はぁ？　届かない？」
			PRINTFORMW 「その口は文句を言うことしかできないのね。呆れたわ」
		ENDIF
		RETURN 1
	ENDIF
	
	;アイマスク
	IF TEQUIP:アイマスク
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「ねぇ、唇に当たってるの分かるわよね？」
			PRINTFORMW 「舐めてみない？」
			PRINTFORMW 「おいしいわよ？」
		;アライメント負
		ELSE
			PRINTFORMW 「ねぇ、唇に当たってるもの、分かるわよね？」
			PRINTFORMW 「そのまま、口を開けて咥えなさい」
			PRINTFORMW 「でないと無理矢理押し込んじゃうわよ？」
		ENDIF
		RETURN 1
	ENDIF
	
	;緊縛中
	IF TEQUIP:縄
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「さあ、私を気持ちよくしてちょうだい」
			PRINTFORMW 「ん、体が動かせない？」
			PRINTFORMW 「大丈夫よ。お口は自由に使えるでしょ？」
		;アライメント負
		ELSE
			PRINTFORMW 「さて、私を気持ちよくして貰いましょうか」
			PRINTFORMW 「うふふ……縛られてるから出来ない……ねぇ」
			PRINTFORMW 「貴方のその口は、そんな良いわけを言う為のものだったかしら？」
		ENDIF
		RETURN 1
	ENDIF
	
	;調教者が自慰中
	IF GETBIT(TEQUIP:自慰中, 1)
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「んっ……ねぇ、見てるだけじゃなくて、貴方も手伝ってくださらない？」
			PRINTFORMW 「ほら、おちんちんの先っぽ舐めて？」
		;アライメント負
		ELSE
			PRINTFORMW 「んっ……ねぇ、いつまでぼーっとしてるつもり？」
			PRINTFORMW 「さっさとしゃぶりなさい。命令よ？」
		ENDIF
		RETURN 1
	ENDIF
	
	;対象が自慰中
	IF GETBIT(TEQUIP:自慰中, 0)
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「ねぇ、貴方ばっかり気持ちよくなってるのはずるいわ」
			PRINTFORMW 「私も気持ちよくして？」
			PRINTFORMW 「ほら、私のおちんちんを貴方のお口で……ね？」
		;アライメント負
		ELSE
			PRINTFORMW 「自分ばっかり気持ちよくなって済むとは思ってないわよね？」
			PRINTFORMW 「そうね、そのだらしなく開いた口で奉仕して貰いましょうか」
		ENDIF
		RETURN 1
	ENDIF
	
	;アライメント正
	IF ALI(0)
		PRINTFORMW 「うふふ……貴方のこと考えていたら、こんなになっちゃったの」
		PRINTFORMW 「責任とって鎮めてくださらない？」
		PRINTFORMW 「貴方のその可愛いお口で」
	;アライメント負
	ELSE
		PRINTFORMW 「さあ、お勤めの時間よ」
		PRINTFORMW 「その生意気な口でご主人様にご奉仕なさいな」
	ENDIF
	RETURN 1
ENDIF

