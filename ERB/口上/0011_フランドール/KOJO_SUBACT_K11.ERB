IF LOCAL && TFLAG:102 && P_DATUICHECK(1) == 3 && !TALENT:MASTER:남자
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;상냥하게 벗긴다(호감도 1000이상·아라이먼트정)
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 1000) && ALI(0)
		;조교자에게 페니스 있어
