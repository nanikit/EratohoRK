;-------------------------------------------------
;REACT90_0, 보통으로 쉬었다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && TFLAG:ACT파생 == 0
	;-------------------------------------------------
	;심약구응답한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL && SELECTCOM == GETNUM(TRAINNAME, "심약구응답한다")
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;무뚝뚝하게응답한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "무뚝뚝하게응답한다")
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;붙임성좋게응답한다
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "붙임성좋게응답한다")
		PRINTFORMW 「신체 쉴 수 있어 활기가 가득. 후훗, 이것으로 또%MASTERNAME%와 엣치할 수 있어요♪」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;강하게응답한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "강하게응답한다")
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;좀더하드하게해
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "좀더하드하게해")
		PRINTFORMW 「--끝나면, 많이 굉장한 일 합시다♪」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;기분 좋게 해
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "기분좋게해")
		PRINTFORMW 「보류, 지금은 휴가중인 것이니까, 착석 해 주세요♪」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;가게해!
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == GETNUM(TRAINNAME, "가게해!")
		PRINTFORMW 「안돼, 지금은 쉬어. 혼자서 가서는 안 돼요♪」
		RETURN LOCAL
	ENDIF
ENDIF

