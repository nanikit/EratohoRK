;-------------------------------------------------
;COM14, 쾌감을참는다
;-------------------------------------------------
;·일정 조건으로 「용서를빈다」취급
;-------------------------------------------------
@KOJO_COM_K11_14
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;회화
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 0
		PRINTFORMW 「우후후, 사실은 기분 좋네요%MASTERNAME%」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;애무
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 1
		IF RAND:3 == 0
			PRINTFORMW 「%MASTERNAME%좀 더 참아. 참으면 할수록, 기분 좋게 될 수 있는거야♪」
			PRINTFORMDW 프랑은%CALLNAME:MASTER%의 귓전으로 달콤한 소리로 속삭이면서, 용서 없게 페니스를 잡아당겨 세운다
		ELSEIF RAND:2 == 0
			PRINTFORMW 「참고 있다%MASTERNAME%를 보면, 고집이라도 기분 좋게 해 주고 싶어져 버린다♪」
		ELSE
			PRINTFORMW 「(이)나, 좀 더 솔직해져도 좋아%MASTERNAME%♪」
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;도구
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 2
		SELECTCASE RANDCOM(2, 1)
			CASE 0
				PRINTFORMW 「참고 있다%MASTERNAME%를 보면, 고집이라도 기분 좋게 해 주고 싶어져 버린다♪」
			CASE 1
				PRINTFORMW 「(이)나, 좀 더 솔직해져도 좋아%MASTERNAME%♪」
			ENDSELECT
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;성교
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 3
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;수치
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 4
		PRINTFORMW 「사실은 부끄러운 일, 기분이 좋지요?」
		PRINTFORMW 「우후후, 모두에게는 입다물고 주기 때문에, 솔직해지며♪」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;봉사
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 5
		PRINTFORMW 「좀 더 해 주지 않으면, 뭔가 좀 더……」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;가학
	;기입 체크
	LOCAL:1 = 1
	;------------------------------------------------- 
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 6
		PRINTFORMW 「사실은 아픈 일, 기분이 좋지요?」
		PRINTFORMW 「우후후, 모두에게는 입다물고 주기 때문에, 솔직해지며♪」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;이상
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 8
		;상사상애
		IF FAVOR(3000, 3000)
			PRINTFORMW
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
			PRINTFORMW
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;휴게
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 9
		;상사상애
		IF FAVOR(3000, 3000)
			PRINTFORMW
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
			PRINTFORMW
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;성교 봉사
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GET_ACTM(TFLAG:ACT) == 10
		PRINTFORM 「감사할 수 있다─를 참아, 프랑을 기분 좋게 해 주고 있는 거야? 아핫, 기쁘다
		CALL K11_PRINT_HEARTCBW
		RETURN LOCAL
	ENDIF
ENDIF

