;-------------------------------------------------
;ACT311, 谷渡り
;-------------------------------------------------
;・３Ｐの一種
;・調教者と助手が上下に重なり、調教対象が二人のヴァギナを交互に犯す
;-------------------------------------------------
@KOJO_ACT_K11_311
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;抜かずに続行
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:抜かず
		CALL PRINT_KOJO_COLORW(ASSI, "『あんっ……あっ…だめっ！…それ、だめぇっ…！』")
		PRINTFORMW 「%CNAME(ASSI)%の声…なんていやらしいの……」
		PRINTFORMW 「ねぇ、ちょっと! いつまで二人でヤってるのよ！」
		PRINTFORMW 「私だって、さっきからずっと…っふあぁっ…！…あんっ…そんな…いきなりされたら……」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;状況描写
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:ACT派生
		SELECTCASE TCVAR:イきそう
			;調教対象がイキそうなのを我慢している
			CASE IS > 5
				PRINTFORMW 「ほら、もっと腰振ってあげないと、%CNAME(ASSI)%が可哀相よ？」
				PRINTFORMW 「あら、イキそうなの？ という事らしいわよ、%CNAME(ASSI)%」
				PRINTFORMW 「思いっきり締め付けて搾り取ってあげて♪」
			;調教対象がイキそう
			CASE IS > 0
				PRINTFORMW 「あッ…あんっ…激しいっ、や…んあぁっ…」
				PRINTFORMW 「イきそう…なのね？ ねぇ、%CNAME(MASTER)%はどっちに注いでくれるのかしら？」
			;通常
			CASEELSE
				PRINTFORMW 「んっ…あっ…！…いいとこで抜かれると辛いわ、これ…」
				PRINTFORMW 「でも……あんあん鳴いてる%CNAME(ASSI)%も可愛いくて……」
				PRINTFORMW 「ほら%CNAME(ASSI)%、キスしましょう。ちゅぅっ…ん……」
		ENDSELECT
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;調教者がイキそう(Ｖ)
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && PALAM:MASTER:TARGET:41 >= 8500
		IF FAVOR(3000, 3000)
			PRINTFORMW
			PRINTFORMW
		ELSE
			PRINTFORMW
			PRINTFORMW
		ENDIF
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;一般@PERCENT
	;記入チェック
	LOCAL:1 = 100
	;-------------------------------------------------
	IF PERCENT(LOCAL:1)
		;相思相愛
		IF FAVOR(3000, 3000)
			PRINTFORMW 「さぁ、いいわよ。私と%CNAME(ASSI)%を一度に犯して頂戴」
			PRINTFORMW 「でも、最初は私からじゃないと駄目なんだからね！」
			PRINTFORMW 「%CNAME(ASSI)%には悪いけど、少し待ってもらうわ。うふっ…」
		;それ以外
		ELSE
			PRINTFORMW 「ほら、好きな方から突いていいわよ…」 
			PRINTFORMW 「%CNAME(MASTER)%はどっちから犯してくれるのかしら？ ウフフ…」 
		ENDIF
		RETURN LOCAL
	ENDIF
ENDIF

