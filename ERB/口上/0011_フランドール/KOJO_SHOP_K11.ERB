;-------------------------------------------------
;프랑 구상 숍 이벤트 구상 파일
;-------------------------------------------------
;숍으로부터 불려 가는 구상을 정리한 파일
;-------------------------------------------------
;*************************************************
;숍 이벤트 구상
;*************************************************

;=================================================
;EV101, 의상 변경
;=================================================
;·숍으로부터 의상 변경을 실시했을 경우에 표시
;-------------------------------------------------
@KOJO_EVENT_K11_101(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=
;  1의 위…(0, 이제(벌써) 입고 있다, 1, 갈아입은, 2, 부끄러워서 입을 수 없는, 3, 싫어해 입지 않는, 9, 가지고 있지 않다)
;  10의 위…(0, 통상, 10, 반나체, 20, 전라, 30, 속옷 없음)
;  백의 위… 코스프레를 위해서(때문에) 보류
;  천의 위…(0, 조교자, 1000, 조교 대상)
;  예(조교 대상이 부끄러워해전라로 할 수 없었다=1022)
;-------------------------------------------------

;-------------------------------------------------
;조교자가 갈아입었다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기

	;공략 후의 놀이 같은 것. 눈치채지지 않았으면 고생한 의미 없고

	;통상이 되었다
	IF ARG == 1
		IF DAY >= 31
			PRINTFORMDL 
			PRINTFORML 호감도 3000 + 복장[통상] 으로 코스프레 이벤트가 발생합니다. 소질:착의플레이선호로 더욱 발생하기 쉬워집니다.
			PRINTFORMDW
		ENDIF
		RETURN 2
	ENDIF
	;반나체가 되었다
	IF ARG == 11
		IF DAY >= 31
			PRINTFORMDL 
			PRINTFORML 호감도 3000 + 복장[통상] 으로 코스프레 이벤트가 발생합니다. 소질:착의플레이선호로 더욱 발생하기 쉬워집니다.
			PRINTFORMDW 
		ENDIF
		RETURN 2
	ENDIF
	
	;전라가 되었다
	IF ARG == 21
		IF DAY >= 31
			PRINTFORMDL 
			PRINTFORML 호감도 3000 + 복장[통상] 으로 코스프레 이벤트가 발생합니다. 소질:착의플레이선호로 더욱 발생하기 쉬워집니다.
			PRINTFORMDW 
		ENDIF
		RETURN 2
	ENDIF
	
	;…… 스으스으 한다
	IF ARG == 31
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교자가 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기
	;반나체라도 부끄럽다
	IF ARG == 12
		RETURN 2
	ENDIF
	;평상시부터전라는 조금……
	IF ARG == 22
		RETURN 2
	ENDIF
	;팬츠가 없기 때문에 부끄러운 걸!
	IF ARG == 32
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교자가 싫어해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기
	;반나체는 싫어
	IF ARG == 13
		RETURN 2
	ENDIF
	;평상시부터전라는 싫어
	IF ARG == 23
		RETURN 2
	ENDIF
	;스으스으 하는 것은 싫어
	IF ARG == 33
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상을 갈아입게 했다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;통상이 되었다
	IF ARG == 1001
		RETURN 2
	ENDIF
	;반나체가 되었다
	IF ARG == 1011
		RETURN 2
	ENDIF
	;전라가 되었다
	IF ARG == 1021
		RETURN 2
	ENDIF
	;…… 스으스으 한다
	IF ARG == 1031
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상이 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;반나체라도 부끄럽다
	IF ARG == 1012
		RETURN 2
	ENDIF
	;평상시부터전라는 부끄럽다
	IF ARG == 1022
		RETURN 2
	ENDIF
	;팬츠가 없기 때문에 부끄러운 걸!
	IF ARG == 1032
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상이 싫어해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;반나체는 싫어
	IF ARG == 1013
		RETURN 2
	ENDIF
	;평상시부터전라는 싫어
	IF ARG == 1023
		RETURN 2
	ENDIF
	;스으스으 하는 것은 싫어
	IF ARG == 1033
		RETURN 2
	ENDIF
ENDIF

;=================================================
;EV102, 조교자 변경
;=================================================
;·조교자를 교대시켰을 때에 표시
;-------------------------------------------------
@KOJO_EVENT_K11_102(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 구상주가 다른 캐릭터에게 맡긴, 1, 구상주가 조교자가 되었다)
;구조교자는 「%CALLNAME:SHOP_OLDTARGET%」, 신조교자는 「%CALLNAME:SHOP_NEWTARGET%」로 참조할 수 있다
;구상주는 TARGET로 참조
;-------------------------------------------------

;-------------------------------------------------
;다른 조교자에게 교대
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 0
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;프랑님이 조교자가 되었다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 1
	RETURN LOCAL
ENDIF

;=================================================
;EV110, 마력 사용
;=================================================
;·마력을 소비해 어떠한 특전을 구입했을 때에 표시
;-------------------------------------------------
@KOJO_EVENT_K11_110(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 마력 사용전, 1, 조교 기교, 2, 지식을 얻기 전, 3, 약학지식 취득, 4, 매혹 취득, 5, 금단의지식 취득, 6, 도구사용 취득,
;	7, 기술을 얻기 전, 8, 혀사용 취득, 9, 기묘한손가락취득, 10, 결박능숙, 11, 새디즘 소거/새디즘 취득, 12, 불결무시 취득)
;이벤트 구상: 아이템
;인수 내역…
;ARG=(13, 아이템 연성전, 14, 로터 연성, 15, 바이브 연성, 16, 애널비즈 연성, 17, 페니스밴드 연성, 18, 애널바이브 연성
; 19, 클리캡 연성, 20, 니플캡 연성, 21, 오나홀 연성)
;이벤트 구상:고문도구 연성
;인수 내역…
;ARG=(22, 고문도구 연성전, 23, 아이마스크 연성, 24, 채찍 연성, 25, 줄연성, 26, 볼개그 연성, 27,   바늘 연성, 28, 관장세트 연성
; 29, 삼각목마 연성, 30, 대거울 연성, 31, 목걸이 연성, 32, 도구를 커스터마이즈 했다)
;그 외 이벤트
;인수 내역…
;ARG=(33, 후타나리화/되돌리는, 34, 거유화/되돌리는, 35, 빈유화/되돌리는, 36, 정력의 한계 가격인상승)
;-------------------------------------------------

;-------------------------------------------------
;프랑님마력 사용전
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 0
	PRINTFORMW 「%MASTERNAME%로부터 뽑은 정력으로 뭔가 해 버리려고」
	RETURN 2
ENDIF

;-------------------------------------------------
;프랑님의 조교 기교가 업
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 1
	PRINTFORMW 「후후, 좀 더 재미있는 일 생각났어요」
	RETURN 2
ENDIF

;-------------------------------------------------
;프랑님매혹 취득
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 4
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;프랑님 아이템 연성전
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 13
	PRINTFORMW 「엣치의 도구를 만들어 버려야지♪」
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;프랑님페니스밴드 연성
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 17
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;프랑님고문도구 연성전
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 22
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;프랑님SM상품 구입
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG > 22 && ARG < 32
	RETURN 2
ENDIF

;-------------------------------------------------
;프랑님후타나리화 or후타나리 소거
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 33
	;후타나리화
	IF TALENT:TARGET:121
		PRINTFORMW 「아하하하핫, 자지 붙여 버렸다♪」
		PRINTFORMW 「응…… 도, 뭔가 불끈불끈 한다. 빨리 사용하고 싶구나…♪」
	ELSE
	;후타나리 소거
		PRINTFORMW 「자지 질렸어요」
		PRINTFORMW 「거기에 자지로 갈 때보다, 보지라든지 엉덩이로 갔을 때(분)편이 기분이 좋은걸」
	ENDIF
	RETURN 2
ENDIF

;-------------------------------------------------
;프랑님거유화 or통상에 되돌린다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 34
	;거유화
	IF TALENT:TARGET:109
		PRINTFORMW 「와아…… 이것 나의 젖가슴?」
		PRINTFORMW 「조금 방해이지만…… 우후후, 이것으로%MASTERNAME%를 좀 더 즐겁게 해 주어요♪」
	;바탕으로 되돌린다
	ELSE
		PRINTFORMW 「아, 원부터 조금 커지고 있다, 누님에게 보이게 해 주고 싶구나」
	ENDIF
	RETURN 2
ENDIF	

;-------------------------------------------------
;프랑님빈유화 or통상에 되돌린다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 35
	;빈유화
	IF TALENT:TARGET:108
		PRINTFORMW 「이제 되어, 젖가슴은 그 중 커져요」
	;바탕으로 되돌린다
	ELSE
		PRINTFORMW 「아, 원부터 조금 커지고 있다, 누님에게 보이게 해 주고 싶구나」
	ENDIF
	RETURN 2
ENDIF

;-------------------------------------------------
@KOJO_EVENT_K11_111(ARG)
;-------------------------------------------------
;주인님으로서 새로운 조교자를 고용했다
;-------------------------------------------------
;·새로운 조교자가 TARGET가 되어 있기 (위해)때문에, 구상주는 TARGET는 아니기 때문에 주의
;·구상주를 참조하고 싶은 경우는 FLAG:2로
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시, 2, 표시해 가입한 캐릭터의 가입 구상을 스킵)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;------------------------------------------------------------------------------------------------------
	;프랑은 데포 상태라면 이름을 경칭 생략
	; ↓에 임의의 이름을 기술해 comment out를 제외하면 기능합니다 사용하고 싶다면 좋아하게
	;------------------------------------------------------------------------------------------------------
	;CSTR:(FLAG:2):호칭목록 = 전의 이름_부르게 하고 싶은 이름 /%CSTR:(FLAG:2):호칭목록%
	PRINTFORMW 「안녕히,%CNAME(TARGET, (FLAG:2))%.」
	RETURN 2
ENDIF

;-------------------------------------------------
@KOJO_EVENT_K11_112(ARG)
;-------------------------------------------------
;프랑님이 조교자로서 가입했다
;-------------------------------------------------
;·주인님의 이름은 「FLAG:2」로 참조할 수 있다
;·주인님이 고용 구상으로 기입 체크 2를 선택하고 있으면 표시되지 않는다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;------------------------------------------------------------------------------------------------------
	;comment out
	; ↓에 임의의 이름을 기술해 comment out를 제외하면 기능합니다 사용하고 싶다면 좋아하게
	;------------------------------------------------------------------------------------------------------
	;CSTR:(TARGET):호칭목록 =파르시_마조돼지 /%CSTR:(TARGET):호칭목록%
	RETURN 2
ENDIF

