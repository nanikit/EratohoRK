;-------------------------------------------------
@KOJO_EVENT_K11_201(ARG)
;-------------------------------------------------

;-------------------------------------------------
;介抱
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;相思相愛
	IF TALENT:TARGET:ご主人様 && FAVOR(3000, 3000)
		IF RAND:2 == 0
			PRINTFORMW 「……お、、さ ま……」
			PRINTFORMW 「%MASTERNAME%？ %MASTERNAME%？」
			PRINTFORMW 「あっ、%MASTERNAME%……良かったぁ」
		ELSE
			PRINTFORMW 「%MASTERNAME%、起きた？」
			PRINTFORMW 「途中で気絶しちゃうなんて、ちょっと激しくエッチしすぎちゃったね」
			PRINTFORM 「でもぉ、とっても気持ちよかったぁ…♪ 次もしようね
			CALL K11_PRINT_HEARTCBW(,3,,,)
			PRINTFORMDW フランは頬に手を当て、恍惚とした表情で%CALLNAME:MASTER%を見つめている
		ENDIF
	;それ以外
	ELSE
		PRINTFORMW 「起きた？」
		PRINTFORMW 「もう、次はもっと我慢してちょうだいね」
	ENDIF
	RETURN LOCAL
ENDIF

