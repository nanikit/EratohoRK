;────────────────────────────────────
;EV102, 調教者変更
;────────────────────────────────────
;・調教者を交代させたときに表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_102(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(0, 口上主が他のキャラに任せた、1, 口上主が調教者になった)
;前調教者は「%CALLNAME:SHOP_OLDTARGET%」、新調教者は「%CALLNAME:SHOP_NEWTARGET%」で参照できる
;口上主はTARGET
;-------------------------------------------------
;このバリアントでは反抗刻印等は調教者毎に管理される仕様なので
;MARK:MASTER:反抗刻印は0にしかならないので注意。
;-------------------------------------------------
#DIM 新調教者
#DIM 前調教者
新調教者 = SHOP_NEWTARGET
前調教者 = SHOP_OLDTARGET

;━━━━━━━━━━━━━━━━━━━━━━━━
;他の調教者に交代
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 0 && TALENT:TARGET:ご主人様
	IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
		SELECTCASE RANDEVENT(2, 0)
			CASE 0
				PRINTFORMW 「あっ、そう！　そういう事するんだ」
				PRINTFORMW 「ま、精々%CNAME(新調教者, TARGET)%に可愛がってもらえば？」
				PRINTFORMW 「べ、別にそんな事、大して気にしないからいいけど…」
			CASE 1
				;オートモード時には弾く		というかオートの時はSHOP系を考慮する必要はなかった。まぁいいけど
				IF !FLAG:オートモード
					PRINTFORMW 「それってどういう事？　まさか%CNAME(新調教者, TARGET)%に心変わりしたとか？」
					PRINTFORMW 「ほら、はっきり言いなさいよ」
					PRINTL
					PRINTFORMDL [0] そんな事ある訳ない！　雛様一筋です！
					PRINTFORMDL [1] 答えない
					CALL INPUTINTD(0, 1)
					SELECTCASE RESULT
						CASE 0
							PRINTFORMW 「なっ！？…」
							PRINTFORMW 「ま、まぁ、そこまで言うなら、少し位なら認めてあげてもいいわよ？」
						CASE 1
							PRINTFORMW 「……面白いわね？　フ、フフフ…」
							PRINTFORMDW 雛の周囲で禍々しい厄が渦巻き始めた……
							PRINTFORMW 「ねぇ、もう一度答えるチャンスをあげてもいいけど、どうする？」
							PRINTL
							PRINTFORMDL [0] ご免なさいご免なさい雛様一筋です許して下さい！
							CALL INPUTINTD(0)
							SELECTCASE RESULT
								CASE 0
									PRINTFORM 「はい、良く出来ました
									CALL PRINT_SYMBOL_K39
									PRINTFORMW 「ていうか、最初からそう言いなさいよねっ！」
							ENDSELECT
					ENDSELECT
				;オートモードでない場合
				ELSE
					;PRINTW
				ENDIF
		ENDSELECT
	ELSE
		IF MARK:TARGET:反抗刻印 >= 1
			PRINTFORMW 「思ったようにはいかないわ……」
			PRINTFORMW 「暫くの間、%CNAME(新調教者, TARGET)%に任せた方がいいかしら」
		ELSE
			PRINTFORMW 「では%CNAME(新調教者, TARGET)%、あとはよろしくお願い」
			PRINTFORMW 「こういう事を人に任せるのって、ちょっとどうかとも思うけど……」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;雛さまが調教者になった
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 1
	;雛がご主人様
	IF TALENT:TARGET:ご主人様
		IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
			PRINTFORMW 「やっぱりこういうのって、人任せにするものじゃないわね」
			PRINTFORMW 「ん、なによ。%CNAME(MASTER, TARGET)%まさか……」
			PRINTFORMW 「%CNAME(前調教者, TARGET)%の方が良かったとか言わないわよね？」
		ELSE
			PRINTFORMW 「お世話様、%CNAME(前調教者, TARGET)%。後は私が」
			PRINTFORMW 「うん、まぁ…なんとかなると思うけど……」
		ENDIF
	;その他
	ELSE
		SELECTCASE RANDEVENT(2, 10)
			CASE 0
				PRINTFORMW 「まぁ%CNAME(前調教者, TARGET)%がどうしてもと言うのなら…」
				PRINTFORMW 「でも、本当にそれでいいのかしら？」
			CASE 1
				PRINTFORMW 「こういう事を人任せにする%CNAME(前調教者, TARGET)%も、ちょっとどうかと思うけど…」
				PRINTFORMW 「でも、頼まれたからには仕方ないわね」
		ENDSELECT
	ENDIF
	RETURN 2
ENDIF

