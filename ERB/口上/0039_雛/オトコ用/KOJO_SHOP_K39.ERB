;雛さま巨乳化or通常に戻す
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && ARG == 34
	;巨乳化
	IF TALENT:TARGET:巨乳
		;1/2
		IF RAND:2
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「うふ、これで胸を使って色々出来そうね」
				PRINTFORM 「楽しませてあげるから、期待しててね
				CALL PRINT_SYMBOL_K39
			ELSE
				PRINTFORMW 「やはり胸は大きい方が良いものなのでしょうか？」
			ENDIF
		[SKIPSTART]
		;調教対象が巨乳
		ELSEIF TALENT:MASTER:巨乳
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ねぇ、ちょっと大きさを比べてみない？」
				PRINTFORM 「う～ん、殆ど一緒みたいね
				CALL PRINT_SYMBOL_K39
			ELSE
				PRINTFORMW 「ふふ、これでお揃いですね♪」
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORML 「ほら、触ってみて？」
				PRINTFORM 「偽者じゃない本物のおっぱいよ
				CALL PRINT_SYMBOL_K39
				PRINTFORMW 「この魔力ってほんとに何でも出来るのね」
			ELSE
				PRINTFORMW 「胸は大きい方がいいですか？」
			ENDIF
		ENDIF
	;元に戻す
	ELSE
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「元の姿が一番落ち着くわね」
		ELSE
			PRINTFORMW 「自分本来の姿が一番落ち着きますね」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

