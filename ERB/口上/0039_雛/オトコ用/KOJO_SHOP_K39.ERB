IF 口上記入チェック(100) && ARG == 33
	;ふたなり化
	IF TALENT:TARGET:ふたなり
		;1/2
		IF RAND:2
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ねぇ、覚悟はいい？」
				PRINTFORMW 「もう後悔しても遅いからね？　うふっ」
			ELSE
				PRINTFORMW 「これで突いて欲しいんですね？」
				PRINTFORM 「いいですよ、楽しみにしていて下さいね
				CALL PRINT_SYMBOL_K39
			ENDIF
		[SKIPSTART]
		オトコ限定なので機能しない
		;調教対象が処女
		ELSEIF TALENT:MASTER:処女
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ほんと良く出来てるわね、コレ」
				PRINTFORMW 「ほら、触ってみる？　クスクス」
			ELSE
				PRINTFORMW 「本当にいいんですか？」
				PRINTFORM 「後悔しないでくださいね
				CALL PRINT_SYMBOL_K39
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「なんか凄い違和感があるんだけど…」
				PRINTFORM 「でも折角だから、コレで楽しむ事にするわ
			ELSE
				PRINTFORMW 「なんだか不思議な感じがしますね」
				PRINTFORM 「でも折角ですから、精々楽しませてもらいますよ
			ENDIF
			CALL PRINT_SYMBOL_K39
		ENDIF
	;ふたなり消去
	ELSE
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「やっぱりこの方が落ち着く感じね」
		ELSE
			PRINTFORMW 「やはり元のままが１番でしょうか」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;オトコ限定口上にしてしまったので、箇所によっては死に口上になっているという
