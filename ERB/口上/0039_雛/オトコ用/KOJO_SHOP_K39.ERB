;========================================================================
;숍 이벤트 구상
;========================================================================

;─
;EV101, 의상 변경
;─
;·숍으로부터 의상 변경을 실시했을 경우에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_101(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=
;  1의 위…(0, 이제(벌써) 입고 있다, 1, 갈아입은, 2, 부끄러워서 입을 수 없는, 3, 싫어해 입지 않는, 9, 가지고 있지 않다)
;  10의 위…(0, 통상, 10, 반나체, 20, 전라, 30, 속옷 없음)
;  백의 위… 코스프레를 위해서(때문에) 보류
;  천의 위…(0, 조교자, 1000, 조교 대상)
;  예(조교 대상이 부끄러워해 전라로 할 수 없었다=1022)
;-------------------------------------------------
#DIM 상사상애
상사상애 = GET_KFLAG_K39("상사상애")

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 갈아입었다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상이 되었다
		CASE 1
			IF 상사상애
				;폰트 조작하지 않으면 HTML계보다 사용하기 편리하다. 개행없이 사용할 수 있는 것도 GOOD. HTML계에 없는 이점
				CALL COLORPRINT("「역시 이__0 xC0C0C0__ 『__0 x64F86A__액__0 xC0C0C0__』__0 xB5213B__의 표가 없으면")
				CALL PRINT_SYMBOL_K39
			ELSE
				PRINTFORMW 「역시 이것이 제일 어울릴까요?」
			ENDIF
			RETURN 2
		;반나체가 되었다
		CASE 11
			IF 상사상애
				PRINTFORMW 「알몸보다 속옷 모습을 좋아해?」
			ELSE
				PRINTFORMW 「조금 부끄럽지만, 어쩔 수 없네요」
			ENDIF
			RETURN 2
		;전라가 되었다
		CASE 21
			IF 상사상애
				PRINTFORMW 「빤히 볼 수 있으면, 조금 부끄럽다…」
			ELSE
				PRINTFORMW 「저, 그렇게 볼 수 있으면 부끄럽습니다…」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;…… 스으스으 한다		현재는 실장되어 있지 않다
		CASE 31
			IF 상사상애
				PRINTFORMW 「이것… 조금 침착하지 않을지도…」
			ELSE
				PRINTFORMW 「뭔가 침착하지 않네요…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 부끄러워해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		;반나체라도 부끄럽다
		CASE 12
			IF 상사상애
				PRINTFORMW 「그렇게 부끄러운 모습은 싫어…」
			ELSE
				PRINTFORMW 「그건 좀 싫어, 입니다…」
			ENDIF
			RETURN 2
		;평상시부터 전라는 조금……
		CASE 22
			IF 상사상애
				PRINTFORMW 「그런 바보 같은 것은 싫어…」
			ELSE
				PRINTFORMW 「거절합니다…」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;팬츠가 없기 때문에 부끄러운 걸!
		CASE 32
			IF 상사상애
				PRINTFORMW 「침착하지 않기 때문에 싫어…」
			ELSE
				PRINTFORMW 「그런 침착하지 않은 모습은 싫습니다…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 싫어해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		;반나체는 싫어
		CASE 13
			IF 상사상애
				PRINTFORMW 「그런 것 싫어…」
			ELSE
				PRINTFORMW 「싫습니다…」
			ENDIF
			RETURN 2
		;평상시부터 전라 같은거 싫어
		CASE 23
			IF 상사상애
				PRINTFORMW 「싫어요, 그렇게 바보 같은 모습…」
			ELSE
				PRINTFORMW 「나를 욕보이고 싶습니까?」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;스으스으 하는 것은 싫어
		CASE 33
			IF 상사상애
				PRINTFORMW 「안돼!   그런 것 절대로 안됏!」
			ELSE
				PRINTFORMW 「그런 것 안됩니다…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 갈아입는 것을 그만두었다		FIRSTEVENT(30)
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:TARGET%의 의상은 벌써[　　]로 설정되어 있습니다
;------------------------------------------------
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		[SKIPSTART]
		;통상인 채
		CASE 0
		;반나체인 채
		CASE 10
		;전라인 채
		CASE 20
		[SKIPEND]
		CASEELSE
			PRINTFORMW 「응. 역시 이대로 좋다\@ 상사상애 ? 일까# 입니까\@」
			RETURN 2
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상을 갈아입게 했다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상이 되었다
		CASE 1001
			IF 상사상애
				PRINTFORMW 「역시 그 모습이 어울리고 있어요」
			ELSE
				PRINTFORMW 「우후, 잘 어울리고 있어요」
			ENDIF
			RETURN 2
		;반나체가 되었다
		CASE 1011
			IF 상사상애
				PRINTFORMW 「그러한 것도 싫지 않아요?」
			ELSE
				PRINTFORMW 「이것은 이것대로 좋다고 생각합니다」
			ENDIF
			RETURN 2
		;전라가 되었다
		CASE 1021
			IF 상사상애
				PRINTFORM 「후후, 추잡하네요
			ELSE
				PRINTFORMW 「저, 부끄럽지 않습니까?」
				PRINTFORM 「당신이 좋다면 그것으로 좋습니다만
			ENDIF
			CALL PRINT_SYMBOL_K39
			RETURN 2
		[SKIPSTART]
		;…… 스으스으 한다
		CASE 1031
			IF 상사상애
				PRINTFORMW 「머뭇머뭇 해 버려. 침착하지 않은거야?   킥킥」
			ELSE
				PRINTFORMW 「침착하지 않습니까?   우대신」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 부끄러워해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG & 10) == 2
	SELECTCASE ARG
		;반나체라도 부끄럽다
		CASE 1012
			IF 상사상애
				PRINTFORMW 「안돼?   방법 없네요…」
			ELSE
				PRINTFORMW 「부끄럽습니까?   어쩔 수 없네요…」
			ENDIF
			RETURN 2
		;평상시부터 전라 같은거 부끄럽다
		CASE 1022
			IF 상사상애
				PRINTFORMW 「뭐, 기분은 모르지도 없지만…」
			ELSE
				PRINTFORMW 「유감입니다만, 방법 없네요…」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;팬츠가 없기 때문에 부끄러운 걸!
		CASE 1032
			IF 상사상애
				PRINTFORMW 「이제 와서 부끄럽다니, 조금 이상하지 않아?」
			ELSE
				PRINTFORMW 「싫다고 말한다면 강요는 하지 않지만…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 싫어해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG & 10) == 3
	SELECTCASE ARG
		;반나체는 싫어
		CASE 1013
			IF 상사상애
				PRINTFORMW 「저기, 아무래도 안돼?」
				PRINTFORMW
			ELSE
				PRINTFORMW 「안돼, 입니까…」
			ENDIF
			RETURN 2
		;평상시부터 전라 같은거 싫어
		CASE 1023
			IF 상사상애
				PRINTFORMW 「부끄러운 것은 알지만, 이제 와서가 아닌거야?」
			ELSE
				PRINTFORMW 「그렇게 싫으면 방법 없네요…」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;스으스으 하는 것은 싫어
		CASE 1033
			IF 상사상애
				PRINTFORMW 「앗, 그렇게! 거기까지 싫으면 별로 좋지만」
			ELSE
				PRINTFORMW 「그래, 입니까… 방법 없네요…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상을 갈아입게 하는 것을 그만둔 FIRSTEVENT(70)
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:MASTER%의 의상은 벌써[　　]로 설정되어 있습니다
;ARG(=1000, 통상인 채/=1010, 반나체인 채/=1020, 전라인 채)
;------------------------------------------------
IF 구상기입체크(100) && ARG >= 1000 && (ARG % 10) == 0
	PRINTFORMW 「우선,\@ ARG == 1020 ? 알몸#\@인 채로 좋다\@ 상사상애 ? 일까# 입니까\@」
	PRINTFORM 「%CNAME(MASTER, TARGET)%도 그 모습이 마음에 들고 있는 것 같다\@ 상사상애 ? 이고# 이고\@군요…
	CALL PRINT_SYMBOL_K39
	RETURN 2
ENDIF


;─
;EV102, 조교자 변경
;─
;·조교자를 교대시켰을 때에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_102(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 구상주가 다른 캐릭터에게 맡긴, 1, 구상주가 조교자가 되었다)
;전조교자는 「%CALLNAME:SHOP_OLDTARGET%」, 신조교자는 「%CALLNAME:SHOP_NEWTARGET%」로 참조할 수 있다
;구상주는 TARGET
;-------------------------------------------------
;이 바리안트에서는 반항 각인등은 조교자 마다 관리되는 사양인 것으로
;MARK:MASTER:반항 각인은 0밖에 되지 않기 때문에 주의.
;-------------------------------------------------
#DIM 신조교자
#DIM 전조교자
신조교자 = SHOP_NEWTARGET
전조교자 = SHOP_OLDTARGET

;━━━━━━━━━━━━━━━━━━━━━━━━
;다른 조교자에게 교대
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 0 && TALENT:TARGET:주인님
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		SELECTCASE RANDEVENT(2, 0)
			CASE 0
				PRINTFORMW 「앗, 그렇게!   그런 일 한다」
				PRINTFORMW 「뭐, 최대한%CNAME(신조교자, TARGET)%에 귀여워해 받으면?」
				PRINTFORMW 「벼, 별로 그런 일, 그다지 신경쓰지 않기 때문에 좋지만…」
			CASE 1
				;오토 모드시에는 연주한다		라고 할까 오토때는 SHOP계를 고려할 필요는 없었다. 뭐 좋지만
				IF !FLAG:오토모드
					PRINTFORMW 「그 거 어떻게 말하는 일?   설마%CNAME(신조교자, TARGET)%에 변심 했다든가?」
					PRINTFORMW 「이봐요, 분명히 말하세요」
					PRINTL
					PRINTFORMDL [0] 그런 일 있을 이유 없다!   추님 외곬입니다!
					PRINTFORMDL [1] 대답하지 않는다
					CALL INPUTINTD(0, 1)
					SELECTCASE RESULT
						CASE 0
							PRINTFORMW 「낫!? …」
							PRINTFORMW 「뭐, 뭐, 거기까지 말한다면, 조금 위라면 인정해 주어도 괜찮아요?」
						CASE 1
							PRINTFORMW 「…… 재미있네요?   후, 후후후…」
							PRINTFORMDW 병아리의 주위로 불길한 액이 소용돌이치기 시작했다……
							PRINTFORMW 「저기, 한번 더 대답할 찬스를 주어도 괜찮지만, 어떻게 해?」
							PRINTL
							PRINTFORMDL [0] 미안해요 미안해요 추님 외곬입니다 허락해 주세요!
							CALL INPUTINTD(0)
							SELECTCASE RESULT
								CASE 0
									PRINTFORM 「네, 자주(잘) 할 수 있었습니다
									CALL PRINT_SYMBOL_K39
									PRINTFORMW 「라고 할까, 최초부터 그렇게 말하세요!」
							ENDSELECT
					ENDSELECT
				;오토 모드가 아닌 경우
				ELSE
					;PRINTW
				ENDIF
		ENDSELECT
	ELSE
		IF MARK:TARGET:반항각인 >= 1
			PRINTFORMW 「생각한 것처럼은 가지 않아요……」
			PRINTFORMW 「잠시의 사이,%CNAME(신조교자, TARGET)%에 맡기는 것이 좋을까」
		ELSE
			PRINTFORMW 「그럼%CNAME(신조교자, TARGET)%, 나머지는 아무쪼록 부탁」
			PRINTFORMW 「이런 일을 사람에게 맡기는 것은, 조금 부디이라고도 생각하지만……」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님이 조교자가 되었다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 1
	;병아리가 주인님
	IF TALENT:TARGET:주인님
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「역시 이런 것은, 임무 전가로 하는 것이 아니네요」
			PRINTFORMW 「응, 뭐야.%CNAME(MASTER, TARGET)%설마……」
			PRINTFORMW 「%CNAME(전조교자, TARGET)%가 좋았다고 말하지 않네요?」
		ELSE
			PRINTFORMW 「보살핌,%CNAME(전조교자, TARGET)%. 뒤는 내가」
			PRINTFORMW 「응, 뭐… 어떻게든 된다고 생각하지만……」
		ENDIF
	;그 외
	ELSE
		SELECTCASE RANDEVENT(2, 10)
			CASE 0
				PRINTFORMW 「아무튼%CNAME(전조교자, TARGET)%가 아무래도라고 말한다면…」
				PRINTFORMW 「그렇지만, 정말로 그것으로 좋은 것일까?」
			CASE 1
				PRINTFORMW 「이런 일을 임무 전가로 한다%CNAME(전조교자, TARGET)%도, 조금 어떨까라고 생각하지만…」
				PRINTFORMW 「그렇지만, 부탁받은 이상에는 어쩔 수 없네요」
		ENDSELECT
	ENDIF
	RETURN 2
ENDIF

;=================================================
;EV110, 마력 사용
;=================================================
;·마력을 소비해 어떠한 특전을 구입했을 때에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_110(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 마력 사용전, 1, 조교 기교, 2, 지식을 얻기 전, 3, 약학지식 취득, 4, 매혹 취득, 5, 금단의지식 취득,
;     6, 도구사용 취득, 7, 기술을 얻기 전, 8, 혀사용 취득, 9, 기묘한손가락취득, 10, 결박능숙,
;     11, 새디즘 소거/새디즘 취득, 12, 불결무시 취득, 38, 착의플레이선호, 39, 허리사용)
;
;이벤트 구상:아이템
;인수 내역…
;ARG=(13, 아이템 연성전, 14, 로터 연성, 15, 바이브 연성, 16, 애널 비즈 연성, 17, 페니스 밴드 연성,
;     18, 애널 바이브 연성, 19, 클리캡 연성, 20, 니플캡 연성, 21, 오나홀 연성)
;
;이벤트 구상:고문도구 연성
;인수 내역…
;ARG=(22, 고문도구 연성전, 23, 아이마스크 연성, 24, 채찍 연성, 25, 줄연성, 26, 볼개그 연성, 27, 바늘 연성,
;     28, 관장 세트 연성, 29, 삼각 목마 연성, 30, 대거울 연성, 31, 목걸이 연성, 32, 도구를 커스터마이즈 했다)
;
;그 외 이벤트
;인수 내역…
;ARG=(33, 후타나리화/되돌리는, 34, 거유화/되돌리는, 35, 빈유화/되돌리는, 36, 정력의 한계 가격인상승)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님마력 사용전
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 0
	IF GET_KFLAG_K39("상사상애")
		PRINTFORMW 「마력은 편리하지만, 너무 사용하는 것도 생각할 일이군요」
	ELSE
		PRINTFORM 「모처럼의 마력이기 때문에, 사용하지 않으면 과분하네요
		CALL PRINT_SYMBOL_K39
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님의 조교 기교가 업
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 1
	IF GET_KFLAG_K39("상사상애")
		PRINTFORMW 「어때?   조속히 시험해 봐?   우대신」
	ELSE
		PRINTFORM 「이것으로 좀 더 즐겨 받을 수 있을 것 같네요
		CALL PRINT_SYMBOL_K39
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님SM상품 연성
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && INRANGE(ARG, 23, 31)
	IF GET_KFLAG_K39("상사상애")
		PRINTFORMW 「이 마력이라는거 정말 이상한 힘이군요」
		PRINTFORM 「뭐 그렇지만…, 이것으로 좀 더 즐길 수 있을 것 같구나
		CALL PRINT_SYMBOL_K39
	ELSE
		PRINTFORMW 「마력으로 이런 것까지 연성 생기게 되다니…」
		PRINTFORM 「그렇지만, 이것으로 좀 더 즐길 수 있을 것 같네요
		CALL PRINT_SYMBOL_K39
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님도구를 커스터마이즈
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 32
	PRINTFORMW 「도구를 커스터마이즈라는건 어떻게 말하는 일\@ GET_KFLAG_K39("상사상애") ? 일까# 인 것이지요\@？」
	PRINTFORMW 「잘 모른다\@ GET_KFLAG_K39("상사상애") ? 원이군요# 군요\@……」
	RETURN 2
ENDIF

;기른 곳에서 이 구상에서는 기능하지 않는 것이 되어 있다고 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님후타나리화 or후타나리 소거
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 33
	;후타나리화
	IF TALENT:TARGET:후타나리
		;1/2
		IF RAND:2
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「저기, 각오는 좋아?」
				PRINTFORMW 「이제(벌써) 후회해도 늦으니까?   우대신」
			ELSE
				PRINTFORMW 「이것으로 찔렀으면 좋네요?」
				PRINTFORM 「좋아요, 기대하고 있어 주세요
				CALL PRINT_SYMBOL_K39
			ENDIF
		[SKIPSTART]
		남자 한정인 것으로 기능하지 않는다
		;조교 대상이 처녀
		ELSEIF TALENT:MASTER:처녀
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「정말 좋게 되어있어요, 이것」
				PRINTFORMW 「이봐요, 손대어 봐?   킥킥」
			ELSE
				PRINTFORMW 「정말로 좋습니까?」
				PRINTFORM 「후회하지 말아 주세요
				CALL PRINT_SYMBOL_K39
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「뭔가 굉장한 위화감이 있지만…」
				PRINTFORM 「그렇지만 모처럼이니까, 이것으로 즐기는 일로 해요
			ELSE
				PRINTFORMW 「왠지 이상한 느낌이 드네요」
				PRINTFORM 「그렇지만 모처럼이기 때문에, 최대한 즐겁게 해 받아요
			ENDIF
			CALL PRINT_SYMBOL_K39
		ENDIF
	;후타나리 소거
	ELSE
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「역시 이 (분)편이 침착하는 느낌이군요」
		ELSE
			PRINTFORMW 「역시 원인 채가 1번입니까」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;남자 한정 구상으로 해 버렸으므로, 개소에 따라서는 공수상이 되어 있다고 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님거유화 or통상에 되돌린다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 34
	;거유화
	IF TALENT:TARGET:거유
		;1/2
		IF RAND:2
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「우후, 이것으로 가슴을 사용해 여러가지 할 수 있을 것 같구나」
				PRINTFORM 「즐겁게 해 주기 때문에, 기대하고 있어
				CALL PRINT_SYMBOL_K39
			ELSE
				PRINTFORMW 「역시 가슴은 큰 것이 좋은 것입니까?」
			ENDIF
		[SKIPSTART]
		;조교 대상이 거유
		ELSEIF TALENT:MASTER:거유
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「저기, 조금 크기를 비교해 보지 않을래?」
				PRINTFORM 「응, 대부분 함께 같구나
				CALL PRINT_SYMBOL_K39
			ELSE
				PRINTFORMW 「후후, 이것으로 갖춤이군요♪」
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("상사상애")
				PRINTFORML 「이봐요, 손대어 봐?」
				PRINTFORM 「가짜가 아닌 진짜의 젖가슴이야
				CALL PRINT_SYMBOL_K39
				PRINTFORMW 「이 마력은 정말 뭐든지 할 수 있는 거네」
			ELSE
				PRINTFORMW 「가슴은 큰 것이 좋습니까?」
			ENDIF
		ENDIF
	;바탕으로 되돌린다
	ELSE
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「원래의 모습이 제일 침착해요」
		ELSE
			PRINTFORMW 「자신 본래의 모습이 제일 침착하네요」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;병아리님빈유화 or통상에 되돌린다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 35
	;빈유화
	IF TALENT:TARGET:빈유
		;1/2
		IF RAND:2
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「보통은 큰 것이 좋아하는 것이 아닌거야?」
				PRINTFORMW 「당신은 조금 괴짜였다거나 해?」
			ELSE
				PRINTFORMW 「아무래도 바뀐 기호를 가지고 계신 같네요?」
			ENDIF
		[SKIPSTART]
		;조교 대상이 거유
		ELSEIF TALENT:MASTER:거유
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「저기, 어째서 히죽히죽 하고 있는 거야?」
				PRINTFORMW 「이봐요, 분명하게 설명하세요!」
			ELSE
				PRINTFORMW 「조금 복잡한 기분이 되네요…」
			ENDIF
		;조교 대상이 빈유
		ELSEIF TALENT:MASTER:빈유
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「자기보다 큰 가슴을 보게 되는 것이 싫어?」
			ELSE
				PRINTFORMW 「질투의 감정은 액을 낳는 원이지만」
				PRINTFORMW 「이것은 과연 조금…」
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("상사상애")
				PRINTFORMW 「저기, 자주(잘) 괴짜라고 말해지지 않아?」 
			ELSE
				PRINTFORMW 「조금 바뀐 기호를 가지고 계신 같네요?」
			ENDIF
		ENDIF
	;바탕으로 되돌린다
	ELSE
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「%RAND_SPLIT("후후, 역시 원의 모습이 좋을 것입니다? /원래인 채가 좋다는 일? /이 (분)편이 편하고 좋네요")%」
		;그 이외
		ELSE
			PRINTFORMW 「%RAND_SPLIT("원의 모습이 제일이군요/역시, 이 (분)편이 침착하네요")%」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;─
;EV111, 주인님으로서 새로운 조교자를 고용했다
;─
@KOJO_MALE_EVENT_K39_111(ARG)
;-------------------------------------------------
;·새로운 조교자가 TARGET가 되어 있기 (위해)때문에, 구상주는 TARGET는 아니기 때문에 주의
;·구상주를 참조하고 싶은 경우는 FLAG:주인님으로
;-------------------------------------------------
;반환값(1, 표시, 2, 표시해 가입한 캐릭터의 가입 구상을 스킵)
IF 구상기입체크(100)
	;상대가에 새가 아니면 "씨"붙어라. 에 있어라면 경칭 생략. 가시나무 명인의 연회장면에서도 무심코 함께 있었고 어떠한 교류는 있어 그렇게적인
	SIF NO:TARGET != 40 && CALLNAME:TARGET != "에 있어"
		CSTR:(FLAG:주인님):호칭목록 = %CALLNAME:TARGET%_%CALLNAME:TARGET%씨/%CSTR:(FLAG:주인님):호칭목록%
	IF !GET_KFLAG_K39("상사상애")
		PRINTFORMW 「일부러 와 주어 고마워요,%CNAME(TARGET, (FLAG:주인님))%」
		PRINTFORMW 「응, 뭐라고 할까 그……, 조금 도왔으면 좋지만」
	ENDIF
	RETURN 1
ENDIF

;─
;EV112, 병아리님이 조교자로서 가입했다
;─
@KOJO_EVENT_K39_112(ARG)
;-------------------------------------------------
;·주인님의 이름은 「FLAG:주인님」로 참조할 수 있다
;·주인님이 고용 구상으로 반환값 2를 돌려주고 있으면 표시되지 않는다
;-------------------------------------------------
IF 구상기입체크(100)
	SIF NO:(FLAG:주인님) != 40 && CALLNAME:(FLAG:주인님) != "에 있어"
		CSTR:TARGET:호칭목록 = %CALLNAME:(FLAG:주인님)%_%CALLNAME:(FLAG:주인님)%씨/%CSTR:TARGET:호칭목록%
	SELECTCASE NO:(FLAG:주인님)
		;에 있어
		CASE 40
			PRINTFORMW 「드무네요. 갑자기 호출하거나 해 무슨 일이야?」
			PRINTFORMW 「먼저 말해 두지만, 갓빠들의 귀찮은 일에 말려 들어가는 것은 미안이니까!」
		;그 외
		CASEELSE
			PRINTFORMW 「아무래도,%CNAME((FLAG:주인님), TARGET)%. 액신에 뭔가 용무입니까?」
	ENDSELECT
	RETURN 1
ENDIF
