		;調教対象が巨乳
		ELSEIF TALENT:MASTER:巨乳
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ねぇ、なんでニヤニヤしてるの？」
				PRINTFORMW 「ほらっ、ちゃんと説明しなさいよっ！」
			ELSE
				PRINTFORMW 「ちょっと複雑な気持ちになりますね…」
			ENDIF
		;調教対象が貧乳
		ELSEIF TALENT:MASTER:貧乳
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「自分より大きい胸を見せつけられるのが嫌なの？」
			ELSE
				PRINTFORMW 「妬みの感情は厄を生み出す元ですけど」
				PRINTFORMW 「これは流石にちょっと…」
			ENDIF
		[SKIPEND]
		;1/2
		ELSE
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ねぇ、よく変わり者って言われない？」 
			ELSE
				PRINTFORMW 「ちょっと変わった嗜好をお持ちのようですね？」
			ENDIF
		ENDIF
	;元に戻す
	ELSE
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「%RAND_SPLIT("ふふ、やっぱり元の姿の方が良いでしょう？/元のままが良いって事？/この方が楽でいいわね")%」
		;それ以外
		ELSE
			PRINTFORMW 「%RAND_SPLIT("元の姿が一番ですね/やっぱり、この方が落ち着きますね")%」
		ENDIF
	ENDIF
	RETURN 2
ENDIF

;────────────────────────────────────
;EV111, ご主人様として新しい調教者を雇った
;────────────────────────────────────
@KOJO_MALE_EVENT_K39_111(ARG)
;-------------------------------------------------
;・新しい調教者がTARGETになっているため、口上主はTARGETではないので注意
;・口上主を参照したい場合はFLAG:ご主人様で
;-------------------------------------------------
;戻り値（1,表示、2,表示して加入したキャラの加入口上をスキップ）
IF 口上記入チェック(100)
	;相手がにとりでなければ"さん"付け。にとりなら呼び捨て。茨歌仙の宴会場面でも何気に一緒に居たし何らかの交流はありそう的な
	SIF NO:TARGET != 40 && CALLNAME:TARGET != "にとり"
		CSTR:(FLAG:ご主人様):呼び方リスト = %CALLNAME:TARGET%_%CALLNAME:TARGET%さん/%CSTR:(FLAG:ご主人様):呼び方リスト%
	IF !GET_KFLAG_K39("相思相愛")
		PRINTFORMW 「わざわざ来てくれてありがとう、%CNAME(TARGET, (FLAG:ご主人様))%」
		PRINTFORMW 「うん、なんていうかその……、ちょっと手伝って欲しいんだけど」
	ENDIF
	RETURN 1
ENDIF

;────────────────────────────────────
;EV112, 雛さまが調教者として加入した
;────────────────────────────────────
@KOJO_EVENT_K39_112(ARG)
;-------------------------------------------------
;・ご主人様の名前は「FLAG:ご主人様」で参照できる
;・ご主人様が雇用口上で戻り値2を返していると表示されない
;-------------------------------------------------
IF 口上記入チェック(100)
	SIF NO:(FLAG:ご主人様) != 40 && CALLNAME:(FLAG:ご主人様) != "にとり"
		CSTR:TARGET:呼び方リスト = %CALLNAME:(FLAG:ご主人様)%_%CALLNAME:(FLAG:ご主人様)%さん/%CSTR:TARGET:呼び方リスト%
	SELECTCASE NO:(FLAG:ご主人様)
		;にとり
		CASE 40
			PRINTFORMW 「珍しいわね。急に呼び出したりしてどうしたの？」
			PRINTFORMW 「先に言っておくけど、河童たちの面倒事に巻き込まれるのは御免だからね！」
		;その他
		CASEELSE
			PRINTFORMW 「どうも、%CNAME((FLAG:ご主人様), TARGET)%。厄神に何か御用ですか？」
	ENDSELECT
	RETURN 1
ENDIF
