;///////////////////////////////////
;REACT27, 쓰러트린다·개별 리액션
;///////////////////////////////////
[SKIPSTART]
상정되는 COM…
	회화계 반응
		COM:여리게대답한다/무뚝뚝하게대답한다/붙임성좋게대답한다/강하게대답한다/용서를빈다/기분좋게해줘
	고통계 반응
		COM:무서워한다

·지문은 REACT분류(받아들인다/적극적으로 한다, 태연하게 한다/용서를빈다/소극적으로 한다)로 분기
[SKIPEND]
@KOJO_MALE_REACT_K39_27 ;TFLAG:ACT파생(=0, 쓰러트린다/=1, 쓰러트리기 종료)
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC STRING
#DIMS DYNAMIC MSTR
FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
STRING    '= RAND_SPLIT("후후/우대신/후후/우대신/키득/킥킥") + "…"
MSTR      '= CNAME(MASTER, TARGET)

;쓰러트리기 종료라면 1을 돌려주어 끝
SIF TFLAG:ACT파생
	RETURN 1

;REACT27, ACT파생 0:쓰러트린다
;==========================
IF !TFLAG:ACT파생
	;REACT분류가 받아들인다
	;---------------------
	IF IS_COMGRONAME("받아들인다")
		;REACT부하에 의한 파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동", "무리가 없는 행동"
				{
				PRINTFORMW 「밀어 넘어뜨려져 기쁜 듯한 얼굴 하고 있다\@ FAV_RATING 
				? 남자는, 조금 어때? 라는 생각도 드는데 
				# 남자는 어떻습니까? 그렇다고 할 생각도 듭니다만 \@…」
				}
				CALL HTMLPRINTW(@"「이것은 이것대로 나쁘지 않다\@ FAV_RATING ? 원이군요 # 군요 \@%SYMBOL%」")
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
			CASE "무리를 한 행동"
				{
				PRINTFORML 「그래그래, 그것으로 좋다\@ FAV_RATING 
				? 의 것 # 응이에요 
				\@。%MSTR%라도 그렇지만도 않다\@ FAV_RATING 
				? 의 것이군요 # 응이군요 \@?」
				}
				PRINTFORMW 「지금부터 나로 되는 일에 기대하고 있다\@ FAV_RATING ? 의 것이군요 # 응이군요 \@?」
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
				CALL HTMLPRINTW(@"「%MSTR%의 그 표정을 보면 판단\@ FAV_RATING ? 원 # 야 \@…%SYMBOL%」")
		ENDSELECT
		RETURN 1
	ENDIF

	;//소비 기력의 파생을 마련하고 싶지만 지금은 전송한다
	;REACT분류가 적극적 따르는·태연하게 한다
	;---------------------------------
	IF IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;호의 호감도로 파생
		IF FAV_RATING
			PRINTFORMW 「상당히 얌전해져 버려…」
			{
			PRINTFORMW 「%RAND_SPLIT(
			"그 거, 뒤는 마음대로 해라는 일?/" + 
			"는, 뒤는 좋을대로 시켜 받아요. 후후…/" + 
			"후후…, 좋아요. 그대로 가만히 하고 있어……")%」
			}
		ELSE
			PRINTFORMW 「아라, 얌전하네요?」
			CALL HTMLPRINTW(@"「그럼, 뒤는 내의 좋아하게 시켜 받아요%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류가용서를빈다
	;---------------------
	IF IS_COMGRONAME("용서를빈다")
		IF FAV_RATING
			PRINTFORMW 「이봐요, 얌전하게 해…」
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT(
			"귀여워해 준다/" + 
			"나쁘게는 하지 않아요/" + 
			"움직여지면 그…, 다양하게 하기 어렵고")%…%SYMBOL%」")
			}
		ELSE
			CALL HTMLPRINTW(@"「%RAND_SPLIT(", 이제(벌써) 놓치지 않아요/얌전하게 하고 있어 주세요? 후후…")%%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류가 소극적 따른다
	;---------------------
	IF IS_COMGRONAME("소극적으로 한다")
		;COM10, 부끄러워한다
		IF IS_NOWCOMNAME("부끄러워한다")
			IF FAV_RATING
				PRINTFORMW 「나에게 밀어 넘어뜨려지는 것이 그렇게 부끄러운거야?」
				CALL HTMLPRINTW(@"「후후, 사랑스럽다. 키스 하고 싶어져 버린다…%SYMBOL%」")
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
				;키스를 날조
				CALL SET_KISS
			ELSE
				CALL HTMLPRINTW(@"「아라, 이런 일로부끄러워한다는…. 사랑스럽네요%SYMBOL%」")
			ENDIF
			RETURN 1
		ENDIF

		;그 외의 COM의 경우
		IF FAV_RATING
			PRINTFORML 「그것, 조금 너무 소극적이지 않아?」
			PRINTFORMW 「뒤는 나에게 맡겨라라는 일이야?」
			CALL HTMLPRINTW(@"「뭐…, 그렇다면 그걸로. 나는 별로 좋지만%SYMBOL%」")
		ELSE
			PRINTFORML 「그다지 내켜하는 마음이 아닌 것 같지만…」
			PRINTFORMW 「그렇다고 해서, 저항할 생각도 없네요?」
			CALL HTMLPRINTW(@"「우후%HTML_AH2_K39()% 에서는, 그런 일이라면……」")
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;REACT로 반드시 완결시킨다
RETURN 1
