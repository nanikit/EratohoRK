@KOJO_MALE_REACT_K39_5 ;TFLAG:ACT派生(0=服を脱ぐように命令/1=服を着るように命令)
#DIM DYNAMIC FAV_RATING
FAV_RATING = GET_KFLAG_K39("相思相愛")

;ここでは負荷2を弾かない

;[*]REACT分類が"消極的従う/受け入れる/積極的従う/平然と従う"以外である場合はREACT分類"拒否"にセットされる

;REACT5_0, 服を脱ぐように命令
;============================
IF !TFLAG:ACT派生
	;消極的に従った（一枚脱ぐ）
	;=========================
	IF IS_COMGRONAME("消極的従う/受け入れる")
		;全裸になった
		;-------------
		{
		IF (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + 
		TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
		}
			{
			CALL HTMLPRINTW(@"「これでもう隠すものは\@ FAV_RATING 
			? ないわね # ありませんね \@…%HTML_PS_K39()%」")
			}
			;好意好感度で派生
			IF FAV_RATING
				;REACT分類が"受け入れる"
				IF IS_COMGRONAME("受け入れる")
					IF RAND:2
						PRINTFORMW 「ふふ、%CNAME(MASTER, TARGET)%も満更でもなさそう。でしょ？」
					ELSE
						PRINTFORMW 「これから私にされることに期待してるんでしょう？」 
						CALL HTMLPRINTW(@"「隠したって\@ TENSION(TARGET, 8) == "高い" ? ダ～メ # 駄目 \@…%HTML_PS_K39()%」")
					ENDIF
				;REACT分類が"消極的従う"
				ELSE
					PRINTFORMW 「%CNAME(MASTER, TARGET)%は全部脱いじゃうの、ちょっと嫌みたいだけど。ふふっ」
				ENDIF
			ELSE
				IF IS_COMGRONAME("受け入れる")
					PRINTFORMW 「…わかりますよ。これからの事に期待してるんですよね？」
					PRINTFORMW 「そんな顔して否定したって駄目です。ふふ……」
				ELSE
					PRINTFORMW 「仕方なくって感じですね？ ま、%RAND_SPLIT("いいです/別に構いません")%けど」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF

		;緩やかな動きで脱いだ
		;--------------------
		IF IS_COMGRONAME("受け入れる")
			IF FAV_RATING
				{
				CALL HTMLPRINTW(@"「\@ TENSION(TARGET, 8) == "高い" || IS_CONDITION(TARGET, "情欲") 
				? ふふっ…焦れったいけど、興奮しちゃう%HTML_PS_K39()% 
				# 随分勿体つけてくれるじゃない… \@」")
				}
			ELSE
				PRINTFORMW 「随分と勿体つけてくれるんですね？」
				PRINTFORMW 「ま、別に慌てる必要もありませんけど」
			ENDIF
			RETURN 1
		ENDIF

		;仕方なく脱いだ
		;--------------
		IF IS_COMGRONAME("消極的従う")
			{
			CALL HTMLPRINTW(@"「\@ FAV_RATING 
			? その%HTMLFONT("<b>『やれやれ、仕方ないな』</b>", , COLOR2_K39("WHITE"))%って顔やめてくれない？ 
			# %RAND_SPLIT("言われて仕方なく、って感じですね。別にいいですけど…/脱ぐのは嫌だったですか…？")% \@」")
			}
			RETURN 1
		ENDIF
	ENDIF

	;積極的に従った（二枚脱ぐ）
	;=========================
	IF IS_COMGRONAME("積極的従う/平然と従う")
		;全裸になった
		;------------
		{
		IF (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + 
		TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
		}
			{
			PRINTFORML 「ふふ、\@ IS_COMGRONAME("積極的従う") ? 積極的 # 手際がいいわ \@\@ 
			FAV_RATING ? ね # ですね \@。でも…」
			}
			IF FAV_RATING
				PRINTFORMW 「そうやって堂々と見せつけられると、私の方がちょっと恥ずかしくなっちゃうかも…」
				SIF !RAND:3
					CALL HTMLPRINTW(@"「な～んて%HTML_PS_K39()%　今更過ぎるって言われそうだけど。ふふ」")
			ELSE
				PRINTFORMW 「そこまで堂々と見せつけられると、こっちが恥ずかしくなってしまいます…」
				SIF !RAND:3
					PRINTFORMW 「ま、私が言えた義理じゃありませんけど。くすっ」
			ENDIF
			RETURN 1
		ENDIF

		;その他
		;------
		{
		CALL HTMLPRINTW(@"「\@ FAV_RATING 
		? うふ%HTML_PS_K39()%　早くシたくて待ちきれないの？ 
		# 積極的でとってもいいですね%HTML_PS_K39()% \@」")
		}
		RETURN 1
	ENDIF

	;脱がなかった
	;============
	IF IS_COMGRONAME("拒否")
		;恥ずかしくて脱げなかった
		;------------------------
		IF PALAM:MASTER:恥情 > PALAM:MASTER:反抗
			;初回
			IF FIRSTREACT(50)
				IF FAV_RATING
					PRINTFORMW 「%CNAME(MASTER, TARGET)%が恥ずかしいなんて言い出すのはちょっと意外ね…」
					PRINTFORMW 「正直…、もうそんな段階じゃないと思うんだけど？」
				ELSE
					PRINTFORML 「私に見られるのがそんなに恥ずかしいですか？」
					CALL HTMLPRINTW(@"「結構純粋なんですね%HTML_PS_K39()%」")
				ENDIF
				RETURN 1
			ENDIF

			;その他
			IF FAV_RATING
				PRINTFORMW 「今更恥ずかしいとか……、ちょっとそれはないんじゃない？」
			ELSE
				PRINTFORML 「困りましたね…」
				PRINTFORMW 「脱いでもらわないと、厄は祓えないのですが…」
			ENDIF
			RETURN 1
		ENDIF

		;嫌がって脱がなかった
		;--------------------
		IF PALAM:MASTER:反抗 > PALAM:MASTER:恥情
			{
			PRINTFORMW 「\@ FAV_RATING 
			? 服を着たままシたいの 
			# 服を着たままするのが好きなんですか \@？」
			}
			RETURN 1
		ENDIF
	ENDIF
ENDIF

