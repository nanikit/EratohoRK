;히나 구상 회화계 개별 리액션 구상 파일

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;


;        #
;회화계 REACT0～9
;        #
[SKIPSTART]
	#TFLAG:ACT
		0=지금의기분을얘기한다
		1=자신에게로의기분을얘기한다
		2=성에대한이야기를한다
		3=상냥하게위로한다
		4=엄격하게위협한다
		5=의상의변경을요구한다
		6=합의를요구한다
		7=벌이라고선언한다
		8=고함친다
		9=기분나쁘게웃는다

회화계는 범용 처리에는 향하지 않기 때문에, 범용 리액션(COM)에 흘리지 않고 개별 리액션(REACT)
그리고 완결시켜 버리는 것이 바람직하다고 생각합니다
[SKIPEND]

;///////////////////////
;REACT0, 지금의기분을얘기한다
;///////////////////////
[SKIPSTART]
·ACT파생에의 대입은 조교 대상의 리액션에 응해@ACTION_APPLY2_0에서 행해진다

·TFLAG:ACT파생
  ├0=말씨 나쁘게 대답했다
  ├1=용기를 내 강하게 맞았다
  ├2=야유
  ├3=분명히 지금의 기분을 고했다
  ├4=그저 나쁘지는 않다
  └5=기분 좋은 일 하고 싶다)
[SKIPEND]
@KOJO_MALE_REACT_K39_0
#DIM DYNAMIC FAV_RATING
FAV_RATING = GET_KFLAG_K39("상사상애")

;부하 2라면 REACT를 날린다
SIF GET_COM_RANK() == "무리를 한 행동"
	RETURN 0

;ACT파생, 0 말씨 나쁘게 질문에 답한 // REACT분류가 소극적 따른다 것으로, 리액션이빈정거린다 이외의 경우
;================================
IF !TFLAG:ACT파생
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	PRINTFORMW 「\@ FAV_RATING ? 응, 뭔가 말했어?  # 미안해요, 자주(잘) 들리지 않았던 것입니다만… \@」
	RETURN 1
ENDIF

;ACT파생, 1 용기를 내 강하게 맞은 // 리액션이강하게대답한다의 경우. REACT분류는 "거부"에 세트 된다
;==================================
IF TFLAG:ACT파생 == 1
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	PRINTFORMW 「\@ FAV_RATING ? 상당히, 위세가 좋구나 # 건강은 좋은 것 같네요 \@…」
	RETURN 1
ENDIF

;ACT파생, 2 야유 「지금의 기분은, 이라고 (들)묻어도」 // 리액션이빈정거린다의 경우. REACT분류는 "거부"에 세트 된다
;==========================================
IF TFLAG:ACT파생 == 2
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF FAV_RATING
		PRINTFORML 「뭐야 그것, 야유?」
		PRINTFORMW 「그러한 것은 좋아하지 않지만…」
	ELSE
		PRINTFORML 「야유, 입니까…?」
		PRINTFORMW 「너무 그러한 것은, 좋아하지 않습니다만…」
	ENDIF
	RETURN 1
ENDIF

;ACT파생, 3분명히 지금의 기분을 고한 // REACT분류가 적극적 따르는 것 경우
;=======================================
IF TFLAG:ACT파생 == 3
	;첫회
	IF FIRSTREACT(30)
		{
		CALL HTMLPRINTW(@"「\@ FAV_RATING 
		? 후후, 즐겨 받아지고 있는 것 같구나 
		# 그렇습니까. 즐겨 받아지고 있는 것 같고 최상입니다 \@%HTML_PS_K39()%」")
		}
		RETURN 1
	ENDIF

	;상사상애
	IF FAV_RATING
		;역강간으로 질내사정의 뒤
		IF TFLAG:전회질내사정 == 2
			PRINTFORML 「뭐, 당연하지만 말야!」
			PRINTFORMW 「나의 질내에 이렇게 내 둬, 좋지 않았다고 말할 수 있을 이유 없네요」
		;통상
		ELSE
			PRINTFORMW 「언제나 그 정도 솔직하다면, 나도 다양하게 편한 것이지만 말야?」
		ENDIF
		;페이드 아웃
		CALL FADE_TO_CLEARLINE(20, @"「%RAND_SPLIT("후후/킥킥/키득")%…」")
		RETURN 1
	ENDIF

	;조교 대상으로부터의 호의 1000이상
	IF AFFECTION("조교가 끝난 상태", TARGET)
		;역강간으로 질내사정의 뒤  와  그 외
		{
		CALL HTMLPRINTW(@"「\@ TFLAG:전회질내사정 == 2 
		? 나의 질내가 그렇게 좋았던 것입니까…?  
		# 우대신…, 나도%CNAME(MASTER, TARGET)%의 일 좋아해요%HTML_PS_K39()% \@」")
		}
		RETURN 1
	ENDIF

	;조교 대상으로부터의 호의 500이상
	IF AFFECTION("눈을 뜸")
		;역강간으로 질내사정의 뒤
		IF TFLAG:전회질내사정 == 2
			PRINTFORML 「상태가 좋았어? 　%RAND_SPLIT("추잡합니다/천한 말투입니다")%군요…」
			CALL HTMLPRINTW(@"「그렇지만, 후후. 즐겨 받을 수 있던 것 같고 최상입니다…%HTML_PS_K39()%」")
		;통상
		ELSE
			CALL HTMLPRINTW(@"「상당히 솔직해져 왔어요. 후후」")
		ENDIF
		RETURN 1
	ENDIF

	;그 외
	CALL HTMLPRINTW(@"「\@ FAV_RATING ? 우대신, 사랑스럽다 # 솔직하고 좋아요 \@%HTML_PS_K39()%」")
	RETURN 1
ENDIF

;ACT파생, 4그저 나쁘지는 않은 // REACT분류가 태연하게 따르는 것 경우
;=============================
IF TFLAG:ACT파생 == 4
	IF TEQUIP:성교봉사중
		{
		PRINTFORMW 「\@ FAV_RATING 
		? 그저라는건 뭐야 그것…, 뭔가 화납니다만―!?  
		# 그것은… 솔직하게는 기뻐할 수 없네요. 과연, 지금의 상황에서는… \@」
		}
		RETURN 1
	ENDIF

	;그 외
	{
	PRINTFORMW 「\@ FAV_RATING 
	? 조금 걸려요. 그 말투… 
	# 그것은 최상입니다. 조금 석연으로 하지 않지만… \@」
	}
	RETURN 1
ENDIF

;ACT파생, 5 기분 좋은 일 하고 싶은 // ;REACT분류가 받아들인다 것 경우
;===============================
;IF TFLAG:ACT파생 == 5
;ENDIF

;COM0, 침묵한다 // 지금의기분을얘기한다에 있어서 COM"침묵한다"는(질문에 답하지 않는다) 취급
;===============
IF IS_NOWCOMNAME("침묵한다")
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF FAV_RATING
		PRINTFORMW 「저기…, 어째서 입다물고 있는 거야?」
	ELSE
		PRINTFORML 「아무것도 말하고 싶지 않습니까?」
		PRINTFORMW 「나, 그러면 곤란해 버립니다…」
	ENDIF
	RETURN 1
ENDIF

;COM7, 용서를빈다
;================
IF IS_NOWCOMNAME("용서를빈다")
	;「의견을 내다니 좋은 담력이군요!」벌포인트+
	IF TFLAG:REACT파생 == 1
		{
		CALL HTMLPRINTW(@"「\@ FAV_RATING 
		? %RAND_SPLIT("/후")%, 다～째%HTML_PS_K39()% 
		# 미안해요. 이제 와서, 늦습니다… \@」")
		}
		RETURN 1
	ENDIF

	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	{
	CALL HTMLPRINTW(@"「\@ FAV_RATING 
	? %RAND_SPLIT("/후")%, 어떻게 하지%RAND_SPLIT("인/그럴까")%%HTML_PS_K39()% 
	# 이제(벌써)! 언제나 내가 허락한다고 생각되어도 곤란합니다… \@」")
	}
	RETURN 1
ENDIF

;COM33, 무서워한다
;==============
IF IS_NOWCOMNAME("무서워한다")
	;파생 1이라면 범용 리액션에
	SIF TFLAG:REACT파생 == 1
		RETURN 0

	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF FAV_RATING
		PRINTFORML 「저기…, 어째서무서워한다의?」
		PRINTFORMW 「그러한 것 그만두어%RAND_SPLIT("갖고 싶지만/")%…」
	ELSE
		PRINTFORMW 「무서워하게 해 버린 것 같네요…」
	ENDIF
	RETURN 1
ENDIF

;COM14, 쾌감을참는다
;======================
IF IS_NOWCOMNAME("쾌감을참는다")
	;볼개그 장비중
	IF TEQUIP:볼개그
		IF FAV_RATING
			PRINTFORML 「몸 삐걱삐걱 시켜, 필사적으로 참고 있는 것 같지만…」
			PRINTFORMW 「무엇을 그렇게 참고 있는 거야…?」
			CALL HTMLPRINTW(@"「… 은 지금 들어도, 조금 무리같구나…%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「뭔가 필사적으로 견디고 있는 것 같습니다만, 도대체 어떻게 한 것이지요?」
			CALL HTMLPRINTW(@"「아, 대답하지 않아도 괜찮아요. 어차피 도취되어 듣지 않기 때문에. 우훗%HTML_PS_K39()%」")
		ENDIF
		RETURN 1
	ENDIF

	;오나홀 장착중
	IF TEQUIP:오나홀
		IF FAV_RATING
			PRINTFORMW 「필사적으로 참고 있는 것 같지만, 지금 내가 훑으면 어떻게 되어 버릴까?」
			PRINTFORMW 「저기, 어때?   좀 더 분명히 말해」
		ELSE
			PRINTFORMW 「이를 악물지 않으면, 가 버릴 것 같습니까?」
			PRINTFORMW 「내가 움직여 봐도 괜찮아요?   킥킥…」
		ENDIF
		RETURN 1
	ENDIF

	;그 이외
	IF FAV_RATING
		PRINTFORMW 「참지 않으면 가 버릴 것 같아?」
		CALL HTMLPRINTW(@"「후훗, 사랑스럽네요%HTML_PS_K39()%」")
	ELSE
		PRINTFORMW 「참지 않으면 가 버릴 것 같습니까?」
	ENDIF
	RETURN 1
ENDIF

;COM15, 쾌감을받아들인다
;========================
IF IS_NOWCOMNAME("쾌감을받아들인다")
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;오나홀 장착중
	IF TEQUIP:오나홀
		IF FAV_RATING
			PRINTFORML 「즐겨 받아지고 있는 것 같구나?」
			PRINTFORMW 「그렇게 기분 좋으면, 이대로 그것안에 내 버리면?」
		ELSE
			PRINTFORMW 「우후, 기분 좋은 것 같고 최상이네요♪」
			PRINTFORMW 「내가 훑어 좀 더 자주(잘) 해 줄까요?」
		ENDIF
		;페이드 아웃
		CALL FADE_TO_CLEARLINE(20, @"「%RAND_SPLIT("후후/키득")%…」")
		RETURN 1
	ENDIF

	;그 외
	{
	CALL HTMLPRINTW(@"「\@ FAV_RATING 
	? 솔직하고 사랑스럽다%HTML_AH2_K39()%  이제(벌써) 키스 하고 싶어져 버린다%RAND_SPLIT("원/")% 
	# 솔직하게 느껴 주면, 나도 기쁩니다%HTML_PS_K39()% \@」")
	}
	RETURN 1
ENDIF


;////////////////////////////
;REACT1, 자신에게로의기분을얘기한다
;////////////////////////////
[SKIPSTART]

·ACT파생에의 대입은 조교 대상의 리액션에 응해@ACTION_APPLY2_1에서 행해진다

·TFLAG:ACT파생(0=싫지 않은… 일지도 모른다…
  ├1=그런 일이 있을 이유 없다!
  ├2=곤란하네요… 좋아하지 않다고 말하면 화내?
  ├3= 이제(벌써) 심한 일 하지 않으면, 좀 더 좋아하게 될 수 있을지도 모른다
  ├4=분명히 좋아라고 고했다
  └5= 좀 더 굉장한 일 해 주면, 좀 더 좋아하게 될 수 있을지도)

[SKIPEND]
@KOJO_MALE_REACT_K39_1
#DIM DYNAMIC FAV_RATING
FAV_RATING = GET_KFLAG_K39("상사상애")

;부하 2라면 REACT를 날린다
SIF GET_COM_RANK() == "무리를 한 행동"
	RETURN 0

;REACT1_0
;「싫지 않은… 일지도 모른다…」 // REACT분류가 소극적 따른다 것 경우
;================================
IF !TFLAG:ACT파생
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF FAV_RATING
		PRINTFORMW 「이, 이제 와서 그것은 없지요!?   조금 심하지 않아…?」
	ELSE
		PRINTFORML 「그래, 입니까…」
		PRINTFORMW 「일단 미움받지 않다는 일로, 기뻐해도 괜찮을까요…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_1
;「그런 일이 있을 이유 없다!」 // 리액션이강하게대답한다의 경우
;============================
;REACT분류는 "거부"에 세트 된다
IF TFLAG:ACT파생 == 1
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF 0
	ELSEIF FAV_RATING
		PRINTFORML 「하? 도대체 무슨 말을 하고 있어!?」
		PRINTFORMW 「의미를 모르지만?」
	ELSEIF AFFECTION("몹시 사랑함", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORMW 「액신이라고 해도 한사람의 여자입니다. 너무 심한 일, 말하지 마……」
		PRINTFORMDW %CALLNAME:TARGET%는 몸을 진동시켜 울 것 같은 얼굴로 숙여 버렸다…
	ELSE
		PRINTFORMW 「그래, 입니까…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_2,
;야유 「곤란하네요… 좋아하지 않다고 말하면 화내?」 // 리액션이빈정거린다의 경우
;==============================================
IF TFLAG:ACT파생 == 2
	;조교 대상에의 호감도 3000이상
	IF AFFECTION("맹목적으로 사랑", (FLAG:주인님)) && TALENT:TARGET:주인님 && !FAV_RATING
		PRINTFORMW 「그, 그런! 짓궂다니, 심하다…」
		PRINTFORMW 「나는…,%CNAME(MASTER, TARGET)%의 일, 이렇게……」
		PRINTFORMDW %CALLNAME:TARGET%는 당장 울기 시작할 것 같다…
		RETURN 1
	ENDIF

	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF 0
	ELSEIF FAV_RATING
		PRINTFORMW 「화내요!? 당연하겠지요」
	ELSEIF AFFECTION("몹시 사랑함", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORMW 「부탁 이제 그만두어! 그 이상, 말하지 마…」
	ELSE
		PRINTFORMW 「별로? 분노는 하지 않지만. 그렇지만…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_3
;「이제(벌써) 심한 일 하지 않으면, 좀 더 좋아하게 될 수 있을지도 모른다」 // REACT분류가 적극적 따르는 것으로 조교 대상 호의가 적은 경우
;========================================================== // 혹은 REACT분류가 태연하게 따른다
IF TFLAG:ACT파생 == 3
	;볼개그 장비중
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;그 외
	IF 0
	ELSEIF FAV_RATING
		PRINTFORMW 「나, 그렇게 심한 일 해 왔을까…」
	ELSEIF AFFECTION("몹시 사랑함", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORML 「그래, 입니까…」
		PRINTFORMW 「나, 조금은 기대해도 될까요」
	ELSE
		PRINTFORML 「그렇습니까…?」
		PRINTFORMW 「그렇지만, 응. 곤란하군요…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_4
;분명히 좋아한다고 고한 // REACT분류가 적극적 따르는 것으로, 조교 대상 호의가 1000이상
;======================
IF TFLAG:ACT파생 == 4
	;첫회
	IF FIRSTREACT(40)
		IF FAV_RATING
			PRINTFORML 「에에, 알고 있었어요?」
