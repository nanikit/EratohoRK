@TEXTDISPLAY_CATEGORY_10_TEASE_K39
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC RHEART
#DIMS DYNAMIC MSTR
SYMBOL '= HTML_PS_K39()
RHEART '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
MSTR   '= CALLNAME:MASTER

IF GET_KFLAG_K39("相思相愛")
	SELECTCASE AUTORAND(4, 320)
		CASE 0
			PRINTFORMW 「ねぇ、もうイっちゃいそう？　出ちゃいそう？」
			{
			CALL HTMLPRINTW(@"「うふ\@ TENSION(TARGET, 8) == "高い" ? ふ # 
			\@っ%RHEART% だ～め！ 簡単にはイかせてあげないから%SYMBOL%」")
			}
		CASE 1
			PRINTFORML 「そんなに腰振っちゃって、一人でイっちゃうつもり？」
			PRINTFORMW 「私の体使って、自分だけ満足するつもりなの？」
			PRINTFORMW 「そんな人には、ちょっとお仕置きが必要かしらね…」
		CASE 2
			PRINTFORMW 「ねぇ、もう出ちゃうの？ 私の膣内にぶち撒けたいの？」
			{
			CALL HTMLPRINTW(@"「でも……うふ\@ TENSION(TARGET, 8) == "高い" ? ふ # 
			\@%RHEART% そんな簡単にはイかせてあげないから%SYMBOL%」")
			}
		CASE 3
			CALL HTMLPRINTW(@"「んっ、激しい…%RHEART% もうイキそうなのかしら…」")
			PRINTFORMW 「ほら、イきたいなら何か言う事があるんじゃないの？」
			PRINTL
			PRINTFORMDL [0] イかせて！
			PRINTFORMDL [1] 中に出させて！
			CALL INPUTINTD(0, 1)
			SELECTCASE RESULT
				CASE 0
					CALL HTMLPRINTW(@"「そんなにイきたいの？ うふっ、判った。もっと焦らしてあげる%SYMBOL%」")
				CASE 1
					PRINTFORML 「ねぇ、神様を妊娠させるつもりなの？」
					PRINTFORMW 「それとも、中出しして、私を征服した気分にでも浸りたいの？」
					PRINTFORMW 「でも……そう簡単にはいかないわよ？ クスクス…」
			ENDSELECT
	ENDSELECT
;それ以外
ELSE
	SELECTCASE AUTORAND(4, 321)
		CASE 0
			PRINTFORMW 「もう限界ですか？」
			CALL HTMLPRINTW(@"「でも、簡単にはイかせませんよ%SYMBOL%」")
		CASE 1
			PRINTFORMW 「もうイきそうですか？」
			PRINTFORMW 「でも、まだ我慢してもらいますよ？ ふふ…」
		CASE 2
			PRINTFORMW 「もう限界ですか？ 気が早いんですね。でも…」
			CALL HTMLPRINTW(@"「もう少しだけ付き合ってもらいますね%SYMBOL%」")
		CASE 3
			PRINTFORMW 「まさか…、１人で先にイクなんてことしませんよね？」
			CALL HTMLPRINTW(@"「尤も…%MSTR%を先にイかせるつもりもありませんけど%SYMBOL%」")
	ENDSELECT
ENDIF
RETURN 1

