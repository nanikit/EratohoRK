				}
				{
				CALL HTMLPRINTW(@"「や\@ PARAM >= 8500 ? ぁぁ # 
				\@っ、だめっ%AHEART% だめぇっっ！！\@ 
				PARAM >= 8500 ? 死んじゃうっ、死んじゃうっ…！ # これ、きもち…よすぎて \@ッ…！」")
				}
			CASE 1
				{
				CALL HTMLPRINTW(@"「んんっ%AHEART% んくっ！ こ、こんなのっ！ も、もうっ…\@ 
				PARAM >= 8500 ? 耐えられないっ！ # … \@」")
				}
				{
