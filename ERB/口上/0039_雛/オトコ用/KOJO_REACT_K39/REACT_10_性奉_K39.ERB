;調教者Ｖ感覚派生
SELECTCASE ABL:TARGET:Ｖ感覚
	;Ｖ感覚8以上
	CASE IS >= 8
		SELECTCASE AUTORAND(2, 301)
			CASE 0
				{
				CALL HTMLPRINTW(@"「は\@ PARAM >= 8500 ? ぁぁ # 
				\@んっッ%AHEART% %RAND_SPLIT("ん/ン")%っ！ あ%VOICEDDOT%ぁ\@ 
