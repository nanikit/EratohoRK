				PARAM >= 8500 ? あぁっ # 
				\@っ！ ひぁっ！ あっ%AHEART% あっ%AHEART% あッ…%EHEART%」")
				}
		ENDSELECT
	;Ｖ感覚5以上
	CASE IS >= 5
		IF FAV_RATING && PERCENT(75)
			{
			CALL HTMLPRINTW(@"「あ%VOICEDDOT%っ%AHEART% あ\@ 
			PARAM >= 8500 ? あ # 
			\@ぁっ%AHEART% ひぁっ%AHEART% あっ、あ%VOICEDDOT%っ%EHEART% あぁっ…！」")
			}
			{
			CALL HTMLPRINTW(@"「\@ PARAM >= 8500 ? っ%RAND_SPLIT("はぁ/ふぁ/あぁ")% # \@っ…！ \@ PARAM >= 8500 
			? おまんこ、ずぼずぼされてぇっ！ ば、バカになっ%RAND_SPLIT("ひ/ち")%ゃう…ッ 
			# こんな…、こんなに、されたら%RAND_SPLIT("ぁ/")%ッ \@…！」")
			}
		ELSE
			CALL HTMLPRINTW(@"「わ、私\@ PARAM >= 8500 ? ぃ # \@っ！ どうして……こ、こんなにッ！」」")
			{
			CALL HTMLPRINTW(@"「はぁぁっ%AHEART% あ%VOICEDDOT%っ%AHEART% あっ%AHEART% あ\@ 
			PARAM >= 8500 ? ぁ # 
			\@んっ…%AHEART% もう、もうっッ！」")
			}
		ENDIF
	;Ｖ感覚3以上
	CASE IS >= 3
		IF FAV_RATING
			{
			CALL HTMLPRINTW(@"「あ%AHEART% はぁっ…！！？ %RAND_SPLIT("は/あ/ん")%ぁっ%AHEART% " + 
			@"あ%VOICEDDOT%っ%AHEART% あっ、あんっ…%EHEART%」")
			}
			{
			CALL HTMLPRINTW(@"「あ%VOICEDDOT%ぁっ%AHEART%、これだめぇっ%AHEART%！ あ\@ 
			PARAM >= 8500 ? ぁぅ # \@っ、%RAND_SPLIT("はぁ/は/ああ/ん/ン")%ぁっ…%EHEART%」")
			}
		ELSE
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あん")%んっ%AHEART% ん%VOICEDDOT%っ%AHEART% " + 
			@"んはっ%AHEART% あ\@ PARAM >= 8500 ? ぁぁ # \@ぁっ…ッ…！」")
			}
			CALL HTMLPRINTW(@"「はぁ…%RHEART% だ、ダメですっ！ こんなに、こんなにされたらぁっ…！」")
		ENDIF
	;Ｖ感覚3未満
	CASEELSE
		SELECTCASE AUTORAND(2, 302)
			CASE 0
				{
				CALL HTMLPRINTW(@"「%RAND_SPLIT("は/あ/ん")%ぅっ、あ%VOICEDDOT%ぁっ%AHEART% " + 
				@"%RAND_SPLIT("ん/ン")%っ%AHEART% はっ、あ%VOICEDDOT%\@ PARAM >= 8500 ? ぁ # \@ぁっ%EHEART%」")
				}
			CASE 1
				{
				CALL HTMLPRINTW(@"「っ…%RAND_SPLIT("は/ふ/あ/ん")%ぁっ%AHEART% " + 
				@"あっ…%AHEART% %RAND_SPLIT("あ/は")%ぅ……%RAND_SPLIT("ん/ン/んん")%っ！」")
				}
		ENDSELECT
ENDSELECT
RETURN 1

