FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
AHEART    '= HTML_AH_K39()
STRING    '= RAND_SPLIT("후후/우대신/후후/우대신/키득/킥킥") + "…"
MSTR      '= CNAME(MASTER, TARGET)

;조교 대상의 마조끼 5이상 또한 호의 호감도가 일정 이상//25%의 확률로 네스트를 누락 하층에 흐르도록(듯이)
IF ABL:MASTER:마조끼 >= 6 && FAV_RATING && PERCENT(75)
	SELECTCASE AUTORAND(2, 265)
		CASE 0
			PRINTFORMW 「그런 성청 빌려주어지면, 오슬오슬 해 버려……」
			;ACT 볼개그가 아닌 경우는 파생
			{
			CALL HTMLPRINTW(@"「이제(벌써), 견딜 수 없게 되어 버린다%HTML_AH2_K39()% \@ 
			!IS_NOWACTNAME("볼개그") ? %STRING% # \@」")
			}
			;ACT 볼개그의 경우
			SIF IS_NOWACTNAME("볼개그")
				CALL HTMLPRINTW("「이봐요, 군침이 늘어지고 있어요%HTML_AH2_K39()% %STRING%」")
		CASE 1
			PRINTFORML 「달콤한 소리로 울어 버려, 이제(벌써) 어쩔 수 없네요……」
			PRINTFORMW 「――이니까 이런 사람을 좋아하는 일까」
			{
			CALL HTMLPRINTW(@"「\@ RAND:3 
			? 라든지, 말해 보거나 해…%HTML_AH2_K39()% %STRING% 
			# 에서도 이렇게 되었던 것도, 나에게도 책임이 있는거네요… \@」")
			}
	ENDSELECT
	RETURN 1
ENDIF

;조교 대상의 마조끼 3이상
IF ABL:MASTER:마조끼 > 3
	SELECTCASE AUTORAND(4, 266)
		CASE 0
			PRINTFORMW 「%STRING%, 이제(벌써) 기뻐하고 있도록(듯이) 밖에 소문\@ FAV_RATING ? 없네요 # 선이군요 \@」
			{
			CALL HTMLPRINTW(@"「\@ FAV_RATING 
			? 이봐요 # 자 
			\@, 좀 더 울어\@ FAV_RATING 
			? ?  # … 
			\@좀 더 나를 즐겁게 해\@ !FAV_RATING ? 주세요 # \@%SYMBOL%」")
			}
		CASE 1
			PRINTFORMW 「그런 소리로 울어지면 흥분해\@ FAV_RATING ? 낳았지만 # 라고 끝냅니다 \@…」
			{
			PRINTFORMW 「\@ FAV_RATING 
			? 응# 그 
			\@그것…, 일부러 해\@ FAV_RATING 
			? 것이 아닌거야 # 전과 \@?」
			}
		CASE 2
			PRINTFORML 「조금 달콤한 소리가 섞이고 있다\@ FAV_RATING ? 원이야 # 같네요 \@…」
			;ACT 아이마스크의 경우는 파생한다
			{
			PRINTFORMW 「실은\@ IS_NOWACTNAME("아이마스크") 
			? 시야를 빼앗겨 # 
			\@기뻐하고 있다\@ FAV_RATING 
			? 것이 아닌거야 # 것은 아닙니까 \@?」
			}
		CASE 3
			PRINTFORMW 「같은 비명에서도 조금 다른 느낌이\@ FAV_RATING ? 해요 # 하네요 \@…」
			{
			CALL HTMLPRINTW(@"「그, 뭐라고 할까… 나, 조금 흥분해\@ 
			FAV_RATING ? 아니야도 # 라고 끝냅니다 \@%SYMBOL%」")
			}
	ENDSELECT
	RETURN 1
ENDIF

;;;;;;;;;;;
;이하 그 외
;;;;;;;;;;;

;ACT 볼개그의 경우
IF IS_NOWACTNAME("볼개그")
	SELECTCASE AUTORAND(2, 267)
		CASE 0
			CALL HTMLPRINTW(@"「%STRING%, 울어도 소문\@ FAV_RATING ? 없어요 #  선 \@야%SYMBOL%」")
		CASE 1
			{
			PRINTFORMW 「과연 조금 그 상태에서는……, 비명이 되어\@ 
			FAV_RATING ? 없어요?  # 없네요 \@ %STRING%」
			}
			{
			CALL HTMLPRINTW(@"「그렇지만 그런 소리 귀댁 되면, 조금 이상한 기분에\@ 
			FAV_RATING ? # 라고 끝냅니다 \@%SYMBOL%」")
			}
		CASE 2
			{
			PRINTFORMW 「\@ FAV_RATING 
			? 응 # 저,  
			\@혹시…, 조금 불안\@ FAV_RATING 
			? 였다거나 해?  # 를 느끼고 있습니까 \@?」
			}
			PRINTFORMW 「그렇지만 나…,%MSTR%가 무슨 말을 하고 있을까 알아 들을 수 없다\@ FAV_RATING ? 응이지만 # 의 것이지만 \@…」
	ENDSELECT
ELSE
	SELECTCASE AUTORAND(4, 268)
		CASE 0
			{
			PRINTFORMW 「\@ FAV_RATING ? 응,# 기분탓입니까… 
			\@조금 기쁜듯이 울고 있는 기분이\@ 
			FAV_RATING ? 하지만?  # 하네요 \@」
			}
			;ACT줄/아이마스크의 경우
			SIF IS_NOWACTNAME("줄/아이마스크")
				{
				PRINTFORMW 「사실은\@ IS_NOWACTNAME("줄") ? 박등 # 눈가리개야 
				\@의, 싫지 않잖아?  %SYMBOL%」
				}
		CASE 1
			{
			CALL HTMLPRINTW(@"「%STRING%, 그렇게 울어지면 흥분해\@ 
			FAV_RATING ? # 라고 끝냅니다 \@%SYMBOL%」")
			}
		CASE 2
			{
			PRINTFORMW 「%MSTR%의 비명을 들을 때에, 조금 이상한 기분에\@ 
			FAV_RATING ? 위 # 라고 끝냅니다 \@…」
			}
		CASE 3
			;ACT줄/아이마스크의 경우
			SIF IS_NOWACTNAME("줄/아이마스크")
				{
				PRINTFORMW 「\@ IS_NOWACTNAME("줄") ? 조금 줄이 나무개 # 시야를 빼앗기는 것은 포 
				\@산 것입니까? 그렇지만…」
				}
			{
			CALL HTMLPRINTW(@"「그렇게 울어지면, 조금 흥분해\@ 
			FAV_RATING ? 아니야도 # 라고 끝내네요 \@…%SYMBOL%」")
			}
	ENDSELECT
ENDIF
RETURN 1
