;나중에 다시 봐, 각 REACT의 내용에 대부분 차이가 없구나… 가 되었으므로 모두 범용 처리하는 일에

;///////////////////////////
;COM아픔을참는다의 범용 처리
;///////////////////////////
@PRINT_KOJO_REACT_GRU_30_COM_30_K39
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC STRING
#DIMS DYNAMIC MSTR
FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
STRING    '= RAND_SPLIT("후후/우대신/후후/우대신/키득/킥킥") + "…"
MSTR      '= CNAME(MASTER, TARGET)

IF 0
;조교 대상의 마조끼 6이상 또한 상사상애로 REACT파생가 아프지만기분좋다！//25%의 확률로 하층에
ELSEIF ABL:MASTER:마조끼 >= 6 && TFLAG:REACT파생 == 1 && PERCENT(75)
	SELECTCASE AUTORAND(2, 260)
		CASE 0
			PRINT 「이제(벌써) 완전히
			;ACT 마다의 파생을 본다
			SELECTCASE GET_ACTNAME(TFLAG:ACT)
				CASE "스팽킹"
					PRINT 엉덩이 얻어맞아
				CASE "채찍"
					PRINT 채찍으로 얻어맞아
				CASE "바늘"
					PRINT 바늘로
				CASE "줄"
					PRINT 속박되어
				CASE "아이마스크"
					PRINT 눈가리개 되어
			ENDSELECT
			PRINTFORMW 기뻐하는 변태라는 느낌\@ !FAV_RATING ? 입니다 # \@군요…」
			CALL HTMLPRINTW(@"「뭐, 나에게도 책임이 있다\@ FAV_RATING ? 응이다 # 의 것이지요 \@지만%SYMBOL%」")
		CASE 1
			PRINTW 「너무 이런 일 하고 싶고\@ FAV_RATING ? 없다 # 없습니다 \@지만…」
			{
			CALL HTMLPRINTW(@"「\@ FAV_RATING 
			? %RAND_SPLIT(@"%MSTR%…, 이것, 싫지 않은 거네?/이것이 좋다고 말한다면 어쩔 수 없을까.")% %STRING%%SYMBOL% 
			# %MSTR%가 이것을 바라고 있는 듯%RAND_SPLIT("였다/(이었)였습니다")%의 것으로…%SYMBOL %\@」")
			}
	ENDSELECT
	RETURN 1
;조교 대상의 마조끼 3이상
ELSEIF ABL:MASTER:마조끼 > 3
	SELECTCASE AUTORAND(4, 261)
		CASE 0
			PRINTL 「아픈 것을 인내라고 할까, 기뻐하고 있는 것 같게 보이는\@ FAV_RATING ? 이지만?  # 이지만… \@」
			PRINTW 「\@ FAV_RATING ? 응, 그근처 어떻게야 # 그근처 어떻습니까 \@?  %STRING%」
		CASE 1
			PRINTFORM 「\@FAV_RATING ? 응 # 그 \@사실은…,
			;ACT 마다의 파생을 본다
			SELECTCASE GET_ACTNAME(TFLAG:ACT)
				CASE "스팽킹"
					PRINT 엉덩이고인가
				CASE "채찍"
					PRINT 채찍으로 고인가
				CASE "바늘"
					PRINT 바늘로 비난등
				CASE "줄"
					PRINT 박등
				CASE "아이마스크"
					PRINT 눈가리개야
			ENDSELECT
			PRINTFORMW 의를 좋아하는\@ FAV_RATING ? 것이 아닌거야 # 것은 아닙니까 \@?」
			CALL HTMLPRINTW(@"「이봐요… 부끄러워하지 마. 정직하게 말해 봐\@ !FAV_RATING ? 주세요 # \@%SYMBOL%」")
		CASE 2
			{
			PRINTFORMW 「아픔\@ IS_NOWACTNAME("아이마스크") 
			? 라고 할까, 공포라고 할까 # 를 인내라고 할까 
			\@, 뭔가 다른 것을 참고 있는 것처럼 보인다\@ FAV_RATING 
			? 응이지만 # 의 것이지만 \@…」
			}
			PRINTFORMW 「정말로는 어떻게\@ FAV_RATING ? 의 # 응이지요 \@?  %SYMBOL%」
		CASE 3
			;ACT 마다의 파생을 본다
			SELECTCASE GET_ACTNAME(TFLAG:ACT)
				CASE "스팽킹"
					PRINT 자신으로부터 엉덩이를 내밀고 있다
				CASE "채찍"
					PRINT 채찍으로 얻어맞는 것을 바라고 있다
				CASE "바늘"
					PRINT %CNAME(MASTER, TARGET)%자신이 이것을 바라고 있다
				CASE "줄"
					PRINT 구속되고 기뻐하고 있다
				CASE "아이마스크"
					PRINT 시야를 빼앗기고 기뻐하고 있다
			ENDSELECT
			PRINTFORMW 같게 보이는 것은 나의 기분탓\@ !FAV_RATING ? 입니까 # \@?」
			CALL HTMLPRINTW(@"「실은 이것이…,%HTMLFONT("<b>를 좋아하게 되었다</b>", , ,)%, 라든지.%STRING%」」")
	ENDSELECT
	RETURN 1
;그 외를 처리
ELSE
	SELECTCASE AUTORAND(7, 263)
		CASE 0
			{
			PRINTFORML 「이런 것은 좋아\@ FAV_RATING ? 다 # 에서는 \@없었을 것\@ 
			FAV_RATING ? 응이지만 # 의 것이지만 \@」
			}
			PRINTFORMW 「뭔가 나도…, 이상한 기분에\@ FAV_RATING ? 라고 와 버린다 # 라고 끝냅니다 \@…」
		CASE 1
			PRINTFORM 「\@ FAV_RATING ? 응,# 그 \@실은
			;ACT 마다의 파생을 본다
			SELECTCASE GET_ACTNAME(TFLAG:ACT)
				CASE "스팽킹"
					PRINT 엉덩이고인가
				CASE "채찍"
					PRINT 채찍으로 고인가
				CASE "바늘"
					PRINT 바늘로 회야
				CASE "줄"
					PRINT 줄로 박등
				CASE "아이마스크"
					PRINT 눈가리개야
			ENDSELECT
			PRINTFORMW 의……%RAND_SPLIT("상당히 비어/꽤 좋아해")%이기도 해\@ FAV_RATING ? 없다 # 전과 \@?」
			CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
		CASE 2
			{
			PRINTFORMW 「불쌍한 기분도\@ FAV_RATING ? 한다 # 합니다 \@하지만,\@ 
			FAV_RATING ? 는 나, 흥분해 버릴지도, 코레 # 왠지 나…, 흥분해 버립니다 \@…」
			}
		CASE 3
			PRINTFORMW 「꽤 참을성이 많다\@ FAV_RATING ? 원 # 입니다 \@군요…」
		CASE 4
			PRINTFORMW 「아직 여유가 있을 것 같다\@ !FAV_RATING ? 입니다 # \@군요?」
		CASE 5
			PRINTFORMW 사실은 이런 것, 싫지\@ FAV_RATING ? 있는이 아니다 # 사거나 하지 않겠습니까 \@?
		CASE 6
			PRINTFORM 「%MSTR%
			;ACT 마다의 파생을 본다
			SELECTCASE GET_ACTNAME(TFLAG:ACT)
				CASE "스팽킹", "채찍", "바늘"
					PRINT 가 아픔에 참는 그 모습……
				CASE "줄"
					PRINT 의 구속된 모습을 보면
				CASE "아이마스크"
					PRINT 의 눈가리개 된 모습을 보면
			ENDSELECT
			PRINTFORMW , 조금 흥분해\@ FAV_RATING ? 가격 상승 # 라고 끝내네요 \@
			CALL PRINT_SYMBOL_K39
	ENDSELECT
	RETURN 1
ENDIF

