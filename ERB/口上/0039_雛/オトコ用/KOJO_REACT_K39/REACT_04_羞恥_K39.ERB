@KOJO_MALE_REACT_K39_42 ;ACT派生なし
;変数宣言と代入
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC AHEART
FAV_RATING = GET_KFLAG_K39("相思相愛")
AHEART    '= HTML_AH_K39()

;REACT42, 初回 // リアクションが受容である場合
;=============
IF IS_COMGRONAME("消極的従う/平然と従う/積極的従う/受け入れる") && FIRSTREACT(0)
	;積極的従う・受け入れるの場合は、息を荒げながら凝視…という扱い
	{
	CALL HTMLPRINTW(@"「んっ…！ あっ%AHEART% \@ IS_COMGRONAME("積極的従う/受け入れる") 
	? そんな風に見られながら # 人前で \@するのって、なんか…」")
	}
	PRINTFORMW 「凄くゾクゾクして…、癖になりそう\@ !FAV_RATING ? です # \@……」
	RETURN 1
ENDIF

;REAC42, 恥ずかしがる(55%) // 負荷2は弾く
;=========================
IF 口上記入チェック(55) && IS_NOWCOMNAME("恥ずかしがる") && TFLAG:REACT負荷 < 2
	IF FAV_RATING
		{
		CALL HTMLPRINTW(
		@"「%RAND_SPLIT("あ/ん")%んっ%AHEART% " + 
		@"%RAND_SPLIT("ん/ふ/あ")%ぁっ%AHEART% どうして…、こんなに、興奮しちゃうのっ！」")
		}
		{
		CALL HTMLPRINTW(
		@"「や……、%RAND_SPLIT("だめぇっ/いやぁ…")%%AHEART% " + 
		"もう、とまんなくなっちゃうっ…ッ…！」")
		}
	ELSE
		SELECTCASE RANDREACT(2, 10)
			CASE 0
				CALL HTMLPRINTW(@"「あぁっ%AHEART% やっ%AHEART% こ、こんなの駄目、なのに……」")
				PRINTFORMW 「指、止まらなくて……、わたし、おかしく……」
			CASE 1
				PRINTFORML 「そんなに恥ずかしがらないで…」
				PRINTFORMW 「私の浅ましい姿、もっとよく見て下さい…」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF


