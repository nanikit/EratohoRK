;-------------------------------------------------
;KFLAGを定義。未定義のものが渡されたらエラー落ち
;-------------------------------------------------
@GET_KFLAG_K39(ARGS, ARG = -2)
#FUNCTION
ARG = ARG != -2 ? ARG # TARGET
SELECTCASE ARGS
	CASE "相思相愛"
		RETURNF KFLAG:ARG:100 == 1
	CASE "ED経過"
		RETURNF KFLAG:ARG:150 == 1
	CASE "性奉判定"
		RETURNF KFLAG:ARG:200 == 1
	CASEELSE
		CALLF PRINT_ERRORMESSAGE(@"@GET_KFLAG_K39に無効な定義が渡されました")
		RETURNF -1
ENDSELECT
