;========================================================================
;日常イベント口上
;========================================================================
;屈服刻印*5％で実行 屈服刻印は調教者毎に判別される
;イベントが発生した際の内訳はスキンシップ67％、日常33％
;-------------------------------------------------

;────────────────────────────────────
;EV201, 介抱		DAILY_LIFE_NURSING
;────────────────────────────────────
;・調教中気絶した後
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_201(ARG)
;-------------------------------------------------
;引数内訳…ARG(=0,失神で調教中止になった場合/=1,疲労で調教中止になった場合)
;-------------------------------------------------
;介抱の場合、口上主はTARGETで参照可能
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;介抱
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GROUPMATCH(ARG, 0, 1) > 0
	IF GET_KFLAG_K39("相思相愛", TARGET)
		PRINTFORMW 「ご免なさい…私のせいね…」
		PRINTFORMW 「何か精のつく食事でも作るから、あなたはゆっくり休んで？」
	ELSE
		PRINTFORMW 「ご免なさい、やり過ぎてしまいましたね…」
		PRINTFORMW 「他の事は気にせず、ゆっくり休んで下さい」
	ENDIF
	RETURN 1
ENDIF

;========================================================================
;スキンシップ（各種の性経験が入ります）
;========================================================================
;@DAILY_LIFE_1_0～からランダムで選ぶ　RETURN 0されると再抽選、RETURN 1(0以外)で抽選から抜ける
;-------------------------------------------------
;※スキンシップの場合、口上主がTARGETであるとは限らない
;  (ASSI:1)や(ASSI:2)である場合もあったり、ASSIですらない場合もある
;  この箇所での口上主の参照はDAILY_TARGETで
;-------------------------------------------------

;────────────────────────────────────
;EV202, スキンシップ0
;────────────────────────────────────
;・台所でセクハラ
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_202(ARG)
;-------------------------------------------------
;引数内訳…ARG(=0,股間まさぐり/=1,おっぱいまさぐり/=2,無視したら諦めて出て行った/=3,身をゆだねて股間でイかされ
;              =4,身をゆだねておっぱいで/=5,ぱんつお着替えさせてくれました)
;-------------------------------------------------
IF 口上記入チェック(100)
	SELECTCASE ARG
		;スキンシップ0_0 股間まさぐり/0_1 おっぱいまさぐり
		CASE 0, 1
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ねぇ、どう？　ほら、どうなの？」
				PRINTFORMW 「私にもっと弄って欲しいんじゃないの？ クスクス」
			ELSE
				PRINTFORMW 「私の事は気にせず続けてください」
				;股間まさぐり
				IF !ARG
					PRINTFORMW 「ん…少し硬くなってますが、何を期待しているんでしょうか？」
				;おっぱいまさぐり		オトコ限定口上の為現在では死に分岐
				ELSE
					PRINTFORMW 「乳首が硬くなってますが、何か期待しているんですか？」
				ENDIF
			ENDIF
		;スキンシップ0_2 無視したら諦めて出て行った
		CASE 2
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「今はシたくないみたいね…」
				PRINTFORMW 「じゃ、いいわ。そのまま続けて？」
			ELSE
				PRINTFORMW 「その気は無いみたいですね…」
				PRINTFORMW 「分かりました。そのまま続けてくださいね」
			ENDIF
		;スキンシップ0_3 身をゆだねて股間でイかされ
		CASE 3
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ここで抜いて欲しいのね？」
				PRINTFORM 「いいよ…このままイクまでシゴいてあげる
			ELSE
				PRINTFORMW 「うふ、抜いて欲しいんですね？」
				PRINTFORM 「いいですよ、このままイかせてあげます
			ENDIF
			CALL PRINT_SYMBOL_K39
		;スキンシップ0_4 身をゆだねておっぱいで
		CASE 4
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ここでシて欲しいのね？」
				PRINTFORM 「いいよ…このままおっぱいでイかせてあげる
			ELSE
				PRINTFORMW 「うふ、抵抗しないんですね？」
				PRINTFORM 「いいですよ、このままイかせてあげます
			ENDIF
			CALL PRINT_SYMBOL_K39
		;スキンシップ0_5 ぱんつお着替えさせてくれました
		CASE 5
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「こんなに汚しちゃったら着替えが必要ね」
				PRINTFORM 「ふふ、今取ってきてあげる
			ELSE
				PRINTFORMW 「これは流石に着替えが必要みたいですね」
				PRINTFORM 「すぐに取ってくるので、待っていてくださいね
			ENDIF
			CALL PRINT_SYMBOL_K39
	ENDSELECT
	RETURN 1
ENDIF

;────────────────────────────────────
;EV203, スキンシップ1
;────────────────────────────────────
;・食堂でお茶しながら
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_203(ARG)
;-------------------------------------------------
;引数…ARG(=0,お紅茶いかが？/=1,媚薬でむらむらおっぱい気になる/=2,おっぱい見せつけられました
;          =3,あわてて逃げました/=4,むしゃぶりついてご奉仕したらご褒美にぱいずりorクンニでイかされちゃいました(長いｗ))
;-------------------------------------------------
IF 口上記入チェック(100)
	SELECTCASE ARG
		;スキンシップ1_0 お紅茶いかが？
		CASE 0
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ねぇ、たまには紅茶なんてどう？」
				PRINTFORMW 「時にはこういうのもいいと思うんだけど」
			ELSE
				PRINTFORMW 「ちょっとお茶にしませんか？」
				PRINTFORMW 「茶葉を切らしてしまって紅茶しかないんですけど」
			ENDIF
		;スキンシップ1_1 媚薬でむらむらおっぱい気になる
		CASE 1
			PRINTL 
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ねぇ、どこ見てるの？」
				PRINTFORMW 「そんな目で見られると、落ち着かないんだけど？　クスクス」
			ELSE
				PRINTFORM 「目付きがいやらしいですね
				CALL PRINT_SYMBOL_K39
				PRINTFORMW 「私の胸がそんなに気になりますか？」
			ENDIF
		;スキンシップ1_2 おっぱい見せつけられました
		CASE 2
			PRINTL 
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「ほら、見たいんでしょ？」
				PRINTFORM 「よ～く見せてあげるから、もっとこっちに来て
			ELSE
				PRINTFORMW 「そんな目で見られたら断れませんね」
				PRINTFORM 「さぁ、遠慮なく見ていいですよ
			ENDIF
			CALL PRINT_SYMBOL_K39
		;スキンシップ1_3 あわてて逃げました
		CASE 3
			IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
				PRINTFORMW 「こうあからさまに逃げられると、ちょっと傷つくかも…」
			ELSE
				PRINTFORMW 「逃げられてしまいましたね…」
				PRINTFORM 「ま、次の機会に期待しましょう
				CALL PRINT_SYMBOL_K39
			ENDIF
		;スキンシップ1_4 むしゃぶりついてご奉仕したらご褒美にぱいずりorクンニでイかされちゃいました
		CASE 4
			;調教対象にペニス有り
			IF PENIS(MASTER)
				IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
					PRINTFORML 「もう我慢出来ないんでしょ？」
					PRINTFORMW 「ほら、私のおっぱい好きにしていいわよ」
					PRINTFORM 「終わったら胸でちゃんとイかせてあげるから
				ELSE
					PRINTFORMW 「さぁ、いいですよ」
					PRINTFORM 「私の胸、好きなようにしてください
				ENDIF
			;ペニス無し
			ELSE
				IF TALENT:DAILY_TARGET:ご主人様 && GET_KFLAG_K39("相思相愛", DAILY_TARGET)
					PRINTFORML 「もう我慢出来ないんでしょ？」
					PRINTFORM 「ほら、私のおっぱい好きにしていいわよ
				ELSE
					PRINTFORMW 「さぁ、いいですよ」
					PRINTFORM 「私の胸、好きなようにしてください
				ENDIF
			ENDIF
			CALL PRINT_SYMBOL_K39
	ENDSELECT
	RETURN 1
ENDIF

