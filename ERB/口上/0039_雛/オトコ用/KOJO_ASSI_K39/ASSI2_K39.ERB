;*************************************************
;병아리 구상 조수 탈의 구상 파일
;*************************************************

;=================================================
;탈의 서포트 구상
;=================================================
;·조교자 액션의 앞에 조수에 의한 강제탈의가 발생했을 때에 표시
;·어떠한 경우에서도 「구상주=ASSI」라고 되고 불려 간다
;·구상주가 조교자로 주인님이 조수의 경우에도 불려 가 이 경우 일시적으로 「주인님=TARGET」라고 된다
;·이 구상의 시점에서는 아직, TEQUIP는 갱신되어 있지 않다
;·아직도 검증이 불충분
;·유용하는 것은 자유롭지만 자기책임으로
;·일부 실제노동을 보기 위해서(때문에) 기술되어 있지만, 미완이기 때문에 체크는 떨어뜨려 있다
;-------------------------------------------------
@KOJO_DATUI_ASSI_K39
;-------------------------------------------------
;관련 변수…
;강제탈의:TFLAG:102  자주탈의:TFLAG:103
;=(-1, 강제탈의에 저항, 1, 윗도리, 2, 하반신윗도리, 3, 웃도리, 4, 상반신 전부, 5, 하반신 전부,
;  6, 액세서리, 7, 액세서리 이외 모두, 8, 전라까지, 9, 속옷만)
;조수 탈의:TFLAG:110 = 12 때에 발생
;-------------------------------------------------

;-------------------------------------------------
;강제탈의에 저항되었다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 < 0
	;주인님도 뭔가 벗으려고 하고 있다
	IF TFLAG:자주탈의 && TFLAG:110 == 12
		PRINTFORMW 「나는 상관하지 않습니다만,%CALLNAME:TARGET%가 화내요?」
	;통상
	ELSE
		PRINTL
		PRINTFORMW 「나는 상관하지 않습니다만,%CALLNAME:TARGET%가 화내요?」
	ENDIF
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;주인님도 뭔가 벗으려고 하고 있다
;-------------------------------------------------
;·주로 섹스계 행동의 예비 동작으로 발생한다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && TFLAG:110 == 12
	PRINTFORMW 「%CALLNAME:TARGET%도 벗고 있는 것 같고…」
	PRINTFORMW 「당신도 벗어 버려요?」
	RETURN LOCAL
ENDIF
;-------------------------------------------------
;강제탈의·액세서리만
;-------------------------------------------------
;·주로 양말. 그 밖에 장갑이나 소매 따위
;·벗긴 중에 접어들지 않는다! 되기 십상인 것으로 이것만은 부위별 텐프레를 준비
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 == 6
	;전라가 되었다
	IF ZENRACHECK(1)
		PRINTFORMW 
		PRINTFORMW 
	;그 이외
	ELSE
		PRINTFORMW 「그럼, 방해인 것은 취해 버릴까요」
	ENDIF
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·저기 보여라
;-------------------------------------------------
;·이번 탈의로 성기가 보이게 되었을 경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3
	;조교 대상이 남자
	IF TALENT:MASTER:남자
		PRINTFORMW 「꽤 좋은 경치군요…」
	;조교 대상이 후타나리
	ELSEIF TALENT:MASTER:후타나리
		PRINTFORMW 「꽤 좋은 경치군요…」
	;조교 대상이 여자
	ELSE
		PRINTFORMW 「꽤 좋은 경치군요…」
	ENDIF
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·젖가슴 보여라
;-------------------------------------------------
;·이번 탈의로 젖가슴이 보이게 되었을 경우에 표시
;·브라 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;·조교 대상이 남자라고 표시하지 않는다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 && B_DATUICHECK(1) == 3 && !TALENT:MASTER:남자
	PRINTFORMW 「그럼, 당신의 젖가슴을 보여 받읍시다♪」
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·팬티 보여라
;-------------------------------------------------
;·이번 탈의로 팬티(캐릭터에 의해 드로워즈·트렁크스)가 보이게 되었을 경우에 표시
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 && P_DATUICHECK(1) == 2
	;조교 대상이 남자
	IF TALENT:MASTER:남자
		PRINTFORMW 
		PRINTFORMW 
	;조교 대상이 후타나리
	ELSEIF TALENT:MASTER:후타나리
		PRINTFORMW 
		PRINTFORMW 
	;조교 대상이 여자
	ELSE
		PRINTFORMW 
		PRINTFORMW 
	ENDIF
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·브라 보여라
;-------------------------------------------------
;·이번 탈의로 브라(캐릭터에 의해 스포트브라·사라시)가 보이게 되었을 경우에 표시
;·조교 대상이 남자라고 표시하지 않는다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 && B_DATUICHECK(1) == 2 && !TALENT:MASTER:남자
	PRINTFORMW 「당신의 사랑스러운 속옷 모습을, 보여 받을까요」
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·전라에
;-------------------------------------------------
;·「전라까지 벗겼다」만이 아니고, 「지금 벗겼으므로 전라가 되었다」라고 하는 의미
;·「윗도리 밖에 남지 않은 상태로 윗도리를 벗겼다」등도 포함된다
;·「저기 보여라」～「브라 보여라」를 썼을 경우는 기입할 필요는 없다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의 && ZENRACHECK(1)
	;조교 대상이 남자
	IF TALENT:MASTER:남자
		PRINTFORMW 
		PRINTFORMW 
	;조교 대상이 후타나리
	ELSEIF TALENT:MASTER:후타나리
		PRINTFORMW 
		PRINTFORMW 
	;조교 대상이 여자
	ELSE
		PRINTFORM 「우후, 매우 예뻐요」
	ENDIF
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;강제탈의·뭔가 벗겼다
;-------------------------------------------------
;·애무계의 예비 동작이 되기 쉽다
;-------- -----------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:강제탈의
	PRINTFORMW 「자, 빨리 벗어 버려요♪」
	RETURN LOCAL
ENDIF


