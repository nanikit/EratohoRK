;記入チェック
LOCAL = 0
;-------------------------------------------------
IF 口上記入チェック(0)
	;フェラ
	IF TCVAR:ASSI:助手ACT派生 == 1
		IF FAVOR(3000, 3000, ASSI)
			PRINTFORMW
			PRINTFORMW
		;それ以外
		ELSE
			PRINTFORMW 「さぁ、私の口で愉しませてあげます」
			PRINTFORMW 「%CALLNAME:TARGET%が嫉妬するくらいねっとりと舐めてあげますよ♪」 
		ENDIF
	;クンニ
	ELSE
		IF FAVOR(3000, 3000, ASSI)
			PRINTFORMW
			PRINTFORMW
		;それ以外
		ELSE
			PRINTFORMW 
			PRINTFORMW
