				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;구에 의한 애무로 절정(입으로 가슴애무를 강제등)
	;=================================================
	IF (IS_NOWACTNAME("가슴애무강제") && TFLAG:ACT파생 == 2) || TB_CHECK_K39("입")
		SELECTCASE RANDEVENT(2, 285)
			CASE 0
				CALL TB_EX_K39
			CASE 1
				;상사상애
				IF GET_KFLAG_K39("상사상애")
					SELECTCASE RANDEVENT(2, 286)
						CASE 0
							CALL HTMLPRINTW(@"「히앗%LOCALS:2% 안됏, 그렇게 유두핥기라고는 안돼엣!!」")
						CASE 1
							CALL HTMLPRINTW(@"「%RAND_SPLIT("아/읏")%! 응쿳…… 안돼엣%LOCALS:2% 이제(벌써), 이제(벌써)!」")
					ENDSELECT
					SELECTCASE RANDEVENT(2, 287)
						CASE 0
							CALL HTMLPRINTL(@"「가 버린닷%LOCALS:2% 가 버린다앗!! 아앗…!」")
						CASE 1
							CALL HTMLPRINTL(@"「후앗%LOCALS:2% 안됏! 이%LOCALS:2% 구!」")
					ENDSELECT
				;그 외
				ELSE
					SELECTCASE RANDEVENT(2, 288)
						CASE 0
							CALL HTMLPRINTW(@"「응아앗%LOCALS:2% 안됏%LOCALS:2% 그렇게 유두를 빨 수 있으면……!」")
						CASE 1
							CALL HTMLPRINTW(@"「안돼엣%LOCALS:2% 안됩니닷! 이렇게에……%LOCALS:2% 이렇게 되면 ……!」")
					ENDSELECT
					SELECTCASE  RANDEVENT(2, 289)
						CASE 0
							CALL HTMLPRINTL(@"「갑니닷! 가 버립니닷……%LOCALS:2% 아!!」")
						CASE 1
							CALL HTMLPRINTL(@"「이… 간다앗%LOCALS:2% 가 버린다앗!!」")
					ENDSELECT
				ENDIF
				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;페니스에 의한 애무로 절정(파이즈리 따위)
	;=================================================
	IF (IS_NOWACTNAME("파이즈리한다") || TB_CHECK_K39("페니스"))
		SELECTCASE RANDEVENT(2, 290)
			CASE 0
				CALL TB_EX_K39
			CASE 1
				;상사상애
				IF GET_KFLAG_K39("상사상애")
					SELECTCASE RANDEVENT(2, 291)
						CASE 0
							CALL HTMLPRINTW(@"「히앗%LOCALS:2% 안됏%LOCALS:2% 거기뿐 칠하면 안돼엣!!」")
						CASE 1
							CALL HTMLPRINTW(@"「야앗%LOCALS:2% 그렇게 비비면! 가 버린다%LOCALS:2% 가 버린다!!」")
					ENDSELECT
					SELECTCASE RANDEVENT(2, 292)
						CASE 0
							CALL HTMLPRINTL(@"「후앗%LOCALS:2% 안됏%LOCALS:2% 이, 쿠웃!」")
						CASE 1
							CALL HTMLPRINTL(@"「아, 아앗%LOCALS:2% 간다앗%LOCALS:2% 가 버린다앗!!」")
					ENDSELECT
				;그 외
				ELSE
					SELECTCASE RANDEVENT(2, 293)
						CASE 0
							CALL HTMLPRINTW(@"「싫어엇! 안됏%LOCALS:2% 안됩니닷…! 거기뿐, 비비지 마앗!」")
						CASE 1
							CALL HTMLPRINTW(@"「아웃, 아앗%LOCALS:2% 그렇게 비비어지면! 이제(벌써)%LOCALS:2% 이제(벌써), 나……!」")
					ENDSELECT
					SELECTCASE  RANDEVENT(2, 294)
						CASE 0
							CALL HTMLPRINTL(@"「히웃%LOCALS:2% 아…… 간닷, 간다아앗……!!」")
						CASE 1
							CALL HTMLPRINTL(@"「갑니닷! 가 버립니닷……%LOCALS:2% 아!!」")
					ENDSELECT
				ENDIF
				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;그 외 범용 처리
	;-----------------------------------
	CALL TB_EX_K39
	RETURN 1
ENDIF

