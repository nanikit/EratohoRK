;────────────────────────────────────
;EV21, 大満足口上
;────────────────────────────────────
;・調教者の大満足ボーナスを取得した場合に表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_21(ARG)
;-------------------------------------------------
#LOCALSIZE 1
VARSET LOCAL
LOCAL:0 = GET_KFLAG_K39("相思相愛")

;━━━━━━━━━━━━━━━━━━━━━━━━
;雛さま大満足
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	;=================================================
	;愛想よく応答する
	;=================================================
	IF IS_NOWCOMNAME("愛想よく応答する")
		PRINTFORM 「\@ LOCAL:0 ? うふっ、何かご褒美を考えなくちゃね # 愛想がいいのが一番ですね \@
		CALL PRINT_SYMBOL_K39
		PRINTFORMDW 雛は慈愛に満ちた表情で%CALLNAME:MASTER%を見つめている…
		RETURN 1
	ENDIF
	
	;=================================================
	;快感を受け入れる
	;=================================================
	IF IS_NOWCOMNAME("快感を受け入れる")
		PRINTFORM 「\@ LOCAL:0 ? うふっ、可愛い # 素直な子は好きですよ \@
		CALL PRINT_SYMBOL_K39
		PRINTFORMDW 雛は微笑みながら%CALLNAME:MASTER%を見つめている…
		RETURN 1
	ENDIF
	
	;=================================================
	;悲鳴を上げる
	;=================================================
	IF IS_NOWCOMNAME("悲鳴を上げる")
		PRINTFORMW 「\@ LOCAL:0 ? ねぇ、もっと。もっと聞かせて # そんな甘い声で鳴かれると私…、ちょっと興奮してしまいます \@……」
		RETURN 1
	ENDIF
	
	;=================================================
	;愛情を込めてやる
	;=================================================
	IF IS_NOWCOMNAME("愛情を込めてやる")
		PRINTFORMW 「\@ LOCAL:0 ? そんなに優しくされると蕩けちゃう # そんなに優しくされたら、蕩けてしまいます \@…」
		PRINTFORMDW 雛は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		RETURN 1
	ENDIF
	
	;=================================================
	;積極的従う(積極的やる、なすがまま等)
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("積極的従う")
		;-------------------------------------------------
		;ED経過
		;-------------------------------------------------
		IF GET_KFLAG_K39("ED経過", TARGET)
			PRINTFORMW 「好き、大好き…」
			PRINTFORMDW 雛は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		;相思相愛
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「こんな事、駄目なのは分かってる。でも…」
			PRINTFORMW 「\@ RAND:2 ? もう、 # %CNAME(MASTER, TARGET)%と \@離れたくない……」
			PRINTFORMDW 雛は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		;その他
		ELSE
			PRINTFORMW 「積極的に求めてくれると、私も嬉しいです…」
			PRINTFORMDW 雛は微笑みながら%CALLNAME:MASTER%を見つめている…
		ENDIF 
		RETURN 2
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	CALL HTMLPRINTW(@"「\@ LOCAL:0 ? くすくす、いい感じね # ふふ、悪くないですね \@…%HTML_HEARTB(, COLOR2_K39("PINK"))%」")
	RETURN 1
ENDIF

;────────────────────────────────────
;EV22, 状態変化口上
;────────────────────────────────────
;・状態異常が付いたときに表示
;・TCVAR:状態変化が調教者、TCVAR:MASTER:状態変化が調教対象
;・状態変化が起こると3ターン持続する
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_22(ARG)
;-------------------------------------------------
;・近い将来、調教者と調教対象で状態変化口上呼び出しを切り分けたい意向
;・そして状態変化以外の勃起描写、潤滑描写、尿意描写あたりにも口上呼び出しの枠を追加したい考え(2015/01/09)
;-------------------------------------------------
;*************************************************
;EV22_0, 調教者状態変化
;*************************************************
IF 口上記入チェック(100) ;&& ARG == 0
	;調教者の状態変化で派生
	SELECTCASE GET_CONDITION(TARGET)
		CASE "疲弊"
			;状態変化持続ターンを2に
			TCVAR:状態変化持続 = 2
			;2/3の確率で表示無し RESULTに0を返す
			IF RAND:3
				RETURN 0
			;1/3の確率で表示
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? ちょっと%RAND_SPLIT("やりすぎかしら/疲れたわね")% # ちょっと疲れましたね \@…」
				PRINTFORMDW 雛は肩で大きく息をしている…
				RETURN 1
			ENDIF
		CASE "衰弱"
			RETURN 0
		CASE "無気力"
			RETURN 0
		CASE "朦朧"
			RETURN 0
		CASE "情欲"
			IF RAND:3
				RETURN 0
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? も、もう駄目……。我慢、できないわ…… # ご免なさい、私、もう我慢できませんっ！ \@」
				PRINTFORMDW 雛は情欲に濁った目で%CALLNAME:MASTER%を見据えながら近寄ってきた…
				RETURN 1
			ENDIF
		CASE "怒り"
			IF AFFECTION("相思相愛")
				TCVAR:状態変化持続 = 1
			ELSEIF AFFECTION("溺愛")
				IF RAND:3
					TCVAR:状態変化持続 = 2
				ELSE
					TCVAR:状態変化持続 = 1
				ENDIF
			ELSEIF AFFECTION("眷愛")
				IF RAND:3
					TCVAR:状態変化持続 = 3
				ELSE
					TCVAR:状態変化持続 = 2
				ENDIF
			ELSE
				TCVAR:状態変化持続 = 3
			ENDIF
			IF RAND:3
				RETURN 0
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? ねぇ、本気で怒ってもいいの # 私を怒らせて楽しいですか \@？」
				RETURN 1
			ENDIF
		CASE "退屈"
			;調教の経過時間
			SELECTCASE TFLAG:経過時間
				;長い
				CASE IS >= 60
					PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? ねぇ、もう終わりにする # そろそろ止めましょうか \@…？」
					TCVAR:状態変化持続 = 3
				;普通
				CASE IS >= 30
					PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? なんかいまひとつな感じ # どうもいまひとつですね \@…」
					TCVAR:状態変化持続 = 2
				;短い
				CASEELSE
					PRINTFORMW 「\@ GET_KFLAG_K39("相思相愛") ? なんか気分が乗らない感じ # どうも気分が乗りませんね \@…」
					TCVAR:状態変化持続 = 1
			ENDSELECT
			RETURN 1
		CASE "鬱屈"
			;//TARGETの場合、TALENT:狂気がある場合にのみ1/5の確率で鬱屈化
			;//狂気持ちで無い場合は死に口上になる
			RETURN 0
	ENDSELECT
ENDIF

