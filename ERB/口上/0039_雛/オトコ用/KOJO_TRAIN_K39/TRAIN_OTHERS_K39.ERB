;─
;EV21, 대만족 구상
;─
;·조교자의 대만족 보너스를 취득했을 경우에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_21(ARG)
;-------------------------------------------------
#LOCALSIZE 1
VARSET LOCAL
LOCAL:0 = GET_KFLAG_K39("상사상애")

;━━━━━━━━━━━━━━━━━━━━━━━━
;히나님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	;=================================================
	;붙임성좋게응답한다
	;=================================================
	IF IS_NOWCOMNAME("붙임성좋게응답한다")
		PRINTFORM 「\@ LOCAL:0 ? 우대신, 뭔가 포상을 생각하지 않으면 # 애교가 있는 것이 제일이군요 \@
		CALL PRINT_SYMBOL_K39
		PRINTFORMDW 히나는 자애로 가득 찬 표정으로%CALLNAME:MASTER%를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을받아들인다
	;=================================================
	IF IS_NOWCOMNAME("쾌감을받아들인다")
		PRINTFORM 「\@ LOCAL:0 ? 우대신, 사랑스럽다 # 솔직한 아이는 좋아해요 \@
		CALL PRINT_SYMBOL_K39
		PRINTFORMDW 히나는 미소지으면서%CALLNAME:MASTER%를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;비명을올린다
	;=================================================
	IF IS_NOWCOMNAME("비명을올린다")
		PRINTFORMW 「\@ LOCAL:0 ? 응, 좀 더. 좀 더 들려줘 # 그런 달콤한 소리로 울어지면 나…, 조금 흥분해 버립니다 \@……」
		RETURN 1
	ENDIF
	
	;=================================================
	;애정을담아준다
	;=================================================
	IF IS_NOWCOMNAME("애정을담아준다")
		PRINTFORMW 「\@ LOCAL:0 ? 그렇게 상냥하게 여겨지면 녹아 버린다 # 그렇게 상냥하게 여겨지면, 녹아 버립니다 \@…」
		PRINTFORMDW 히나는 물기를 띤 눈동자로%CALLNAME:MASTER%를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;적극적 따른다(적극적한다, 하는대로 등)
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("적극적 따른다")
		;-------------------------------------------------
		;ED경과
		;-------------------------------------------------
		IF GET_KFLAG_K39("ED경과", TARGET)
			PRINTFORMW 「좋아해, 너무 좋아…」
			PRINTFORMDW 히나는 물기를 띤 눈동자로%CALLNAME:MASTER%를 응시하고 있다…
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「이런 일, 안 되는 것은 알고 있다. 그렇지만…」
			PRINTFORMW 「\@ RAND:2 ? 이제(벌써),  # %CNAME(MASTER, TARGET)%와 \@떨어지고 싶지 않다……」
			PRINTFORMDW 히나는 물기를 띤 눈동자로%CALLNAME:MASTER%를 응시하고 있다…
		;그 외
		ELSE
			PRINTFORMW 「적극적으로 요구해 주면, 나도 기쁩니다…」
			PRINTFORMDW 히나는 미소지으면서%CALLNAME:MASTER%를 응시하고 있다…
		ENDIF 
		RETURN 2
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	CALL HTMLPRINTW(@"「\@ LOCAL:0 ? 킥킥, 좋은 느낌이군요 # 후후, 나쁘지 않네요 \@…%HTML_HEARTB(, COLOR2_K39("PINK"))%」")
	RETURN 1
ENDIF

;─
;EV22, 상태변화 구상
;─
;·상태 이상이 붙었을 때에 표시
;·TCVAR:상태변화가 조교자, TCVAR:MASTER:상태변화가 조교 대상
;·상태변화가 일어나면 3 턴 지속한다
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_22(ARG)
;-------------------------------------------------
;·가까운 장래, 조교자와 조교 대상으로 상태변화 구상 호출을 분리하고 싶은 의향
;·그리고 상태변화 이외의 발기 묘사, 윤활 묘사, 소변욕 묘사 근처에도 구상 호출의 테두리를 추가하고 싶은 생각(2015/01/09)
;-------------------------------------------------
;*************************************************
;EV22_0, 조교자 상태변화
;*************************************************
IF 구상기입체크(100) ;&& ARG == 0
	;조교자의 상태변화로 파생
	SELECTCASE GET_CONDITION(TARGET)
		CASE "피폐"
			;상태변화지속 턴을 2에
			TCVAR:상태변화지속 = 2
			;2/3의 확률로 표시 없음 RESULT에 0을 돌려준다
			IF RAND:3
				RETURN 0
			;1/3의 확률로 표시
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 조금%RAND_SPLIT("너무 할까/지쳤군요")% # 조금 지쳤어요 \@…」
				PRINTFORMDW 히나는 어깨로 크게 숨을 쉬고 있다…
				RETURN 1
			ENDIF
		CASE "쇠약"
			RETURN 0
		CASE "무기력"
			RETURN 0
		CASE "몽롱"
			RETURN 0
		CASE "정욕"
			IF RAND:3
				RETURN 0
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 도, 이제 안돼……. 인내, 할 수 없어요…… # 미안해요, 나, 이제 참을 수 없습니다!  \@」
				PRINTFORMDW 히나는 정욕에 탁해진 눈으로%CALLNAME:MASTER%를 응시하면서 접근해 왔다…
				RETURN 1
			ENDIF
		CASE "분노"
			IF AFFECTION("상사상애")
				TCVAR:상태변화지속 = 1
			ELSEIF AFFECTION("몹시 사랑함")
				IF RAND:3
					TCVAR:상태변화지속 = 2
				ELSE
					TCVAR:상태변화지속 = 1
				ENDIF
			ELSEIF AFFECTION("권애")
				IF RAND:3
					TCVAR:상태변화지속 = 3
				ELSE
					TCVAR:상태변화지속 = 2
				ENDIF
			ELSE
				TCVAR:상태변화지속 = 3
			ENDIF
			IF RAND:3
				RETURN 0
			ELSE
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 응, 정말로 화내도 괜찮은거야 # 나를 화나게 해 즐겁습니까 \@?」
				RETURN 1
			ENDIF
		CASE "지루함"
			;조교의 경과시간
			SELECTCASE TFLAG:경과시간
				;길다
				CASE IS >= 60
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 응, 벌써 끝낸다 # 슬슬 멈출까요 \@…?」
					TCVAR:상태변화지속 = 3
				;보통
				CASE IS >= 30
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 몇번 마비와 개인 느껴 # 아무래도 지금 하나군요 \@…」
					TCVAR:상태변화지속 = 2
				;짧다
				CASEELSE
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 는 기분이 타지 않는 느낌 # 아무래도 기분이 타지 않아요 \@…」
					TCVAR:상태변화지속 = 1
			ENDSELECT
			RETURN 1
		CASE "울적"
			;//TARGET의 경우, TALENT:광기가 있는 경우에게만 1/5의 확률로 울적화
			;//광기 소유가 아닌 경우는 공수상이 된다
			RETURN 0
	ENDSELECT
ENDIF

