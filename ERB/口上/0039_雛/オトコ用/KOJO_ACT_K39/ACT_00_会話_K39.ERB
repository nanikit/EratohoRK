;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;병아리 구상 회화계 액션 구상 파일

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;


;       
;회화계 ACT0~9
;       
[SKIPSTART]
	#TFLAG:ACT
		0=지금의 기분을 (듣)묻는다
		1=자신에게로의 기분을 (듣)묻는다
		2=성에 대한 이야기를 한다
		3=상냥하게 위로한다
		4=엄격하게 위협한다
		5=의상의 변경을 요구한다
		6=합의를 요구한다
		7=벌이라고 선언한다
		8=고함친다
		9=기분 나쁘게 웃는다
	#TFLAG:ACT 분류 == 0에 해당
	#ACT9, 기분 나쁘게 웃고는 이상계 ACT 분류로부터 파생하는 경우가 있다
	#범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "회화")로 줍는 것이 가능

	*구상 기입 체크(표시 확률을 입력. 생략 하면 진)
	*RESULT치        (=0, 범용 ACT에 패스, 1, 더 이상 표시하지 않는, 2, 미약등 구상을 스킵 해 더 이상 표시하지 않는다)
[SKIPEND]


;////////////////////
;ACT0, 지금의 기분을 (듣)묻는다
;////////////////////
[SKIPSTART]
	#지금의 기분에 대해 들어 온다
	#조교 대상의 반응에 응해 REACT 이후에 ACT 파생에의 대입을 한다
	#턴 방침이 "이상"이라면 발생하지 않는다
[SKIPEND]
@KOJO_MALE_ACT_K39_0

;첫회 //행동 예약에 의해 조교 첫회의 최초로 여기에 오는 설정
;====
IF FIRSTACT(0)
	PRINTFORML 「기분은 어떻습니까…?」
	PRINTFORMW 「걱정하지 않고, 전부 나에게 맡겨 주세요」
	RETURN 1
ENDIF

;성교 봉사계로 질내 사정의 뒤
;========================
IF TFLAG:전회질내사정 == 2
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		CALL HTMLPRINTW("「우후, 액이 가득 나왔군요…%HTML_PS_K39()%」")
		PRINTFORMW 「나의 질내, 그렇게 좋았어…?」
	ELSE
		PRINTFORML 「액이 많이 나왔어요」
		PRINTFORMW 「나의 질내, 좋았던 것입니까…?」
	ENDIF
	RETURN 1
ENDIF

;조교 대상이 갈 것 같고 자위/구음중인 경우 //TEQUIP:조교 대상 C사용=(1, 자위/2, 구음)
;===========================================
IF TCVAR:MASTER:갈것같다 && GROUPMATCH(TEQUIP:조교대상C사용, 1, 2) > 0
	;갈 것 같은 것을 참고 있는＆초조하게 해 안이 아닌 경우
	IF TCVAR:MASTER:인내 > 0 && TCVAR:초조하게해번 < 2
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORML 「저기, 갈 것 같겠지…?」
			CALL HTMLPRINTW(@"「인내 같은거 하는 일 없는데%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「인내는 몸에 좋지 않아요?」
		ENDIF
	;그 외
	ELSE
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「이제(벌써) 가 버릴 것 같다?   좋아, 곧바로 편하게 해 준다…%HTML_PS_K39()%」")
		ELSE
			PRINTFORML 「갈 것 같네요?」
			CALL HTMLPRINTW(@"「좋아요, 곧 편하게 해 줍니다%HTML_PS_K39()%」")
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;발기
;====
IF TCVAR:MASTER:발기도 >= 1000
	;자위/구음중이 아닌 경우
	IF !TEQUIP:TEQUIP:조교대상C사용
		PRINTFORMDW %CALLNAME%는%PENIS_DESCRIPTION(MASTER)%%CALLNAME:MASTER%의 페니스를 보고 비난했다…
		PRINTL
	ENDIF
	{
	PRINTFORMW 「\@ GET_KFLAG_K39("상사상애", (FLAG:주인님)) 
	? %RAND_SPLIT("어머나, 조금 너무 건강한 것이 아니야? /우후, 액 있고, 액 있고 원♪")% 
	# %RAND_SPLIT("이제 그렇게 되어 버려…/아라, 상당히 건강한 것 같네요?")% \@」
	}
	;사정 직후
	SIF TCVAR:MASTER:사정후경과턴 == 1
		{
		CALL HTMLPRINTW(@"「조금 전 사정했던 바로 직후인데\@ 
		GET_KFLAG_K39("상사상애", (FLAG:주인님)) 
		? 군요…%HTML_PS_K39()% 
		# 。%RAND_SPLIT("후후/키득")% \@」")
		}
	RETURN 1
ENDIF

;그 조교의 최초의 액션
;==========================
IF TFLAG:경과시간 == 1
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORML 「저기, 기분은 어때%RAND_SPLIT("까/?")%」
		{
		CALL HTMLPRINTW(
		@"「%CNAME(MASTER, TARGET)%의 액, 전부 빨아내 준다" + 
		@"%RAND_SPLIT("원/로부터/")%…%HTML_PS_K39()%」")
		}
	ELSE
		PRINTFORML 「기분은 어떻습니까…?」
		{
		CALL HTMLPRINTW(
		@"「걱정하지 않아도,%CNAME(MASTER, TARGET)%의 액은 전부 들이마셔" + 
		@"내 줍니다%RAND_SPLIT("야/로부터/")%…%HTML_PS_K39()%」")
		}
	ENDIF
	RETURN 1
ENDIF

;그 외·조건 없음
;================
IF 구상기입체크(100)
	{
	PRINTFORMW 「\@ GET_KFLAG_K39("상사상애", (FLAG:주인님)) 
	? 응, 기분은 어때
	# 기분은 어떻습니까\@…？」
	}
	RETURN 1
ENDIF


;//////////////////////////
;ACT1, 자신에게로의 기분을 (듣)묻는다
;//////////////////////////
[SKIPSTART]
	#자신의 일을 어떻게 생각할까 (듣)묻는다
	#조교 대상의 반응에 응해 REACT 이후에 ACT 파생에의 대입을 한다
	#턴 방침이 "이상"이라면 발생하지 않는다
	#화제 『호의』가 기출이 되어, 이후, 동일 조교중에서는 억제된다
[SKIPEND]
@KOJO_MALE_ACT_K39_1

;첫회
;====
IF FIRSTACT(0)
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORMW 「이 때이니까 분명히 말하지만, 나는%CNAME(MASTER, TARGET)%의 일 좋아해요」
		PRINTFORMW 「저기,%CNAME(MASTER, TARGET)%는 어때? 분명히 말했으면 좋은거야…」
	ELSE
		PRINTFORML 「저, 하나 (들)물어도 괜찮을까요…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%는 나의 일, 어떻게 생각하고 있습니까?」
		PRINTFORMDW %CALLNAME:TARGET%는 흠칫흠칫 하면서 물어 왔다…
	ENDIF
	RETURN 1
ENDIF

;성교 봉사계로 질내 사정의 뒤
;========================
IF TFLAG:전회질내사정 == 2
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORML 「저기, 나중 기분 좋았던 것이겠지…?」
		PRINTFORMW 「이만큼 내 둬, 좋지 않았다니 말원%RAND_SPLIT("등 있고로부터…/없도록!")%」
	ELSE
		PRINTFORML 「나의 안은 기분 좋았던 것입니까?」
		CALL HTMLPRINTW("「부디 가르쳐 주세요%HTML_PS_K39()%」")
	ENDIF
	RETURN 1
ENDIF

;발기
;====
IF TCVAR:MASTER:발기도 >= 1000
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORML 「나를 봐 불끈불끈 해 서 버렸어?」
		PRINTFORMW 「만약 그러면 기쁘지만…」
	ELSE
		PRINTFORML 「이제 그렇게 되어 버려…」
		PRINTFORMW 「나에게 흥분했다고 한다면 기쁘지만」
	ENDIF
	RETURN 1
ENDIF

;그 조교의 최초의 액션
;==========================
IF TFLAG:경과시간 == 1
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORMW 「저기, 들려줘…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%는 나의 일, 어떻게 생각하고 있는 거야?」
	ELSE
		PRINTFORMW 「돌연이지만 가르쳐 주세요」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%는 나의 일, 어떻게 생각하고 있습니까?」
	ENDIF
	RETURN 1
ENDIF

;그 외·조건 없음
;================
IF 구상기입체크(100)
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORMW 「나,%CNAME(MASTER, TARGET)%를 좋아해…」
		PRINTFORMW 「저기,%CNAME(MASTER, TARGET)%는 어때? 가르쳐 줘…」
	ELSE
		PRINTFORML 「저……, 나의 일 어떻게 생각합니까?」
		PRINTFORMW 「들려주세요…」
	ENDIF
	RETURN 1
ENDIF


;//////////////////////////
;ACT2, 성에 대한 이야기를 한다
;//////////////////////////
[SKIPSTART]
	#조교자가 성에 대한 지식을 말하기 시작한다
	#TFLAG:벌플래그가 서 있으면 발생하지 않는다
	#턴 방침이 "이상"이라면 발생하지 않는다
	#조교자에게 성 지식이 없으면 발생하지 않는다
	#화제 『지식』이 기출이 되어, 이후, 동일 조교내에서는 억제된다
[SKIPEND]
@KOJO_MALE_ACT_K39_2

;첫회 //KFLAG 상태에 따라서는 연주한다
;====
IF !GET_KFLAG_K39("상사상애", (FLAG:주인님)) && FIRSTACT(0)
	PRINTFORML 「%CNAME(MASTER, TARGET)%에는, 나의 일을 좀 더 알아 받을 필요가 있을 듯 합니다」
	PRINTFORMW 「자, 잘 들어 주세요…」
	RETURN 1
ENDIF

;그 외·조건 없음
;================
IF 구상기입체크(100)
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		{
		PRINTFORMW 「%RAND_SPLIT(
		"응, 이런 이야기 (들)물은 일 있어? /" + 
		"새로운 일 생각났지만, 조금 (들)물어 받을 수 있어? /" + 
		"는 그렇구나… 이런 이야기를 들려줄까. 후후/" + 
		"그렇게 자주, 나, 일전에 깨달았지만…")%」
		}
	ELSE
		SELECTCASE RANDACT(3, 1)
			CASE 0
				PRINTFORML 「액신이라고 해도 한사람의 여자이랍니다」
				PRINTFORMW 「좀 더 나의 일, 알아 주세요…」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER, TARGET)%는 이런 이야기를 들은 적 있습니까?」
				PRINTFORMW 「에엣또, 그렇네요. 예를 들면……」
			CASE 2
				{
				CALL HTMLPRINTW(
				@"「그런데, 이번은 어떤 이야기를 들려줄까요. 후후…\@ 
				AFFECTION("몹시 사랑함") 
				? %HTML_PS_K39()% 
				# \@」")
				}
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF


;//////////////////
;ACT3, 상냥하게 위로한다
;//////////////////
[SKIPSTART]
	#조교자가 상냥한 말로 위로해 온다
	#TFLAG:벌플래그가 서 있으면 발생하지 않는다
	#턴 방침이 "하드", "이상"이라면 발생하지 않는다
	#화제 『위로하고』가 기출이 되어, 이후, 동일 조교내에서는 억제된다
[SKIPEND]
@KOJO_MALE_ACT_K39_3

;성교 봉사계로 질내 사정의 뒤
;========================
IF TFLAG:전회질내사정 == 2
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORMW 「뭐? 혹시, 조금 낙담하고 있는 거야?」
		PRINTFORMW 「내가 좋다고 말하고 있기 때문에, 신경쓰는 일 같은거 없는데…」
	ELSE
		PRINTFORML 「%CNAME(MASTER, TARGET)%가 신경쓰는 일 없어요」
		PRINTFORMW 「액을 받아 들이는 것은, 나의 의무이기 때문에」
	ENDIF
	RETURN 1
ENDIF

;조교 대상이 갈 것 같고 자위/구음중인 경우
;===========================================
IF TCVAR:MASTER:갈것같다 && GROUPMATCH(TEQUIP:조교대상C사용, 1, 2) > 0
	;갈 것 같은 것을 참고 있는＆초조하게 해 안이 아닌 경우
	IF TCVAR:MASTER:인내 > 0 && TCVAR:초조하게해번 < 2
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「따로 가 버려도 나는 상관없어요?」
			CALL HTMLPRINTW(@"「신경쓰는 일 같은거 없다%RAND_SPLIT("원…/의에")%%HTML_PS_K39()%」")
		ELSE 
			PRINTFORML 「참지 않고 가 버려도 좋아요?」
			CALL HTMLPRINTW(@"「나,%CNAME(MASTER, TARGET)%가 갈 때의 표정 정말 좋아하기 때문에%HTML_PS_K39()%」")
		ENDIF
	;그 외
	ELSE
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「갈 것 같으니까는 신경쓰지 않아도 괜찮은데」
			CALL HTMLPRINTW(@"「그런 것 신경쓰지 않고 즐겨? 후후…%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「아무것도 부끄러워하는 일은 없어요%RAND_SPLIT("？/")%」
			CALL HTMLPRINTW("「그렇게 되어도, 어쩔 수 없는 것을 하고 있으니까%HTML_PS_K39()%」")
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;그 외·조건 없음
;================
IF 구상기입체크(100)
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		SELECTCASE RANDACT(3, 0)
			CASE 0
				PRINTFORMW 「저기, 뭔가 걱정거리라도 있는 거야?」
			CASE 1
				PRINTFORMW 「액의 일은 전부 나에게 맡겨」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 걱정하는 일 없는거야?」
			CASE 2
				PRINTFORMW 「뭔가 어떤 때에는 숨기지 않고 말해 주면, 나는 기쁠까…」
		ENDSELECT
	ELSE
		SELECTCASE RANDACT(3, 1)
			CASE 0
				PRINTFORML 「아무것도 걱정하는 일은 없어요」
				PRINTFORMW 「액의 일은, 전부 나에게 맡겨 주세요」
			CASE 1
				PRINTFORMW 「다양하게 불안도 있겠지만, 지금은 나를 믿어 주세요」
				PRINTFORMW 「결코 나쁘게는 하지 않기 때문에」
			CASE 2
				PRINTFORMW 「뭔가 신경이 쓰이는 것이 있으면, 나에게 말해 주세요?」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF


;////////////////
;ACT4, 엄격하게 위협한다
;////////////////
[SKIPSTART]
	#어려운 말로 위협한다
	#TFLAG:벌플래그가 서 있으면 발생하지 않는다
	#턴 방침이 "소프트", "이상"이라면 발생하지 않는다
	#화제 『위협한다』가 기출이 되어, 이후, 동일 조교내에서는 억제된다
[SKIPEND]
@KOJO_MALE_ACT_K39_4
#LOCALSSIZE 1
VARSET LOCALS
LOCALS:0 '= RAND_SPLIT("킥킥/킥킥/후후/후훗")

;첫회 //
;====
IF !GET_KFLAG_K39("상사상애", (FLAG:주인님)) && FIRSTACT(0)
	PRINTFORML 「이것은 전부%CNAME(MASTER, TARGET)%의 액을 쫓기 때문에(위해)입니다…」
	PRINTFORMW 「솔직하게 따라서, 머지않아 대단한 일이 되어요?」
	PRINTFORMW 「자주(잘) 기억해 두어 주세요」
	RETURN 1
ENDIF

;성교 봉사계로 질내 사정의 뒤
;========================
IF TFLAG:전회질내사정 == 2
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORML 「이렇게 가득 내 주어 버려…」
		{
		PRINTFORMW 「아기 생겨 버리면, 책임 취해%RAND_SPLIT(
		"주겠지요? /" + 
		"받아요…/" +
		"받으니까요! /" +
		"주는거네요?")%」
		}
	ELSE
		PRINTFORML 「액신이라고 해도 임신은 합니다만. 에에, 아마……」
		PRINTFORMW 「만약 가능하게 되면, 책임은 취해 받아요?」
	ENDIF
	CALL FADEIN(15, @"「%LOCALS:0%……」")
	RETURN 1
ENDIF

;그 외·조건 없음
;================
IF 구상기입체크(100)
	IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
		PRINTFORML 「저기, 잘 기억해 둬…?」
