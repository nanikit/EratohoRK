				ELSE
					{
					PRINTFORMW 「%RAND_SPLIT(
					"후후, 가고 싶어서 방법 없네요?/" + 
					"슬슬 한계같네요. 그렇지만…/" +
					"그렇게 간단하게는 가게 하지 않아요?")%」
					}
					PRINTFORM 「조금만 더 참고 받습니다
				ENDIF
				CALL PRINT_SYMBOL_K39
			;애태움 종료
			CASE 1
				;호의, 호감도로 파생
				IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
					SELECTCASE RANDACT(3, 2)
						CASE 0
							PRINTFORMW 「과연 한계일까? 이봐요, 좋아요」
							CALL HTMLPRINTW(@"「이대로가게해 준다%HTML_PS_K39()%」")
						CASE 1
							CALL HTMLPRINTW(@"「최후는 나의 여기서 비비어가게해 준다%HTML_PS_K39()%」")
							PRINTFORMW 「후후, 당장 폭발할 것 같구나」
						CASE 2
							PRINTFORMW 「좋아… 가게해 준다」
							PRINTFORMW 「%CNAME(MASTER, TARGET)%의 정액, 여기에 걸쳐…」
					ENDSELECT
				ELSE
					SELECTCASE RANDACT(2, 3)
						CASE 0
							PRINTFORMW 「과연 한계같네요?」
							PRINTFORM 「좋아요. 이대로가게해 줍니다
						CASE 1
							PRINTFORMW 「당장이라도 가 버릴 것 같네요?」
							PRINTFORM 「최후는 나의 여기서 비비어가게해 줍니다
					ENDSELECT
					CALL PRINT_SYMBOL_K39
				ENDIF
			;애태움 없음
			CASEELSE
				;호의, 호감도로 파생
				IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
					SELECTCASE RANDACT(2, 4)
						CASE 0
							PRINTFORMW 「이봐요, 이것으로 자주(잘) 알까?」
							PRINTFORM 「자신이 가는 순간, 잘 보고 있어
						CASE 1
							PRINTFORML 「이봐요, 분명하게 보지 않았다고 안돼…」
							PRINTFORM 「%CNAME(MASTER, TARGET)%의 것이 어디서 어떻게 비비어져 어떻게 가 버리는지, 잘 보고 있어
					ENDSELECT
				ELSE
					SELECTCASE RANDACT(2, 5)
						CASE 0
							PRINTFORMW 「응, 이것으로 자주(잘) 알까요…」
							PRINTFORM 「자신이 가는 순간, 잘 보고 있어 주세요
						CASE 1
							PRINTFORMW 「분명하게 보지 않았다고 안됩니다?」
							PRINTFORM 「내가%CNAME(MASTER, TARGET)%를 어떻게 해 나가게 하는지, 잘 보고 있어 주세요
					ENDSELECT
				ENDIF
				CALL PRINT_SYMBOL_K39
		ENDSELECT
		RETURN 1
	ENDIF

	;쓰러트리기 상태 혹은 위로돌리고
	;==========================
	IF TFLAG:위로돌리고 || TEQUIP:쓰러트리기중
		;연속 실행
		IF TFLAG:ACT == TFLAG:전ACT
			;호의, 호감도로 파생
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				{
				CALL HTMLPRINTL(
