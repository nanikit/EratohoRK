			CALL QUAKE(4)
			{
			CALL HTMLPRINTW(
			@"「후앗, 아앗%HTML_AH_K39()%  아%RAND_SPLIT("″/")%," + 
			@"아%HTML_AH_K39()%  개, 이런 건… 이제(벌써), 이제(벌써)!」")
