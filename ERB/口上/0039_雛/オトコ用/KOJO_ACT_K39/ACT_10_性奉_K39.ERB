@KOJO_MALE_ACT_K39_95 ;TFLAG:ACT파생(0=통상/1=연속/2=V성적봉사로부터/3=A성교로부터/4=V성교로부터/5=A성적봉사로부터)
#LOCALSIZE  1
#LOCALSSIZE 1
VARSET LOCAL
VARSET LOCALS
LOCAL :0  = GET_KFLAG_K39("상사상애", (FLAG:주인님))
LOCALS:0 '= RAND_SPLIT("출/사정")

;성교 봉사계 첫회용 처리
CALL CHK_DEBUG_AND_FIRSTTIME_10_K39

[SKIPSTART]
;조교 대상이 동정
;==============
IF 구상 기입 체크(0) && TALENT:MASTER:동정
	PRINTFORMW 「%CNAME(MASTER, TARGET)%의 처음의 여자가 될 수 있다니…」
	PRINTFORMW 「기쁩니다. 에에, 정말로…」
	RETURN 1
ENDIF
[SKIPEND]

;뽑지않고에 속행
;============
IF TFLAG:뽑지않고
	;범용 처리
	CALL PRINT_KOJO_ACT_GRU10_0
	RETURN 1
ENDIF

;ACT파생 1:연속
;===============
IF TFLAG:ACT파생 == 1
	;범용 처리
	CALL PRINT_KOJO_ACT_GRU10_1_K39
	RETURN 1
ENDIF

;ACT파생 2:체위 변경 //다른 V성교 봉사로부터
;===================
IF TFLAG:ACT파생 == 2
	;초조하게해중
	;========
	IF TCVAR:MASTER:초조하게해번 > 1
		;상사상애
		IF LOCAL:0
			PRINTFORMW 「후후, 그렇게 안에%LOCALS:0%하고 싶은거야?」
			CALL HTMLPRINTW("「간단하게 가게 할 생각은 없기 때문에, 최대한 노력해%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「그렇게 당황하는 일 없다고 생각합니다만, 이제(벌써) 가고 싶어서 견딜 수 없네요. 후후」
			CALL HTMLPRINTW(@"「그렇지만, 조금만 더 참고 받아요%HTML_PS_K39()%」")
		ENDIF
		RETURN 1
	ENDIF

	;초조하게해 종료
	;==========
	IF TCVAR:MASTER:초조하게해번 == 1
		PRINTFORMW 「\@ LOCAL:0 ? 응응! 저기%RAND_SPLIT("/")% # 좋아요 \@, 이대로%LOCALS:0%해」
		PRINTFORMW 「%TNAME_K39()%안,%CNAME(MASTER, TARGET)%로 가득 해\@ !LOCAL:0 ? 주세요 # \@…」
		RETURN 1
	ENDIF

	;초조하게해 없음
	;==========
	IF !TCVAR:MASTER:초조하게해번
		;조교자가갈것같다
		;================
		IF PALAM:쾌V > 8500
			CALL QUAKE(4)
			{
			CALL HTMLPRINTW(
			@"「후앗, 아앗%HTML_AH_K39()%  아, 앗" + 
			@"%HTML_AH_K39()%  개, 이런 건… 이제(벌써), 이제(벌써)!」")
			}
			PRINTFORMDW %CALLNAME:TARGET%는 쾌락에 전율하면서도%CALLNAME:MASTER%의 공격에 맞추어, 격렬하게 허리를 흔들고 있다…
			RETURN 1
		ENDIF

		;상사상애
		;========
		IF LOCAL:0
			;조교 대상이갈것같다
			IF TCVAR:MASTER:갈것같다
				CALL QUAKE(4)
				{
				CALL HTMLPRINTW(
				@"「야%RAND_SPLIT("/")%%HTML_AH_K39()%  팥고물," + 
				@"아%HTML_AH_K39()%  는, 격렬하닷……!」")
				}
				PRINTFORMDW 스스로의 한계를 깨달았다%CALLNAME:MASTER%는%CALLNAME:TARGET%를 껴안아, 허리를 격렬하게 내던졌다…
			ELSE
				CALL HTMLPRINTW(@"「우후%HTML_AH2_K39()%  역시 이 (분)편이 뭔가 안심한다%HTML_PS_K39()%」")
				PRINTFORMW 「좋아, 빨리 와… 이대로 마음껏 범해……」
			ENDIF
			RETURN 1
		ENDIF

		;그 외
		;======
		IF !LOCAL:0
			;조교 대상이갈것같다
			IF TCVAR:MASTER:갈것같다
				CALL QUAKE(4)
				{
				CALL HTMLPRINTW(
				@"「앗%HTML_AH_K39()%  아%RAND_SPLIT("응/")%%HTML_AH_K39()%　" + 
				@"는, 격렬합니닷!   아%HTML_AH_K39()%　%RAND_SPLIT("응/")%……!」")
				}
				PRINTFORMDW 스스로의 한계를 깨달았다%CALLNAME:MASTER%는%CALLNAME:TARGET%를 껴안아, 허리를 격렬하게 내던졌다…
			ELSE
				CALL HTMLPRINTW(@"「%CNAME(MASTER, TARGET)%의 얼굴이 보이면 안심합니다%HTML_PS_K39()%」")
				PRINTFORMW 「자, 좋아요. 이대로 나를 범해 주세요…」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF

