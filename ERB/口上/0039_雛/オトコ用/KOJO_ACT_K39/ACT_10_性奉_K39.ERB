	;범용 처리
	CALL PRINT_KOJO_ACT_GRU10_1_K39
	RETURN 1
ENDIF

;ACT파생 2:체위 변경 //다른 V성교 봉사계로부터
;===================
IF TFLAG:ACT파생 == 2
	;초조하게해중
	;========
	IF TCVAR:MASTER:초조하게해번 > 1
		IF LOCAL:0 ;호의, 호감도로 분기
			PRINTFORMW 「후후, 그렇게 안에%LOCALS:0%하고 싶은거야?」
			CALL HTMLPRINTW(@"「간단하게 가게 할 생각은 없기 때문에, 최대한 노력해%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「그렇게 당황하는 일 없다고 생각합니다만, 이제(벌써) 가고 싶어서 견딜 수 없네요. 후후」
			CALL HTMLPRINTW(@"「그렇지만, 조금만 더 참고 받아요%HTML_PS_K39()%」")
		ENDIF
		RETURN 1
	ENDIF

	;초조하게해 종료
	;==========
	IF TCVAR:MASTER:초조하게해번 == 1
		IF LOCAL:0 ;호의, 호감도로 분기
			CALL HTMLPRINTW(@"「응%RAND_SPLIT("응/")%%HTML_AH_K39()%  군요, 이대로%LOCALS:0%해」")
			PRINTFORMW 「%TNAME_K39()%안,%CNAME(MASTER, TARGET)%로 가득 해…」
		ELSE
			PRINTFORMW 「좋아요, 이대로%LOCALS:0%해」
			PRINTFORMW 「나의 안,%CNAME(MASTER, TARGET)%로 가득 해 주세요…」
		ENDIF
		RETURN 1
	ENDIF

	;초조하게함 없음
	;==========
	IF !TCVAR:MASTER:초조하게해번
		;조교자가갈것같다
