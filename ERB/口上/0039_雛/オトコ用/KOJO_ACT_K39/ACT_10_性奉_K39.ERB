				;PALAM:쾌V가 5000이상
				CASE IS >= 5000
					IF LOCAL:0
						{
						CALL HTMLPRINTW(
						@"「아아웃, 읏%HTML_AH_K39()%  나! 　" + 
						@", 안쪽에… 맞아앗……%HTML_PS2_K39()%」")
						}
					ELSE
						{
						CALL HTMLPRINTW(
						@"「아, 앗%HTML_AH_K39()%  개, 이런 건… 야, 야읏! 　" + 
						@"후~……%HTML_PS2_K39()%」")
						}
					ENDIF
				;그 외
				CASEELSE
					IF LOCAL:0
						{
						CALL HTMLPRINTW(
						@"「%RAND_SPLIT("나는/")%괜찮아, 그러니까……" + 
						@"군요, 좀 더 격렬하고…… 응읏%HTML_PS2_K39()%」")
						}
					ELSE
						CALL HTMLPRINTW(@"「좋은, 이에요…… 좀 더, 격렬하게 시라고 주세요…… 읏%HTML_PS2_K39()%」")
					ENDIF
			ENDSELECT
		;A성교 봉사계
		ELSE
			IF 0
			;조교 대상이 참고 있다
			ELSEIF TCVAR:MASTER:인내
				PRINTFORMDL 갈 것 같게 되었다%CALLNAME:MASTER%는 조금 삽입질의 페이스를 떨어뜨렸다…
				IF LOCAL:0 ;호의, 호감도로 분기
					PRINTFORMW 「저기 읏, 이런 것 싫엇… 좀 더, 좀 더 움직여요…」
					CALL HTMLPRINTW(@"「그렇지만…… 곧바로 가서는 안되니까%HTML_PS_K39()%」")
				ELSE
					PRINTFORMW 「읏, 슬슬 한계같네요…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%가 움직이지 않으면, 내가 조금 움직여 줄까요…?」
					CALL FADE_TO_CLEARLINE(12, "「킥킥…」")
				ENDIF
			;조교 대상이갈것같다
			ELSEIF !TCVAR:MASTER:인내 && TCVAR:MASTER:갈것같다
				CALL QUAKE(4)
				IF LOCAL:0 ;호의, 호감도로 분기
					{
					CALL HTMLPRINTW(
					@"「야앗%HTML_AH_K39()%  팥고물, 아앗" + 
					@"%HTML_AH_K39()%  는, 격렬해……!」")
					}
				ELSE
					{
					CALL HTMLPRINTW(
					@"「앗%HTML_AH_K39()%  팥고물%HTML_AH_K39()%　" + 
					"는, 격렬합니닷!   아… 응앗……!」")
					}
				ENDIF
				{
				PRINTFORMDW 스스로의 한계를 깨달았다%CALLNAME:MASTER%는,\@ 
				ABL:A감각 < 4 
				? 아직도 친숙해 지지 않다 # 자포자기하고 잘랐다 
				\@%CALLNAME:TARGET%의 아누스를 격렬하게 범했다…
				}
				PRINTL
			;그 외
			ELSE
				CALL QUAKE(4)
				IF LOCAL:0
					{
					CALL HTMLPRINTW(
					@"「응읏%HTML_AH_K39()%  아, 앗%HTML_AH_K39()%　" + 
					@"엉덩이안이… 스쳐, 라고……%HTML_PS2_K39()%」")
					}
				ELSE
					{
					CALL HTMLPRINTW(
					@"「응앗%HTML_AH_K39()% 팥고물%HTML_AH_K39()%　" + 
					@"나, 엉덩이로… 이렇게…%HTML_PS2_K39()%」")
					}
				ENDIF
			ENDIF
		ENDIF

		;지문을 처리. ACT에 의해 파생 //역강간은 범용 처리하지 않기 때문에 제외
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "정상위시킨다"
				{
				PRINTFORMDW \@ PALAM:쾌V >= 8500 
				? %CALLNAME:TARGET%는%CALLNAME:MASTER%의 허리에 다리 확실히를 관련되어, 스스로도 끝까지 오르려고 하고 있다 
