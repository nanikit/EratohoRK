			{
			CALL HTMLPRINTW(
			@"「え？ もしかして…%HTMLFONT("<b>『初めて』</b>", , COLOR2_K39("WHITE"))%" + 
			@"\@ GET_KFLAG_K39("相思相愛", (FLAG:ご主人様)) ? なの # なんですか \@？」")
			}
			{
			PRINTFORMW 「男の\@ 
			TALENT:MASTER:小柄 
			? 子 
			# 人 
			\@は皆、早い段階からこういう事してるものと思っていた\@ 
			GET_KFLAG_K39("相思相愛", (FLAG:ご主人様)) 
			? んだけど 
			# のですけど \@…」
			}
			{
			PRINTFORMW 「あ、でも……これってある意味、酷い偏見\@ 
			GET_KFLAG_K39("相思相愛", (FLAG:ご主人様)) ? よね # ですよね \@」
			}
		ENDIF
		CALL FADE_TO_CLEARLINE(10, "「ふふ……」")
		RETURN 1
	ENDIF

	;オナホで初回 //調教対象に自慰経験がある場合に限定
	;============
	IF TEQUIP:オナホール && GROUPMATCH(1, EXP:MASTER:自慰経験, EXP:MASTER:調教自慰経験) > 1 && FIRSTACT(1)
		{
