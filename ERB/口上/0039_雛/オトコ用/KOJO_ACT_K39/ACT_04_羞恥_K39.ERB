;=======================
IF TFLAG:ACT派生 == 1
	PRINTFORMW 「愉しんでもらえた\@ LOCAL:0 ? かしら # でしょうか \@」
	;調教対象の状態変化によって派生
	SELECTCASE GET_CONDITION(MASTER)
		CASE "疲弊", "衰弱"
			PRINTFORMW 「…って。随分やつれ\@ LOCAL:0 ? ちゃったわ # てしまいました \@ね」
		CASE "無気力"
			{
			PRINTFORMW 「…って、どうした\@ 
			LOCAL:0 
			? の 
			# んでしょう 
			\@？ すっかり気の抜けた顔になっ\@ 
			LOCAL:0 
			? ちゃ 
			# てしま 
			\@って…」
			}
		CASE "朦朧"
			CALL HTMLPRINTW(@"「あら？ 心ここにあらずって感じ\@ !LOCAL:0 ? です # \@ね…%HTML_PS_K39()%」")
		CASE "情欲"
			{
			CALL HTMLPRINTW(
			@"「\@ TENSION(TARGET, 8) == "高い" ? ふふ、 # \@興奮冷めやらぬって感じ" + 
			@"\@ !LOCAL:0 ? です # \@ね？ 聞くだけ野暮だったかしら%HTML_PS_K39()%」")
			}
		CASE "怒り"
			{
			PRINTFORMW 「\@ LOCAL:0 
			? なによ%RAND_SPLIT("ー/")%、 
			# \@機嫌悪そう\@ 
			!LOCAL:0 
			? です 
			# 
			\@ね。そんなにこれが嫌だったの\@ 
			!LOCAL:0 ? ですか # \@？」
			}
		CASE "退屈"
			PRINTFORMW 「%CNAME(MASTER, TARGET)%にとっては、退屈なだけだったみたい\@ !LOCAL:0 ? です # \@ね…」
		CASE "鬱屈"
			PRINTFORMW 「んー。ちょっと様子がおかしい\@ LOCAL:0 ? わ # です \@ね……」
		CASE "通常"
			PRINTFORMW 「ふふ、お疲れさま\@ !LOCAL:0 ? でした # \@」
	ENDSELECT
	RETURN 1
