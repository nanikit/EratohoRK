	IF GETBIT(TEQUIP:自慰中, 1)
		;好意、好感度で派生
		IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
			PRINTFORMW 「ねぇどうせなら…、%CNAME(MASTER, TARGET)%も自分でシてみせてよ」
			PRINTFORMW 「私一人でよくなっちゃうのも、なんかアレだし…」
		ELSE
			CALL HTMLPRINTW(@"「見てるだけではつまらないですよね？ ふふ…%HTML_PS_K39()%」")
			PRINTFORMW 「いいですよ。%CNAME(MASTER, TARGET)%が自分でするところ、私に見せて下さい」
		ENDIF
		RETURN 1
	ENDIF

	;オナホール装着中
	;================
	IF TEQUIP:オナホール
		;好意、好感度で派生
		IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
			PRINTFORMW 「ほら、自分でシてみせて？」
			CALL HTMLPRINTW(@"「ソレを使ってどうやるのか、じっくり見させてもらうから%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「さ、ソレで扱いて見せてください」
			PRINTFORMW 「今更恥ずかしがる事なんてないんですよ？」
			CALL FADE_TO_CLEARLINE(15, "「ふふ……」")
		ENDIF
		RETURN 1
	ENDIF

	;その他・条件なし //汎用ACTにはパスしない
	;================
	IF 口上記入チェック(100)
		;好意、好感度で派生
		IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
			PRINTFORMW 「ねぇ、自分でシてみせて？」
			PRINTFORM 「%CNAME(MASTER, TARGET)%がイクまで、ちゃんと見ててあげるから
		ELSE
			PRINTFORMW 「では、自慰をしてみせてください」
			PRINTFORM 「どういう風にするのか、じっくり見させてもらいます
		ENDIF
		CALL PRINT_SYMBOL_K39
		RETURN 1
