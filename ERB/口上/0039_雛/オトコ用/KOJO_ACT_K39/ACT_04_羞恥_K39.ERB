[SKIPEND]
@KOJO_MALE_ACT_K39_43 ;TFLAG:ACT派生(0=開始/1=終了)
#LOCALSIZE 1
VARSET LOCAL
LOCAL:0 = GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))

;ACT43_0, 羞恥プレイ開始
;=======================
IF !TFLAG:ACT派生
	;アイマスク装着中
	;================
	IF TEQUIP:アイマスク
		{
		PRINTFORMW 「\@ 
		LOCAL:0 
		? ねぇ、判る 
		# 判りますか 
		\@？ %CNAME(MASTER, TARGET)%は今、鏡の前にいる\@ 
		LOCAL:0 
		? の 
		# んです \@」
		}
		{
		PRINTFORMW 「目隠しで何も見えないでしょうけど…、自分がどういう状況なのかは知っておいて\@ 
		!LOCAL:0 
		? ください 
		# \@」
		}
		{
		PRINTFORMW 「想像力\@ 
		FIRSTACT(0) 
		? が豊かなら 
		# 次第で 
		\@、これはこれで愉しめる状況\@ 
		LOCAL:0 
		? なんじゃないかしら？ 
		# ではあると思いますけど \@」
		}
		CALL FADE_TO_CLEARLINE(15, @"「\@ FIRSTACT(1) ? ふふ # くすくす \@……」")
		RETURN 1
	ENDIF

	;その他・好意、好感度で派生
	;==========================
	IF GET_KFLAG_K39("相思相愛", (FLAG:ご主人様))
		SELECTCASE RANDACT(2, 0)
			CASE 0
				CALL FADEIN(8, "「鏡から目を逸らしては駄目よ」")
				PRINTFORMW 「ほら、もっと良く見て。どう？　ちょっと興奮してきたんじゃない？」
				CALL FADE_TO_CLEARLINE(15, "「クスクス…」")
			CASE 1
				PRINTFORMW 「ねぇ、こーいうのって興奮しない？」
				PRINTFORMW 「私は凄く興奮しちゃう。人に見られてるみたいで、ゾクゾクしちゃうの……」
		ENDSELECT
	ELSE
		PRINTFORMW 「ほら、鏡を見て下さい…」
		PRINTFORMW 「目を逸らさず、ちゃんと見てないと駄目ですよ\@ TENSION(TARGET, 8) == "高い" ? ？ # …… \@」
	ENDIF
	RETURN 1
ENDIF

