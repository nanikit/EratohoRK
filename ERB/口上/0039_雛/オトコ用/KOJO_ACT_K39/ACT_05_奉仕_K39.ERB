;히나 구상 봉사계 액션 구상 파일

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;


;        
;봉사계 ACT50～57
;        
[SKIPSTART]
	#TFLAG:ACT
		50=손으로애무를강제
		51=입으로애무를강제
		52=파이즈리
		53=가랑이
		55=밟핥기강제
		56=이라마치오
		57=가슴애무강제
	#TFLAG:ACT분류 == 5에 해당
	#범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "봉사")로 줍는 것이 가능
	#조교 대상으로, 조교자의 신체를 애무하도록(듯이) 명령한다
	#기본적으로는 명령인 것으로, 이 단계에서는 따를지 어떨지 확정하고 있지 않다(ACT56, 이라마치오는 거부 할 수 없다)

	*ACT54, 풋잡은 12/04/08판으로 숙청
[SKIPEND]


;///////////////////////
;ACT50, 손으로애무를강제//
;///////////////////////
[SKIPSTART]
	#애무 강제/자위 강제/페니스밴드 자위 강제
	#조교 대상(MASTER)으로, 손에 의한 애무를 강제한다
	#조교자에게 페니스가 있으면 반드시 자위 강제가 된다(페니스가 있으면 통상의 애무 강제는 일어나지 않는다)
	#조교자가 페니스밴드장착 안은 페니스밴드 자위 강제가 된다
	#TFLAG:벌플래그가 서 있으면 발생하지 않는다
	#턴방침이 "하드", "이상"이라면 발생하지 않는다
	#쓰러트리기중에는 발생하지 않는다
	#TEQUIP(줄/삼각목마) 장착중에는 발생하지 않는다
	#성교중/성교봉사중에는 발생하지 않는다
	#조교 대상이 따르는지 아닌지는 이 시점에서는 미정
	#REACT분류가 거부/날뛴다/도망치는 것 경우는 거절 취급이 된다

	*지문은 ACT파생 마다 분기. 각 ACT파생내에서는 분기 없음
	*ACT파생 0, 애무때만 쾌Ｂ/쾌ＡSOURCE 증가
	*그 외의 경우는 쾌Ｃ한정
[SKIPEND]
@KOJO_MALE_ACT_K39_50 ;TFLAG:ACT파생(0=애무 강제/1=자위 강제/2=페니스밴드 자위 강제)
#LOCALSIZE  1
#LOCALSSIZE 1
VARSET LOCAL
VARSET LOCALS

;ACT파생 1와 2는 고려하지 않는다
SIF GROUPMATCH(TFLAG:ACT파생, 1, 2)
	RETURN 0

;ACT50_0, 애무 강제//「자신의 성기를 손으로애무 하라고 명령한다」
;=================
IF !TFLAG:ACT파생
	;연속 실행
	;========
	IF TFLAG:ACT == TFLAG:전ACT
		;조교자가 갈 것 같다
		;================
		IF TCVAR:TARGET:갈것같다
			;호의, 호감도로 파생
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				CALL HTMLPRINTW(@"「햐앗! 그런… 손가락으로! 타면, 안돼에%HTML_PS2_K39()%」")
				CALL HTMLPRINTW(@"「가 버린다%HTML_AH_K39()%  가 버리기 때문에! 아아, 안돼엣……!」")
			ELSEIF AFFECTION("몹시 사랑함")
				{
				CALL HTMLPRINTW(
				@"「읏%HTML_AH_K39()%  아%HTML_AH_K39()%" + 
				@"  그런 곳까지… 만지작거려지고……%HTML_PS2_K39()%」")
				}
				PRINTFORMW 「%CNAME(MASTER, TARGET)%에, 손대어져…… 나, 이렇게……!」
			ELSE
				CALL HTMLPRINTW(@"「읏%HTML_AH_K39()%  아%HTML_AH_K39()%  나, 이제(벌써)……」")
				CALL HTMLPRINTW(@"「이제(벌써), 조금으로……! 아앗%HTML_PS2_K39()%」")
			ENDIF
			RETURN 1
		ENDIF

		;그 외
		;======
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「앙%HTML_AH_K39()%  아%HTML_AH_K39()%  아! 뭔가 격렬해… 히앗!」")
			CALL HTMLPRINTW(@"「야, 약한 곳뿐…… 만지작거리는 것인가, 들…%HTML_PS2_K39()%」")
		ELSEIF AFFECTION("몹시 사랑함")
			CALL HTMLPRINTW(@"「핫, 응읏! 추잡한 손놀림이군요…… 아앗%HTML_PS2_K39()%」")
			CALL HTMLPRINTW(@"「어느새, 이렇게…… 아%HTML_AH_K39()%  거기, 만지작거려지면!」")
		ELSE
			CALL HTMLPRINTW(@"「읏%HTML_AH_K39()%  능숙함, 이에요……」")
			CALL HTMLPRINTW(@"「앗%HTML_AH_K39()%  거기, 좀 더 손대어 주세요……」")
		ENDIF
		RETURN 1
	ENDIF

	;성교봉사중//TEQUIP:성교봉사중(정상위/대면좌위/배면좌위) 한편 조교자가 "정욕"한편 TCVAR:갈것같다가 서 있는 경우 한정으로 발생
	;==========//후배위/역강간/Ａ섹스에서는 발생하지 않는다
	IF TEQUIP:성교봉사중
		LOCAL :0 = GET_KFLAG_K39("상사상애", (FLAG:주인님))
		LOCALS:0 '= PALAM:쾌Ｖ > 5000 ? "!" # "…" 
		{
		CALL HTMLPRINTW(
		@"「아\@ PALAM:쾌Ｖ > 5000 ? ″ # \@…… 아아…… 아\@ PALAM:쾌Ｖ > 5000 
		? ″ # \@, 아\@ PALAM:쾌Ｖ > 5000 ? ″아 # \@%HTML_PS2_K39()%」")
		}
		{
		PRINTFORMW 「\@ LOCAL:0 
		? 군요, 응 # 아, 그 
		\@……%RAND_SPLIT(
		@"부탁\@ !LOCAL:0 ? 입니다 # \@. 여기도, 만지작거려\@ !LOCAL:0 ? 주세요 # \@…%LOCALS:0%" + 
		@"/코코가 안타깝다\@ LOCAL:0 ? 의 # 응입니다 \@…%LOCALS:0%  조금 격렬하게 시라고도, 좋다\@ !LOCAL:0 ? 입니다 # \@로부터……")%」
		}
		RETURN 1
	ENDIF

	;그 외·조건 없음//확률로 범용 액션에
	;================
	IF 구상기입체크(50)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMDW %CALLNAME%는%CALLNAME:MASTER%의 수를 잡아 스스로의 신체에 꽉 눌렀다…
			PRINTFORMW 「저기, 알겠죠…… 이제 이렇게 뜨거워지고 있어…」
			PRINTFORMW 「손대어… 만지작거려 돌려… 이것도 저것도 잊게 해……」
		ELSEIF AFFECTION("몹시 사랑함")
			PRINTFORMW 「나,%CNAME(MASTER, TARGET)%에 손대기를 원해서, 이제 인내 할 수 없습니다…」
			PRINTFORMW 「부탁입니다………%CNAME(MASTER, TARGET)%의 손으로, 나를 녹여……」
		ELSE
			PRINTFORMW 「나의 몸은 어떻습니까.%CNAME(MASTER, TARGET)%로부터 봐 매력적입니까?」
			PRINTFORMW 「사양은 필요 없어요. 좋아할 뿐(만큼) 손대어 주세요…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF


;///////////////////////
;ACT51, 입으로애무를강제//
;///////////////////////
[SKIPSTART]
	#쿤닐링구스/펠라치오/안면승마/안면승마애널/
	#조교 대상으로, 입에 의한 애무를 강제한다
	#조교자가 후타나리의 경우, 조교 대상이 위로돌리고라면 1/2으로 안면승마, 그렇지 않으면 페라 강제가 된다
	#조교자에게 페니스가 없고, 조교 대상이 위로돌리고라면 안면승마가 된다
	#안면승마가 선택되고 있었을 때, 안면승마애널에 변화하는 일이 있다
	#모든 조건으로부터 빠졌을 경우는 통상의 쿤닐링구스가 된다
	#TFLAG:벌플래그가 서 있으면 발생하지 않는다
	#쓰러트리기중에는 발생하지 않는다
	#TEQUIP(볼개그/삼각목마) 장착중은 발생하지 않는다
	#조교자가 페니스밴드를 장착하고 있는 경우는 발생하지 않는다
	#성교중/성교봉사중에는 발생하지 않는다
	#REACT분류가 거부/날뛴다/도망치는 것 경우는 거절 취급이 된다
[SKIPEND]
@KOJO_MALE_ACT_K39_51 ;TFLAG:ACT파생(0=쿤닐링구스/1=페라/2=안면승마/3=안면승마애널)

;ACT파생가 페라라면 연주한다
SIF TFLAG:ACT파생 == 1
	RETURN 0

;ACT51_0, 쿤닐링구스
;===============
IF !TFLAG:ACT파생
	;연속 실행
	;========
	IF TFLAG:ACT == TFLAG:전ACT
		;조교자가 갈 것 같다
		;================
		IF TCVAR:TARGET:갈것같다
			;호의, 호감도로 파생
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				CALL HTMLPRINTW(@"「얏, 거기 안돼엣%HTML_AH_K39()%  거기, 약한데……!」")
				{
				CALL HTMLPRINTW(
				@"「안됏, 안돼에%HTML_AH_K39()%  가 버린닷" + 
				@"%HTML_AH_K39()%  가 버리기 때문에……!」")
				}
			ELSEIF AFFECTION("몹시 사랑함")
				{
				CALL HTMLPRINTW(
				@"「응앗%HTML_AH_K39()%  아%HTML_AH_K39()%　" + 
				"후~… 아, 안됩니닷! 그렇게 하면…」")
				}
				CALL HTMLPRINTW(@"「나, 나! 가 버립니다…… 아앗…!」")
			ELSE
				CALL HTMLPRINTW(@"「앗%HTML_AH_K39()%  응응! 그, 그렇게… 되면! 나, 가 버립니다…」")
			ENDIF
			RETURN 1
		ENDIF

		;그 외
		;======
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「응앗! 앗%HTML_AH_K39()%  팥고물%HTML_AH_K39()%  이제(벌써), 약한 곳뿐…」")
		ELSE
			CALL HTMLPRINTW(@"「읏%HTML_AH_K39()%  아%HTML_AH_K39()%  좋은, 입니다…」")
			PRINTFORMW 「기분이 좋아요……」
		ENDIF
		RETURN 1
	ENDIF

	;식스나인
	;==============
	IF GROUPMATCH(TEQUIP:조교대상Ｃ사용, 1, 2)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「응%HTML_AH_K39()%  이것은 뭔가 굉장히 흥분한다…」")
			PRINTFORM 「이봐요, 분명하게 시라고 주기 때문에 내 쪽도 부탁이군요
		ELSE
			PRINTFORMW 「이 모습, 매우 추잡해서 흥분해 버리네요…」
			PRINTFORMW 「차이 있고예요. 좋아할 뿐(만큼) 나를 맛봐 주세요」
			PRINTFORM 「나는 이쪽에서 즐겁게 해 받을테니까
		ENDIF
		CALL PRINT_SYMBOL_K39
		RETURN 1
	ENDIF

	;정액 빨고 강제
	;============
	IF INRANGE(TCVAR:MASTER:질내사정후경과턴, 1, 2)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「저기 봐.%CNAME(MASTER, TARGET)%의 정액이 흘러넘쳐 와 버려. 그러니까…」
			CALL HTMLPRINTW(@"「분명하게 빨아 예쁘게 해? 후후…%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「자신이 더럽힌 곳은, 분명하게 깨끗이 해 받아요」
			CALL HTMLPRINTW(@"「후후, 안됩니다. 싫어해도 허락해 주지 않습니다%HTML_PS_K39()%」")
		ENDIF
		RETURN 1
	ENDIF

	;그 외·조건 없음//확률로 범용 액션에
	;================
	IF 구상기입체크(55)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「저기, 부탁. 빠르게 시라고…」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%에 빨기를 원해서, 이제 인내 할 수 없는거야……」
		ELSEIF AFFECTION("몹시 사랑함")
			PRINTFORMW 「봐 주세요.%CNAME(MASTER, TARGET)%에 빨기를 원해서, 이미 이렇게 되어 있습니다…」
			PRINTFORMW 「이런 야비한 신을 경멸합니까? 그렇지만, 이제(벌써) 나……」
		ELSE
			PRINTFORMW 「그럼 나의 코코를 빨아 받을까요」
			PRINTFORMW 「민감한 곳인 것으로, 정중하게 부탁드릴게요」
		ENDIF
		RETURN 1
	ENDIF
ENDIF


;ACT51_2, 안면승마
;=================
IF TFLAG:ACT파생 == 2
	;연속 실행
	;========
	IF TFLAG:ACT == TFLAG:전ACT
		;조교자가 갈 것 같다
		;================
		IF TCVAR:TARGET:갈것같다
			;호의, 호감도로 파생
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				CALL HTMLPRINTW(@"「야앗, 앗%HTML_AH_K39()%  아%HTML_AH_K39()%! 이제(벌써), 가 버릴지도……」")
				CALL HTMLPRINTW(@"「응아앗%HTML_AH_K39()%  그렇게 되면, 견딜 수 없어…!」")
			ELSEIF AFFECTION("몹시 사랑함")
				CALL HTMLPRINTW(@"「응앗, 아읏%HTML_AH_K39()%  그런, 민감한 곳(뿐)만!」")
				PRINTFORMW 「이, 이런 건…… 이제(벌써), 견딜 수 있다…!」
			ELSE
				CALL HTMLPRINTW(@"「아앗%HTML_AH_K39()%  이렇게 되면, 나…!」")
				PRINTFORMW 「이, 이제(벌써)! 인내… 할 수 없게……!」
			ENDIF
			RETURN 1
		ENDIF

		;그 외//호의, 호감도로 파생
		;======
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「읏, 앗%HTML_PS_K39()%  아! 그런, 곳까지……」")
			CALL HTMLPRINTW(@"「앗, 하앗! 안됏%HTML_AH_K39()%  안돼엣!」")
		ELSE
			CALL HTMLPRINTW(@"「앗%HTML_AH_K39()%  응응! 나쁘고, 없어요」")
			PRINTFORMW 「좀 더, 좀 더 시라고 주세요…」
		ENDIF
		RETURN 1
	ENDIF

	;정액 빨고 강제//55%로 진
	;============
	IF 구상기입체크(55) && INRANGE(TCVAR:MASTER:질내사정후경과턴, 1, 2)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「이봐요, 스스로 더럽힌 곳은 분명하게 깨끗이 해」
			CALL HTMLPRINTW(@"「전부 깨끗이 빨아…%HTML_PS_K39()%」")
		ELSE
			CALL HTMLPRINTW(@"「자, 자신의 정액을 잘 맛봐 주세요%HTML_PS_K39()%」")
			PRINTFORMW 「남기지 않고 예쁘게 하지 않으면 안됩니다?」
		ENDIF
		RETURN 1
	ENDIF

	;그 외·조건 없음//확률로 범용 액션에
	;================
	IF 구상기입체크(55)
		;호의, 호감도로 파생
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			PRINTFORMW 「저기, 이런 추잡한 신님은 경멸해?」
			PRINTFORMW 「그렇지만, 이런 일 하는 것은%CNAME(MASTER, TARGET)%만…」
			PRINTFORMW 「나,%CNAME(MASTER, TARGET)%라고 있으면, 자꾸자꾸 이상해져 버려…」
		ELSEIF AFFECTION("몹시 사랑함")
			PRINTFORMW 「압니까? 이제 기다릴 수 없어서, 이렇게 되어 있습니다…」
			PRINTFORMW 「빨리,%CNAME(MASTER, TARGET)%의 혀로…… 나를 녹여 주세요」
		ELSE
			PRINTFORMW 「우후, 이 몸의 자세라면 도망칠 수 있지 않네요?」
			PRINTFORMW 「이 액신의 추잡한 곳,%CNAME(MASTER, TARGET)%의 혀로 빨아 주세요」
		ENDIF
		RETURN 1
	ENDIF
ENDIF


;ACT51_3, 안면승마애널
;=======================
IF TFLAG:ACT파생 == 3
	;연속 실행
	;========
	IF TFLAG:ACT == TFLAG:전ACT
		;조교자가 갈 것 같다
		;================
		IF PALAM:쾌Ａ > 8500
			;호의, 호감도로 파생
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				CALL HTMLPRINTW(@"「야앗%HTML_AH_K39()%  , 그렇게! 혀로 뚫어서는 안돼엣%HTML_PS2_K39()%」")
