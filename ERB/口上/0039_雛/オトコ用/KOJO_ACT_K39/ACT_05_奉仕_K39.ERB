			{
			CALL HTMLPRINTW(
			@"「아%RAND_SPLIT("/″/응/")%%HTML_AH_K39()%　" + 
			@"아%RAND_SPLIT("/")%%HTML_AH_K39()%  후~… 응읏!」")
			}
			RETURN 1
