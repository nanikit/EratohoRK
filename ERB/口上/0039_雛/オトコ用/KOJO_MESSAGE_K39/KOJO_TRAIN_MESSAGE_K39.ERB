;────────────────────────────────────
;057,胸愛撫強制
;────────────────────────────────────
;リアクション前
@KOJO_MALE_TRAIN_MESSAGE_K39_57
;派生によって分岐
SELECTCASE TFLAG:ACT派生
	;口で愛撫
	CASE 2
		;連続実行
		IF TFLAG:ACT == TFLAG:前ACT
			;調教者がイきそう
			IF PALAM:快Ｂ > 8500
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に乳首を吸われ、今にもイきそうになっている…
				RETURN 2
			;調教対象挿入中
			ELSEIF (GROUPMATCH(TEQUIP:性交奉仕中, 1, 3, 4) > 0 || TEQUIP:性交奉仕中 == 6)
				PRINTFORML 繋がった状態のまま%CALLNAME:MASTER%に%NIPPLE_DESCRIPTION(TARGET)%乳首を吸われ
				PRINTFORML %CALLNAME:TARGET%は甘い吐息を漏らしている…
				RETURN 2
			;その他連続実行
			ELSE
				PRINTFORML %CALLNAME:MASTER%は%CALLNAME:TARGET%の求めに応じ、
				PRINTFORML その%NIPPLE_DESCRIPTION(TARGET)%乳首を舌先で愛撫し続けた…
				RETURN 2
			ENDIF
		;調教対象挿入中
		ELSEIF (GROUPMATCH(TEQUIP:性交奉仕中, 1, 3, 4) > 0 || TEQUIP:性交奉仕中 == 6)
			PRINTFORML %CALLNAME:TARGET%は夢中で腰を揺すりたてる%CALLNAME:MASTER%の口元に
			PRINTFORML %OPPAI_DESCRIPTION(TARGET)%を寄せ、期待に満ちた眼差しで見つめている…
			RETURN 2
		ELSE
			RETURN 0
		ENDIF
	;手で愛撫
	CASEELSE
		;連続実行
		IF TFLAG:ACT == TFLAG:前ACT
			;調教者がイきそう
			IF PALAM:快Ｂ > 8500
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に胸を揉みしだかれ、今にもイきそうになっている…
				RETURN 2
			;調教対象挿入中
			ELSEIF (GROUPMATCH(TEQUIP:性交奉仕中, 1, 3, 4) > 0 || TEQUIP:性交奉仕中 == 6)
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に激しく突かれながらその胸を揉みしだかれ、嬌声を上げている…
				RETURN 2
			;その他連続実行
			ELSE
				PRINTFORML %CALLNAME:MASTER%は%CALLNAME:TARGET%の求めに応じ、その%OPPAI_DESCRIPTION(TARGET)%を愛撫し続けた…
				RETURN 2
			ENDIF
		;調教対象挿入中
		ELSEIF (GROUPMATCH(TEQUIP:性交奉仕中, 1, 3, 4) > 0 || TEQUIP:性交奉仕中 == 6)
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%と繋がったまま、その手をとり%OPPAI_DESCRIPTION(TARGET)%に導いた…
			RETURN 2
		ELSE
			RETURN 0
		ENDIF
ENDSELECT
