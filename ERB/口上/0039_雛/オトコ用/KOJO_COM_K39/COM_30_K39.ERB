IF 口上記入チェック(100) && !TFLAG:REACT派生
	;━━━━━━━━━━━━━━━━━━━━━━━━
	;負荷2：無理をした行動
	;━━━━━━━━━━━━━━━━━━━━━━━━
	IF GET_COM_RANK() == "無理をした行動"
		IF LOCAL:0
			PRINTFORMW 「ねぇ、冗談でも%RAND_SPLIT("そういうの/怯えてみせるとか")%止めて欲しいんだけど？」
			PRINTFORMW 「こう見え%RAND_SPLIT("たって/ても")%、けっこう傷付くんだから…」
		ELSE
			PRINTFORMW 「あの…、%RAND_SPLIT("わざわざ/無理に")%怯えてみせる事にどんな意味が\@ TENSION(TARGET, 8) == "高い" ? ？ # あるのでしょう \@」
			CALL FADEIN(10, "「ま、そうですね……」")
			{
			PRINTFORMW 「%CNAME(MASTER, TARGET)%の\@ COMCOR_POSI() 
			? 怯えの対象はこの私ではなく、ちょっと違うみたいですけど。ね # 本心は解りませんが、そういう視線を向けられると傷付きます…… \@」
			}
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF 0
	ELSEIF LOCAL:0
		PRINTFORMW 「ど、どうして…」
		PRINTFORMW 「ねぇ、怯えるのはやめてよ…」
	ELSEIF AFFECTION("溺愛")
		PRINTFORMW 「怯えるなんてあんまりです…」
	ELSEIF AFFECTION("眷愛")
		PRINTFORMW 「そんなに怯えないでくださいよ…」
	ELSEIF AFFECTION("嫌悪")
		PRINTFORMW 「そんなに怖いですか…？」
		PRINTFORMW 「確かに私は厄神ですけど……そんなあからさまに怯えられると傷つきます…」
	ELSE
		PRINTFORMW 「いくら厄神だからって、そんなに怯える事ないと思うんですが…」
	ENDIF
	RETURN 1
ENDIF
