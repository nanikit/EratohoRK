@KOJO_MALE_COM_K39_32
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;暴れ失敗！
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && INRANGE(TFLAG:REACT派生, 1, 2)
	;=================================================
	;負荷2：無理をした行動
	;=================================================
	IF GET_COM_RANK() == "無理をした行動"
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「無理に暴れてみせる意味が良く判らないけど…まぁ」
			{
			PRINTFORM 「構って欲しくてやってると思えば、\@ TENSION(TARGET, 8) == "高い" 
			? 可愛い # 微笑ましい \@って%RAND_SPLIT("思えなくも無い/気がしないではない")%けど、ね
			}
			CALL PRINT_SYMBOL_K39
		ELSE
			;//RAND_SPLITは("hoge/")でhogeと空文字列を返す
			PRINTFORMW 「そうやって暴れてみせるのが、%CNAME(MASTER, TARGET)%の%RAND_SPLIT("本心/本意")%とも思えませんけど%RAND_SPLIT("ね/")%」
			PRINTFORMW 「ただ…、何が目的でそんな事をするのか、ちょっと気にはなります」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT派生１：縄拘束による暴れ失敗
	;=================================================
	IF TFLAG:REACT派生 == 1
		;相思相愛
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「中々面白いけど…」
			PRINTFORMW 「それって何の意味も無いと思うわよ？」
		;それ以外
		ELSE
			CALL FADEIN(10, "「あ、あの……」")
			PRINTFORMW 「その状態で暴れても無駄だと思うんですけど…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT派生２：制止による暴れ失敗
	;=================================================
	IF TFLAG:REACT派生 == 2
		VARSET LOCALS
		LOCALS:0 '= RAND_SPLIT("暴れて/抵抗して/そんな事して")
		;=================================================
		;ACT分類：休憩
		;=================================================
		IF MENUMATCH(TFLAG:ACT, "休憩")
			;相思相愛
			IF GET_KFLAG_K39("相思相愛")
				PRINTFORMW 「ねぇ、なんで暴れるの？」
				PRINTFORMW 「意味がわからないんだけど…」
			;それ以外
			ELSE
				PRINTFORMW 「あの、いま暴れるのにどんな意味が%RAND_SPLIT("？/あるんでしょう")%」
				PRINTFORMW 「\@ IS_NOWACTNAME("治療する") ? 私に治療されるのがが嫌なんですか？ それとも # 意味などなく \@、ただ暴れたいだけなんでしょうか……」
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		;相思相愛
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「もうっ、\@ IS_CONDITION(TARGET, "怒り") ? いい加減にし # ちょっと落ち着き \@なさいよ！」
			PRINTFORMW 「%LOCALS:0%も無駄だって判らないの？」
		;それ以外
		ELSE
			PRINTFORMW 「駄目ですよ、大人しくしてください」
			PRINTFORMW 「どのみち、%LOCALS:0%も無駄ですよ…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;=================================================
;REACT派生０：暴れが通った
;=================================================
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;相思相愛
	IF GET_KFLAG_K39("相思相愛") 
		PRINTFORML 「もうっ、なんでそんなに暴れるの？」
		PRINTFORMW 「いい加減にしてよ…」
	;その他
	ELSE
		PRINTFORMW 「あの、そんなに暴れないでください…」
	ENDIF
	RETURN 1
ENDIF
