	;無理をした行動
	;=================================================
	IF GET_COM_RANK() == "無理をした行動"
		PRINTFORMW 「痛いのなら我慢せずに、そう言えば良い\@ LOCAL:0 ? のに # と思%RAND_SPLIT("いますけど/うのですが")% \@…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%、ちょっと無理して\@ LOCAL:0 ? ない # ませんか \@？」
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：愛撫(手淫/貝あわせ/素股足/コキ以外)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "愛撫")
		;相思相愛
		IF LOCAL:0
			PRINTFORMW 「今のが痛かったの？」
			PRINTFORMW 「もうそんなに下手じゃ無いはずなんだけど…」
		;その他
		ELSE
			SELECTCASE RANDCOM(2, 100)
				CASE 0
					PRINTFORMW 「ご免なさい、ちょっと痛かったでしょうか？」
				CASE 1
					PRINTFORMW 「気をつけてしたつもりだったのですが」
					PRINTFORMW 「痛がらせてしまったみたいですね…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：道具(バイブ/Ａバイブ/Ａビーズのみ)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "道具")
		;相思相愛
		IF LOCAL:0
			SELECTCASE RANDCOM(2, 200)
				CASE 0
					PRINTFORMW 「もう少し馴染ませる必要があるみたいね…」
				CASE 1
					PRINTFORMW 「コレ使うのはまだ早かったみたいね…？」
					PRINTFORMW 「分かった。もう少し馴らしてからシてあげる♪」
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RANDCOM(2, 201)
				CASE 0
					PRINTFORMW 「まだ痛みがあるでしょうか？」
					PRINTFORMW 「馴染むまでもう少しかかりそうですね…」
				CASE 1
					PRINTFORMW 「これを使うのは少し早過ぎたでしょうか…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：性交(性交系全て)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "性交")
		;相思相愛
		IF LOCAL:0
			PRINTFORML 「まだ痛みがあるの？」
			PRINTFORMW 「もう少し馴染ませる必要があるみたいね…」
		;それ以外
		ELSE
			PRINTFORML 「まだ痛みがあるでしょうか？」
			PRINTFORMW 「馴染むまでもう少しかかりそうですね…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：奉仕(イラマチオのみ？)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "奉仕")
		IF 0
		ELSEIF IS_NOWACTNAME("イラマチオ")
			;相思相愛
			IF LOCAL:0
				PRINTFORMW 「ねぇ、苦しいの？ 呼吸が出来ないの？」
				PRINTFORMW 「悪いと分かっていても、そういう顔されると……物凄く興奮しちゃう…」
			;それ以外
			ELSE
				PRINTFORMW 「やはり喉奥まで突かれるのは苦しいでしょうか」
				PRINTFORMW 「でも興奮しますね、これ……」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：加虐(罵倒以外)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "加虐")
		;相思相愛
		IF LOCAL:0
			PRINTFORMW 「ふ……ふふふっ、なんて可愛いの…」
			PRINTFORMW 「あっ、やだ…、ちょっと濡れてきちゃった…」
		;それ以外
		ELSE
			SELECTCASE RANDCOM(3, 600)
				CASE 0
					PRINTFORMW 「なかなか頑張りますね」
					PRINTFORM 「いいですよ、とっても
					CALL PRINT_SYMBOL_K39
				CASE 1
					PRINTFORM 「痛みに耐える表情が、とってもいいですよ
					CALL PRINT_SYMBOL_K39
				CASE 2
					PRINTFORMW 「どうしてなのか分からないけど…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%の苦悶の表情を見ると、ゾクゾクしてしまいます…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類：異常(フィスト系のみ)
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "異常")
		PRINTFORMW 「\@ LOCAL:0 ? 私が言うのも変な話だけど # 私が言うのもおかしな話ですが \@…」
		PRINTFORMW 「よく\@ LOCAL:0 ? 耐えられるわね # よく我慢できますね \@？」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 「\@ LOCAL:0 ? そんなに痛いなら早く言えばいいのに、って思うけど # ごめんなさい。ちょっと痛かったでしょうか \@…」
	RETURN 1
ENDIF
