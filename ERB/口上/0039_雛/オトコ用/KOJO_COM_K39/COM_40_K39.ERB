#LOCALSSIZE 1
VARSET LOCALS
LOCALS:0 = %RAND_SPLIT("아″/아″/(이)나/히앗")%

IF 구상기입체크(100) && !TFLAG:REACT파생
	;=================================================
	;ACT분류:성적봉사
	;=================================================
	IF MENUMATCH(TFLAG:ACT, "성적봉사")
		IF GET_KFLAG_K39("상사상애")
			;조교자가 갈 것 같다
			IF TCVAR:갈것같다 || ((PALAM:쾌V + PALAM:쾌A + PALAM:쾌B) >= 8500)
				;조교 대상이 갈 것 같다
				IF TCVAR:MASTER:갈것같다
					PRINTFORM 「%LOCALS:0%
					CALL AUTOHEARTCB_K39
					PRINTFORM   는, 벗길 수 있는 쉿…
					CALL PRINT_SYMBOL2_K39
					;V 성교봉사중
					IF V_SEX(MASTER)
						PRINTFORM   아
						CALL AUTOHEARTCB_K39
						PRINTFORM 　%RAND_SPLIT("둔다/안쪽에")%…… 맞아
						CALL AUTOHEARTCB_K39
					;A 성교봉사중
					ELSE
						PRINTFORMW   개, 이런 건%RAND_SPLIT("라고/")%
						CALL AUTOHEARTCB_K39
						PRINTFORMW   엉덩이… 부서져 버린다앗…
						CALL AUTOHEARTCB_K39
					ENDIF
					PRINTFORM   아%RAND_SPLIT("″/")%……
					CALL PRINT_SYMBOL2_K39
				;그 외
				ELSE
					;범용 처리
					CALL PRINT_KOJO_COM_44_K39
					PRINTFORM   나의 나카… 굉장히 뜨겁고…… 나, 기분 좋게은
					CALL PRINT_SYMBOL2_K39
					PRINTFORM 「나…%CNAME(MASTER, TARGET)%%RAND_SPLIT("에 안겨 행복/(와)과라면 행복")%
					CALL AUTOHEARTCB_K39
					PRINT   행복
					CALL AUTOHEARTCB_K39
					PRINTFORM   따뜻해서… 나!  %CNAME(MASTER, TARGET)%와라면…
					CALL PRINT_SYMBOL2_K39
				ENDIF
			ELSE
				;범용 처리
				CALL PRINT_KOJO_COM_44_K39
				;소비 기력으로 파생
				SELECTCASE GET_COM_RANK()
					CASE "자연스러운 행동"
						PRINTFORMW   여유만만이군요…… 조금%RAND_SPLIT("아니꼽게 거슬릴지도/분할지도/화난다")%…」
						PRINTFORM 「%RAND_SPLIT("아/(이)나//히아")%%RAND_SPLIT("/")%
						CALL PRINT_SYMBOL2_K39
						PRINTFORMW   이제(벌써)! 그렇게…, 나의 약한 곳…… 뿐!」
					CASE "무리가 없는 행동"
						PRINTFORMW   나를 기분 좋게 시키려고, 노력해 주고 있는 거네. 기쁘다…」
						PRINTFORMW 「그렇지만…%CNAME(MASTER, TARGET)%%RAND_SPLIT("에도, 나의 신체로 기분 좋아졌으면 좋겠다/도 즐겨 주지 않으면, 나도 즐겁지 않다")%의」
						PRINTFORMW 「그러니까… 말야?  %CNAME(MASTER, TARGET)%가 바란다면, 좀 더 격렬하게 여겨져도.%RAND_SPLIT("나는/나는 상관없으니까/나는 괜찮기 때문에")%……」
					CASE "무리를 한 행동"
						PRINTFORMW   여유 충분히, 라는 것도 아닌 것 같구나.%RAND_SPLIT("고집/허세")%붙여 버려…」
						PRINTFORM 「그렇지만, 후후……%CNAME(MASTER, TARGET)%의 그러한 곳, 나는\@ CFLAG:TARGET:의존도 > 4000 ? 대 # \@를 좋아하는 사람%RAND_SPLIT("야/")%…
						CALL PRINT_SYMBOL_K39
				ENDSELECT
			ENDIF
		ELSE
			;조교자가 갈 것 같다
			IF TCVAR:갈것같다 || ((PALAM:쾌V + PALAM:쾌A + PALAM:쾌B) >= 8500)
				;조교 대상이 갈 것 같다
				IF TCVAR:MASTER:갈것같다
					PRINTFORM 「%LOCALS:0%
					CALL AUTOHEARTCB_K39
					PRINTFORM 　%RAND_SPLIT("는, 격렬해져! 격렬합니다/망가진다! 부서져 버립니닷")%
					CALL PRINT_SYMBOL2_K39
					PRINTFORMW 「그렇게%RAND_SPLIT("……/")%되면, 나…%RAND_SPLIT("、/")%이제(벌써)……」
				ELSE
					;범용 처리
					CALL PRINT_KOJO_COM_44_K39
					PRINTFORMW 　%RAND_SPLIT("좋습니다! 좋습니다/기분, 좋은… 기분이 좋은… 입니닷!")%!! 아″… 아아…」
					PRINTFORM 「부디 멈추지 마! 이대로…… 좀 더! 나를! 나의 나카를…… 좀 더…
					CALL PRINT_SYMBOL2_K39
				ENDIF
			ELSE
				;범용 처리
				CALL PRINT_KOJO_COM_44_K39
				;소비 기력으로 파생
				SELECTCASE GET_COM_RANK()
					CASE "자연스러운 행동"
						PRINTFORMW 　%RAND_SPLIT("여유 충분히라는 느낌/상당히 여유가 있는")%군요…」
						PRINTFORMW 「그렇지만%RAND_SPLIT(@"%CNAME(MASTER, TARGET)%라면…/")%%CNAME(MASTER, TARGET)%에라면, 이런 식으로 되어도. 나는……」
					CASE "무리가 없는 행동"
						PRINTFORMW 　%RAND_SPLIT("기분/")%좋아요, 매우…」
						PRINTFORMW 「나는 괜찮기 때문에…%CNAME(MASTER, TARGET)%가 바란다면, 좀 더 격렬하게 해도……」
					CASE "무리를 한 행동"
						PRINTFORMW   그\@ IS_NOWACTNAME("정상위시킨다/대면좌위시킨다/역강간") ? 겉모습 # 허리사용 \@정도에는 여유 없는 것 같네요…」
						PRINTFORM 「후후…
						CALL AUTOHEARTCB2_K39
						PRINTFORMW   알아 버립니다. 이렇게 해 연결되고 있으면, 더욱 더…
						CALL PRINT_SYMBOL_K39
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	IF GET_KFLAG_K39("상사상애")
		;소비 기력으로 파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				IF MENUMATCH(TFLAG:ACT, "봉사")
					PRINTFORM 「%RAND_SPLIT("아/응/아/후아")%…
					CALL AUTOHEARTCB_K39
					PRINTFORMW   나%RAND_SPLIT("가 느끼는 곳/의 기분이 좋은 곳")%, 전부 알려져 버린 것 같다…」
					PRINTFORMW 「그렇지만… 이렇게 해%CNAME(MASTER, TARGET)%에 좋은 것 같게 되어 버리는 것도,%RAND_SPLIT("싫지 않아군요/나쁘지 않은, 일지도")%…」
				ELSE
					PRINTFORMW 「후후,%RAND_SPLIT("여유 충분히라는 느낌이군요/이런 것 아무렇지도 않다는 것은 얼굴 해 버려")%」
					PRINTFORMW 「오히려,%CNAME(MASTER, TARGET)%…… 이래서야 어딘지 부족하다고 생각해 있거나 하지 않아?」
				ENDIF
			CASE "무리가 없는 행동"
				IF MENUMATCH(TFLAG:ACT, "봉사")
					PRINTFORM 「나를%RAND_SPLIT("기쁘게 하는데/기분 좋게 시키려고")%열심히인 거네. 후후, 사랑스럽다……
					CALL PRINT_SYMBOL_K39
					;조교자의 BASE:만족이 50%이상
					IF TENSION(TARGET, 8) == "높다"
						PRINTFORM 「머리%RAND_SPLIT("스윽스윽/스윽스윽")%준비위…
						CALL PRINT_SYMBOL_K39
					ENDIF
				ELSE
					PRINTFORMW 「이 정도의 일, 지금의%CNAME(MASTER, TARGET)%에는 아무것도 아닌 것 같구나」
					PRINTFORMW 「오히려…, 조금 어딘지 부족하다고 생각하지 않아? 키득…」
				ENDIF
			CASE "무리를 한 행동"
				PRINTFORMW 「아무렇지도 않은 얼굴 하고 있지만, 사실은 여유 없겠지요?」
				PRINTFORMW 「후후, 무리해 버려……」
		ENDSELECT
	ELSE
		;소비 기력으로 파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운 행동"
				IF MENUMATCH(TFLAG:ACT, "봉사")
					PRINTFORMW 「응…… 이제(벌써) 완전히,%CNAME(MASTER, TARGET)%에 좋은 것 같게 되어 버려. 나…」
					PRINTFORMW 「그렇지만, 이런 것도 나쁘지 않네요……」
				ELSE
					PRINTFORMW 「아라? 상당히 여유가 있는 것 같네요」
					PRINTFORMW 「이제(벌써), 이런 것으로는 물건 충분하지 않습니까? 후후」
				ENDIF
			CASE "무리가 없는 행동"
				PRINTFORM 「%RAND_SPLIT("후후/키득")%、
				IF MENUMATCH(TFLAG:ACT, "봉사")
					PRINTFORM 그렇게 열심히 해 주면, 나도 기뻐져 버립니다…
					CALL PRINT_SYMBOL_K39
				ELSE
					PRINTFORMW 적극적이어 매우 좋네요? 무엇보다…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%에 있어, 이 정도의 일은 아무것도 아닌 것 같지만」
				ENDIF
			CASE "무리를 한 행동"
				PRINTFORMW 「아무것도 아닌 것 같은 얼굴 하고 있습니다만, 실은 조금 무리하고 있지 않습니까?」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

