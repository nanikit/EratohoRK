@KOJO_MALE_COM_K39_1
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 1:애원 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	CALL FADEIN(10, @"「\@ GET_KFLAG_K39("상사상애") ? 응, 나쁘지만# 저, 죄송합니다만\@…」")
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운 행동", "무리가 없는 행동"
			PRINTFORMW 「이런 일 그만두어, 라고 말해져도\@ GET_KFLAG_K39("상사상애") ? 곤란합니다만?# 곤란하네요…\@」
		CASE "무리를 한 행동"
			PRINTFORMW 「이제 그만두어, 라고 말해져도\@ GET_KFLAG_K39("상사상애") ? 곤란해요# 곤란해 버리네요\@…」
	ENDSELECT
	;COM, 허가를청한다에 패스
	;CALL KOJO_MALE_COM_K39_7
	;RETURN RESULT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 0:심약구응답
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;=================================================
	;무리를 한 행동
	;=================================================
	IF GET_COM_RANK() == "무리를 한 행동"
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「%RAND_SPLIT("일부러/그렇게/저기…,")%건강이 없는 모습을 하는 의미가 뭔가 있는 거야?」
			PRINTFORM 「단지, 상관했으면 좋은 것뿐일지도 모르지만
			CALL PRINT_SYMBOL_K39
		ELSE
			PRINTFORMW 「\@ TENSION(MASTER, 0) == "높다" ? 그렇게 지치고 있는 것 같게도 보이지 않습니다만# 확실히, 조금 지친 것 같습니다만\@…」
			PRINTFORMW 「그 반응…, 조금 무리가 있는 것은 아닙니까? 후훗」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	IF GET_KFLAG_K39("상사상애")
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「저기, 어떻게 해 버렸어…?」
			CASE 1
				PRINTFORMW 「컨디션에서도 나쁜거야?   벌써 끝내?」
		ENDSELECT
	ELSE
		PRINTFORMW 「어떻게 한 것이지요…?」
		PRINTFORMW 「건강해 보이지 않네요…」
	ENDIF
	RETURN 1
ENDIF
;COM1, 심약구응답한다

