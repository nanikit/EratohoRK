;=====================================================================
;특수한 반응 COM70～
;=====================================================================

;─
;COM70, 가게해!
;─
;·REACT분류:받아들인다
;·REACT인상:좋은인상
;·의존도 변화(+10, +6)
;-------------------------------------------------
;선택 가능한 조건…
;·초조하게함 안은 아니다
;·TCVAR:MASTER:갈것같다가 서 있다
;·애무계 ACT(키스/밀어넘어뜨리기 이외) 한편 각 부위의 PALAM가 5000이상이다
;※상기 조건에 들어맞는 경우는 가능
;-------------------------------------------------
;·TFLAG:REACT파생(0=들은체 만체 한다/1=가게해 준다/2=가게해 주지 않는다)
;-------------------------------------------------
;ACT에 있어서 TCVAR:MASTER:초조하게해번 == 1(가게 한다)에서 만나도 COM70의 선택으로 TCVAR:MASTER:초조하게해번 > 1(초조하게 한다)(이)가 되는 경우 있어
;현재의 사양에서는 이 손의 변화를 참조할 방법은 없다. 본체측의 대응이 바람직한다!
;-------------------------------------------------
;·TFLAG:REACT파생(0=들은체 만체 한다/1=가게해 준다/2=가게해 주지 않는다)
;-------------------------------------------------
@KOJO_MALE_COM_K39_70
;-------------------------------------------------
#LOCALSSIZE 5
VARSET LOCALS
;LOCALS:0 = \@ TENSION(TARGET, 8) == "높다" ? 병아리# 나\@
LOCALS:1 '= RAND_SPLIT("출/사정")
LOCALS:2 '= RAND_SPLIT("중/질내/나카")

;━━━━━━━━━━━━━━━━━━━━━━━━
; REACT파생 2:가게해 주지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 2
	;=================================================
	;성교봉사중:정상위
	;=================================================
	IF IS_NOWACTNAME("정상위시킨다") || TEQUIP:성교봉사중 == 1
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「우훗, 그렇게\@ IS_POLICY("오늘", "자 만들기") ? %TNAME_K39()%를 임신하게 하고 싶다# 중에%LOCALS:1%하고 싶다\@의?」
			PRINTFORM 「그것은 좋지만…… 좀 더 즐겁게 해
		;그 이외
		ELSE
			PRINTFORMW 「그렇게 나의%LOCALS:2%에%LOCALS:1%하고 싶습니까?」
			PRINTFORM 「우후, 아직 안됩니다. 좀 더 즐겁게 해 주세요
		ENDIF
		;PS로 닫는다
		CALL PRINT_SYMBOL_K39
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:후배위
	;=================================================
	IF IS_NOWACTNAME("후배위시킨다") || TEQUIP:성교봉사중 == 2
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「이런 짐승 같은 모습으로,%TNAME_K39()%\@ IS_POLICY("오늘", "자 만들기") ? 에 교배한다# 안에 낸다\@생각이야?」
			PRINTFORM 「… 응, 별로 좋지만. 그렇지만, 간단하게는 가게 하지 않으니까
			CALL PRINT_SYMBOL_K39
		;그 이외
		ELSE
			PRINTFORMW 「이런 짐승 같은 모습으로, 나의%LOCALS:2%를 더럽히고 싶습니까?」
			PRINTFORMW 「추잡하네요♪그런 사람은 조금 초조하게함 여(어) 줍니다. 우훗…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:역강간
	;=================================================
	IF IS_NOWACTNAME("역강간") || TEQUIP:성교봉사중 == 3
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「그렇게 안에%LOCALS:1%하고 싶어? 그렇지만 이 몸의 자세라면…」
			PRINTFORM 「가게 하는 것도… 초조하게 하는 것도……, 모두는 나나름이라는 느낌 자라
		;그 이외
		ELSE
			PRINTFORMW 「우후, 그렇게 나의 안에%LOCALS:1%하고 싶습니까?」
			PRINTFORM 「그렇지만 간단하게는가게해 주지 않아요
		ENDIF
		CALL PRINT_SYMBOL_K39
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:대면좌위
	;=================================================
	IF IS_NOWACTNAME("대면좌위시킨다") || TEQUIP:성교봉사중 == 4
		PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 안돼…….%TNAME_K39()%, 아직 부족함%RAND_SPLIT("야/원/응이니까")% # 안됩니다…… 좀 더 즐겁게 해 주세요\@…」
		CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
		PRINTFORMDL %CALLNAME:TARGET%는%CALLNAME:MASTER%에 키스를 품질,
		PRINTFORMDW 허리를 요염하게 구불거리게 해,%CALLNAME:MASTER%를 초조하게함 시작했다…
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:배면좌위
	;=================================================
	IF IS_NOWACTNAME("배면좌위시킨다") || TEQUIP:성교봉사중 == 5
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「우훗, 그렇게\@ IS_POLICY("오늘", "자 만들기") ? %TNAME_K39()%를 임신하게 하고 싶다# 중에%LOCALS:1%하고 싶다\@의?」
			PRINTFORM 「그것은 좋지만…… 좀 더 즐겁게 해
		;그 이외
		ELSE
			PRINTFORMW 「그렇게 나의%LOCALS:2%에%LOCALS:1%하고 싶습니까?」
			PRINTFORM 「우후, 아직 안됩니다. 좀 더 즐겁게 해 주세요
		ENDIF
		CALL PRINT_SYMBOL_K39
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:애널섹스
	;=================================================
	IF IS_NOWACTNAME("애널섹스시킨다") || TEQUIP:성교봉사중 == 6
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「그렇게 엉덩이안에 사정하고 싶은거야?」
			PRINTFORM 「그렇지만, 아직 다～메. 좀 더 초조하게함 여(어) 주기 때문에
		;그 이외
		ELSE
			PRINTFORMW 「엉덩이 중(안)에서 가고 싶습니까?」
			PRINTFORM 「우후, 아직 안됩니다. 좀 더 노력해 주세요
		ENDIF
		CALL PRINT_SYMBOL_K39
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	;상사상애
	IF GET_KFLAG_K39("상사상애")
		PRINTFORMW 「우후훗, 그렇게 가고 싶은거야?」
		PRINTFORM 「그렇지만 아직 안돼
		CALL PRINT_HEARTCBW_K39(COLOR2_K39("pink"), , , COLOR2_K39("HINA"), "  좀 더 초조하게함 여(어) 준다♪」")
	;그 이외
	ELSE
		PRINTFORMW 「이제 인내 할 수 없습니까?」
		PRINTFORM 「그렇지만, 조금만 더 즐겁게 해 받아요
		CALL PRINT_SYMBOL_K39
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 1:가게해 준다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	;=================================================
	;V 성교봉사중
	;=================================================
	IF V_SEX(MASTER) || INRANGE(TEQUIP:성교봉사중, 1, 5)
		;-------------------------------------------------
		;성교봉사중:역강간
		;-------------------------------------------------
		IF IS_NOWACTNAME("역강간") || TEQUIP:성교봉사중 == 3
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				PRINTFORM 「좋아, 소망 대로…… 조임과는 준다…
				CALL PRINT_SYMBOL_K39
				PRINTFORMW 「%TNAME_K39()%의%LOCALS:2%에%LOCALS:2%시켜 주어요…」
			;그 이외
			ELSE
				PRINTFORMW 「…… 좋아요.%CNAME(MASTER, TARGET)%의 액, 전부 받아 들여 줍니다」
				PRINTFORMW 「나의 안에 마음껏 토해%LOCALS:1%해 주세요…」
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「좋아…,%LOCALS:2%에%LOCALS:1%해…… 흘러넘쳐 버릴 정도로 쏟아…」
			PRINTFORMW 「%TNAME_K39()%안, 전부%CNAME(MASTER, TARGET)%의 것으로 해…」
		;그 이외
		ELSE
			PRINTFORMW 「좋아요,%CNAME(MASTER, TARGET)%의 액, 전부 받아 들여 줍니다」
			PRINTFORMW 「나의 안에 마음껏 토해%LOCALS:1%해 주세요…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중:애널섹스
	;=================================================
	IF IS_NOWACTNAME("애널섹스시킨다") || A_SEX(MASTER)
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			PRINTFORMW 「좋아… 엉덩이안에%LOCALS:1%해…」
			PRINTFORMW 「%LOCALS:0%가 전부 받아 들여 준다…」
		;그 이외
		ELSE
			PRINTFORMW 「나의 엉덩이 중(안)에서 가고 싶습니까?」
			PRINTFORMW 「좋아요.%CNAME(MASTER, TARGET)%의 액을 쏟아 주세요」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외·조건 없음
	;-------------------------
	;상사상애
	IF GET_KFLAG_K39("상사상애")
		PRINTFORMW 「좋아…… 가게해 준다」
		PRINTFORM 「자,%CNAME(MASTER, TARGET)%의 액을 내
		CALL PRINT_SYMBOL_K39
	;그 이외
	ELSE
		PRINTFORMW 「인내 할 수 없네요…」
		PRINTFORMW 「좋아요,%CNAME(MASTER, TARGET)%의 액을 내 주세요」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 0:「흐음」
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	CALL hmm_K39
	RETURN 1
ENDIF

;*************************************************
;함수 두는 곳
;*************************************************
;HMM라면 위화감이니까 hmm...
;-------------------------------------------------
@hmm_K39
IF GET_KFLAG_K39("상사상애")
	SELECTCASE AUTORAND(2, 300)
		CASE 0
			PRINTFORMW 「흐음. 그래서, 구체적으로 나에게 어떻게 해 주었으면 한거야?」
			PRINTFORMW 「잘 모르지만」
		CASE 1
			PRINTFORMW 「앗, 그렇게! … 그래서, 그것이 어떻게든 했어?」
			CALL FADE(20, COLOR2_K39("HINA"), COLOR2_K39("black"), "「킥킥…」")
			CALL FADE_CLINE(1)
		CASE 2
			PRINTFORML 「그런 말을 들어도…, 저기? 후후…」
	ENDSELECT
ELSE
	SELECTCASE AUTORAND(2, 301)
		CASE 0
			PRINTFORMW 「그런 일 말해져도…, 정직, 곤란해 버리네요」
		CASE 1
			PRINTFORMW 「미안해요. 뭔가 말했습니까?」
			PRINTFORMW 「후후……」
		CASE 2
			PRINTFORMW 「과연.%CNAME(MASTER, TARGET)%의 말하고 싶은 일은 알았습니다만…」
			PRINTFORMW 「그것을 (들)물어 줄지 어떨지는, 또 다른 문제이예요」
			CALL FADE(20, COLOR2_K39("HINA"), COLOR2_K39("black"), "「후훗…」")
			CALL FADE_CLINE(1)
	ENDSELECT
ENDIF
