	IF GET_KFLAG_K39("相思相愛")
		SELECTCASE RANDDATUI(3, 60)
			CASE 0
				PRINTFORMW 「下着姿の方がむしろ興奮するって？」
			CASE 1
				PRINTFORMW 「ねぇ、このブラ可愛いと思わない？」
			CASE 2
				PRINTFORMW 「新しいの付けてみたんだけど、気が付いてる？」
		ENDSELECT
	ELSE
		PRINTFORMW 「人によっては裸より下着姿の方が興奮するらしいですが」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%はどうなんでしょうね？　うふふ」
	ENDIF
	RETURN 1
ENDIF

