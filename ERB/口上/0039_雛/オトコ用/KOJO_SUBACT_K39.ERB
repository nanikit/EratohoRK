;========================================================================
;調教アクション・コマンド口上
;========================================================================

;────────────────────────────────────
;脱衣アクション口上
;────────────────────────────────────
;・調教者アクションの前に強制脱衣や自主脱衣が発生したときに表示
;・この口上の時点ではまだTEQUIPは更新されていない
;・初回分岐は「初めて見た/見せた」とは限らない。
;　ショップメニューで脱がせたことがあるかもしれないし、調教の流れで完全に脱がせずに露出だけしている場合もある。
;　もしかすると助手だったときに見たことがあるかもしれない。そういう意味では簡易的なものである。
;　厳密な意味での「初見」フラグを用意するかどうかは要望次第ということで。
;-------------------------------------------------
@KOJO_MALE_DATUI_K39
;-------------------------------------------------
;関連変数…
;強制脱衣：TFLAG:102　自主脱衣：TFLAG:103
;=(-1, 強制脱衣に抵抗、1, 上着、2, 下半身上着、3, 上衣、4, 上半身全部、5, 下半身全部、
;　6, アクセサリー、7, アクセサリー以外全て、8, 全裸まで、9, 下着だけ)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;強制脱衣に抵抗された FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 < 0
	;=================================================
	;初回
	;=================================================
	IF FIRSTDATUI(0)
		PRINTFORMW 「そんなに脱ぐのが嫌\@ !GET_KFLAG_K39("相思相愛") ? ですか # \@？」
		RETURN 1
	ENDIF
	
	;=================================================
	;自分は脱いだ
	;=================================================
	IF TFLAG:自主脱衣
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「まぁ別にいいけど」
			PRINTFORMW 「私は脱がさせて貰うわね」
		ELSE
			PRINTFORMW 「仕方無いですね」
			PRINTFORMW 「とりあえず私は脱がさせてもらいますけど」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF GET_KFLAG_K39("相思相愛")
		PRINTFORMW 「服着たままシたいの？」
	ELSE
		PRINTFORMW 「私の前で脱ぐのは嫌ですか？」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;二人とも何か脱いだ FIRSTDATUI(10)
;------------------------------------------------
;・主にセックス系行動の予備動作で発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 > 0 && TFLAG:自主脱衣 && !IS_NOWASSIACTNAME(, "脱衣")
	IF GET_KFLAG_K39("相思相愛")
		PRINTFORMW 「ねぇ、お互いに見せっこしてみる？　クスクス」
	ELSE
		PRINTFORMW 「準備はいいみたいですね？」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;自主脱衣・アクセサリーのみ FIRSTDATUI(20)
;------------------------------------------------
;・主に靴下。他には手袋や袖など
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:自主脱衣 == 6 && !IS_NOWASSIACTNAME(, "脱衣") && !TEQUIP:アイマスク
	;=================================================
	;全裸になった（脱ぐ前は靴下以外裸とかそういう感じ）
	;=================================================
	IF TFLAG:自主脱衣 && ZENRACHECK(0)
		IF GET_KFLAG_K39("相思相愛")
			PRINTFORMW 「ほら、もっと良く見て」
			PRINTFORMW 「あなたには全部見て欲しいの…」
		ELSE
			PRINTFORMW 「私の体どうですか？」
			PRINTFORMW 「もっと良く見ていいんですよ♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 「これはもう要\@ GET_KFLAG_K39("相思相愛") ? らないわね # りませんね \@」
	RETURN 1
ENDIF

