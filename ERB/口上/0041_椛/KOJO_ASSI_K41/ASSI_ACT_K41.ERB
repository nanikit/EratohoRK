;────────────────────────────────────
;AACT71, 薬品使用
;────────────────────────────────────
@KOJO_ACT_ASSI_K41_71
;-------------------------------------------------
IF 口上記入チェック(100)
	;=================================================
	;媚薬使用
	;=================================================
	IF 口上記入チェック(100) && GETBIT(TFLAG:媚薬等使用, 1)
		;口上主がご主人様
		IF TALENT:ASSI:ご主人様
			PRINTFORMW 「お薬、失礼しますね」
			PRINTFORMW 「自分で飲みますか？　それとも私が飲ませますか？」
		;普通の助手
		ELSE
			PRINTFORMW 「わ、私が口移しでですか！？」
			PRINTFORMW 「それってつまりキス……になりますよね」
			PRINTFORMW 「い、いえ。もちろんちゃんとやりますけど」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ローション使用
	;=================================================
	IF 口上記入チェック(100) && GETBIT(TFLAG:媚薬等使用, 0)
		;口上主がご主人様
		IF TALENT:ASSI:ご主人様
			PRINTFORMW 「では、私はこれを塗りますね」
			PRINTFORMW 「行きますよー。ぬるぬる～」
		;普通の助手
		ELSE
			PRINTFORMW 「これを塗ってあげればいいのですね？」
			PRINTFORMW 「分かりました。ぬるぬる……と」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

