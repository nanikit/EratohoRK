;-------------------------------------------------
;대요정 구상 숍 이벤트 구상 파일
;-------------------------------------------------
;숍으로부터 불려 가는 구상을 정리한 파일입니다
;쓰지 않아도 괜찮을지도.
;-------------------------------------------------
;*************************************************
;숍 이벤트 구상
;*************************************************

;=================================================
;EV101, 의상 변경
;=================================================
;·숍으로부터 의상 변경을 실시했을 경우에 표시
;-------------------------------------------------
@KOJO_EVENT_K4_101(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=
;  1의 위…(0, 이제(벌써) 입고 있다, 1, 갈아입은, 2, 부끄러워서 입을 수 없는, 3, 싫어해 입지 않는, 9, 가지고 있지 않다)
;  10의 위…(0, 통상, 10, 반나체, 20, 전라, 30, 속옷 없음)
;  백의 위… 코스프레를 위해서(때문에) 보류
;  천의 위…(0, 조교자, 1000, 조교 대상)
;  예(조교 대상이 부끄러워해전라로 할 수 없었다=1022)
;-------------------------------------------------

;-------------------------------------------------
;조교자가 갈아입었다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기
	;통상이 되었다
	IF ARG == 1
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	
	;반나체가 되었다
	IF ARG == 11
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	
	;전라가 되었다
	IF ARG == 21
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	
	;…… 스으스으 한다
	IF ARG == 31
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교자가 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기
	;반나체라도 부끄럽다
	IF ARG == 12
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;평상시부터전라는 조금……
	IF ARG == 22
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;팬츠가 없기 때문에 부끄러운 걸!
	IF ARG == 32
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교자가 싫어해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG < 1000
;소분기
	;반나체는 싫어
	IF ARG == 13
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;평상시부터전라는 싫어
	IF ARG == 23
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;스으스으 하는 것은 싫어
	IF ARG == 33
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상을 갈아입게 했다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;통상이 되었다
	IF ARG == 1001
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;반나체가 되었다
	IF ARG == 1011
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;전라가 되었다
	IF ARG == 1021
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;…… 스으스으 한다
	IF ARG == 1031
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상이 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;반나체라도 부끄럽다
	IF ARG == 1012
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;평상시부터전라는 부끄럽다
	IF ARG == 1022
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;팬츠가 없기 때문에 부끄러운 걸!
	IF ARG == 1032
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;조교 대상이 싫어해 갈아입을 수 없다
;-------------------------------------------------
;기입 체크
LOCAL = 0
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;소분기
	;반나체는 싫어
	IF ARG == 1013
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;평상시부터전라는 싫어
	IF ARG == 1023
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
	;스으스으 하는 것은 싫어
	IF ARG == 1033
		PRINTFORMW 
		PRINTFORMW 
		RETURN 2
	ENDIF
ENDIF

;=================================================
;EV102, 조교자 변경
;=================================================
;·조교자를 교대시켰을 때에 표시
;-------------------------------------------------
@KOJO_EVENT_K4_102(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 구상주가 다른 캐릭터에게 맡긴, 1, 구상주가 조교자가 되었다)
;구조교자는 「%CALLNAME:SHOP_OLDTARGET%」, 신조교자는 「%CALLNAME:SHOP_NEWTARGET%」로 참조할 수 있다
;-------------------------------------------------

;-------------------------------------------------
;다른 조교자에게 교대
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 0
	IF FAVOR(0, 2000)
		PRINTFORMW 「후훗, 나만이%CNAME(MASTER)%의 정령을 독점으로 하는 것도 좋지 않은 거네」
		PRINTFORMW 「그렇지만 분명하게, 나의 몫은 남겨 두어 주면 기쁘구나……」
		PRINTFORMW 「그러면%CALLNAME:SHOP_NEWTARGET%, 잘 부탁드릴게요♪  로 원으로 원♪」
	ELSE
		PRINTFORMW 「%CALLNAME:SHOP_NEWTARGET%에 폐를 끼치지 않게 부탁드릴게요」
		PRINTFORMW 「%CALLNAME:SHOP_NEWTARGET%도, 너무 그다지 의욕에 넘치지 말아 주세요」
		PRINTFORMW 「내가 받는 분을 남겨 두어 받을 수 있으면…… 기쁩니다」
	ENDIF
	RETURN 2
ENDIF

;-------------------------------------------------
;대요정님이 조교자가 되었다
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 1
	IF FAVOR(0, 2000)
		PRINTFORMW 「후훗…… 기쁜♪  또%CNAME(MASTER)%와 즐거운 일이 생기는 것이군」
		PRINTFORMW 「%CNAME(MASTER)%의 일은, 매우 마음에 드는거야?   알아?」
		PRINTFORM 「많이, 귀여워해 올리니까요～
		CALL HEARTCB
		PRINTW 」
	ELSE
		PRINTFORMW 「후훗……%CNAME(MASTER)%를 마음대로 할 수 있는 것이군요…… 기쁩니다♪」
		PRINTFORMW 「곧바로…… 나의 것으로 해 주네요♪」
	ENDIF
	RETURN 2
ENDIF

;────────────────────────────────────
;EV110, 마력 사용
;────────────────────────────────────
;·마력을 소비해 어떠한 특전을 구입했을 때에 표시
;-------------------------------------------------
@KOJO_MS_EVENT_K4_110(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 마력 사용전, 1, 조교 기교, 2, 지식을 얻기 전, 3, 약학지식 취득, 4, 매혹 취득, 5, 금단의지식 취득,
;     6, 도구사용 취득, 7, 기술을 얻기 전, 8, 혀사용 취득, 9, 기묘한손가락취득, 10, 결박능숙,
;     11, 새드 소거/새드 취득, 12, 불결무시 취득, 38, 착의플레이선호, 39, 허리사용)
;
;이벤트 구상: 아이템
;인수 내역…
