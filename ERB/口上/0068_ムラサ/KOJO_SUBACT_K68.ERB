;========================================================================
;調教アクション・コマンド口上
;========================================================================

;────────────────────────────────────
;脱衣アクション口上
;────────────────────────────────────
;・調教者アクションの前に強制脱衣や自主脱衣が発生したときに表示
;・この口上の時点ではまだTEQUIPは更新されていない
;・初回分岐は「初めて見た/見せた」とは限らない。
;　ショップメニューで脱がせたことがあるかもしれないし、調教の流れで完全に脱がせずに露出だけしている場合もある。
;　もしかすると助手だったときに見たことがあるかもしれない。そういう意味では簡易的なものである。
;　厳密な意味での「初見」フラグを用意するかどうかは要望次第ということで。
;-------------------------------------------------
@KOJO_DATUI_K68
;-------------------------------------------------
;関連変数…
;強制脱衣：TFLAG:102　自主脱衣：TFLAG:103
;=(-1, 強制脱衣に抵抗、1, 上着、2, 下半身上着、3, 上衣、4, 上半身全部、5, 下半身全部、
;　6, アクセサリー、7, アクセサリー以外全て、8, 全裸まで、9, 下着だけ)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;強制脱衣に抵抗された FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 < 0
	PRINTL
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTDATUI(0)
		PRINTFORMW 「あら、そんなに嫌がらなくても」
		PRINTFORMW 「そのままだと、服が汚れてしまいますよ？」
		PRINTL
		RETURN 1
	ENDIF
	
	;=================================================
	;自分は脱いだ
	;=================================================
	IF 口上記入チェック(100) && TFLAG:自主脱衣
		;ぱーふぇくと両想い
		IF KFLAG:700
			PRINTFORMW 「私は脱いでるのにそんなに嫌がらなくても～」
			PRINTFORMW 「わざと焦らしてるのね？このいけずぅ」
			PRINTL
			RETURN 1
		ENDIF
		
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「うーん、そんなに嫌がらなくても」
			PRINTFORMW 「服を脱いで誘っている女性を前にして」
			PRINTFORMW 「美味しい状況だ、と言う風には思ってくれないのですかね？」
		;アライメント負
		ELSE
			PRINTFORMW 「ふーん…まぁ、やる事は変わらないし」
			PRINTFORMW 「汚れ塗れの臭い服を着て生活したいのなら止めないですよ」
		ENDIF
		PRINTL
		RETURN 1
	ENDIF
	
	;=================================================
	;ぱーふぇくと両想い
	;=================================================
	IF 口上記入チェック(100) && KFLAG:700
		PRINTFORMW 「えー？どうして嫌がるんですかー？」
		PRINTFORMW 「服、脱ぎたくないの？もしかして着たまましたい…とか？」
		PRINTFORMW 「%CALLNAME:MASTER%がしたいのなら、私はそれでもいいですけどね」
		PRINTL
		RETURN 1
	ENDIF
	
	;アライメント正
	IF ALI(0)
		PRINTFORMW 「だから、服が汚れてしまいますよ？」
		PRINTFORMW 「%CALLNAME:MASTER%にもこの環境を早く受け入れて欲しいものですね」
	;アライメント負
	ELSE
		PRINTFORMW 「ふーん…まぁ、やる事は変わらないし」
		PRINTFORMW 「汚れ塗れの臭い服を着て生活したいのなら止めないですよ」
	ENDIF
	PRINTL
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;二人とも何か脱いだ FIRSTDATUI(1)
;------------------------------------------------
;・主にセックス系行動の予備動作で発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 > 0 && TFLAG:自主脱衣 && !IS_NOWASSIACTNAME(, "脱衣")
	PRINTL
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTDATUI(1)
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「はい、早く%CALLNAME:MASTER%も脱いで」
			PRINTFORMDW ムラサは自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
			PRINTFORMW 「さぁ、一緒に気持ち良くなりましょう？」
		;アライメント負
		ELSE
			PRINTFORMW 「ほら、早く%CALLNAME:MASTER%も脱ぎなさい」
			PRINTFORMDW ムラサは自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
			PRINTFORMW 「あまり待たせると苛めてしまうわよ？」
		ENDIF
		PRINTL
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者が[情欲]状態
	;=================================================
	IF 口上記入チェック(100) && T_COND("情欲")
		;ぱーふぇくと両想い
		IF KFLAG:700
			PRINTFORMW 「ねぇ、%CALLNAME:MASTER%も早く脱いで、早く早く♪」
			PRINTFORMDW 水蜜は自分の衣服を勢い良く脱ぎ捨てながら、%CALLNAME:MASTER%を急かす
			PRINTFORMW 「早くいっぱいしてね、待ちきれないわ♪」
			PRINTL
			RETURN 1
		ENDIF
		
		;アライメント正
		IF ALI(0)
			PRINTFORMW 「早く%CALLNAME:MASTER%も脱いで、なるべく急いでね？」
			PRINTFORMDW ムラサは手早く自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
			PRINTFORMW 「私、もう我慢できないわ…早く気持ち良くしてね？」
		;アライメント負
		ELSE
			PRINTFORMW 「ふふ、欲情してしまったわ…ほら、早く%CALLNAME:MASTER%も脱ぎなさい」
			PRINTFORMDW ムラサは自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
			PRINTFORMW 「満足させてくれないと苛めてしまうわよ？」
		ENDIF
		PRINTL
		RETURN 1
	ENDIF
	
	;=================================================
	;ぱーふぇくと両想い
	;=================================================
	IF 口上記入チェック(100) && KFLAG:700
		PRINTFORMW 「ふふっ、%CALLNAME:MASTER%も早く脱いで♪」
		PRINTFORMDW 水蜜は自分の衣服を脱ぎ捨て、%CALLNAME:MASTER%が服を脱ぐのを手伝った
		PRINTFORMW 「気持ち良い事いっぱいしようね♪」
		PRINTL
		RETURN 1
	ENDIF
	
	;アライメント正
	IF ALI(0)
		PRINTFORMW 「はい、早く%CALLNAME:MASTER%も脱いで」
		PRINTFORMDW ムラサは自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
		PRINTFORMW 「さぁ、一緒に気持ち良くなりましょう？」
	;アライメント負
	ELSE
		PRINTFORMW 「ほら、早く%CALLNAME:MASTER%も脱ぎなさい」
		PRINTFORMDW ムラサは自分の衣服を脱ぎながら、%CALLNAME:MASTER%にそう告げる
		PRINTFORMW 「あまり待たせると苛めてしまうわよ？」
	ENDIF
	PRINTL
	RETURN 1
ENDIF

