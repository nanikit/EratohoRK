		RETURN 1
	ENDIF
	
	;=================================================
	;전라가 되었다(벗기 전은 양말 이외 알몸이라든지 그러한 느낌)
	;=================================================
	IF 구상기입체크(0) && ZENRACHECK(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

