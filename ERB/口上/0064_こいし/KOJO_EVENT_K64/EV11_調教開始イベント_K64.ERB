	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(0) && FIRSTEVENT(120)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;初回以外
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

