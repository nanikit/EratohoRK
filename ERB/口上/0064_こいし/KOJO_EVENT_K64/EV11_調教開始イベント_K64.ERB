	;=================================================
	IF 口上記入チェック(0) && FIRSTEVENT(70)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;初回以外
	;=================================================
	IF 口上記入チェック(0)
		;イマイチな感じになってきている…
		;ちょっと考えよう
		SELECTCASE RANDEVENT(2, 70)
			CASE 0
				PRINTFORMW 「うふふ…今日はアレ、何回やろうかな～」
				PRINTFORMDW そう言いながらこいしは小さな象牙色の多面体──サイコロのようなものを転がしている
				PRINTFORMW 「これで決めてみる？」
				PRINTFORMDW 差し出されたサイコロらしき立方体の各面にはそれぞれ 貳、惨、伍、漆、拾壹、拾惨 と書かれていた…
			CASEELSE
				CALL K64_EV11_恐怖_トラウマ分岐()
				RETURN RESULT
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

