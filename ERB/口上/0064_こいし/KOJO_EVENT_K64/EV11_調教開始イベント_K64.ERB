IF 口上記入チェック(0) && CFLAG:調教回数 == 10 && !ARG && TALENT:TARGET:ご主人様
	;=================================================
	;記念イベント中止のお知らせ
	;=================================================
	IF 口上記入チェック(0) && ARG
		IF ARG == 1		;疲れているのは口上主
			PRINTFORMW 
		ELSE			;疲れているのは調教対象
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

