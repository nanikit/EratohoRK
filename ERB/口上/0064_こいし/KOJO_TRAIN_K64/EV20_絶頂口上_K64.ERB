	;=================================================
	;同時絶頂(調教対象が同時にＶ絶頂)
	;=================================================
	IF 口上記入チェック(0) && NOWEX:MASTER:Ｖ絶頂
		SELECTCASE NOWEX:TARGET:射精	;調教者射精種による分岐
			CASE 1						;空射精
				PRINTFORMW 
			CASEELSE					;その他
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;中に出して！
	;=================================================
	IF 口上記入チェック(0) && IS_NOWCOMNAME("中に出して！")
		SELECTCASE NOWEX:TARGET:射精	;調教者射精種による分岐
			CASE 1						;空射精
				PRINTFORMW 
			CASEELSE					;その他
				IF RAND:2	;仮置き
					PRINTFORMW 「あら、いいの？ 本当にいいの？ 中に、中に出しちゃうんだよ？」
					PRINTFORMW 「中出し…膣内射精…ぞくぞくしちゃう♪ あ、ぞくぞく来たぁ♪」
				ELSE
					PRINTFORMW 「うふ、うふふ…ご主人様の精子をおねだりなんてイケナイ子ね…♪」
					PRINTFORMW 「いま、%K64_CNAME()%の欲張りおまんこも躾けなおしてあげる…」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;中に出さないで！
	;=================================================
	IF 口上記入チェック(0) && IS_NOWCOMNAME("中に出さないで！")
		SELECTCASE NOWEX:TARGET:射精	;調教者射精種による分岐
			CASE 1						;空射精
				PRINTFORMW 
			CASEELSE					;その他
				IF RAND:2	;仮置き
					PRINTFORMW 「ちょっ、そんなに締め付けたらわたし…あ！」
					PRINTFORMW 「───っ、と。 ふぅ、出ちゃった♪ ごめんね？」
				ELSE
					PRINTFORMW 「まったく、ペットのくせにあーしてこーしてとナマイキだわ…」
					PRINTFORMW 「ほら、ご主人様の精子、ありがたく受け取りなさい！」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;条件から漏れたものはここで処理
	;---------------------------------------------
	SELECTCASE NOWEX:TARGET:射精	;調教者射精種による分岐
		CASE 1						;空射精
			PRINTFORMW 
		CASEELSE					;その他
			PRINTFORMW 
	ENDSELECT
	RETURN 1
ENDIF

