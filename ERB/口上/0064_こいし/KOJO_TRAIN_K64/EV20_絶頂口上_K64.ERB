	IF 구상기입체크(0) && (IS_NOWACTNAME("애널섹스") || TEQUIP:TARGET:성교중 == 6) && (TEQUIP:TARGET:조교자페니스밴드 || MA_PLAYER(1) == 103)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;애널바이브로 절정
	;=================================================
	IF 구상기입체크(0) && MA_PLAYER(1) == 111
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;관장기＋플러그로 절정
	;=================================================
	IF 구상기입체크(0) && MA_PLAYER(1) == 132
		;-------------------------------------------------
		;배설시에 절정
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWACTNAME("관장세트") && TFLAG:ACT파생 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;COM:도구를제외한다로 절정
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("도구를제외한다")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;---------------------------------------------
		;그 외
		;---------------------------------------------
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교 대상Ｂ절정
;*************************************************
;·조교 대상이Ｂ절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상Ｂ절정, NOWEX:MASTER:Ｂ절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:Ｂ절정
	;=================================================
	;가슴의 몸싸움으로 조교자와 조교 대상 동시절정
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("가슴애무") && TFLAG:ACT파생 == 2) && NOWEX:TARGET:Ｂ절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;젖의 몸싸움으로 조교 대상만 절정(가설)
	;=================================================
	IF 구상기입체크(0) && IS_NOWACTNAME("가슴애무") && TFLAG:ACT파생 == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;동시에 분유
	;=================================================
	;·NOWEX:TARGET:분유의 값(1. 드라이분유/분유, 2. 대량분유)
	;·Ｂ절정시, 모유체질이면 우선 분출한다
	;·다른 장소에 집어 넣어 사용하는지, 이 안에 분기를 마련해야 하는 것인지도 모른다
	;·BASE:MASTER:모유 < 50 + RAND:5 * 10이 드라이분유의 조건
	;·NOWEX:MASTER:분유에서는 구별이 붙지 않기 때문에, STR:1035로 판정
	;·만약↑이 변변치않을 때는 DOWNBASE:MASTER:모유 < 300으로 드라이분유/분유의 구별을 할 수 있을지도
	;-------------------------------------------------
	IF 구상기입체크(0) && NOWEX:MASTER:분유
		;분유종에 의한 분기. 그렇지만 드라이분유라든지 보통으로 나올까?
		SELECTCASE STR:1035
			CASE "드라이분유"
				PRINTFORMW 
			CASE "분유"
				PRINTFORMW 
			CASE "대량분유"
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;자위로Ｂ절정
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에 의한 애무로 절정(유두 들이마셔)
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("가슴애무") && TFLAG:ACT파생 == 1) || MB_PLAYER(1) == 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;REACT분류와 SELECTCOM 분기를 설치
	;현재 상태로서는 TARGET가 후타나리, 남자 이외의 경우는 ACT_ABLE로 차지고 있기 때문에 조교자페니스밴드를 고려할 필요는 없다
	;에서도, @ACT_ABLE52의 판정에 조교자페니스밴드를 넣어도 좋은 것 같은 생각은 든다
	;=================================================
	;페니스에 의한 애무로 절정(파이즈리)
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("파이즈리") || MB_PLAYER(1) == 3)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;니플캡으로 절정
	;=================================================
	IF 구상기입체크(0) && (MB_PLAYER(1) == 113 || TEQUIP:TARGET:니플캡)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;새어 오면 여기서 처리
	;---------------------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교자Ｃ절정
;*************************************************
;·조교자가Ｃ절정 하는 경우에 표시
;·여기에서는 「여자 아이의Ｃ절정」을 상정하고 있다
;·사정은 사정 구상의 분기에서 쓰는 것이 바람직함
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교자Ｃ절정, NOWEX:TARGET:Ｃ절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:TARGET:Ｃ절정
	;=================================================
	;다중절정
	;=================================================
	IF 구상기입체크(0) && (NOWEX:TARGET:Ｖ절정 || NOWEX:TARGET:Ａ절정 || NOWEX:TARGET:Ｂ절정)
		IF NOWEX:TARGET:Ｖ절정 && NOWEX:TARGET:Ａ절정 && NOWEX:TARGET:Ｂ절정	;Ｃ+ＶＡＢ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ｖ절정 && NOWEX:TARGET:Ａ절정						;Ｃ+ＶＡ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ｖ절정 && NOWEX:TARGET:Ｂ절정						;Ｃ+ＶＢ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ａ절정 && NOWEX:TARGET:Ｂ절정						;Ｃ+ＡＢ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ｖ절정												;Ｃ+Ｖ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ａ절정												;Ｃ+Ａ절정
			PRINTFORMW 
		ELSE																	;Ｃ+Ｂ절정
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(0) && TEQUIP:TARGET:성교봉사중
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;자위로Ｃ절정 bit1로 조교자
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에 의한 애무로 절정(쿤닐링구스 따위)
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("입으로애무를강제") && GROUPMATCH(TFLAG:ACT파생, 0, 2)) || TC_PLAYER(1) == 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;TC_PLAYER라면 조교 대상의 페니스밴드를 주울 수 없어서 ACT & ACT파생인 만큼 한다
	;=================================================
	;페니스, 혹은 페니스밴드에 의한 애무로 절정
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("성기를서로켠다") && GROUPMATCH(TFLAG:ACT파생, 1, 2))
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조개 겹옷으로 절정
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("성기를서로켠다") && !TFLAG:ACT파생) || TC_PLAYER(1) == 8)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;여기까지 떨어져 오지 않는 것이 바람직하지만, 가능성을 고려하지 않을 수도 없다
	;모든 조건을 빠져 버렸을 경우의 최후의 보루적인 개소
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교자Ｖ절정
;*************************************************
;·조교자가Ｖ절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교자Ｖ절정, NOWEX:Ｖ절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:TARGET:Ｖ절정
	;=================================================
	;동시에Ａ절정/Ｂ절정(다중절정)
	;=================================================
	IF 구상기입체크(0) && (NOWEX:TARGET:Ａ절정 || NOWEX:TARGET:Ｂ절정)
		IF NOWEX:TARGET:Ａ절정 && NOWEX:TARGET:Ｂ절정	;Ｖ+ＡＢ절정
			PRINTFORMW 
		ELSEIF NOWEX:TARGET:Ａ절정						;Ｖ+Ａ절정
			PRINTFORMW 
		ELSE											;Ｖ+Ｂ절정
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;자위로Ｖ절정(바이브) 바이브를 소지, 한편 처녀가 아닌 경우는 발생한다
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교 봉사로Ｖ절정
	;=================================================
	IF 구상기입체크(0) && V_SEX(MASTER)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교자Ａ절정
;*************************************************
;·조교자가Ａ절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교자Ａ절정, NOWEX:TARGET:Ａ절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:TARGET:Ａ절정
	;=================================================
	;동시에Ｂ절정(다중절정)
	;=================================================
	IF 구상기입체크(0) && NOWEX:TARGET:Ｂ절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마애널로 절정
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("입으로애무를강제") && TFLAG:ACT파생 == 3) || TA_PLAYER(1) == 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;Ａ성교 봉사로
	;=================================================
	IF 구상기입체크(0) && A_SEX(MASTER)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;샌 것은 여기서 처리
	;---------------------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교자Ｂ절정
;*************************************************
;·조교자가Ｂ절정 하는 경우에 표시
;-------------------------------------------------
;·손으로애무 강제 분기를 추가해야 할?
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교자Ｂ절정, NOWEX:TARGET:Ｂ절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:TARGET:Ｂ절정
	;=================================================
	;동시에 분유
	;=================================================
	;·NOWEX:TARGET:분유의 값(1. 드라이분유/분유, 2. 대량분유)
	;·Ｂ절정시, 모유체질이면 우선 분출한다
	;·다른 장소에 집어 넣어 사용하는지, 이 안에 분기를 마련해야 하는 것인지도 모른다
	;·BASE:TARGET:모유 < 50 + RAND:5 * 10이 드라이분유의 조건
	;·NOWEX:TARGET:분유에서는 구별이 붙지 않기 때문에, STR:1045로 판정
	;·만약↑이 변변치않을 때는 DOWNBASE:TARGET:모유 < 300으로 드라이분유/분유의 구별을 할 수 있을지도
	;-------------------------------------------------
	IF 구상기입체크(0) && NOWEX:TARGET:분유
		;분유종에 의한 분기. 그렇지만 드라이분유라든지 보통으로 나올까?
		SELECTCASE STR:1045
			CASE "드라이분유（조교자）"
				PRINTFORMW 
			CASE "분유（조교자）"
				PRINTFORMW 
			CASE "대량분유（조교자）"
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;후배위/대면좌위/배면좌위시킨다로Ｂ절정
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("후배위시킨다/대면좌위시킨다/배면좌위시킨다") || GROUPMATCH(TEQUIP:TARGET:성교봉사중, 2, 4, 5))
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;자위로Ｂ절정
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에 의한 애무로 절정 ACT57 파생 2를 상정
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("가슴애무강제") && TFLAG:ACT파생 == 2) || TB_PLAYER(1) == 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;페니스에 의한 애무로 절정(파이즈리 따위) ACT17는 페니스밴드에 대해서는 발생하지 않기 때문에 페니스만 참조
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("파이즈리한다") || TB_PLAYER(1) == 3)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;샌 것은 여기서 처리
	;---------------------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교 대상 실금
;*************************************************
;·조교 대상이 실금 하는 경우에 표시
;·조교 대상으로[소변버릇]이 없으면 발생하지 않는다
;·Ｃ절정없이 실금만이라고 하는 것은 드물게 있는 케이스
;-------------------------------------------------
;·NOWEX:MASTER:방뇨의 값은(1. 실금) 마셔. 대량 실금이라든지는 없다
;·소변욕은 기본적으로는Ｃ절정/사정/쾌Ｃ로 정해진다. 그 때문에, Ｃ절정도 사정도 없게 실금 하는 것은 있다
;·다른 요인으로서 영양제와 미약을 들 수 있다. 이것들을 마셨다(사용되었다)라고 하고도 소변욕은 증가하므로, 여기서 한계 돌파의 가능성도
;·다음에 각처에 실금 분기를 들어갈 수 있을지도 모르지만 매우 대단한 듯함
;·우선 여기에는 다리 호된 훈련이라든지 페라라든지 여러 가지 들어갈 수 있어 보자. 주인님과 배큐엄 카의 구별이 되지 않는 애완동물에게 무자비한(ry
;·노력한 곳에서 소변버릇 없으면 전멸인 것이 조금 저것이지만 말야
;·콤비 네이션은 우선 보류
;·자위 얼굴에 사정하기 사정시라든지 페라시는 FLAG:지문제어 세워 지문 날조 해 대응하는 것. 좋다?
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 실금, NOWEX:MASTER:방뇨
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:방뇨
	;=================================================
	;성교봉사중에…
	;=================================================
	IF 구상기입체크(0) && (IS_NOWACTNAME("역강간/정상위시킨다/후배위시킨다/대면좌위시킨다/배면좌위시킨다/애널섹스시킨다") || TEQUIP:TARGET:성교봉사중)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에 의한 애무중에…
	;=================================================
	IF 구상기입체크(0) && ((IS_NOWACTNAME("입으로애무") && !TFLAG:ACT파생) || MC_PLAYER(1) == 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;다리에 의한 애무중에…(풋잡)
	;=================================================
	IF 구상기입체크(0) && GET_ACTNAME(GET_NORMALACTNUM(TFLAG:ACT)) == "풋잡한다"
		;-------------------------------------------------
		;ACT가 콤비 네이션에 파생(조수 협력시)
		;-------------------------------------------------
		;IF 구상 기입 체크(0) && ASSI:1 >= 0 && TCVAR:(ASSI:1):조수방침 == GET_ASSIMENUNUM("콤비 네이션")
		;	PRINTFORMW 
		;	RETURN 1
		;ENDIF
		
		;-------------------------------------------------
		;ACT가 콤비 네이션은 아니다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWACTNAME("풋잡한다")
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;-----------------------------------
	;그 외를 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF
;확실히 방뇨만의 경우에, 지문을 회피하는 잠정 처리
IF NOWEX:MASTER:방뇨 && MASTER_EX() == 4096
	;절정시의 지문이 나와 버리므로 응급 처치
	FLAG:지문제어 = 1
	RETURN 0
ENDIF
