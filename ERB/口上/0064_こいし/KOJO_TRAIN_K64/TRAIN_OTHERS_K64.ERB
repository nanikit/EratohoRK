;─
;EV21, 대만족 구상
;─
;·조교자의 대만족 보너스를 취득했을 경우에 표시
;-------------------------------------------------
;=================================================
@KOJO_EVENT_K64_21(ARG)
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━━
;진하고님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크()
	;=================================================
	;붙임성좋게응답한다
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("붙임성좋게응답한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을받아들인다/기분좋다！
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을받아들인다/기분좋다！")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;안에내！
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("안에내！")
		;분기가 필요하구나… 우선 다음에
		;어느 쪽으로 냈으면 좋은 것인지, 사정했는지, 양은 어떤가
		CALL HTMLPRINTW(@"「후우…%K64_SYMBOL("BHeartW")%」")
		PRINTFORMW 「%K64_CNAME()%의 보지, 굉장히 기분 좋았어요♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;비명을올린다
	;=================================================
	IF 구상기입체크() && IS_NOWCOMNAME("비명을올린다")
		PRINTFORMW 「지금의 울음 소리, 굉장히 좋았어요」
		PRINTFORMW 「나, 코훈 해 버렸다♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;애정을담아준다
	;=================================================
	IF 구상기입체크() && IS_NOWCOMNAME("애정을담아준다")
		CALL HTMLPRINTW(@"「와─이♪아래층─응%K64_SYMBOL("BHeartW")%」")
		RETURN 1
	ENDIF
	
	;SELECTCOM == 15는 상층에서 처리
	;=================================================
	;REACT분류가 적극적 따른다(적극적한다, 하는대로 등)
	;=================================================
	IF 구상기입체크(0) && IS_COMGRONAME("적극적 따른다/받아들인다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;그 외를 처리
	;---------------------------------------------
	SELECTCASE RANDEVENT(2, 0)
		CASE 0
			PRINTFORMW 「이건이나 보고 인화성이 되어 버리네요」
		CASEELSE
			PRINTFORMW 「응, 좋은 느낌이군요♪」
			PRINTFORMW 「우후후,%K64_CNAME()%도 나의 기쁘게 하는 방법이 알게 되었을까 ♪」
	ENDSELECT
	RETURN 1
ENDIF


;─
;EV22, 상태변화 구상
;─
;·상태 이상이 붙었을 때에 표시
;·TCVAR:상태변화가 조교자, TCVAR:MASTER:상태변화가 조교 대상
;·상태변화가 일어나면 3 턴 지속한다
;-------------------------------------------------
;=================================================
@KOJO_EVENT_K64_22(ARG)
;=================================================

;*************************************************
;EV22_0, 조교자 상태변화
;*************************************************
;T_COND TARGET가 지정한 상태라면 진을 돌려준다
;------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;진하고님[피폐]화 FIRSTEVENT(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·소모시    피로를 위해 소극적 행동이 우선(육체적·미만)
;------------------------------------------------
IF 구상기입체크() && T_COND("피폐")
	;=================================================
	;첫회(반드시 표시)
	;=================================================
	IF 구상기입체크() && FIRSTEVENT(0)
		PRINTFORMW 「어, 어?」
		PRINTFORMW 「미안, 뭔가 나 지치고 있는 것 같다…」
		PRINTFORMW (피로라든지 의식한 적 없었는데―)
		RETURN 2
	ENDIF
	
	;-----------------------------------
	;그 외(25%)
	;-----------------------------------
	IF 구상기입체크(25)
		SELECTCASE RANDEVENT(3, 0)
			CASE 0
				PRINTFORMW 「정말로…… 손이 걸리는 애완동물이예요…」
				PRINTFORMDW 진하고는 난폭한 숨을 내쉬어, 어려울 것 같은 얼굴로 이쪽을 보고 있다
			CASE 1
				PRINTFORMW 「하아… 오늘은 온천에라도 잠겨,\@ GET_TIME() == "낮" ? 낮잠 해 # 빨리 잠 \@없으면 안 돼요…」
			CASEELSE
				PRINTFORMW 「……… 지쳤다」
				PRINTFORMW 「지쳤어요. 어떻게든 해요」
		ENDSELECT
		RETURN 2
	ENDIF
	RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;진하고님[쇠약]화 FIRSTEVENT(1)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·강소모시  피로를 위해 소극적 행동이 우선(육체적·강)
;------------------------------------------------
IF 구상기입체크() && T_COND("쇠약")
	;=================================================
	;조교내 첫회(반드시 표시)
	;=================================================
	IF 구상기입체크() && ONCEEVENT(1)
		PRINTFORMW (우… 너무 조금 노력했는지…)
		RETURN 2
	ENDIF
	
	;-----------------------------------
	;그 외(25%)
	;-----------------------------------
	IF 구상기입체크(25)
		PRINTDATAW 
			DATAFORM 「우우… 역시 조금 너무 의욕에 넘쳤는지도」
			DATAFORM 「… 무엇? 나는 전혀에─와라. 이런 건, 아무것도 아니에요…」
		ENDDATA
		RETURN 2
	ENDIF
	RETURN 0
ENDIF

