		RETURN 2
	ENDIF
	
	;=================================================
	;조교내 첫회(반드시 표시)
	;=================================================
	IF 구상기입체크() && ONCEEVENT(7)
		PRINTFORMDW 진하고는 어려울 것 같은 얼굴을 하고 있다…
		PRINTFORMW 「……………」
		RETURN 2
	ENDIF
	
	;-----------------------------------
	;그 외(25%)
	;-----------------------------------
	IF 구상기입체크(25)
		SELECTCASE RANDEVENT(7)
			CASE 0
				PRINTFORMDW 어딘지 모르게, 진하고의 기운이 없는 것 처럼 보인다…
				PRINTFORMW 「……………」
			CASEELSE
				PRINTFORMDW 진하고는 드물고 성실한 얼굴을 하고 있다…
				PRINTFORMW 「……………」
		ENDSELECT
		RETURN 2
	ENDIF
	RETURN 0
ENDIF
