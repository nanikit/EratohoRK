	ENDIF
	
	;=================================================
	;로터로(애널로터)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "애널로터"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;애널바이브로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "애널바이브"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;손으로(애널애무)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "애널애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교 대상 B절정
;*************************************************
;·조교 대상이 B절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 B절정, NOWEX:MASTER:B절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:B절정
	;=================================================
	;동시에 분유
	;=================================================
	IF 구상기입체크(0) && NOWEX:MASTER:분유
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에서(유두빨고)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "유두빨고"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;로터로(유두로터)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "유두로터"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;손으로(가슴애무)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "가슴애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

