;─
;조수 조교 이벤트 구상
;─
;조수에 의한 절정 따위, 조교중에 발생하는 이벤트를 정리한 파일
;-------------------------------------------------

;***********************************************************
;AEV20, 조수 절정 구상
;***********************************************************
;·조수의 손으로 조교자나 조교 대상이 절정에 이를 때 표시
;· 각 절정은 개별의 호출을 가지지 않고, 모두 파생으로 처리된다
;-------------------------------------------------
;파일명이 TRAIN가 되고 있지만, 분류상은 어디까지나 조수 이벤트 구상인 일에 주의
;까닭에 사용해야 할 첫회 판정 함수는 FIRSTEVENTASSI가 된다
;FIRSTEVENTASSI라면 호출해 마다 번호가 관리되므로 FIRSTTIME에 비하면 관리는 꽤 편해질 것
;-------------------------------------------------
;·구상주는 ASSI
;·그것 같은 분위기로 지어 보았지만, 둘러싸 봉으로 두드릴 필요가 있다. 절정 구상은 실제 위험함
;·조교 대상을 절정 시켰을 경우 뿐만이 아니라, 조교자를 절정 시켰을 경우도 불린다. 그런 행동은 현재 없겠지만
;·만일 조수가 조교자를 가게 하면, 쌍방의 절정 구상이 불리는, 일 것. 아이에에에!
;·그렇지 않아도 조수가 조교 대상을가게했다경우도 쌍방의 절정 구상이… 남아미다브트!
;·조수는 절정 하지 않는 것이 적어도의 구제. 그것도 바뀔지도 모르지만… 아박!
;·인수 ARG는 사용되어 있지 않다. 사요나라!
;=================================================
@KOJO_EVENT_ASSI_K64_20(ARG)
;=================================================
;인수 내역…
;ARG=()
;-------------------------------------------------
;관련 함수…
;CARESS_SPOT(인수 0)
;인수 0=캐릭터 등록 번호. 생략 하면 TARGET
;반환값:그 캐릭터가 MASTER의 어디를 애무하고 있을까
;(0. 하고 있지 않다/1. C/2.V/3.A/4.B/5. 입)
;-------------------------------------------------

;*************************************************
;질내사정(MASTER→ASSI)
;*************************************************
;·역강간으로 질내사정 하는 경우에 표시. 현재외는 없다고 생각하지만…
;·사정량에 의한 분기는 필요하게 응하고 추가해 사용하는 것이 바람직함
;·우선 나왔는지 나오지 않았던 것일까는 신경쓰지 않으면 능숙하게 쓸 수 없는 생각이 든다
;·조수성교봉사중에 조교자가 전립선으로가게했다경우의 거동이 어떻게 될까 신경이 쓰이는 것이지만… 시험하지 않았다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;질내사정(조교 대상→조수)
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:사정 && (NOWASSIACTNAME(ASSI) == "역강간" || TFLAG:조수에삽입중)
	;최저한, 나왔는지 나오지 않았는지 있으면 어떻게든 되겠죠(거짓말)
	SELECTCASE NOWEX:MASTER:사정	;조교 대상 사정종에 의한 분기
		CASE 1						;드라이사정
			PRINTFORMW 
		;CASE 3						;대량사정
		;	PRINTFORMW 
		;CASE 4						;조루
		;	PRINTFORMW 
		;CASE 5						;전립선
		;	PRINTFORMW 
		CASEELSE					;그 외(통상)
			PRINTFORMW 
	ENDSELECT
	RETURN 1
ENDIF

;*************************************************
;조교 대상 사정
;*************************************************
;·조교 대상이 사정하는 경우에 표시
;·사정량에 의한 분기는 필요하게 응하고 가감(상태)해 사용하는 것이 바람직함
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 사정, NOWEX:MASTER:사정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:사정
	;=================================================
	;조교자에게 질내사정(전립선 고문)
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TFLAG:전회질내사정, 2, 4) && TCVAR:MASTER:전립선자극 && ((TALENT:MASTER:남자 && NOWEX:MASTER:A절정) || (TALENT:MASTER:후타나리 && NOWEX:MASTER:V절정))
		SELECTCASE NOWEX:MASTER:사정	;조교 대상 사정종에 의한 분기
			CASE 1						;드라이사정
				PRINTFORMW 
			CASEELSE					;전립선(아마)
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;조교자에게 애널내 사정(전립선 고문)
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TFLAG:전회질내사정, 6, 8) && TCVAR:MASTER:전립선자극 && ((TALENT:MASTER:남자 && NOWEX:MASTER:A절정) || (TALENT:MASTER:후타나리 && NOWEX:MASTER:V절정))
		SELECTCASE NOWEX:MASTER:사정	;조교 대상 사정종에 의한 분기
			CASE 1						;드라이사정
				PRINTFORMW 
			CASEELSE					;전립선(아마)
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전립선사정
	;=================================================
	IF 구상기입체크(0) && NOWEX:MASTER:사정 == 5
		;-------------------------------------------------
		;페니스로(성교) ※후타나리용
		;-------------------------------------------------
		IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "성교"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;애널바이브로 ※남자용
		;-------------------------------------------------
		IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "애널바이브"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;바이브로 ※후타나리용
		;-------------------------------------------------
		IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "바이브"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;로터로(애널로터) ※남자용
		;-------------------------------------------------
		IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "애널로터"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;손으로(손가락삽입) ※후타나리용
		;-------------------------------------------------
		IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "손가락삽입"
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------
		;샌 것은 여기서 처리
		;-------------------------------------
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;오나홀로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "C도구"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에서
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "입으로애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;가랑이로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "가랑이"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;파이즈리로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "파이즈리한다"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;다리로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "풋잡한다"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;손으로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "손으로애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;어느 조건에도 히트 하지 않고 새어 오면 여기서 처리
	;라고 할까 기본적으로 여기에 떨어지는 것 같은 것은 맛이 없다
	;---------------------------------------------
	;최저한, 나왔는지 나오지 않았는지 있으면 어떻게든 되겠죠(거짓말)
	SELECTCASE NOWEX:MASTER:사정	;조교 대상 사정종에 의한 분기
		CASE 1						;드라이사정
			PRINTFORMW 
		;CASE 3						;대량사정
		;	PRINTFORMW 
		;CASE 4						;조루
		;	PRINTFORMW 
		CASEELSE					;그 외(통상/전립선)… 전립선은 위에서 줍자
			PRINTFORMW 
	ENDSELECT
ENDIF

;*************************************************
;조교 대상 C절정
;*************************************************
;·조교 대상이 C절정 하는 경우에 표시
;·여기에서는 「여자 아이의 C절정」을 상정하고 있다
;·사정 구상을 방치하면 사정도 이 개소에서 처리되어 슬픈 일이 되기 때문에, 최저한 어떠한 것을 준비해야 할
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 C절정, NOWEX:MASTER:C절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:C절정
	;=================================================
	;다중절정
	;=================================================
	IF 구상기입체크(0) && (NOWEX:MASTER:V절정 || NOWEX:MASTER:A절정 || NOWEX:MASTER:B절정)
		IF NOWEX:MASTER:V절정 && NOWEX:MASTER:A절정 && NOWEX:MASTER:B절정	;C+VAB 절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:V절정 && NOWEX:MASTER:A절정						;C+VA절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:V절정 && NOWEX:MASTER:B절정						;C+VB절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:A절정 && NOWEX:MASTER:B절정						;C+AB절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:V절정												;C+V절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:A절정												;C+A절정
			PRINTFORMW 
		ELSE																	;C+B절정
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;조교 대상이 C절정과 동시에 실금(조교 대상으로[소변버릇]이 없으면 발생하지 않는다)
	;=================================================
	IF 구상기입체크(0) && NOWEX:MASTER:방뇨
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;클리캡으로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "C도구"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;구에서(쿤닐링구스)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "입으로애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;짝 맞추기 놀이로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "가랑이"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;손으로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "손으로애무"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교 대상 V절정
;*************************************************
;·조교 대상이 V절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 V절정, NOWEX:MASTER:V절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:V절정
	;==================================================
	;동시에 A절정/B절정(다중절정)
	;=================================================
	IF 구상기입체크(0) && (NOWEX:MASTER:A절정 || NOWEX:MASTER:B절정)
		IF NOWEX:MASTER:A절정 && NOWEX:MASTER:B절정	;V+AB절정
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:A절정						;V+A절정
			PRINTFORMW 
		ELSE											;V+B절정
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;페니스로(성교)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "성교"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;바이브로
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "바이브"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;손으로(손가락삽입)
	;=================================================
	IF 구상기입체크(0) && NOWASSIACTNAME(ASSI) == "손가락삽입"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;샌 것은 여기서 처리
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;조교 대상 A절정
;*************************************************
;·조교 대상이 A절정 하는 경우에 표시
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상 A절정, NOWEX:MASTER:A절정
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && NOWEX:MASTER:A절정
