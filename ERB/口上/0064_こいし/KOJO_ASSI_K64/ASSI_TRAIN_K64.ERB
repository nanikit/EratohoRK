;***********************************************************
;AEV20, 助手絶頂口上
;***********************************************************
;・助手の手で調教者か調教対象が絶頂に達するときに表示
;・各絶頂は個別の呼び出しを持たずに、全て派生で処理される
;-------------------------------------------------
;ファイル名がTRAINとなっているが、分類上はあくまでも助手イベント口上である事に注意
;故に使うべき初回判定関数はFIRSTEVENTASSIになる
;FIRSTEVENTASSIなら呼び出し毎に番号が管理されるのでFIRSTTIMEに比べると管理はかなり楽になるはず
;-------------------------------------------------
;・口上主はASSI
;・それっぽい雰囲気に仕立ててみたが、囲んで棒で叩く必要がある。絶頂口上は実際ヤバイ
;・調教対象を絶頂させた場合だけでなく、調教者を絶頂させた場合も呼ばれる。そんな行動は今のところ無いが
;・万が一助手が調教者をイかせると、双方の絶頂口上が呼ばれる、はず。アイエエエ！
;・そうでなくとも助手が調教対象をイかせた場合も双方の絶頂口上が…ナムアミダブツ！
;・助手は絶頂しないのがせめてもの救い。それも変わるかもしれないけど…アバーッ！
;・引数ARGは使用されていない。サヨナラ！
;=================================================
@KOJO_EVENT_ASSI_K64_20(ARG)
;=================================================
;引数内訳…
;ARG=()
;-------------------------------------------------
;関連関数…
;CARESS_SPOT(引数0)
;引数0=キャラ登録番号。省略するとTARGET
;戻り値:そのキャラがMASTERのどこを愛撫しているか
;(0.してない/1.Ｃ/2.Ｖ/3.Ａ/4.Ｂ/5.口)
;-------------------------------------------------

;*************************************************
;膣内射精(MASTER→ASSI)
;*************************************************
;・逆レイプで膣内射精する場合に表示。今のところ他は無いと思うが…
;・射精量による分岐は必要に応じ追加して使うのが望ましい
;・とりあえず出たのか出なかったのかは気にしないと上手く書けない気がする
;・助手性交奉仕中に調教者が前立腺でイかせた場合の挙動がどうなるか気になるのだけど…試してない
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;膣内射精(調教対象→助手)
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && NOWEX:MASTER:射精 && (NOWASSIACTNAME(ASSI) == "逆レイプ" || TFLAG:助手に挿入中)
	;最低限、出たか出なかったかあればなんとかなるでしょ(嘘)
	SELECTCASE NOWEX:MASTER:射精	;調教対象射精種による分岐
		CASE 1						;空射精
			PRINTFORMW 
		;CASE 3						;大量射精
		;	PRINTFORMW 
		;CASE 4						;早漏
		;	PRINTFORMW 
		;CASE 5						;前立腺
		;	PRINTFORMW 
		CASEELSE					;その他(通常)
			PRINTFORMW 
	ENDSELECT
	RETURN 1
ENDIF

;*************************************************
;調教対象射精
;*************************************************
;・調教対象が射精する場合に表示
;・射精量による分岐は必要に応じ加減して使うのが望ましい
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;調教対象射精, NOWEX:MASTER:射精
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && NOWEX:MASTER:射精
	;=================================================
	;調教者に膣内射精 (前立腺責め)
	;=================================================
	IF 口上記入チェック(0) && GROUPMATCH(TFLAG:前回中出し, 2, 4) && TCVAR:MASTER:前立腺刺激 && ((TALENT:MASTER:オトコ && NOWEX:MASTER:Ａ絶頂) || (TALENT:MASTER:ふたなり && NOWEX:MASTER:Ｖ絶頂))
		SELECTCASE NOWEX:MASTER:射精	;調教対象射精種による分岐
			CASE 1						;空射精
				PRINTFORMW 
			CASEELSE					;前立腺(多分)
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者にアナル内射精 (前立腺責め)
	;=================================================
	IF 口上記入チェック(0) && GROUPMATCH(TFLAG:前回中出し, 6, 8) && TCVAR:MASTER:前立腺刺激 && ((TALENT:MASTER:オトコ && NOWEX:MASTER:Ａ絶頂) || (TALENT:MASTER:ふたなり && NOWEX:MASTER:Ｖ絶頂))
		SELECTCASE NOWEX:MASTER:射精	;調教対象射精種による分岐
			CASE 1						;空射精
				PRINTFORMW 
			CASEELSE					;前立腺(多分)
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;前立腺射精
	;=================================================
	IF 口上記入チェック(0) && NOWEX:MASTER:射精 == 5
		;-------------------------------------------------
		;ペニスで(性交) ※ふたなり用
		;-------------------------------------------------
		IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "性交"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;アナルバイブで ※オトコ用
		;-------------------------------------------------
		IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "アナルバイブ"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;バイブで ※ふたなり用
		;-------------------------------------------------
		IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "バイブ"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;ローターで(アナルローター) ※オトコ用
		;-------------------------------------------------
		IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "アナルローター"
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;手で(指挿入) ※ふたなり用
		;-------------------------------------------------
		IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "指挿入"
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------
		;漏れたものはここで処理
		;-------------------------------------
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;オナホールで
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "Ｃ道具"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;口で
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "口で愛撫"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;素股で
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "素股"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;パイズリで
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "パイズリする"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;足で
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "足コキする"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;手で
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "手で愛撫"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;どの条件にもヒットせずに漏れてきたらここで処理
	;というか基本的にここに落ちるようでは不味い
	;---------------------------------------------
	;最低限、出たか出なかったかあればなんとかなるでしょ(嘘)
	SELECTCASE NOWEX:MASTER:射精	;調教対象射精種による分岐
		CASE 1						;空射精
			PRINTFORMW 
		;CASE 3						;大量射精
		;	PRINTFORMW 
		;CASE 4						;早漏
		;	PRINTFORMW 
		CASEELSE					;その他(通常/前立腺)…前立腺は上で拾おうよ
			PRINTFORMW 
	ENDSELECT
ENDIF

;*************************************************
;調教対象Ｃ絶頂
;*************************************************
;・調教対象がＣ絶頂する場合に表示
;・ここでは「女の子のＣ絶頂」を想定している
;・射精口上を放置すると射精もこの箇所で処理されて悲しい事になるんで、最低限何らかのものを用意すべき
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;調教対象Ｃ絶頂, NOWEX:MASTER:Ｃ絶頂
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && NOWEX:MASTER:Ｃ絶頂
	;=================================================
	;多重絶頂
	;=================================================
	IF 口上記入チェック(0) && (NOWEX:MASTER:Ｖ絶頂 || NOWEX:MASTER:Ａ絶頂 || NOWEX:MASTER:Ｂ絶頂)
		IF NOWEX:MASTER:Ｖ絶頂 && NOWEX:MASTER:Ａ絶頂 && NOWEX:MASTER:Ｂ絶頂	;Ｃ+ＶＡＢ絶頂
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:Ｖ絶頂 && NOWEX:MASTER:Ａ絶頂						;Ｃ+ＶＡ絶頂
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:Ｖ絶頂 && NOWEX:MASTER:Ｂ絶頂						;Ｃ+ＶＢ絶頂
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:Ａ絶頂 && NOWEX:MASTER:Ｂ絶頂						;Ｃ+ＡＢ絶頂
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:Ｖ絶頂												;Ｃ+Ｖ絶頂
			PRINTFORMW 
		ELSEIF NOWEX:MASTER:Ａ絶頂												;Ｃ+Ａ絶頂
			PRINTFORMW 
		ELSE																	;Ｃ+Ｂ絶頂
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象がＣ絶頂と同時に失禁 (調教対象に[おもらし癖]が無ければ発生しない)
	;=================================================
	IF 口上記入チェック(0) && NOWEX:MASTER:放尿
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;クリキャップで
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "Ｃ道具"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;口で(クンニ)
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "口で愛撫"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;貝合わせで
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "素股"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;手で
	;=================================================
	IF 口上記入チェック(0) && NOWASSIACTNAME(ASSI) == "手で愛撫"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;漏れたものはここで処理
	;-----------------------------------
	PRINTFORMW 
	RETURN 1
ENDIF

