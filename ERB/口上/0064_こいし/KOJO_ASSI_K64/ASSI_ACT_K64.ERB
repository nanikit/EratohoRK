;=================================================
;*************************************************
;AACT7_0, 愛撫
;*************************************************
IF 口上記入チェック(0) && !TCVAR:ASSI:助手ACT派生
	PRINTFORMW 
	RETURN 1
ENDIF

;*************************************************
;AACT7_1, 手淫
;*************************************************
IF 口上記入チェック() && TCVAR:ASSI:助手ACT派生 == 1
	;=================================================
	;布地越し
	;=================================================
	IF 口上記入チェック() && PANTSCHECK(1)
		IF TCVAR:MASTER:イきそう	;調教対象がイきそう
			IF TALENT:ASSI:ご主人様	;こいしがご主人様
				PRINTFORMW 「くすっ、まさか\@ PANTSCHECK(1) >= 2 ? %OUTER_B(MASTER, 1)% # %INNER_B(MASTER, 1)% \@の上から触られたくらいでイッたりしないよね？」
				PRINTFORMW 「だって、このまま出したら%K64_CNAME(MASTER)%の%INNER_B(MASTER, 1)%が……うふふ」
			ELSE					;こいしがご主人様ではない
				PRINTFORMW 「ねぇ%K64_CNAME(TARGET)%、このままイかせちゃってもいいの？」
				PRINTFORMW 「%K64_CNAME(MASTER, 1)%の%INNER_B(MASTER, 1)%汚れちゃうけど」
				PRINTFORMW 「あらかじめ脱いでおかないヤツが悪い、そういう考えもあるかもね♪」
			ENDIF
		ELSE						;調教対象がイきそうではない
			IF TALENT:ASSI:ご主人様	;こいしがご主人様
				PRINTFORMW 「うふふ、ここに何を隠しているの？」
				PRINTFORMW 「大事なものでも隠しているの？ 触った感じ、大したものでもないと思うけど♪」
			ELSE					;こいしがご主人様ではない
				PRINTFORMW 「そんなに怖がらなくても大丈夫よ」
				PRINTFORMW 「何も握りつぶしたりはしないから♪」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象がイきそう
	;=================================================
	IF 口上記入チェック() && TCVAR:MASTER:イきそう
		IF TALENT:ASSI:ご主人様	;こいしがご主人様
			PRINTFORMW 「あれ？ %K64_CNAME(MASTER, 1)%イきそうになってない？」
			PRINTFORMW 「じゃあ先っぽだけくるくる～ってシてあげる」
		ELSE					;こいしがご主人様ではない
			PRINTFORMW 「%K64_CNAME(TARGET)%、%K64_CNAME(MASTER, 1)%がそろそろ射精するかも♪」
			PRINTFORMW 「もう少し焦らしちゃう？」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF TALENT:ASSI:ご主人様	;こいしがご主人様
		PRINTFORMW 「普段%K64_CNAME()%がオナニーする時は、こんな感じかな？」
		PRINTFORMW 「くすくす」
	ELSE					;こいしがご主人様ではない
		PRINTFORMW 「くすくす…暴れちゃダメよ？」
		PRINTFORMW 「暴れたら、おちんちんがちぎれちゃうかも♪」
	ENDIF
	RETURN 1
ENDIF


