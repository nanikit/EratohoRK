;=================================================
;EV101, 衣装変更
;=================================================
;・ショップから衣装変更を行った場合に表示
;-------------------------------------------------
@KOJO_EVENT_K64_101(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=
;　一の位…(0.もう着てる/1.着替えた/2.恥ずかしくて着られない/3.嫌がって着ない/9.持ってない)
;　十の位…(0.通常/10.半裸/20.全裸/30.下着無し)
;　百の位…コスプレのために保留
;　千の位…(0.調教者/1000.調教対象)
;　例 (調教対象が恥ずかしがって全裸にできなかった=1022)
;-------------------------------------------------

;-------------------------------------------------
;調教者が着替えた
;-------------------------------------------------
IF 口上記入チェック() && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		CASE 1		;通常になった
			PRINTFORMW 「やっぱり飼い主はちゃんと服を着るものよ」
			PRINTFORMW 「ペットに服を着せるのはどうかと思うけど」
			RETURN 2
		CASE 11		;半裸になった
			PRINTFORMW 「こういうのが、セクシーって言うんでしょ？」
			;貧乳でガックシというささやかな抵抗(でもホントはロリコンだったりするんだよ)
			;ここは弄り甲斐がありそうな気がするわね…ちょっとバリエーションとか分岐とか考えておこう
			IF TALENT:TARGET:貧乳
				PRINTFORMDW %CALLNAME:MASTER%は、こいしの胸元を一瞥すると皮肉めいた深い溜息をついた
				PRINTFORMW 「え…何よそれ。%INNER_T(TARGET, 1)%がダメだっていうの？ それとも…」
			ENDIF
			RETURN 2
		CASE 21		;全裸になった
			PRINTFORMW 「…？ 別に恥ずかしくはないわ」
			PRINTFORMW 「ペットに裸を見られても、何とも思わないもの」
			RETURN 2
		;CASE 31	;……すーすーする ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教者が恥ずかしがって着替えられない
;-------------------------------------------------
IF 口上記入チェック() && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		CASE 12		;半裸だって恥ずかしい
			PRINTFORMW 「んー…なんか%K64_CNAME()%には勿体無い気がするわ」
			PRINTFORMW 「また今度にしましょ」
			RETURN 2
		CASE 22		;普段から全裸はちょっと……
			PRINTFORMW 「んー…なんか%K64_CNAME()%には勿体無い気がするわ」
			PRINTFORMW 「また今度にしましょ」
			RETURN 2
		;CASE 32	;パンツが無いから恥ずかしいもん！ ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 「んー…なんか%K64_CNAME()%には勿体無い気がするわ」
		;	PRINTFORMW 「また今度にしましょ」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教者が嫌がって着替えられない
;-------------------------------------------------
IF 口上記入チェック() && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		CASE 13		;半裸は嫌
			PRINTFORMW 「…なんか寒くない？」
			PRINTFORMW 「やっぱ着てないとダメね。身体が冷えちゃう」
			RETURN 2
		CASE 23		;普段から全裸なんて嫌
			PRINTFORMW 「…なんか寒くない？」
			PRINTFORMW 「やっぱ着てないとダメね。身体が冷えちゃう」
			RETURN 2
		;CASE 33	;すーすーするのは嫌 ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 「…なんか寒くない？」
		;	PRINTFORMW 「やっぱ着てないとダメね。身体が冷えちゃう」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教者が着替えるのをやめた
;-------------------------------------------------
IF 口上記入チェック(0) && ARG < 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		CASE 0		;通常のまま
			PRINTFORMW 
			RETURN 2
		CASE 10		;半裸のまま
			PRINTFORMW 
			RETURN 2
		CASE 20		;全裸のまま
			PRINTFORMW 
			RETURN 2
		;CASE 30	;すーすーするまま ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教対象を着替えさせた
;-------------------------------------------------
IF 口上記入チェック() && ARG >= 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		CASE 1001	;通常になった
			PRINTFORMW 「ペットのクセに服を着せてもらえるなんて」
			PRINTFORMW 「%K64_CNAME()%は幸せ者ね」
			RETURN 2
		CASE 1011	;半裸になった
			;この書き方だと初回と２回目以降の分岐が欲しい雰囲気
			PRINTFORMW 「半分くらいペットらしさが出てきたから、服も半分」
			PRINTFORMW 「何？ 文句あるの？」
			RETURN 2
		CASE 1021	;全裸になった
			;この書き方だと露出癖による分岐が欲しい雰囲気
			PRINTFORMW 「そうよ、ペットはそれが自然なの」
			PRINTFORMW 「恥ずかしい？ 何言ってるの。ペットに恥なんて無いわ」
			RETURN 2
		;CASE 1031	;……すーすーする ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教対象が恥ずかしがって着替えられない
;-------------------------------------------------
IF 口上記入チェック() && ARG >= 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		CASE 1012	;半裸だって恥ずかしい
			PRINTFORMW 「ちょっと%K64_CNAME()%、ペットとしての自覚が足りないんじゃない？」
			PRINTFORMW 「もっと厳しく躾けないといけないわね」
			RETURN 2
		CASE 1022	;普段から全裸なんて恥ずかしい
			PRINTFORMW 「ちょっと%K64_CNAME()%、ペットとしての自覚が足りないんじゃない？」
			PRINTFORMW 「もっと厳しく躾けないといけないわね」
			RETURN 2
		;CASE 1032	;パンツがないから恥ずかしいもん！ ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 「ちょっと%K64_CNAME()%、ペットとしての自覚が足りないんじゃない？」
		;	PRINTFORMW 「もっと厳しく躾けないといけないわね」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教対象が嫌がって着替えられない
;-------------------------------------------------
IF 口上記入チェック() && ARG >= 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		CASE 1013	;半裸は嫌
			PRINTFORMW 「まぁ、生意気ね！ ペットのクセに」
			PRINTFORMW 「ちょっと自分の立場を思い知らせてやる必要がありそうね」
			RETURN 2
		CASE 1023	;普段から全裸なんて嫌
			PRINTFORMW 「まぁ、生意気ね！ ペットのクセに」
			PRINTFORMW 「ちょっと自分の立場を思い知らせてやる必要がありそうね」
			RETURN 2
		;CASE 1033	;すーすーするのは嫌 ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 「まぁ、生意気ね！ ペットのクセに」
		;	PRINTFORMW 「ちょっと自分の立場を思い知らせてやる必要がありそうね」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;調教対象を着替えさせるのをやめた
;-------------------------------------------------
IF 口上記入チェック(0) && ARG >= 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		CASE 1000	;通常のまま
			PRINTFORMW 
			RETURN 2
		CASE 1010	;半裸のまま
			PRINTFORMW 
			RETURN 2
		CASE 1020	;全裸のまま
			PRINTFORMW 
			RETURN 2
		;CASE 1030	;すーすーするまま ※現在のRevでは実装されていない箇所
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF


;=================================================
;EV102, 調教者変更 FIRSTEVENT()
;=================================================
;・調教者を交代させたときに表示
;-------------------------------------------------
@KOJO_EVENT_K64_102(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(0, 口上主が他のキャラに任せた、1, 口上主が調教者になった)
;旧調教者は「%CALLNAME:SHOP_OLDTARGET%」、新調教者は「%CALLNAME:SHOP_NEWTARGET%」で参照できる
;口上主はTARGETで参照
;-------------------------------------------------

;-------------------------------------------------
;他の調教者に交代
;-------------------------------------------------
IF 口上記入チェック() && ARG == 0
	SELECTCASE NO:SHOP_NEWTARGET	;交代先のキャラで分岐
		CASE 61		;さとり
			IF TALENT:TARGET:ご主人様	;こいしがご主人様
				PRINTFORMW 「お姉ちゃん、ちょっとお願いなんだけど…」
				PRINTFORMW 「%K64_CNAME(MASTER, 2)%の躾、ちょっとの間だけ代わってくれない？」
				IF CFLAG:TARGET:調教レベル < 3					;こいしの調教Lvが3未満
					PRINTFORMW 「どうにもまだ慣れなくて…」
					PRINTFORMW 「お姉ちゃんのやり方を見てみたいの」
				ELSEIF MARK:TARGET:反抗刻印 >= 5 && !FAVOR(500)	;こいしの調教対象からの反抗刻印Lv5以上 かつ 調教対象からの好意500未満
					PRINTFORMW 「%K64_CNAME(MASTER, 2)%って、すっっっごく反抗的なの」
					PRINTFORMW 「ちょっとお姉ちゃんからもガツンとやってくれると助かるわ」
				ELSEIF !FAVOR(-100)								;こいしの調教対象からの好意が-100未満
					PRINTFORMW 「なんだか、私嫌われてる気がするの」
					PRINTFORMW 「どうしたらいいかわかんなくて…」
				ELSEIF CFLAG:TARGET:消耗 > 5					;こいしの疲労が蓄積している
					PRINTFORMW 「ちょっと疲れちゃったから…」
					PRINTFORMW 「温泉にでも浸かってくるわ」
				ELSE											;上記以外
					PRINTFORMW 「いや、%K64_CNAME(MASTER, 2)%に飽きたわけじゃないよ？」
					PRINTFORMW 「ちょっと休憩よ休憩♪」
				ENDIF
			ELSEIF TALENT:SHOP_NEWTARGET:ご主人様	;さとりがご主人様
				IF ASSI:1 == SHOP_NEWTARGET || ASSI:2 == SHOP_NEWTARGET	;さとりが助手だった
					PRINTFORMW 「あれ、もういいの？ ちょっと面白かったのに…」
				ELSE							;上記以外
					PRINTFORMW 「あ、お姉ちゃんお帰りなさい」
				ENDIF
			ELSE						;どちらもご主人様でない
				PRINTFORMW 「お姉ちゃんが躾をやるの？ それなら安心だね♪」
			ENDIF
		CASE 62, 63	;燐/空
			IF TALENT:TARGET:ご主人様	;こいしがご主人様
				PRINTFORMW 「%CALLNAME:SHOP_NEWTARGET%、ちょっと出かけてくるから、%K64_CNAME(MASTER, 2)%の面倒見ておいてくれない？」
			ELSEIF TALENT:SHOP_NEWTARGET:ご主人様	;燐/空がご主人様
				PRINTFORMW 「え、もう終わり？ せっかく面白くなってきたのに」
			ELSE						;どちらもご主人様でない
				PRINTFORMW 「え、%CALLNAME:SHOP_NEWTARGET%が躾をやるの？ 大丈夫かなぁ…」
			ENDIF
		CASEELSE	;上記以外
			IF TALENT:TARGET:ご主人様	;こいしがご主人様
				IF MARK:TARGET:反抗刻印 >= 5	;こいしの調教対象からの反抗刻印Lv5以上
					PRINTFORMW 
				ELSE							;上記以外
					PRINTFORMW 
				ENDIF
			ELSEIF TALENT:SHOP_NEWTARGET:ご主人様	;交代相手がご主人様
				IF MARK:TARGET:反抗刻印 >= 5	;こいしの調教対象からの反抗刻印Lv5以上
					PRINTFORMW 「こんなに面倒な子だなんて聞いてなかったわ」
				ELSE							;上記以外
					PRINTFORMW 「あれ、もういいの？」
				ENDIF
			ELSE						;どちらもご主人様でない
				IF MARK:TARGET:反抗刻印 >= 5	;こいしの調教対象からの反抗刻印Lv5以上
					PRINTFORMW 「%K64_CNAME(MASTER, 2)%は言うこと聞かない子だから大変よ？」
				ELSE							;上記以外
					PRINTFORMW 「はい。あとはよろしくね」
				ENDIF
			ENDIF
	ENDSELECT
	RETURN 2
ENDIF

;-------------------------------------------------
;こいしさまが調教者になった
;-------------------------------------------------
IF 口上記入チェック() && ARG == 1
	IF SHOP_OLDTARGET < 0	;交代元がいない
		;このケースが依然として起こりうるか検証しなおさないとならないけれど
		SELECTCASE NO:(FLAG:2)	;誰がご主人様かで分岐
			CASE 61		;さとり
				PRINTFORMW 「お姉ちゃん、%K64_CNAME(MASTER, 2)%で遊んでくるね♪」
			CASE 62, 63	;燐/空
				PRINTFORMW 「%CALLNAME:(FLAG:2)%、ちょっと%K64_CNAME(MASTER, 2)%借りるね♪」
			CASE 64		;こいし
				PRINTFORMW 「さて、%K64_CNAME(MASTER, 2)%を可愛がってこようかな」
			CASEELSE	;上記以外
				PRINTFORMW 「じゃあ私が%K64_CNAME(MASTER, 2)%の躾してくるね」
		ENDSELECT
	ELSE		;交代元がいる…普通はこっち
		SELECTCASE NO:SHOP_OLDTARGET	;誰が交代元かで分岐
			CASE 61		;さとり
				IF TALENT:TARGET:ご主人様	;こいしがご主人様
					PRINTFORMW 「ありがと、お姉ちゃん♪」
					IF CFLAG:TARGET:調教レベル < 3					;こいしの調教Lvが3未満
						PRINTFORMW 「私もお姉ちゃんみたいにできるかなぁ…」
					ELSEIF MARK:TARGET:反抗刻印 >= 5 && !FAVOR(500)	;こいしの調教対象からの反抗刻印Lv5以上 かつ 調教対象からの好意500未満
						PRINTFORMW 「どう？ 強情だったでしょ？」
						IF MARK:SHOP_OLDTARGET:反抗刻印 == 0 && FAVOR(100, 0, SHOP_OLDTARGET)	;さとりの調教対象からの反発刻印Lv0 かつ 調教対象からの好意100以上
							PRINTFORMW 「え？ 全然そんなことなかった？」
							PRINTFORMW 「そんなぁ…どういうことなの…」
						ENDIF
					ELSEIF !FAVOR(-100)								;こいしの調教対象からの好意が-100未満
						PRINTFORMW 「これで少しは懐いてくれるかなぁ」
					ELSEIF CFLAG:TARGET:消耗 > 5					;こいしの疲労が蓄積している
						PRINTFORMW 「本当はもう少し休みたかったけど…」
					ELSE											;上記以外
						PRINTFORMW 「あとは自分でやるわ」
					ENDIF
				ELSEIF TALENT:SHOP_OLDTARGET:ご主人様	;さとりがご主人様
					IF CFLAG:TARGET:調教レベル < 3					;こいしの調教Lvが3未満
						PRINTFORMW 「うーん、私にできるかなぁ」
					ELSEIF !FAVOR(-100)								;こいしの調教対象からの好意-100未満
						PRINTFORMW 「えー、私じゃダメだと思うよ？ なんか嫌われてるし」
					ELSEIF CFLAG:TARGET:消耗 > 5					;こいしの疲労が蓄積している
						PRINTFORMW 「私、疲れてるんだけど…やらないとダメ？」
					ELSEIF FAVOR(2000, 2000)						;こいしの調教対象からの好意が2000以上かつ、調教対象への好意が2000以上
						PRINTFORMW 「え、%K64_CNAME(MASTER, 2)%で遊んでいいの？！ やったぁ♪」
					ELSEIF FAVOR(0, 1500)							;こいしの調教対象への好意が1500以上
						PRINTFORMW 「ん、いいよ♪」
						PRINTFORMW 「でも、お姉ちゃんの知らない間に%K64_CNAME(MASTER, 2)%は私のペットになっちゃうかも」
					ELSEIF FAVOR(1500) && CFLAG:TARGET:M好意 > CFLAG:SHOP_OLDTARGET:M好意	;こいしの調教対象からの好意が1500以上かつ、さとりのそれよりも大きい
						PRINTFORMW 「うふふ、お姉ちゃんより私の方が懐かれてるもんね」
						PRINTFORMW 「心が読めなくても、無意識の行動でわかるよ♪」
					ELSEIF CFLAG:TARGET:調教レベル >= 10			;こいしの調教Lvが10以上
						PRINTFORMW 「私に任せてよ、お姉ちゃん」
						PRINTFORMW 「豪華客船に乗ったつもりでいてね♪」
						;あからさまなデスノボリ。そして処女航海で氷山に接触、沈没するのであった…
					ELSE											;上記以外
						PRINTFORMW 「仕方ないなぁ…」
					ENDIF
				ELSE						;どちらもご主人様でない
					PRINTFORMW 「うーん、お姉ちゃんの方が心も読めるし、向いてると思うんだけどなぁ」
					PRINTFORMW 「もしかしてお姉ちゃん、もう嫌われたとか？」
				ENDIF
			CASE 62, 63	;燐/空
				IF TALENT:TARGET:ご主人様	;こいしがご主人様
					PRINTFORMW 「%CALLNAME:SHOP_OLDTARGET%、ありがとうね」
					PRINTFORMW 「あとは自分でやるわ」
				ELSEIF TALENT:SHOP_OLDTARGET:ご主人様	;燐/空がご主人様
					PRINTFORMW 「%CALLNAME:SHOP_OLDTARGET%のおもちゃで遊んでいいの？」
				ELSE						;どちらもご主人様でない
					PRINTFORMW 「%CALLNAME:SHOP_OLDTARGET%よりは私の方がうまくやれるかも」
				ENDIF
			CASEELSE	;上記以外
				IF TALENT:TARGET:ご主人様	;こいしがご主人様
					PRINTFORMW 
				ELSEIF TALENT:SHOP_OLDTARGET:ご主人様	;交代相手がご主人様
					IF MARK:TARGET:反抗刻印 >= 5	;こいしの調教対象からの反抗刻印Lv5以上
						PRINTFORMW 「えー、私がやるの？」
					ELSE							;上記以外
						PRINTFORMW 「私にもできるかなぁ」
					ENDIF
				ELSE						;どちらもご主人様でない
					IF MARK:TARGET:反抗刻印 >= 5	;こいしの調教対象からの反抗刻印Lv5以上
						PRINTFORMW 「えー、私がやるの？」
					ELSE							;上記以外
						PRINTFORMW 「私にもできるかなぁ」
					ENDIF
				ENDIF
		ENDSELECT
	ENDIF
	RETURN 2
ENDIF


;=================================================
;EV110, 魔力使用
;=================================================
;・魔力を消費して何らかの特典を購入したときに表示
;-------------------------------------------------
@KOJO_EVENT_K64_110(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(0:魔力使用前,
;		1:調教技巧,
;		2:知識を得る前 - 3:調合知識, 4:魅惑, 5:禁断の知識, 6:道具使い, 37:撮影技能
;		7:技術を得る前 - 8:舌使い, 9:器用な指先, 39:腰使い, 10:縛り上手, 11:サド取得/消去, 38:着衣プレイ好き取得/消去、12:汚れ無視
;		13:アイテム錬成前 - 14:ローター, 15:バイブ, 16:アナルビーズ, 17:ペニスバンド,
;							18:アナルバイブ, 19:クリキャップ, 20:ニプルキャップ, 21:オナホール
;		22:責め具錬成前 - 23:アイマスク, 24:鞭, 25:縄, 26:ボールギャグ, 27:針, 28:浣腸セット, 29:三角木馬, 30:大鏡錬成, 31:首輪錬成
;		32:道具をカスタマイズ, 33:ふたなり化/戻す, 34:巨乳化, 35:貧乳化, 36:精力)
;-------------------------------------------------

;-------------------------------------------------
;こいしさま魔力使用前
;-------------------------------------------------
;・[102] - 魔力を消費する FIRSTEVENT(0)
;-------------------------------------------------
IF 口上記入チェック() && ARG == 0
	;=================================================
	;初回 魔力1以上
	;=================================================
	IF 口上記入チェック() && CFLAG:TARGET:魔力 > 0 && FIRSTEVENT(0)
		;長ったらしい駄文を何度も見たくないので初回のみ表示
		PRINTFORMW 「この力って何なのかしら…」
		PRINTFORMW 「私達が普段使う妖力とはまた違うみたいなんだけど…」
		PRINTFORMW 「まずは何が出来るのか試してみないとね♪」
		RETURN 2
	ENDIF
	
	;-----------------------------------
	;その他
	;-----------------------------------
	RETURN 0	;初回以外用の短い文章を思いついたら書くかも
ENDIF

;-------------------------------------------------
;こいしさまの調教技巧がアップ
;-------------------------------------------------
;・[ 2] 調教技巧を高める FIRSTEVENT(1)
;-------------------------------------------------
;・調教中に上がるようになったから、あまり使わないんだよねー
;-------------------------------------------------
IF 口上記入チェック() && ARG == 1
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(0) && FIRSTEVENT(1)
		PRINTFORMW 
		RETURN 2
	ENDIF
	
	;-----------------------------------
	;その他
	;-----------------------------------
	CALL BIGPRINT_RAINBOW2W("こいしちゃんのレベルが上がった！", 20, "「", "」")
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「わーい、飼い主として成長した気がするわ」
