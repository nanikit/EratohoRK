;REACT5, 衣装の変更を要求する・個別リアクション
;────────────────────────────────────
;・調教対象のリアクションによって一枚脱ぐ、二枚脱ぐ、脱がないに分かれる
;・REACT分類が"消極的従う", "受け入れる"の場合は一枚脱ぐ
;・REACT分類が"積極的従う", "平然と従う"の場合は二枚脱ぐ
;・その他の場合は脱がない(PALAM恥情/反抗の数値によって、恥ずかしくて脱がない、嫌がって脱がないに分かれる)
;-------------------------------------------------
@KOJO_REACT_K64_5
;-------------------------------------------------
;・TFLAG:ACT派生(0=服を脱ぐように命令/1=服を着るように命令)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_0, 服を脱ぐように命令
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && TFLAG:ACT派生 == 0
	;=================================================
	;一枚脱いだ(消極的反応)
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う/受け入れる")
		;-------------------------------------------------
		;最後の一枚を脱いで全裸になった
		;-------------------------------------------------
		IF 口上記入チェック(0) && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			IF IS_NOWCOMNAME("なすがまま")		;COMなすがまま			ただ%CALLNAME:TARGET%に言われるまま従った…
				PRINTFORMW 
			ELSEIF IS_COMGRONAME("受け入れる")	;REACT分類受け入れる	全裸になり、期待と興奮で肌が火照るのを感じた
				PRINTFORMW 
			ELSE								;REACT分類消極的従う	仕方なく最後の一枚を脱ぎ、全裸になった
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;COM52, なすがまま
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_NOWCOMNAME("なすがまま")
			;%CALLNAME:MASTER%はただ%CALLNAME%に言われるまま従った…
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;緩やかな動きで服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("受け入れる")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;仕方なく服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;上層の分岐を全て埋めた場合は不要な箇所
		;逆に埋めない場合はこの箇所が補完の役目を果たします
		;-------------------------------------------------
		;その他・条件なし
		;-------------------------------------------------
		IF 口上記入チェック(0)
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;二枚脱いだ(積極的反応)
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う/平然と従う")
		;-------------------------------------------------
		;全裸になった
		;-------------------------------------------------
		IF 口上記入チェック(0) && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;積極的に脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;手際よく脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("平然と従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;上層の分岐を全て埋めた場合は不要な箇所
		;逆に埋めない場合はこの箇所が補完の役目を果たします
		;-------------------------------------------------
		;その他・条件なし
		;-------------------------------------------------
		IF 口上記入チェック(0)
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;脱がなかった
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("拒否")
		IF PALAM:MASTER:恥情 > PALAM:MASTER:反抗	;恥ずかしくて脱げなかった
			PRINTFORMW 
		ELSE										;嫌がって脱がなかった
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF


;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_1, 服を着るように命令
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(0) && TFLAG:ACT派生 == 1
	;=================================================
	;COM52, なすがまま
	;=================================================
	IF 口上記入チェック(0) && IS_NOWCOMNAME("なすがまま")
		;%CALLNAME:MASTER%はただ%CALLNAME%に言われるまま従った…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;一枚着た(消極的反応)
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う/受け入れる")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;二枚着た(積極的反応)
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う/平然と従う")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;地の文では"嫌がって"という扱い
	;=================================================
	;着なかった
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("拒否")
		PRINTFORMW 「ええっ？！ 服を着るの、嫌なの？！」
		PRINTFORMW 「んー、それもある意味ペットらしい…のかな？」
		PRINTFORMW 「ただの露出狂かもしれないけど♪」
		RETURN 1
	ENDIF
ENDIF
;REACT5, 衣装の変更を要求する

;────────────────────────────────────
;REACT6, 合意を求める・個別リアクション
;────────────────────────────────────
;・調教対象のリアクションに応じてREACT分類毎に派生する
;-------------------------------------------------
@KOJO_REACT_K64_6
;-------------------------------------------------
;手間を厭わないのであれば、TALENT:MASTER:オトコ等で内部で分けるのもアリかもしれません
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_0, 小さい声で了承する FIRSTREACT(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が消極的従うの場合
;-------------------------------------------------
IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う")
	;=================================================
	;REACT負荷0, 覚悟を決めたように、小さい声で了承した(自然な行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "自然な行動"
		;%CALLNAME:MASTER%は覚悟を決めたように、小さい声で了承した…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷1, 小さい声で了承した(無理のない行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理のない行動"
		;%CALLNAME:MASTER%は小さい声で了承した…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷2, (無理をした行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理をした行動"
		IF COMCOR_POSI()	;実は積極	恥ずかしさに消え入りそうな声で了承した
			;%CALLNAME:MASTER%は恥ずかしさに消え入りそうな声で了承した…
			PRINTFORMW 
		ELSE				;実は嫌		悔しさに顔を歪めながらも了承した
			;%CALLNAME:MASTER%は悔しさに顔を歪めながらも了承した…
			PRINTFORMW 「あははっ」
			PRINTFORMW 「こんなことわざわざ聞くまでも無く、いきなり力ずくで奪っちゃえばいいと思ってたんだけど…」
			PRINTFORMW 「わざわざ聞いた甲斐があったわ。%K64_CNAME()%のその顔、もう最高♪」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_1, 好きにすればいいと答えた FIRSTREACT(1)
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が平然と従うの場合
;-------------------------------------------------
IF 口上記入チェック() && IS_COMGRONAME("平然と従う")
	;=================================================
	;REACT負荷0, 別に嫌じゃないと言った(自然な行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "自然な行動"
		;%CALLNAME:MASTER%は%CALLNAME:TARGET%に別に嫌じゃないと言った…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷1, 好きにすればいいと答えた(無理のない行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理のない行動"
		;%CALLNAME:MASTER%は%CALLNAME:TARGET%に好きにすればいいと答えた…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷2, (無理をした行動)
	;=================================================
	IF 口上記入チェック() && GET_COM_RANK() == "無理をした行動"
		IF COMCOR_POSI()	;実は積極	したいならいいと言った
			;%CALLNAME:MASTER%は%CALLNAME:TARGET%がしたいならいいと言った…
			PRINTFORMW 「あら、シたいのは%K64_CNAME()%の方じゃないの？ くすくす」
		ELSE				;実は嫌		勝手にすればいいと答えた
			;%CALLNAME:MASTER%は%CALLNAME:TARGET%に勝手にすればいいと答えた…
			PRINTFORMW 「あれ？ いいの？ ちょっと意外…」
			PRINTFORMW 「けど、後で悔やむ事になっても知らないわよ…うふふ」
			PRINTFORML            ﾄﾗｳﾏ
			PRINTFORMW 「ステキな想い出をつくってあげるから♪」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_2, 意を決して捧げてもいいと告げる FIRSTREACT(2)
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が積極的従うの場合
;-------------------------------------------------
IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う")
	;=================================================
	;REACT負荷0, 初めてをあげられて嬉しいと答えた(自然な行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "自然な行動"
		;%CALLNAME:MASTER%は%CALLNAME:TARGET%に初めてをあげられて嬉しいと答えた…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷1, 意を決して捧げてもいいと告げた(無理のない行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理のない行動"
		;%CALLNAME:MASTER%は意を決して%CALLNAME:TARGET%に自分の初めてを捧げてもいいと告げた…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷2, もう守れないならばせめて優しくされるようにと同意した(無理をした行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理をした行動"
		;%CALLNAME:MASTER%はもう守れないならばせめて優しくされるようにと、
		;%CALLNAME:TARGET%に初めてを捧げることに愛想よく同意した…
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_3, 気持ち良くしてくれるんだったら初めてをあげてもいい FIRSTREACT(3)
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類が受け入れるの場合
;-------------------------------------------------
IF 口上記入チェック() && IS_COMGRONAME("受け入れる")
	;=================================================
	;COM52, なすがまま
	;=================================================
	;・一応枠を作ってみた。レアかもしれないけれど、この流れに落ちる可能性はあるよね？
	;・COMで汎用処理するのはちょっと勿体無い部分のような気がしたのよ…
	;・だからといってCOMでACT見に行くのはおかしいと思ったのでREACTで処理…でいいのかな？
	;・この場合@TRAIN_MESSAGE2は表示されず、代わりに@REACTION_MESSAGEが表示される
	;・状況により内容はバラバラだが、いずれもREACT分類が"受け入れる"扱いであり、合意を得られる
	;-------------------------------------------------
	;・ボールギャグ噛ませた上で抵抗不能にさせてから無理矢理合意を得ることも出来る…気がする
	;・敢えて一度拒否させてから、作為的にこの流れに落とし込んで、そのまま奪ってしまうと楽しい？
	;・しかし、抵抗不能の相手から合意を得るか。合意、ね… とりあえず私は面白い方を選ぶわ。卑怯とは言うまいね？
	;・あ、こいしちゃんって基本的に合意求めないんだったわ。どうしようかしら。強制合意か、合意無視か…
	;-------------------------------------------------
	IF 口上記入チェック() && IS_NOWCOMNAME("なすがまま")
		;-------------------------------------------------
		;REACT派生8, おなにーに夢中で聞いてない(自慰中)
		;-------------------------------------------------
		IF 口上記入チェック(0) && TFLAG:REACT派生 == 8
			;%CALLNAME:MASTER%は自慰に没頭してまともに返事ができなかった…
			;%CALLNAME:TARGET%は不満そうだ…
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;REACT派生9, 気持ちよくって生返事(快感がある)
		;-------------------------------------------------
		IF 口上記入チェック(0) && TFLAG:REACT派生 == 9
			;%CALLNAME:MASTER%は快感に身悶えしながら頷くことしかできなかった…
			PRINTFORMW 「うふふ、やっぱりね。そうだと思ってたんだ♪」
			PRINTFORMW 「%K64_CNAME()%は気持ちよければ何でもいいんでしょ？」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;REACT派生11, 自分でもわからないまま恥ずかしいことを口走る(上記以外)
		;-------------------------------------------------
		IF 口上記入チェック() && TFLAG:REACT派生 == 11
			;%CALLNAME:MASTER%はどこか茫然としたまま受け答えした…
			;自分でもよく解らないままに恥ずかしいことを口走っている気がする…
			PRINTFORMW 「うふふ。ねぇ、本当にそんなことしちゃっても大丈夫なの？」
			PRINTFORMW 「もうキャンセルは受け付けないからね♪」
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;REACT負荷0, 初めてを捧げるから気持ち良くして欲しいと言った(自然な行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "自然な行動"
		;%CALLNAME:MASTER%は%CALLNAME:TARGET%に初めてを捧げるから、
		;その分もっと気持ち良くして欲しいと言った…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷1, 気持ち良くしてくれるんだったら初めてをあげてもいいと答えた(無理のない行動)
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理のない行動"
		;%CALLNAME:MASTER%は少し悩んでから、気持ち良くしてくれるんだったら
		;%CALLNAME:TARGET%に初めてをあげてもいいと答えた…
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT負荷2, 初めてを捧げろと言うなら、辛さを忘れられるくらい気持ち良くしてと言った
	;=================================================
	IF 口上記入チェック(0) && GET_COM_RANK() == "無理をした行動"
		;%CALLNAME:MASTER%は%CALLNAME:TARGET%に、初めてを捧げろと言うなら
		;辛さを忘れられるくらい気持ち良くして欲しいと言った…
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_4, 合意しない FIRSTREACT(4)
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT分類がその他の場合(その他のREACT分類の場合、@ACTION_APPLY2_6にて"拒否"にセットされる)
;-------------------------------------------------
IF 口上記入チェック() && IS_COMGRONAME("拒否")
	;%CALLNAME:MASTER%の合意をもらえなかった%CALLNAME:TARGET%は少し失望したようだ…
	
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック() && FIRSTREACT(4)
		PRINTFORMW 「ふーん、それはよかった」
		PRINTFORMW 「そうでないと奪う愉しみが６割引だもの♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;その他
	;=================================================
	IF 口上記入チェック()
		PRINTFORMW 「そんなに大事なの？ くすっ、%K64_CNAME()%って面白いね♪」
		RETURN 1
	ENDIF
ENDIF
