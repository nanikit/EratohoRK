;========================================================================
;愛撫系 ACT10～19 & 27
;========================================================================
;・TFLAG:ACT(10=手で愛撫/11=胸愛撫/12=口で愛撫/13=アナル愛撫/14=アナル舐め/15=キス/16=性器を擦り合う/17=パイズリする/18=足コキする/19=指挿入れ/27=押し倒す)
;・TFLAG:ACT分類 == 1に該当
;・ACT18, 足コキは異常系から派生する場合あり
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "愛撫")で拾う事が可能
;・調教者が調教対象を愛撫する。この口上の時点で調教者はすでに手を出している
;・媚薬を使用するケースがある
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では口上側のRESULT(終端のRETURN 1など)によって口上が表示されたのか否か、
;・また箇所毎の特有の挙動(媚薬等口上をスキップなど)を制御しています
;・@口上記入チェックの場合代入処理を省略できますが、RESULT値は口上側で決めうちする必要があります
;・特別な理由がない限り、テンプレとしてはRETURN 1を記述しておきます
;-------------------------------------------------

;────────────────────────────────────
;ACT10, 手で愛撫(ACT10_0, 愛撫する/ACT10_1, 手淫する/ACT10_2, ペニスバンド手淫)
;────────────────────────────────────
;・Ｃ中心に愛撫する
;・調教対象にペニスがある場合、及びペニスバンド装着時には派生コマンドになる
;・調教対象が自慰中の場合は解除される
;・調教者の手に汚れがある場合不快SOURCEが変動する
;・調教者技巧、愛撫、器用な指先素質に応じて勃起度上昇
;-------------------------------------------------
;・こいしちゃんはちんこ弄りたがりなので、ちんこがあると確率が跳ね上がる。1日7回は弄る
;・ちんこ無くてもペニバンさえあれば突っ込んでから弄る
;・普通の愛撫は結構レアかもしれない。EXTRAモード向き？
;-------------------------------------------------
@KOJO_ACT_K64_10
;-------------------------------------------------
;・TFLAG:ACT派生(0=愛撫/1=手淫/2=ペニスバンド手淫)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT10_1, 手淫する
;━━━━━━━━━━━━━━━━━━━━━━━━
;・男性器への愛撫を行う、つまり手コキ
;・他の派生と比べてSOURCE:MASTER:快Ｃは+200%
;・調教者技巧、愛撫、器用な指先素質に応じて勃起度上昇
;-------------------------------------------------
;必要に応じて、TEQUIP:オナホールやTCVAR:MASTER:勃起度
;PANTSCHECK(1)等で分岐を設ける事もできます。
;まともにやると手間が尋常じゃないので、効率よく記述する事をお勧めします
;-------------------------------------------------
IF 口上記入チェック() && TFLAG:ACT派生 == 1
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック() && FIRSTACT(0)
		PRINTFORMW 「えへへ、%K64_CNAME()%のおちんちんに触っちゃった♪」
		SIF TALENT:MASTER:ふたなり
			PRINTFORMW 「%K64_CNAME()%って変わってるよね…本当は女の子っておちんちん生えないんだよ？」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教内初回
	;=================================================
	IF 口上記入チェック(0) && ONCEACT(0)
		IF PANTSCHECK(1)	;下半身服装がある場合
			PRINTFORMW 
		ELSE				;その他
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;オナホール装備中
	;=================================================
	IF 口上記入チェック(0) && TEQUIP:オナホール
		IF TALENT:MASTER:童貞
			IF TEQUIP:TARGET:アイマスク
				;シチュが限定的すぎな気がするけど気にしたら負け。そしてまず発生しない。自分でも何がしたいのかはよくわからないｗ
				;関数の運用実験？
				LOCALS = %K64_SYMBOL("BHeartW")%
				SELECTCASE RANDACT(3, 0)
					CASE 0
						CALL HTMLPRINTW(@"「あっ%LOCALS% あっ%LOCALS% 気持ち、いいよ%LOCALS% %K64_CNAME()%のおちんちん%LOCALS%」")
						CALL HTMLPRINTW(@"「熱いの、ゴリゴリって…%LOCALS%」")
						PRINTFORMW 「…どう？ 雰囲気出るでしょ？ えへへー♪」
					CASE 1
						CALL HTMLPRINTW(@"「私のおまんこ、気持ちいい？ うふふ、いっぱい締め付けてあげるね%LOCALS%」")
						CALL HTMLPRINTW(@"「ほらっ%LOCALS% イッちゃえ%LOCALS%」")
						PRINTFORMW 「…どう？ 雰囲気出るでしょ？ くすくす♪」
					CASEELSE
						PRINTFORMW 「%K64_CNAME()%の童貞こどもおちんちんは小さくて、私は全然気持ちよくないけど…」
						CALL HTMLPRINTW(@"「%K64_CNAME()%は気にしないで私のおまんこで気持ちよくなってね%LOCALS%」")
						PRINTFORMW 「…なーんて。傷ついちゃった？ くすくす♪」
				ENDSELECT
			ELSE
				SELECTCASE RANDACT(2, 1)
					CASE 0
						PRINTFORMW 「どう？ おもちゃのおまんことセックス、気持ちいい？」
						PRINTFORMW 「よく出来てると思うよ、これ。本物の女の子から型を取ったのかな」
						PRINTFORMW 「ねぇ…どんな女の子のおまんこだと思う？」
						PRINTFORMW 「ひょっとしたら貴方の知ってる娘だったりして。くすくす♪」
					CASEELSE
						PRINTFORMW 「…もっと喜んだらどう？」
						PRINTFORMW 「こうやってご主人様がおもちゃのおまんことセックスさせてあげてるのに♪」
				ENDSELECT
			ENDIF
		ELSE
			PRINTFORMW 「これ、%K64_CNAME()%にはちょっと贅沢すぎるんじゃない？」
			PRINTFORMW 「その辺の靴下で十分な気がしてきたわ…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象がイキそう
	;=================================================
	IF 口上記入チェック(0) && TCVAR:MASTER:イきそう
		;-------------------------------------------------
		;前から愛撫
		;-------------------------------------------------
		IF 口上記入チェック(0) && TCVAR:MASTER:位置前後 == 1
			SELECTCASE TCVAR:MASTER:焦らし度	;焦らし度によって分岐
				CASE IS > 1	;焦らし中
					PRINTFORMW 
				CASE 1		;焦らし終了
					PRINTFORMW 
				CASEELSE	;焦らし無し
					PRINTFORMW 
			ENDSELECT
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;後ろから愛撫
		;-------------------------------------------------
		IF 口上記入チェック(0) && TCVAR:MASTER:位置前後 == 2
			SELECTCASE TCVAR:MASTER:焦らし度	;焦らし度によって分岐
				CASE IS > 1	;焦らし中
					PRINTFORMW 
				CASE 1		;焦らし終了
					PRINTFORMW 
				CASEELSE	;焦らし無し
					PRINTFORMW 
			ENDSELECT
			RETURN 1
		ENDIF
		
		;位置前後分岐を埋めるなら必要なし。埋めないならここで補完させましょう
		;不要な分岐は削除、もしくはコメントアウトすることで負荷は軽くなります
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 口上記入チェック()
			SELECTCASE TCVAR:MASTER:焦らし度	;焦らし度によって分岐
				CASE IS > 1	;焦らし中
					IF PANTSCHECK(1)	;布地越しに
						PRINTFORMW 「%INNER_B(MASTER, 1)%の中で出しちゃったら大変だもの」
						PRINTFORMW 「がまんがまん♪」
					ELSE				;直に
						PRINTFORMW 「うふふふ…」
						PRINTFORMW 「これ、気持ちいいのかな？ それとも苦しい？」
					ENDIF
				CASE 1		;焦らし終了
					IF PANTSCHECK(1)	;布地越しに
						PRINTFORMW 「%INNER_B(MASTER, 1)%穿いたまま気持ちよくなっちゃって平気なの？」
						PRINTFORMW 「くすっ…もう手遅れだと思うけど♪」
					ELSE				;直に
						PRINTFORMW 「もう少し可愛がってあげてもよかったんだけど…」
						PRINTFORMW 「まぁいいわ。いい声で鳴いてね♪」
					ENDIF
				CASEELSE	;焦らし無し
					IF PANTSCHECK(1)	;布地越しに
						PRINTFORMW 「…このままイッちゃう？ %INNER_B(MASTER, 1)%の中で」
						CALL HTMLPRINTW(@"「どうする？ くすくす%K64_SYMBOL("BHeartW")%」")
					ELSE				;直に
						PRINTFORMW 「ビクビクして…そろそろ限界なんじゃない？」
						CALL HTMLPRINTW(@"「どーしよっかなー%K64_SYMBOL("BHeartW")%」")
					ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;連続実行
	;=================================================
	IF 口上記入チェック() && TFLAG:ACT == TFLAG:前ACT
		;-------------------------------------------------
		;顔面騎乗中
		;-------------------------------------------------
		IF 口上記入チェック() && TRUECHECK(TEQUIP:TARGET:顔面騎乗, TEQUIP:TARGET:顔面騎乗アナル) > 0
			PRINTFORMW 「このまま握りつぶしちゃおうかな～？」
			PRINTFORMW 「……冗談よ。うふふ」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 口上記入チェック(0)
			IF TCVAR:MASTER:位置前後 == 1	;前から愛撫
				PRINTFORMW 
			ELSE							;後ろから愛撫
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック()
		IF TCVAR:MASTER:位置前後 == 1	;前から愛撫
			PRINTFORMW 「面白いね、おちんちんって♪」
		ELSE							;後ろから愛撫
			IF TALENT:TARGET:ふたなり
				CALL HTMLPRINTW(@"「えへへ、こうしていると私がオナニーしてるみたい%K64_SYMBOL("BHeartW")%」")
			ELSE
				PRINTFORMW 「うふふ、こうしていると私のおちんちんみたいね♪」
				CALL HTMLPRINTW(@"「あれ？ でも%K64_CNAME()%は私のものだから…やっぱり『私の』だね%K64_SYMBOL("BHeartW")%」")
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;ACT10_1, 手淫する

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT10_2, ペニスバンド手淫
;━━━━━━━━━━━━━━━━━━━━━━━━
;・調教対象がペニスバンド装着時に発生
