	ENDIF
	
	;=================================================
	;連続実行
	;=================================================
	IF 口上記入チェック(0) && TFLAG:ACT == TFLAG:前ACT
		;-------------------------------------------------
		;顔面騎乗中
		;-------------------------------------------------
		IF 口上記入チェック(0) && TRUECHECK(TEQUIP:TARGET:顔面騎乗, TEQUIP:TARGET:顔面騎乗アナル) > 0
			IF TCVAR:MASTER:位置前後 == 1	;前から愛撫
				PRINTFORMW 
			ELSE							;後ろから愛撫
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 口上記入チェック(0)
			IF TCVAR:MASTER:位置前後 == 1	;前から愛撫
				PRINTFORMW 
			ELSE							;後ろから愛撫
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック()
		IF TCVAR:MASTER:位置前後 == 1		;前から愛撫
			PRINTFORMW 「どう？ おちんちん気持ちいい？」
		ELSE								;後ろから愛撫
			PRINTFORMW 「くすくす、オトコの子みたいにされるのはどんな気持ち？」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
