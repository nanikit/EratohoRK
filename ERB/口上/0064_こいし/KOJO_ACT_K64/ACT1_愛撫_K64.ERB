	;調教内初回
	;=================================================
	IF 口上記入チェック(0) && ONCEACT(1)
		IF PANTSCHECK(1)	;下半身服装がある場合
			PRINTFORMW 
		ELSE				;その他
			PRINTFORMW 
		ENDIF
