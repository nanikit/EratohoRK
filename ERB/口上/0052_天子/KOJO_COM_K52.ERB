;-------------------------------------------------
;COM0, 沈黙する
;-------------------------------------------------
;・従ったことにはならない
;・質問に対して黙秘した場合は軽く反抗した扱いになる
;-------------------------------------------------
@KOJO_COM_K52_0
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;相思相愛
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && KFLAG:0 == 1
		PRINTFORMW 「何でこんなタイミングで黙ってしまうのかしら？」
		PRINTFORMW 「私、何か変な事でも言った？」
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;溺愛
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 2000)
		PRINTFORMW 「何よ、黙らなくたっていいじゃないのよ！」
		PRINTFORMW 「私は貴方と仲良くなってきて、もっともっと一杯話したい事があるのに！」
		RETURN LOCAL
	ENDIF 
	
	;-------------------------------------------------
	;恋慕
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 1000)
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;不倶戴天
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && !FAVOR(-300)
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;一般
	PRINTFORMW 「何よ、黙らなくたっていいじゃないのよ！」
	PRINTFORMW 「少しぐらい、話し相手になってくれたっていいじゃない！」
	RETURN LOCAL
ENDIF

