;二人とも何か脱いだ FIRSTDATUI(10)
;------------------------------------------------
;・主にセックス系行動の予備動作で発生する
;・ワンピース脱衣ならこの時点で「ブラ・パンツ見せ」は死ぬ？
;・ソックスならまだ可能性も
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:強制脱衣 > 0 && TFLAG:自主脱衣 && !IS_NOWASSIACTNAME(, "脱衣")
	;=================================================
	;初回
	;=================================================
	IF 口上記入チェック(100) && FIRSTDATUI(10)
		PRINTFORMW 「…………ふふっ♪」
		PRINTFORMW 「お互い無言だと、逆にドキドキしちゃいますね…」
		PRINTFORMW 「ほら、お洋服貸してください？」
		PRINTFORMW 「皺にならないように、ちゃんと畳んでおきますね♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者が[情欲]かつ全裸になった
	;=================================================
	IF 口上記入チェック(100) && T_COND("情欲") && ZENRACHECK(0)
		;ご主人様妊娠中！！
		IF TALENT:TARGET:妊娠 == 1
			PRINTFORMDW %RAND_SPLIT("ぽっこと膨らんだ妊婦腹/なだらかな丸みを帯びた腹")%にそっと手を添え、最後の一枚に指をかける…
			PRINTFORMW 「うんしょっ……えへへっ、全部脱いじゃいました…」
		;妊娠してないよ！
		ELSE
			PRINTFORMW 「%RAND_SPLIT("うんしょ/よいしょ")%っ%RAND_SPLIT("と/")%……えへへっ、全部脱いじゃいました…」
		ENDIF
		IF RAND:2 == 0
			PRINTFORMW 「リリーの%RAND_SPLIT("ドキドキ/興奮")%、伝わるかな？」
		ELSE
			PRINTFORMW 「リリーの%RAND_SPLIT("恥ずかしい/えっちな")%ところ…全部見せてあげるね♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(100)
		IF FAVOR(1000, 1000)
			IF RAND:2 == 0
				PRINTFORMW 「恥ずかしいけど…%RAND_SPLIT("ふふ/")%」
				PRINTFORMW 「なんだか%RAND_SPLIT("ドキドキ/こーふん")%しちゃ%RAND_SPLIT("いますね/うね")%」
			ELSE
				PRINTFORMW 「%RAND_SPLIT("お洋服が/")%皺にならないように、ちゃんと畳んでー…%RAND_SPLIT("よし/うん")%っ」
				PRINTFORMW 「%RAND_SPLIT("ほら、おいで/どうですか/えへへ")%…ぎゅってすると、さっきよりも%RAND_SPLIT("ドキドキしちゃう/あったかい/気持ちいい")%ね…♪」
			ENDIF
		ELSE
			IF RAND:2 == 0
				PRINTFORMW 「着たままもいいですけど…」
				PRINTFORMW 「やっぱりリリーは%RAND_SPLIT("こっちの/あなたの体温が感じられる")%方が好きですね…%RAND_SPLIT("ふふ/えへへ")%♪」
			ELSE
				PRINTFORMW 「準備はいいですか？」
				PRINTFORMW 「それじゃ%RAND_SPLIT("そろそろ/さっそく")%始めちゃいましょか」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF

