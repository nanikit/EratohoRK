;────────────────────────────────────
;ACT27, 押し倒す
;────────────────────────────────────
;・押し倒す/押し倒し終了
;・既に押し倒している場合、解放になる
;・TFLAG:お仕置きフラグがたっていると発生しない
;・今日の方針が"イチャイチャ"だと発生しない
;・TEQUIP:ペニスバンド装着中には発生しない
;・焦らし中には発生しない
;-------------------------------------------------
@KOJO_ACT_K15_27
;口上独自内部処理
CALL DECIDEACTCHECK_K15
;-------------------------------------------------
;TFLAG:ACT派生（=0,押し倒す/=1,押し倒し終了）
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT27_0, 押し倒す			%CALLNAME:MASTER%を押し倒した…
;━━━━━━━━━━━━━━━━━━━━━━━━
;地の文に派生はありませんが、@TARGET_DESCRIPTIONで状況描写が入る可能性があります
;-------------------------------------------------
IF 口上記入チェック(100) && !TFLAG:ACT派生
	;アイマスク装着中
	IF TEQUIP:アイマスク
		PRINTFORMW 「周りが見えないと不安ですか？　ふふっ♪」
		PRINTFORM 「大丈夫……リリーが傍に居ますから
		CALL HEARTCB
		PRINTFORMW 」
		PRINTL  
	ENDIF
	IF FAVOR(0, 2000)
		SELECTCASE RANDACT(4, 0)
			CASE 0
				PRINTFORMW 「ほら、これでもう動けないね」
				PRINTFORMW 「これからいっぱいエッチな事してあげますからね♪」
			CASE 1
				PRINTFORM 「すこしの間……ぎゅってさせてくださいね……んー
				CALL HEARTCB
				PRINTFORMW 」
				PRINTFORMW 「……ふふっ♪　やっぱり%CNAME(MASTER, TARGET)%って優しいね」
				PRINTFORMW 「このままエッチな事してあげますね……」
			CASE 2
				PRINTFORMW 「リリーのドキドキ、伝わってますか？」
				PRINTFORMW 「……んー、わからないなら……教えてあげますね♪」
			CASE 3
				PRINTFORMW 「ねぇ、リリーの欲しいものわかりますか？」
				PRINTFORMW 「……『精』ですか？　………ぶーっ！不正解ですっ！」
				PRINTL  
				PRINTFORM 「正解は……『%CNAME(MASTER, TARGET)%』ですよ
				CALL HEARTCB
				PRINTFORMW 」
				PRINTFORMW 「もう間違わない様に、ちゃんと教えてあげますね♪」
		ENDSELECT
	ELSE
		SELECTCASE RANDACT(4, 0)
			CASE 0
				PRINTFORMW 「リリーに押し倒されちゃったね」
				PRINTFORMW 「イタズラ妖精に押し倒された子はどうなちゃうんでしょう？」
				PRINTFORMW 「ふふっ♪　わからないなら教えてあげますね♪」
			CASE 1
				PRINTFORMW 「動けませんか？ ふふっ♪」
				PRINTFORMW 「それじゃぁ、たっぷりイタズラしちゃいますね♪」
			CASE 2
				PRINTFORMW 「%CNAME(MASTER, TARGET)%のドキドキ、伝わってきますよ……」
				PRINTFORMW 「緊張、恐怖、不安……あれ、期待もありますね？ ふふっ♪」
				PRINTFORMW 「期待通り、えっちなイタズラしちゃいますねっ♪」
			CASE 3
				PRINTFORMW 「ねぇ、リリーの欲しいものわかりますか？」
				PRINTFORMW 「……『精』ですか？　………ふふっ、正解ですっ♪」
				PRINTL  
				PRINTFORMW 「正解したごほーびに……たーっぷり搾り取ってあげますね♪」
		ENDSELECT
	ENDIF
	
	;ACT分類　0,会話、1,愛撫、2,道具、3,性交、4,羞恥、5,奉仕、6,加虐、7,異常、8,使役、9,休憩
	;調教方針　1,ノーマルプレイ、2,異常プレイ、3,休憩、4,ハードプレイ、5,ソフトプレイ
	;行動予約/方針予約/ACT分類予約
	;今日の方針が子作り
	IF IS_POLICY("今日", "子作り") && PERCENT(90)
			;分類性交
			TFLAG:ACT分類予約 = 3
			WAIT
	ELSEIF AFFECTION("相思相愛") && PERCENT(50)
		IF !TALENT:MASTER:処女 && TEQUIP:アイマスク && PERCENT(30)
			;逆レイプ
			TCVAR:行動予約 = 103
			WAIT
		ELSEIF !TALENT:MASTER:処女 && PERCENT(30)
			;分類性交
			TFLAG:ACT分類予約 = 3
			WAIT
		ELSEIF PERCENT(30)
			;口で愛撫
			TCVAR:行動予約 = 12
			WAIT
		ELSEIF PERCENT(30)
			;パイズリする
			TCVAR:行動予約 = 17
			WAIT
		ELSEIF PERCENT(30)
			;アナル舐め
			TCVAR:行動予約 = 14
			WAIT
		ELSEIF PERCENT(30)
			;性器を擦り合う
			TCVAR:行動予約 = 16
			WAIT
		ENDIF
	ELSEIF AFFECTION("調教完了") && PERCENT(50)
		IF !TALENT:MASTER:処女 && TEQUIP:アイマスク && PERCENT(30)
			;逆レイプ
			TCVAR:行動予約 = 103
			WAIT
		ELSEIF !TALENT:MASTER:処女 && PERCENT(20)
			;分類性交
			TFLAG:ACT分類予約 = 3
			WAIT
		ELSEIF PERCENT(30)
			;口で愛撫
			TCVAR:行動予約 = 12
			WAIT
		ELSEIF PERCENT(30)
			;パイズリする
			TCVAR:行動予約 = 17
			WAIT
		ELSEIF PERCENT(30)
			;性器を擦り合う
			TCVAR:行動予約 = 16
			WAIT
		ELSEIF PERCENT(30)
			;アナル舐め
			TCVAR:行動予約 = 14
			WAIT
		ENDIF
	ELSEIF AFFECTION("目覚め") && PERCENT(50)
		IF PERCENT(30)
			;パイズリする
			TCVAR:行動予約 = 17
			WAIT
		ELSEIF PERCENT(30)
			;口で愛撫
			TCVAR:行動予約 = 12
			WAIT
		ELSEIF PERCENT(30)
			;アナル舐め
			TCVAR:行動予約 = 14
			WAIT
		ELSEIF PERCENT(30)
			;性器を擦り合う
			TCVAR:行動予約 = 16
			WAIT
		ENDIF
	ENDIF
	RETURN 1
ENDIF

