		;アナルビーズ使用中
		IF TEQUIP:アナルビーズ
			PRINTFORMW 
		;アナルバイブ使用中
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;満足ボーナスで"ハード"を選んでいる場合はお仕置きフラグが立ったままになるので
	;IF 口上記入チェック(25)のように確率にした方が良い場合もある
	;=================================================
	;お仕置きモード
	;=================================================
	IF 口上記入チェック(0) && TFLAG:お仕置きフラグ
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(0)
		PRINTFORMW
		RETURN 1
	ENDIF
