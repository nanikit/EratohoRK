	;=================================================
	;前COMがイかせて！
	;=================================================
	IF 口上記入チェック(0) && IS_PREVCOMNAME("イかせて！")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが気持ち良くして
	;=================================================
	IF 口上記入チェック(0) && IS_PREVCOMNAME("気持ち良くして")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;調教対象がイキそう
	;=================================================
	IF 口上記入チェック(0) && TCVAR:MASTER:イきそう
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;調教者も自慰中
	;=================================================
	IF 口上記入チェック(0) && GETBIT(TEQUIP:自慰中, 1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;オナホール装着中
	;=================================================
