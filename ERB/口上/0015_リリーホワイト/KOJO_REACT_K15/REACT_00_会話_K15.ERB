;────────────────────────────────────
;REACT5, 衣装の変更を要求する・個別リアクション
;────────────────────────────────────
;・調教対象のリアクションによって一枚脱ぐ、二枚脱ぐ、脱がないに分かれる
;・REACT分類が"消極的従う", "受け入れる"の場合は一枚脱ぐ
;・REACT分類が"積極的従う", "平然と従う"の場合は二枚脱ぐ
;・その他の場合は脱がない(PALAM恥情/反抗の数値によって、恥ずかしくて脱がない、嫌がって脱がないに分かれる)
;-------------------------------------------------
@KOJO_REACT_K15_5
;-------------------------------------------------
;・TFLAG:ACT派生(0=服を脱ぐように命令/1=服を着るように命令)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT5_0, 服を脱ぐように命令
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:ACT派生 == 0
	;=================================================
	;一枚脱いだ(消極的反応)
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("消極的従う/受け入れる")
		;-------------------------------------------------
		;最後の一枚を脱いで全裸になった
		;-------------------------------------------------
		IF 口上記入チェック(0) && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			;REACT分類受け入れる			全裸になり、期待と興奮で肌が火照るのを感じた
			IF IS_COMGRONAME("受け入れる")
				PRINTFORMW 
			;REACT分類消極的従う			仕方なく最後の一枚を脱ぎ、全裸になった
			ELSE
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;緩やかな動きで服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("受け入れる")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;仕方なく服を一枚脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;上層の分岐を全て埋めた場合は不要な箇所
		;逆に埋めない場合はこの箇所が補完の役目を果たします
		;-------------------------------------------------
		;その他・条件なし
		;-------------------------------------------------
		IF 口上記入チェック(100)
			IF FAVOR(0, 2000)
				IF RAND:2 == 0
					PRINTFORMW 「恥ずかしそうな%CNAME(MASTER)%の表情……ゾクゾクしちゃいます♪」
					PRINTFORM 「……%CNAME(MASTER)%のえっちな身体、リリーにもっとよく見せてください
					CALL HEARTCB
					PRINTFORMW 」
				ELSE
					PRINTFORMW 「リリーに裸を見られるのが恥ずかしいんですか？」
					PRINTFORMW 「……違いますよね？」
					PRINTFORMW 「『見られること』じゃなくて『見られて感じちゃうこと』が恥ずかしいの」
					PRINTFORM 「露出狂の才能まであるなんて、本当にヘンタイさんなんですね
					CALL HEARTCB
					PRINTFORMW 」
				ENDIF
			ELSE
				IF RAND:2 == 0
					PRINTFORMW 「そんなに焦らされると、リリーも興奮しちゃいます♪」
					PRINTFORMW 「ほら、続けてください……恥ずかしいところ、リリーが全部見てあげますから」
				ELSE
					PRINTFORMW 「お顔真っ赤っかですね……ふふっ♪」
					PRINTFORMW 「もっと恥ずかしそうなお顔、リリーに見せて欲しいな」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;二枚脱いだ(積極的反応)
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("積極的従う/平然と従う")
		;-------------------------------------------------
		;全裸になった
		;-------------------------------------------------
		IF 口上記入チェック(0) && (TEQUIP:MASTER:アクセサリ + TEQUIP:MASTER:靴下 + TEQUIP:MASTER:下半身下着 + TEQUIP:MASTER:上半身下着 + TEQUIP:MASTER:下半身上着 + TEQUIP:MASTER:上半身上着 + TEQUIP:MASTER:全身上着) == 0
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;積極的に脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;手際よく脱ぎ捨てた
		;-------------------------------------------------
		IF 口上記入チェック(0) && IS_COMGRONAME("平然と従う")
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;上層の分岐を全て埋めた場合は不要な箇所
		;逆に埋めない場合はこの箇所が補完の役目を果たします
		;-------------------------------------------------
		;その他・条件なし
		;-------------------------------------------------
		IF 口上記入チェック(100)
			IF FAVOR(0, 2000)
				IF RAND:2 == 0
					PRINTFORMW 「おまたスースーしますか？　ふふっ♪」
					PRINTFORMW 「おいで♪　リリーが温めてあげます♪」
				ELSE
					PRINTFORMW 「勢いよく脱いじゃって、すっかりリリーの言いなりですね♪」
					PRINTFORMW 「もうちょっと恥じらいがあった方が興奮するんですけど……ふふっ♪」
				ENDIF
			ELSE
				PRINTFORMW 「ふふっ♪　そんなに焦って脱がなくても大丈夫ですよ」
				PRINTFORMW 「もっとゆっくり、二人の時間を楽しみましょう♪」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;脱がなかった
	;=================================================
	IF 口上記入チェック(100) && IS_COMGRONAME("拒否")
		;恥ずかしくて脱げなかった
		IF PALAM:MASTER:恥情 > PALAM:MASTER:反抗
			IF FAVOR(0, 2000)
				PRINTFORMW 「着たままえっちしたいんですか？」
				PRINTFORMW 「それとも、リリーの手で脱がせて欲しい……とか？」
			ELSE
				PRINTFORMW 「リリーがお手伝いしないと、お着替えも一人でできないんですか？」
				PRINTFORMW 「その方が、よっぽど恥ずかしいと思いますけど……」
			ENDIF
		;嫌がって脱がなかった
		ELSE
			IF FAVOR(0, 2000)
				PRINTFORMW 「リリーに無理矢理脱がされたいのかな？　ふふっ♪」
				PRINTFORMW 「いいですよ♪　でも、ちゃんと抵抗して、恥ずかしがってくださいね？」
				PRINTFORM 「その方が……興奮しちゃいますから
				CALL HEARTCB
				PRINTFORMW 」
			ELSE
				IF RAND:2 == 0
					PRINTFORMW 「お顔真っ赤にしちゃって、かわいいっ♪」
					PRINTFORMW 「%CNAME(MASTER)%の恥ずかしがる顔、もっと見たくなっちゃいました」
				ELSE
					PRINTFORMW 「むー、恥ずかしくて脱げませんか……」
					PRINTFORMW 「でも、このままするとお洋服皺になっちゃいますよ？」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF


