;━━━━━━━━━━━━━━━━━━━━━━━━
;[릴리 화이트]님마력 사용전
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 0
	;=================================================
	;첫회
	;=================================================
	IF 구상기입체크(0) && FIRSTEVENT(0)
		PRINTFORML 「」
		RETURN 1
	ENDIF
	
	;-------------------------
	;첫회 이외의 처리
	;-------------------------
	IF RAND:2 == 0
		PRINTFORML 「그럼,%RAND_SPLIT("어떻게 하지요/신경이 쓰이는 것은 있습니까")%?」
	ELSE
		PRINTFORML 「%RAND_SPLIT("어떻게 할까요/네～와/헤매네요")%……%RAND_SPLIT("응/좋아")%、%CNAME(MASTER)%가 선택해도 좋아요♪」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[릴리 화이트]님의 조교 기교가 업
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && ARG == 1
	PRINTFORML 
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[릴리 화이트]님지식을 얻기 전
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 2
	PRINTFORML 「공부는 서투릅니다만…」
	RETURN 1
