;숍 이벤트 구상
;========================================================================

;─
;EV101, 의상 변경
;─
;·숍으로부터 의상 변경을 실시했을 경우에 표시
;-------------------------------------------------
@KOJO_EVENT_K15_101(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=
;  1의 위…(0, 이제(벌써) 입고 있다, 1, 갈아입은, 2, 부끄러워서 입을 수 없는, 3, 싫어해 입지 않는, 9, 가지고 있지 않다)
;  10의 위…(0, 통상, 10, 반나체, 20, 전라, 30, 속옷 없음)
;  백의 위… 코스프레를 위해서(때문에) 보류
;  천의 위…(0, 조교자, 1000, 조교 대상)
;  예(조교 대상이 부끄러워해 전라로 할 수 없었다=1022)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 갈아입었다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상이 되었다
		CASE 1
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORML 「아기도 있고, 따뜻하게 하지 않으면이군요♪」
			ELSE
				IF RAND:2 == 0
					PRINTFORML 「역시 평소의 옷이 제일 침착하네요」
				ELSE
					PRINTFORML 「이것이라면%RAND_SPLIT("벗기는 즐거움도 있어/착의 엣치도 성과")%것이군요♪」
				ENDIF
			ENDIF
			RETURN 1
		;반나체가 되었다
		CASE 11
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				;플래이버야!
				PRINTFORML 「시 스루의 마타니티드레스는… 변태♪」
			ELSE
				PRINTFORML 「조금 홀가분하게 되었습니닷」
			ENDIF
			RETURN 1
		;전라가 되었다
		CASE 21
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORML 「조금 스으스으 하네요…」
				PRINTFORML 「이봐요, 배의 아이가 감기 걸리지 않도록…」
				PRINTFORML 「언제나 릴리의 일, 꽉 하고 있어 주세요♪」
			ELSE
				IF RAND:2 == 0
					PRINTFORML 「%RAND_SPLIT("알몸이 되어 버렸습니다/가랑이가 스으스으 합니다")%… 에헤헷」
					PRINTFORML 「이것이라면, 언제 어디서나 엣치할 수 있네요♪」
				ELSE
					PRINTFORML 「그러면, 마지막 한 장도…… 에헤헷%RAND_SPLIT(", 벗어 버렸습니다/")%♪」
				ENDIF
			ENDIF
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 부끄러워해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		;반나체라도 부끄럽다
		CASE 12
			PRINTFORML 「%RAND_SPLIT("그/그러니까… 그/혐이 아닙니다만")%, 아직 부끄러워서…」
			RETURN 1
		;평상시부터 전라는 조금……
		CASE 22
			PRINTFORML 「%RAND_SPLIT("어와/무리 말하지 말아 주세요/그")%… 아직 마음의 준비가…… 우우…」
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 싫어해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		;반나체라도 부끄럽다
		CASE 13
			PRINTFORML 「저, 아직 부끄러워서…」
			RETURN 1
		;평상시부터 전라는 조금……
		CASE 23
			PRINTFORML 「엣또… 아직 마음의 준비가…… 우우…」
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교자가 갈아입는 것을 그만두었다
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:TARGET%의 의상은 벌써[　　]로 설정되어 있습니다
;------------------------------------------------
IF 구상기입체크(0) && ARG < 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		;통상인 채
		CASE 0
			PRINTFORML 
			RETURN 1
		;반나체인 채
		CASE 10
			PRINTFORML 
			RETURN 1
		;전라인 채
		CASE 20
			PRINTFORML 
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상을 갈아입게 했다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상이 되었다
		CASE 1001
			IF RAND:2 == 0
				PRINTFORML 「역시 그것이 제일 어울리네요」
			ELSE
				PRINTFORML 「역시 그것이 제일 어울리네요」
				PRINTFORML 「뭐,%RAND_SPLIT("곧바로/조교가 시작하면")%벗겨 버립니다만 말이죠♪」
			ENDIF
			RETURN 1
		;반나체가 되었다
		CASE 1011
			PRINTFORML 「조금 두근두근 해 버리네요」
			RETURN 1
		;전라가 되었다
		CASE 1021
			IF RAND:2 == 0
				PRINTFORML 「%RAND_SPLIT("이제(벌써)/이봐요")%, 숨겨서는 안됩니다?」
				PRINTFORML 「%RAND_SPLIT("부끄러운 곳/엣치한 신체")%, 릴리에 전부 보여 주세요♪」
			ELSE
				PRINTFORML 「알몸이 되고 싶다고… 후훗」
				PRINTFORML 「%RAND_SPLIT("변태씨/엣치하네요")%♪」
			ENDIF
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 부끄러워해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG & 10) == 2
	SELECTCASE ARG
		;반나체라도 부끄럽다
		CASE 1012
			PRINTFORML 「므우, 유감입니다…」
			RETURN 1
		;평상시부터 전라 같은거 부끄럽다
		CASE 1022
			PRINTFORML 「안돼… 입니까?」
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상이 싫어해 갈아입을 수 없다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG >= 1000 && (ARG & 10) == 3
	SELECTCASE ARG
		;반나체는 싫어
		CASE 1013
			PRINTFORML 「므우, 유감입니다…」
			RETURN 1
		;평상시부터 전라 같은거 싫어
		CASE 1023
			PRINTFORML 「안돼… 입니까?」
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;조교 대상을 갈아입게 하는 것을 그만둔 FIRSTEVENT(70)
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:MASTER%의 의상은 벌써[　　]로 설정되어 있습니다
;------------------------------------------------
IF 구상기입체크(0) && ARG >= 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		;통상인 채
		CASE 1000
			PRINTFORML 
			RETURN 1
		;반나체인 채
		CASE 1010
			PRINTFORML 
			RETURN 1
		;전라인 채
		CASE 1020
			PRINTFORML 
			RETURN 1
	ENDSELECT
ENDIF

;─
;EV102, 조교자 변경
;─
;·조교자를 교대시켰을 때에 표시
;-------------------------------------------------
@KOJO_EVENT_K15_102(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 구상주가 다른 캐릭터에게 맡긴, 1, 구상주가 조교자가 되었다)
;구조교자는 「%CALLNAME:SHOP_OLDTARGET%」, 신조교자는 「%CALLNAME:SHOP_NEWTARGET%」로 참조할 수 있다
;구상주는 TARGET로 참조
;-------------------------------------------------
;·반항각인은 조교자 마다 보존되므로, MARK:MASTER:반항각인 따위와 해 버리면 0밖에 되지 않기 때문에 주의
;·이것은 굴복각인도 같습니다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;다른 조교자에게 교대
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 0
	IF AFFECTION("상사상애")
		PRINTFORML 「뒤는%CALLNAME:SHOP_NEWTARGET%에 맡기네요」
		PRINTFORML 「%CNAME(MASTER)%, 좋은 아이로 합니다?   후후」
		PRINTFORM 「그러면… 릴리는%CNAME(MASTER)%의 침대에서 기다리고 있기 때문에, 천천히 즐겨 와 주세요
		CALL HEARTCB
		PRINTFORMW 」
	ELSE
		PRINTFORML 「%CNAME(MASTER)%、%CALLNAME:SHOP_NEWTARGET%에 폐를 끼치지 않도록요?」
		PRINTFORML 「그것과… 가끔으로 좋으니까……」
		PRINTFORMW 「조교가 끝나면, 릴리의 방에도 놀러 왔으면 좋구나♪」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;[릴리 화이트]님이 조교자가 되었다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 1
	;주인님
	IF TALENT:TARGET:주인님
		;상사상애
		IF AFFECTION("상사상애")
			IF RAND:2 == 0
				PRINTFORML 「어서 오세요,%CNAME(MASTER)%♪」
				PRINTFORM 「외로운 생각을 시킨 만큼, 충분히 사랑해 주세요
				CALL HEARTCB
				PRINTFORMW 」
			ELSE
				PRINTFORMW 「릴리에 조교해 주었으면 합니까?   …」
				PRINTFORMW 「그러면, 우선은 릴리를 『꾹』은 껴안아 주세요」
				PRINTFORMW 「그리고 『외롭게 해 미안해요』 『제대로 참을 수 있었군요, 훌륭하다』는 머리 스윽스윽 해…」
				PRINTFORM 「상냥하게 침대에 데려 가… 가득한 사랑으로, 릴리를 채워 주세요
				CALL HEARTCB
				PRINTFORMW 」
			ENDIF
		;그 외
		ELSE
			PRINTFORML 「%CNAME(MASTER)%, 분명하게 좋은 아이로 하고 있었습니까?」
			PRINTFORML 「…… 와,%CNAME(MASTER)%가 어떤 변태씨에게 조교되어 버렸는가……」
			PRINTFORMW 「침대 위에서 충분히 들려주어 받으니까요♪」
		ENDIF
	;보통 조수
	ELSE
		;상사상애
		IF AFFECTION("조교 완료")
			PRINTFORM 「%RAND_SPLIT("로리콘/변태/마조돼지")%씨 안녕하세요
			CALL HEARTCB
			PRINTFORML 」
			PRINTFORML 「또 릴리에 조교되고 싶어져 버린 것입니까?   후후」
			PRINTFORML 「그러면…… 오늘부터 또, 자위 금지로 노력해요」
			PRINTFORM 「참은 만큼만, 릴리가 충분히 귀여워해 줄테니까
			CALL HEARTCB
			PRINTFORMW 」
		;그 외
		ELSE
			PRINTFORML 「저, 아무쪼록 잘 부탁드립니다」
			PRINTFORML 「%CNAME(SHOP_OLDTARGET, TARGET)%같이 능숙하게 할 수 있을까 모르지만…」
			PRINTFORMW 「릴리와 함께, 가득 즐겨요♪」
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;─
;EV110, 마력 사용
;─
;·마력을 소비해 어떠한 특전을 구입했을 때에 표시
