ELSEIF KFLAG:102 < 8
	IF FAVOR(0, 2000)
		KFLAG:102 += (RAND:3 > 0)
	ELSE
		KFLAG:102 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:102 > 7
	CFLAG:1002 = 0
ELSEIF KFLAG:102 < 1
	CFLAG:1002 = 100
ENDIF

