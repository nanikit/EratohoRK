@DECIDEACTCHECK_K15
;조교주의 행동 결정 후에 불려 가는 함수. 조교주의 행위에 응해, 행동 마다 지정된 변수를 조작해, 행동 금지 플래그를 ON/OFF 한다.
;========================================================================
;회화계 ACT0～9(0=지금의기분을얘기한다/1=자신에게로의기분을얘기한다/2=성에대한이야기를한다/3=상냥하게위로한다/4=엄격하게위협한다/5=의상의변경을요구한다/6=합의를요구한다/7=벌이라고선언한다/8=고함친다/9=기분나쁘게웃는다)

;지금의기분을얘기한다의 빈도를 체크
;TFLAG:90, 91	이번, 전회의 액션의 번호
IF TFLAG:90 == 0
	KFLAG:100 -= 5
;TFLAG:79, 80(전회, 이번 메뉴)		0, 회화, 1, 애무, 2, 도구, 3, 성교, 4, 수치, 5, 봉사, 6, 가학, 7, 이상, 8, 사역, 9, 휴식
ELSEIF TFLAG:80 == 0 && CFLAG:1000 == 0
	KFLAG:100 -= 2
ELSEIF KFLAG:100 < 8
	IF FAVOR(0, 2000)
		KFLAG:100 += (RAND:3 > 0)
	ELSE
		KFLAG:100 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:100 > 7
	CFLAG:1000 = 0
ELSEIF KFLAG:100 < 1
	CFLAG:1000 = 100
ENDIF

;자신에게로의기분을얘기한다의 빈도를 체크
IF TFLAG:90 == 1
	KFLAG:101 -= 5
ELSEIF TFLAG:80 == 0 && CFLAG:1000 == 0
	KFLAG:101 -= 2
ELSEIF KFLAG:101 < 8
	IF FAVOR(0, 2000)
		KFLAG:101 += (RAND:3 > 0)
	ELSE
		KFLAG:101 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:101 > 7
	CFLAG:1001 = 0
ELSEIF KFLAG:101 < 1
	CFLAG:1001 = 100
ENDIF

;성에대한이야기를한다의 빈도를 체크
