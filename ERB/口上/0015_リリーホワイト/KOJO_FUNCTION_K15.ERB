@DECIDEACTCHECK_K15
;조교주의 행동 결정 후에 불려 가는 함수. 조교주의 행위에 응해, 행동 마다 지정된 변수를 조작해, 행동 금지 플래그를 ON/OFF 한다.
;========================================================================
;회화계 ACT0~9(0=지금의 기분을 (듣)묻는다/1=자신에게로의 기분을 (듣)묻는다/2=성에 대한 이야기를 한다/3=상냥하게 위로한다/4=엄격하게 위협한다/5=의상의 변경을 요구한다/6=합의를 요구한다/7=벌이라고 선언한다/8=고함친다/9=기분 나쁘게 웃는다)

;지금의 기분을 (듣)묻는 것 빈도를 체크
;TFLAG:90, 91	이번, 전회의 액션의 번호
IF TFLAG:90 == 0
	KFLAG:100 -= 5
;TFLAG:79, 80(전회, 이번 메뉴)		0, 회화, 1, 애무, 2, 도구, 3, 성교, 4, 수치, 5, 봉사, 6, 가학, 7, 이상, 8, 사역, 9, 휴게
ELSEIF TFLAG:80 == 0 && CFLAG:1000 == 0
	KFLAG:100 -= 2
ELSEIF KFLAG:100 < 8
	IF FAVOR(0, 2000)
		KFLAG:100 += (RAND:3 > 0)
	ELSE
		KFLAG:100 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:100 > 7
	CFLAG:1000 = 0
ELSEIF KFLAG:100 < 1
	CFLAG:1000 = 100
ENDIF

;자신에게로의 기분을 (듣)묻는 것 빈도를 체크
IF TFLAG:90 == 1
	KFLAG:101 -= 5
ELSEIF TFLAG:80 == 0 && CFLAG:1000 == 0
	KFLAG:101 -= 2
ELSEIF KFLAG:101 < 8
	IF FAVOR(0, 2000)
		KFLAG:101 += (RAND:3 > 0)
	ELSE
		KFLAG:101 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:101 > 7
	CFLAG:1001 = 0
ELSEIF KFLAG:101 < 1
	CFLAG:1001 = 100
ENDIF

;성에 대한 이야기를 하는 것 빈도를 체크
IF TFLAG:90 == 2
	KFLAG:102 -= 5
ELSEIF TFLAG:80 == 0 && CFLAG:1000 == 0
	KFLAG:102 -= 2
ELSEIF KFLAG:102 < 8
	IF FAVOR(0, 2000)
		KFLAG:102 += (RAND:3 > 0)
	ELSE
		KFLAG:102 += 1 + RAND:2
	ENDIF
ENDIF
IF KFLAG:102 > 7
	CFLAG:1002 = 0
ELSEIF KFLAG:102 < 1
	CFLAG:1002 = 100
ENDIF

;========================================================================
;애무계 ACT10~19 & 27(10=손으로 애무/11=가슴 애무/12=입으로 애무/13=애널 애무/14=애널 빨고/15=키스/16=성기를 서로 켠다/17=파이즈리 한다/18=풋잡 한다/19=손가락 삽입해라/27=밀어 넘어뜨린다)
;구에서 애무
IF TFLAG:90 == 12
		KFLAG:112 += 2
ELSEIF KFLAG:112 < 8
	KFLAG:112 += 1
ENDIF
IF KFLAG:112 > 7
	CFLAG:1012 = 0
ELSEIF KFLAG:112 < 1
	CFLAG:1012 = 100
ENDIF

;키스의 빈도 체크
IF TFLAG:90 == 15
	;호감도 2000 미만에서의 키스는 생각보다는 귀중
	IF FAVOR(0, 2000)
		KFLAG:115 -= 5
	ENDIF
ELSEIF KFLAG:115 < 8
	KFLAG:115 += 1
ENDIF
IF KFLAG:115 > 7
	CFLAG:1015 = 0
ELSEIF KFLAG:115 < 1
	CFLAG:1015 = 100
ENDIF

;파이즈리 한다
IF TFLAG:90 == 17
		KFLAG:117 += 10
ELSEIF KFLAG:117 < 8
	KFLAG:117 += 1
ENDIF
IF KFLAG:117 > 7
	CFLAG:1017 = 0
ELSEIF KFLAG:117 < 1
	CFLAG:1017 = 100
ENDIF

