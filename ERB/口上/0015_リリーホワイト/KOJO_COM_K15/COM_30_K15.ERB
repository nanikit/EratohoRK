;=====================================================================
;苦痛系反応
;=====================================================================
;──────────────────────────────────
;COM30, 痛みを我慢する
;──────────────────────────────────
;・REACT分類:消極的従う
;・REACT印象:好印象小
;・SOURCE:痛みが無い場合は弾かれる
;-------------------------------------------------
;・トリガーとなる行動、ソース、パラメーター
;・SOURCE:痛み/拘束
;-------------------------------------------------
;-------------------------------------------------
;・TFLAG:REACT派生(0=痛みを我慢/1=痛いけど気持ち良い！)
;-------------------------------------------------
;=================================================
@KOJO_COM_K15_30
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM30_0, 派生：痛みを我慢する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;=================================================
	;アナル愛撫
	;=================================================
	;//このような場合はIS_NOWACTNAMEを使ってください。マジックナンバーは極力使わない方向でひとつ@/L
	IF 口上記入チェック(100) && IS_NOWACTNAME("アナル愛撫/アナル舐め/アナルバイブ/アナルビーズ/アナルセックス")
	
	;//GROUPMATCHを使うと"||"を使うよりシンプルでスマートな記述になります。評価順絡みの不具合とも無縁です
	;//GET_ACTNAMEを使うことで、IS_NOWACTNAMEと同様に文字列で参照する事も可能になります
	;IF 口上記入チェック(100) && GROUPMATCH(TFLAG:ACT, 13, 14, 22, 23, 35)

	;//()で囲まない場合、評価順の関係で想定した通りの動作にはなりません
	;//そしてこういった記述はGROUPMATCHが実装される以前のやり方で、今となってはかなり古めかしい望ましくないやり方です
	;IF 口上記入チェック(100) && (TFLAG:ACT == 13 || TFLAG:ACT == 14 || TFLAG:ACT == 23 || TFLAG:ACT == 22 || TFLAG:ACT == 35)
		SELECTCASE RANDCOM(2, 11)
			CASE 0
				PRINTFORM 「んっ……ちゃんと痛いの我慢できたね…%RAND_SPLIT("いい子いい子/えらいぞ/よく頑張りましたね")%っ
				CALL HEARTCB
				PRINTFORMW 」
				PRINTFORM 「ゆっくりお尻も慣らしていって……敏感な性器にしてあげますからね
				CALL HEARTCB
				PRINTFORMW 」
			CASE 1
				PRINTFORMW 「まだ痛みますか？……もう少し、頑張ってくださいね」
				PRINTFORM 「おしりアクメが癖になっちゃうくらい……えっちな性器に変えてあげますからね
				CALL HEARTCB
				PRINTFORMW 」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;%CALLNAME:MASTER%は苦痛を堪えながら、%CALLNAME%の行為を受け入れた…
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「痛いのにちゃんと我慢できましたね…%RAND_SPLIT("いい子いい子/えらいぞ")%っ♪」
					PRINTFORMW 「ほら、今度はもう少し激しくしますから……無理ならちゃんと言うんですよ？」
				CASE 1
					PRINTFORMW 「痛みも受け入れてくれるなんて……ほんとに健気な子ですね」
					PRINTFORMW 「ちゃんと気持ち良くしてあげますから、もう少し我慢してくださいね」
			ENDSELECT
			;%CALLNAME:MASTER%は苦悶の呻きを漏らしながら痛みを我慢した…
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「んっ…声、ちょっと漏れちゃいましたね？」
					PRINTFORM 「ちゃんと我慢しなきゃダメじゃないですかぁ
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「泣かないなんて、強い子ですね……%RAND_SPLIT("いい子いい子/えらいぞ")%っ♪」
					PRINTFORMW 「これなら、もう少し激しくしても大丈夫かな？ くすっ♪」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;無理のない行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理のない行動"
		;%CALLNAME:MASTER%は痛みを我慢した…
		;2/3の確立で真
		IF RAND:3
			SELECTCASE RANDCOM(3, 2)
				CASE 0
					PRINTFORMW 「んっ……このくらいはもう大丈夫そうですね」
					PRINTFORMW 「それとも、これじゃ少し物足りないかな？……くすっ♪」
				CASE 1
					PRINTFORMW 「痛いのも我慢できるようになってきましたね♪」
					PRINTFORM 「痛みが快感に変わる瞬間まで……あと少しですよっ
					CALL HEARTCB
					PRINTFORMW 」
				CASE 2
					PRINTFORMW 「ほら、ゆっくり息をしてみて？」
					PRINTFORMW 「すぅ～、はぁ～……はい、今度はもう少し激しくしますからねっ♪」
			ENDSELECT
		;1/3の確立で真
		ELSE
			IF 0
			ELSEIF AFFECTION("相思相愛") && PERCENT(80)
				PRINTFORMW 「痛いの、我慢してくれるんですね……嬉しいです……」
				PRINTFORMW 「もっと痛くしても、苦しくしても……リリーを受け入れてくれますか？」
			ELSEIF AFFECTION("溺愛") && PERCENT(70)
				PRINTFORMW 「痛いですか？ もう少し頑張ってくださいね」
				PRINTFORMW 「痛みの後には、気持ちい事が待ってますからね♪」
			ELSEIF AFFECTION("眷愛") && PERCENT(60)
				PRINTFORMW 「苦しいですか？ ふふっ、とってもいい表情ですよ♪」
				PRINTFORMW 「リリーを満足させてくれたら、ちゃんとご褒美あげますからね♪」
			ELSEIF AFFECTION("恐怖") || (!ALI(0) && !RAND:3) || T_COND("怒り")
				PRINTFORMW 「あれ？ もっといい声で鳴いてくれると思ったんですけど……」
				PRINTFORMW 「ドＭ豚さんには、もっと激しくしないとダメみたいですね♪」
			ELSE
				PRINTFORMW 「痛いの、我慢してくれるんですね……嬉しいです……」
				PRINTFORM 「次はもう少し強くしますけど……%CNAME(MASTER)%なら大丈夫そうですね
				CALL HEARTCB
				PRINTFORMW 」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;自然な行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "自然な行動"
		;-------------------------------------------------
		;ACTがイラマチオ
		;-------------------------------------------------
		;%CALLNAME:MASTER%は喉奥まで抉られる苦しみに嗚咽を漏らしながらも、%CALLNAME%の行為を受け入れた
		IF 口上記入チェック(100) && IS_NOWACTNAME("イラマチオ")
			PRINTFORM 「ほらっ♪ もっと苦しそうにしてっ
			CALL HEARTCB
			PRINTFORMW 」
			PRINTFORM 「喉の奥で……リリーのおちんちん、しっかり味わってくださいねっ
			CALL HEARTCB
			PRINTFORMW 」
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		SELECTCASE RANDCOM(3, 10)
			CASE 0
				PRINTFORMW 「んっ……ちゃんと痛いの我慢できましたね」
				PRINTFORMW 「ほら、おいで……はい、%RAND_SPLIT("いい子いい子/えらいぞ/よく頑張りましたね")%っ
				CALL HEARTCB
				PRINTFORMW 」
			CASE 1
				PRINTFORMW 「声、ちょっと漏れちゃいましたね」
				PRINTFORMW 「もうっ……ちゃんと我慢しなきゃメっですよ？」
			CASE 2
				PRINTFORMW 「泣かないなんて、強い子ですねっ♪」
				PRINTFORMW 「これなら、もう少し激しくしても大丈夫かな？ くすっ♪」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM30_1, 派生：痛いけど気持ち良い！
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && TFLAG:REACT派生 == 1
	;=================================================
	;アナル愛撫
	;=================================================
	IF 口上記入チェック(100) && IS_NOWACTNAME("アナル愛撫/アナル舐め/アナルバイブ/アナルビーズ/アナルセックス")
		SELECTCASE RANDCOM(2, 22)
			CASE 0
				PRINTFORM 「んっ……ちゃんと痛いの我慢できたね…%RAND_SPLIT("えらいぞぉ/よく頑張りましたね")%っ
				CALL HEARTCB
				PRINTFORMW 」
				PRINTFORM 「でも、おしりで感じちゃうエッチな豚さんには『おしおき』が必要かな
				CALL HEARTCB
				PRINTFORMW 」
				PRINTFORMW 「まぁ、『おしおき』って言っても%CNAME(MASTER)%にとっては『ごほーび』かもしれませんけどね♪」
			CASE 1
				PRINTFORMW 「まだ痛みますか？……もう少し、頑張ってくださいね♪」
				PRINTFORM 「ほら……おしり『ぐりぐり』ってしますから、はしたなく喘いでくださいねっ
				CALL HEARTCB
				PRINTFORMW 」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;%CALLNAME:MASTER%は様々な感情を飲み込み、快感と苦痛を同時にこらえた…
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 20)
				CASE 0
					PRINTFORMW 「あれ？　もしかして、痛みとは違う感覚に気付いちゃいました？」
					PRINTFORMW 「それはね……『快感』って言うんですよっ♪ ドＭの ブ タ さんっ♪」
				CASE 1
					PRINTFORMW 「痛みを我慢してる、ってだけじゃなさそうですね……表情に戸惑いが見えますよ？」
					PRINTFORMW 「ねぇ……もしかして、こんなので感じちゃってるんですか？ くすくすっ♪」
			ENDSELECT
			;%CALLNAME:MASTER%は痛みの中に快感を意識し、怯えながら両方を我慢した…
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 21)
				CASE 0
					PRINTFORMW 「リリーが言うのもなんですけど……%CNAME(MASTER)%、素質があると思うんですよね」
					PRINTFORMW 「ほら、ここ……ほんとは少し『気持ちいい』って思ってませんか？」
					PRINTFORM 「自分に素直になって、リリーと一緒に堕ちちゃいましょうよ……ねっ
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「痛みに、リリーに怯えてるだけじゃないですよね？」
					PRINTFORMW 「痛いのに感じてしまう、自分の身体の変化に戸惑ってるんですね……」
					PRINTFORM 「大丈夫ですよ……リリーに身を任せて、あるがままの自分を受け入れてくださいね
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;消費気力による派生
	SELECTCASE GET_COM_RANK()
		;%CALLNAME:MASTER%は痛みを紛らわすため、同時にこみ上げる快感にすがりついた…
		CASE "自然な行動"
			SELECTCASE RANDCOM(3, 20)
				CASE 0
					PRINTFORMW 「痛みの奥の、痺れる様な快感……%CNAME(MASTER)%も見つけちゃいましたね」
					PRINTFORMW 「これからもっと痛くしてあげますから、ちゃんと集中して感じるんですよ？」
					PRINTFORM 「大丈夫……%CNAME(MASTER)%なら、とっても気持ち良くなれますよ
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「こんな事で感じちゃうなんて……ほんとに%RAND_SPLIT("ヘンタイ/ドＭ")%さんなんですねぇ……」
					PRINTFORMW 「でも安心してくださいね？ リリーはそんな%CNAME(MASTER)%が大好きですからっ♪」
				CASE 2
					PRINTFORMW 「ほんとうに、快楽に貪欲な豚さんですね……」
					PRINTFORMW 「ほら、体裁なんか気にしないで？ リリーだけしか見てませんから、ね？」
					PRINTFORM 「痛いのなんか忘れて、快感で頭いっぱいにしてくださいね
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
		;快感と苦痛が同時に襲い、%CALLNAME:MASTER%はくぐもった声を漏らした…
		CASE "無理のない行動"
			SELECTCASE RANDCOM(3, 21)
				CASE 0
					PRINTFORMW 「あれ？ 今ちょっとえっちな声が聴こえたような……」
					PRINTFORMW 「もしかして、感じちゃってるんですか？」
					PRINTFORMW 「くすっ♪ ほんとうに、%CNAME(MASTER)%って可愛いですねっ♪」
				CASE 1
					PRINTFORMW 「えっちな声、漏れちゃってますよ？」
					PRINTFORMW 「あれ？ もしかして、気が付いてなかったんですか？ くすっ♪」
				CASE 2
					PRINTFORMW 「痛いのに、なんでそんなに甘い声が出せるんでしょうか？？」
					PRINTFORMW 「……答えは『感じてるから』、ですよね♪」
					PRINTFORMW 「ほら、体裁なんか気にしないで？」
					PRINTFORM 「痛いのなんか忘れて、もっと感じていいんですよ
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

;──────────────────────────────────
;COM31, 悲鳴を上げる
;──────────────────────────────────
;・REACT分類:消極的従う
;・REACT印象:0から遠ざかる
;-------------------------------------------------
;・TFLAG:REACT派生(0=痛みによる悲鳴/1=恐怖による悲鳴)
;-------------------------------------------------
;=================================================
@KOJO_COM_K15_31
;=================================================
;派生で別けるのが面倒なので、どちらでも違和感でない程度にしてまとめてしまう
;━━━━━━━━━━━━━━━━━━━━━━━━
;無理をした行動
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;%CALLNAME:MASTER%は媚びるような悲鳴を上げた…
			;%CALLNAME:MASTER%は%CALLNAME%の好む苦痛の悲鳴を自然に声に出した…
	;実はまんざらでもないかつ、調教者が怒りではない
	IF COMCOR_POSI() && !T_COND("怒り")
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「%CNAME(MASTER)%の悲鳴、まるで誘ってるみたいですね……くすっ♪」
				PRINTFORMW 「まだまだ余裕そうで安心しました♪」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER)%って意外に演技派なんですね♪」
				PRINTFORMW 「もしかして『ＳＭごっこ』がご希望ですか？ くすっ♪」
			CASE 2
				PRINTFORMW 「うーん、ちょっとわざとらしいかな……うふふっ♪」
				PRINTFORMW 「無理して悲鳴を上げなくても大丈夫ですからねっ♪」
		ENDSELECT
			;%CALLNAME:MASTER%は怯えたように悲鳴を上げてみせた…
			;%CALLNAME:MASTER%は悲鳴を上げて苦痛に悶えてみせた…
	;その他
	ELSE
		SELECTCASE RANDCOM(3, 1)
			CASE 0
				PRINTFORMW 「%CNAME(MASTER)%の悲鳴、まるで誘ってるみたいですね……くすっ♪」
				PRINTFORMW 「ほら、次はどうして欲しいんですか？」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER)%って意外に演技派なんですね♪」
				PRINTFORMW 「もっと余裕をなくさないと、本当の悲鳴は聴けないって事でしょうか？」
			CASE 2
				PRINTFORMW 「ちょっとわざとらしいですよっ」
				PRINTFORMW 「もう少し激しくしたら、本気の悲鳴も聴けるんでしょうか？」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;=================================================
;アナル系への刺激（A愛撫/舐め/Aバイブ/Aビーズ/Aセックス/浣腸）
;=================================================
	IF 口上記入チェック(100) && IS_NOWACTNAME("アナル愛撫/アナル舐め/アナルバイブ/アナルビーズ/アナルセックス/浣腸セット")
		SELECTCASE RANDCOM(2, 2)
			CASE 0
				PRINTFORMW 「んっ……ちゃんと可愛い声で鳴けましたね♪」
				PRINTFORMW 「もう少ししたら、おしりでも感じられるようになりますから……」
				PRINTFORM 「それまで、あとちょっとだけ頑張ってみましょうねっ
				CALL HEARTCB
				PRINTFORMW 」
			CASE 1
				PRINTFORMW 「おしり、まだ痛みますか？」
				PRINTFORMW 「むぅ……ゆ～っくりしますから、もう少しだけ頑張ってくださいね♪」
				PRINTFORM 「いきますよ……ぐりぐりぃ…ぐ～りぐ～りっ
				CALL HEARTCB
				PRINTFORMW 」
		ENDSELECT
		RETURN 1
	ENDIF
	

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM31_0, 派生：痛みによる悲鳴
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
	;================================================
	;ACT分類が会話（ACT:罵倒がここで処理される場合あり）
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		SELECTCASE RANDCOM(2, 9)
			CASE 0
				PRINTFORMW 「あっ、すみませんっ！」
				PRINTFORMW 「リリーったらお話に夢中になっちゃって……痛かったですか？」
			CASE 1
				PRINTFORMW 「あっ、すみませんっ！　ちょっと刺激が強すぎました？」
				PRINTFORMW 「こんなに痛がるなんて……むむ、予想外です……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT分類が愛撫
	;================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "愛撫")
		;=================================================
		;調教者の状態が怒り
		;=================================================
		IF 口上記入チェック(100) && T_COND("怒り")
			SELECTCASE RANDCOM(2, 10)
				CASE 0
					PRINTFORMW 「敏感な場所ですもんね……ふふっ♪」
					PRINTFORM 「大丈夫ですよ、そう簡単に壊したりはしませんから……ね
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「かわいい声ですね……でもこれで、今の状況がわかりましたか？」
					PRINTFORMW 「%CNAME(MASTER)%を気持ち良くするのも、痛くするのも、全部リリーの自由なんですよ♪」
			ENDSELECT
			PRINTL  
			PRINTFORMDW リリーは優し気な笑みを浮かべているが、その声色は確かな怒気を孕んでいる…
			RETURN 1
		ENDIF
		
		;=================================================
		;ACT18, 足コキするの場合
		;=================================================
		IF 口上記入チェック(100) && IS_NOWACTNAME("足コキする")
			SELECTCASE RANDCOM(3, 11)
				CASE 0
					PRINTFORMW 「くすくすっ♪　%CNAME(MASTER)%が痛がってくれて、リリーも少し安心しました♪」
					PRINTFORM 「だってこれで感じちゃったら、もうただのブタさんですもん……ねっ
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「優しく踏んでるんですけど、なかなか力加減が難しいですね……」
					PRINTFORMW 「でも%CNAME(MASTER)%……痛がってる割りに、おちんちん元気になってませんか？」
				CASE 2
					PRINTFORMW 「リリーの足の裏で、硬くて熱いのが脈打ってて……」
					PRINTFORM 「踏む度に悲鳴を上げる%CNAME(MASTER)%の%RAND_SPLIT("はしたない/えっちな/可愛い")%顔を見てると……んっ
					CALL HEARTCB
					PRINTFORMW 」
					PRINTFORM 「これは……なんだかゾクゾクしちゃいますっ
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
			PRINTL  
			PRINTFORMDW リリーは%CALLNAME:MASTER%の股間を踏みつけながら、%RAND_SPLIT("サディスティック/嗜虐的")%な喜びを感じ始めているようだ……
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		IF AFFECTION("相思相愛") && PERCENT(60)
			PRINTFORMW 「%RAND_SPLIT("すっすみませんっ……/あわっあわわわわ……")%」
			PRINTFORMW 「あの……痛かった、ですか？　うぅ……」
			PRINTL  
