;=====================================================================
;奉仕系反応 COM40番台
;=====================================================================
;・調教者への愛撫を含む行動で見られる
;・ACT分類:奉仕+トリガーイベント制
;-------------------------------------------------
;────────────────────────────────────
;COM40, 拒否する
;────────────────────────────────────
;・REACT分類:拒否
;・REACT印象:悪印象大
;・お仕置きポイント追加
;・依存度変化(-2, -4)
;-------------------------------------------------
;・REACT派生なし
;=================================================
@KOJO_COM_K15_40
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM40, 拒否する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100) && !TFLAG:REACT派生
		;実はまんざらでもない
			;%CALLNAME:MASTER%は迷いを見せながらも要求には応じなかった…
		;実は嫌
			;%CALLNAME:MASTER%は気圧されながらも要求は呑まなかった…
		;無理のない行動
			;%CALLNAME:MASTER%は%CALLNAME:TARGET%の要求を拒否した…
		;自然な行動
			;%CALLNAME:MASTER%は強い意志で要求を撥ねつけた…
	;=================================================
	;ACT分類が性交
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "性交")
		;-------------------------------------------------
		;無理をした行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;実はまんざらでもない
			IF COMCOR_POSI()
				PRINTFORMW 「%RAND_SPLIT("ダメ/イヤ")%なんですか？　……ふふっ」
				PRINTFORMW 「いいですよ……%RAND_SPLIT("気の済むまで迷って/そうやって、もっとリリーを焦らして")%くださいね」
			;実は嫌
			ELSE
				PRINTFORMW 「むぅ、%RAND_SPLIT("中々/結構")%頑固ですね……」
				PRINTFORMW 「ちっぽけな%RAND_SPLIT("プライド/自尊心")%なんて捨てちゃった方がいいですよ？」
				PRINTFORMW 「そうしたら、いつでもリリーが犯してあげますからね♪」
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「怖くなっちゃいましたか？」
				PRINTFORMW 「それとも、%RAND_SPLIT("無理矢理/強引に")%して欲しいんでしょうか……ふふっ」
			CASE 1
				PRINTFORMW 「リリーはもう準備万端なんですけど……%RAND_SPLIT("ダメ/イヤ/無理")%ですか？」
				PRINTFORMW 「そんなに%RAND_SPLIT("嫌がら/拒ま")%れると、ちょっと%RAND_SPLIT("ショックなん/むっとしちゃうん")%ですけど……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類が羞恥
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "羞恥")
		;-------------------------------------------------
		;自慰・相互自慰
		;-------------------------------------------------
		IF 口上記入チェック(100) && IS_NOWACTNAME("自慰")
			;調教者も自慰中
			IF GETBIT(TEQUIP:自慰中, 1)
				IF RAND:3 == 0
					PRINTFORMW 「こんなに気持ちぃのに…%RAND_SPLIT("はぁ/あん/ふぅ/んぅ")%っ…」
					PRINTFORMW 「もっと%RAND_SPLIT("えっちな/")%オカズがなきゃ…できませんか？」
					PRINTFORM 「それなら…%RAND_SPLIT("ア/ン/あ/ん")%ッ…リリーがもっと%RAND_SPLIT("えっちな/すごい/えっちぃ/ヘンタイな")%おなにー、見せてあげるね…
					CALL HEARTCB
					PRINTFORMW 」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「%RAND_SPLIT("ん/あ/ひん")%っ…%RAND_SPLIT("はぁ/んぅ/うぅ")%♪　おなにーしてくれっないんですか？」
					PRINTFORMW 「リリーひとりじゃ…ッ…寂しいれすよ？」
				ELSE
					PRINTFORMW 「こんなに気持ちぃのに…%RAND_SPLIT("はぁ/あん/ふぅ/んぅ")%ッ…」
					PRINTFORMW 「してくれないなら…ッ…それもいいれすけど…」
					PRINTFORM 「そのかわり…リリーの%RAND_SPLIT("オナ二―/ひとりえっち")%、ちゃんと見ててくらさいね
					CALL HEARTCB
					PRINTFORMW 」
				ENDIF
			;顔騎されながら自慰
			ELSEIF TRUECHECK(TEQUIP:顔面騎乗, TEQUIP:顔面騎乗アナル) > 0
				IF RAND:2 == 0
					PRINTFORMW 「%RAND_SPLIT("ぺろぺろ/ごほーし")%するのに夢中でそれどころじゃないのかな？　ふふっ」
					PRINTFORMW 「%RAND_SPLIT("ぐ～りぐり/ほら/ほらぁ/ぐりぐり")%っ♪　お口塞がれて苦しいですか？　んっ、もっとフリフリしてあげますね♪」
					PRINTFORM 「オナニーしてくれないなら、そのぶん%RAND_SPLIT("ぺろぺろ/ごほーし/リリーの椅子として")%頑張らなきゃダメですよっ
					CALL HEARTCB
					PRINTFORMW 」
				ELSE
					PRINTFORMW 「%RAND_SPLIT("んんぅ/はぁ、あん/んくっはぁ")%っ♪　もうっ、何言ってるか、んっ、わかんないですよ？」
					PRINTFORM 「こんなに\@ TALENT:MASTER:オトコ ? ビンビンに勃起させ # おまんこ濡らし \@てるのに、『%RAND_SPLIT("無理です/イヤだ/できません")%』なんて言いませんよねぇ
					CALL HEARTCB
					PRINTFORMW 」
				ENDIF
			;自慰
			ELSE
				IF RAND:2 == 0
					PRINTFORMW 「やっぱり自分でするより、リリーに\@ TALENT:MASTER:オトコ ? おちんちん # おまんこ \@%RAND_SPLIT("なでなで/すりすり")%される方が好きなんですか？　ふふっ♪」
					PRINTFORMW 「でも、%RAND_SPLIT("聞き分けの無い/言う事聞かない")%%RAND_SPLIT("豚さん/ロリコンさん/ヘンタイさん/悪い子")%に、いつまでもリリーが優しいと思ったら大間違いですからね？」
				ELSE
					PRINTFORMW 「%RAND_SPLIT("もう…/ねぇ、")%リリーが%RAND_SPLIT("お手伝いし/一緒にしてあげ/手伝ってあげ")%なきゃ%RAND_SPLIT("オナニーも一人で/一人でオナニーも")%できないんですか？」
					PRINTFORMW 「リリーも%RAND_SPLIT("お世話するの/甘えんぼさん/お手伝いするの")%は%RAND_SPLIT("嫌いじゃない/嫌ではないん")%ですけど、なんでもリリー任せになると%RAND_SPLIT("流石に/ちょっと")%困っちゃいますよ……」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;羞恥無理をした行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;実はまんざらでもない
			IF COMCOR_POSI()
				PRINTFORMW 「本当はリリーに見られて気持ちよくなってるんじゃないですか？」
				PRINTFORMW 「ほら……恥ずかしがらないでリリーに見せて欲しいなぁ♪」
			;実は嫌
			ELSE
				PRINTFORMW 「恥ずかしくて%RAND_SPLIT("嫌/ダメ")%ですか？……ふふっ」
				PRINTFORMW 「それじゃあ無理矢理にでも、させちゃおうかなぁ♪」
			ENDIF
			RETURN 1
		ENDIF
		;---------------------
		;羞恥その他
		;---------------------
		;無理のない行動
		IF TFLAG:REACT負荷
			PRINTFORMW 「そういうのダメですよ？」
			PRINTFORMW 「イヤイヤしてないで、ちゃんとリリーに見せて欲しいなぁ♪」
		;自然な行動
		ELSE
			PRINTFORMW 「そんなに%RAND_SPLIT("言う/嫌がる/イヤ")%なら…仕方ないですね」
			PRINTFORMW 「リリーも鬼じゃないですし…%CNAME(MASTER)%の為にもっと『愉しい事』考えてあげますねっ♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;無理をした行動
	;=================================================
	IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
		;実はまんざらでもない
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 10)
				CASE 0
					PRINTFORMW 「迷って%RAND_SPLIT("るなら/ないで")%受け入れちゃいましょうよ…ねっ♪」
					PRINTFORMW 「それとも、リリー%RAND_SPLIT("お姉ちゃん/")%が%RAND_SPLIT("強引に/無理矢理")%してあげなきゃ嫌なのかな？　ふふっ♪」
				CASE 1
					PRINTFORMW 「ねぇ、本当の気持ちはどうなんです？」
					PRINTFORMW 「素直じゃない子は、%RAND_SPLIT("オシオキして/もっといじめて")%あげなきゃいけませんねっ」
			ENDSELECT
		;実は嫌
		ELSE
			SELECTCASE RANDCOM(2, 11)
				CASE 0
					PRINTFORMW 「声が震えちゃってますよ？」
					PRINTFORMW 「無理にやれとは言いませんけど……ふふっ」
					PRINTFORMW 「ちゃんと後でオシオキ、しますからねっ♪」
				CASE 1
					PRINTFORMW 「ねぇ、嫌なんですか？　むぅ……」
					PRINTFORMW 「ちょっと%RAND_SPLIT("優しく/甘く/甘やか")%し過ぎちゃったんでしょうか……」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		;無理のない行動
		IF TFLAG:REACT負荷
			;BASE:7, 苛立ち派生
			SELECTCASE TENSION(TARGET, 7)
				CASE "高い"
					PRINTFORMW 「ねぇ、どうしてそんなに聞き分けが無いんですか？」
					PRINTFORMW 「リリーだって頑張ってるのに……」
					PRINTFORMW 「ちょっと%RAND_SPLIT("優しく/甘く/甘やか")%し過ぎちゃったんでしょうか……」
				CASE "一般"
					PRINTFORMW 「%CNAME(MASTER)%にももう少し協力して欲しいんですけど…」
					PRINTFORMW 「う～ん……気持ちい事でも、やっぱり嫌なんですか？」
				CASE "低い"
					PRINTFORMW 「ねぇ、%RAND_SPLIT("そんな事/嫌だなんて/ダメなんて")%言わないで？」
					PRINTFORMW 「%RAND_SPLIT("慣れれば/やってみたら")%案外気持ちいい事かもしれませんし…ね♪」
			ENDSELECT
		;自然な行動
		ELSE
			;BASE:7, 苛立ち派生
			SELECTCASE TENSION(TARGET, 7)
				CASE "高い"
					PRINTFORMW 「ちょっと%RAND_SPLIT("優しく/甘く/甘やか")%し過ぎちゃったんでしょうか……」
					PRINTFORMW 「リリーが%CNAME(MASTER)%のご主人様だって事、ちゃんと教え込まなきゃいけませんね」
				CASE "一般"
					PRINTFORMW 「そんな事、リリーに言っちゃっていいんですか？」
					PRINTFORMW 「%RAND_SPLIT("ドＭの/ロリコンの")%%RAND_SPLIT("ヘンタイ/豚")%さんの癖に……ふふっ♪」
				CASE "低い"
					PRINTFORMW 「まだ%RAND_SPLIT("こんなに抵抗する/気持ちが折れない")%なんて…ふふっ」
					PRINTFORMW 「ねぇ、自分が今どんな格好をしてるのか理解してますか？」
					PRINTFORM 「%RAND_SPLIT("情けない/えろえろな/ロリコンの")%ヘンタイ奴隷さんっ
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
		ENDIF
	ELSE
		IF 0
		ELSEIF (AFFECTION("相思相愛") || CFLAG:TARGET:依存度 > CFLAG:MASTER:依存度) && !T_COND("怒り")
			PRINTFORMW 「プレイの一環だってわかってても、その反応はイラっとしちゃいますね…」
			PRINTFORMW 「とりあえず……聞き分けの無い子はお仕置きですよね♪」
		ELSEIF AFFECTION("溺愛") && !T_COND("怒り")
			PRINTFORMW 「あっ……お気に召しませんでしたか？」
			PRINTFORMW 「気持ちよくしてあげたいんですけど…中々難しいですね…」
		ELSEIF AFFECTION("反抗") || (!ALI(0) && !RAND:3) || T_COND("怒り")
			PRINTFORMW 「全然素直になってくれませんね」
			PRINTFORMW 「痛いのは好きじゃないんですけど……」
			PRINTFORMW 「%CNAME(MASTER)%がその気なら、リリーも考え方を変えなきゃいけませんね……」
		ELSE
			PRINTFORMW 「素直じゃない子はどうなるのか、忘れちゃいました？」
			PRINTFORMW 「何度も痛い目見ないとわからないんでしょうか……」
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;──────────────────────────────────
;COM41, 消極的やる
;──────────────────────────────────
;・REACT分類:消極的従う
;・REACT派生1は"消極的なのが気に入らない"となりお仕置きポイント追加
;・REACT印象:好印象小
;-------------------------------------------------
;・TFLAG:REACT派生 == 0, 消極的だけどいいか
;・依存度変化(-3, +2)
;-------------------------------------------------
;・TFLAG:REACT派生 == 1, 消極的なのが気に入らない
;・依存度変化(-5, 0)
;=================================================
@KOJO_COM_K15_41
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM41_0 & 1, 消極的やる
;━━━━━━━━━━━━━━━━━━━━━━━━
;早い段階から選択可能になる可能性が高く、進展した後は負荷2で選択肢になる場合もあったり
;ある程度注力すべき箇所であるように思う。という訳でちょっと真面目にやってみる
;-------------------------------------------------
IF 口上記入チェック(100) && GROUPMATCH(TFLAG:REACT派生, 0, 1) > 0
	;消極的なのが気に入らない
			;%CALLNAME:MASTER%の消極的な姿勢に、%CALLNAME%は不満そうだ…
	;消極的だけどいいか
		;実はまんざらでもない
				;%CALLNAME:MASTER%は自分から行動する勇気がないようだ…
		;実は嫌
				;%CALLNAME:MASTER%は悔しそうにしつつも逆らおうとはしない…
		;無理のない行動
			;%CALLNAME:MASTER%はどうにか%CALLNAME%の言う通りにしている…
		;自然な行動
			;%CALLNAME:MASTER%は%CALLNAME%に逆えない…
	;=================================================
	;ACT分類が性交
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "性交") && SEX_INITIATIVE(MASTER)
		;-------------------------------------------------
		;無理をした行動
		;-------------------------------------------------
		IF 口上記入チェック(100) && GET_COM_RANK() == "無理をした行動"
			;実はまんざらでもない
			IF COMCOR_POSI()
				SELECTCASE RANDCOM(3, 0)
					CASE 0
						PRINTFORMW 「ほら、リリーに合わせて%RAND_SPLIT("動いてごらん/腰振ってごらん/もっと腰振ってみよっか/パンッパンッてしてごらん")%っ♪」
						PRINTFORMW 「リリーがちゃんとリードしてあげますから…自分で%RAND_SPLIT("気持ちい場所/感じる場所")%を探してみようねっ
						CALL HEARTCB
						PRINTFORMW 」
					CASE 1
						PRINTFORM 「%RAND_SPLIT("あ/はん/ん/ひん")%っ
						CALL HEARTCB
						PRINTFORMW 　ほら、せっかくの名器なのに使わなきゃ勿体ないですよ♪」
						PRINTFORMW 「%RAND_SPLIT("もしかして/それとも")%リリーに全部お任せするつもりなんですか？」
					CASE 2
						PRINTFORMW 「もう、%RAND_SPLIT("マグロさん相手じゃ/こんなえっちじゃ")%つまんないですっ」
						PRINTFORMW 「もっと%RAND_SPLIT("腰を振って/可愛く喘いで/積極的になって")%リリーを楽しませてくれなきゃダメですよ♪」
				ENDSELECT
			;実は嫌
			ELSE
				SELECTCASE RANDCOM(2, 1)
					CASE 0
						PRINTFORMW 「ふふっ、%RAND_SPLIT("生意気な/悔しそうな/泣きそうな")%顔しちゃって…」
						PRINTFORMW 「その顔見てると%RAND_SPLIT("虐めたくなっ/ゾクゾクし")%ちゃいます……%RAND_SPLIT("は/")%んっ、ほら%RAND_SPLIT("ぁ/")%っ♪」
						PRINTFORMW 「どうせなら%CNAME(MASTER)%も楽しんだらどうですか？　%RAND_SPLIT("ロリコン/マゾ豚/")%さんっ♪」
					CASE 1
						PRINTFORMW 「%RAND_SPLIT("あれ/ノリが悪いですね")%、マグロさんプレイですか？　%RAND_SPLIT("…ふふっ/ふ～ん…")%」
						PRINTFORMW 「それなら、リリーに%RAND_SPLIT("オナホ/ダッチワイフ/物")%みたいな扱いされても…文句は言いませんよね♪ %RAND_SPLIT("は/")%んっ、ほら%RAND_SPLIT("ぁ/")%っ♪」
						PRINTFORM 「ただの『穴』扱いが嫌なら、もっと%RAND_SPLIT("えっちに腰を振って/可愛く喘いで/積極的になって/リリーを楽しませて")%くださいねっ
						CALL HEARTCB
						PRINTFORMW 」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		;無理のない行動
		IF TFLAG:REACT負荷
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORMW 「%RAND_SPLIT("まだまだ/なかなか")%慣れませんねぇ…」
					PRINTFORMW 「ほら…もっと締め付けて、んっ…一緒に楽しみましょうよっ♪」
				CASE 1
					PRINTFORMW 「ほら、リリー言う通りに動いて%RAND_SPLIT("ごらん/みよっか/みようね/みて")%♪」
					PRINTFORMW 「いつまでも%RAND_SPLIT("お人形さんみたいな扱い/マグロさん")%じゃ%CNAME(MASTER)%も%RAND_SPLIT("つまらない/愉しめない")%んじゃないですか？」
			ENDSELECT
		;自然な行動
		ELSE
			SELECTCASE RANDCOM(2, 3)
				CASE 0
					PRINTFORMW 「リリーの言いなりなんですね…ふふっ♪」
					PRINTFORM 「ほら、もっと奥まで%RAND_SPLIT("パンパンして/トントンして/グリグリして/突いて/掻き混ぜて")%あげますから…%RAND_SPLIT("可愛く/はしたなく/いやらしく")%%RAND_SPLIT("喘いで/イって/乱れて")%くださいね
					CALL HEARTCB
					PRINTFORMW 」
				CASE 1
					PRINTFORMW 「せっかくリリーが%RAND_SPLIT("犯して/パンパンして/気持ち良くして")%あげてるのに、ちょっと消極的過ぎませんか？」
					PRINTFORM 「豚さんらしくもっと%RAND_SPLIT("貪欲になって/『ぶひぃっ♪』って鳴いて/ブヒブヒ言って")%リリーを愉しませて欲しいなぁっ
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT分類が羞恥
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "羞恥")
		IF IS_NOWACTNAME("自慰")
			;調教者も自慰中
			IF GETBIT(TEQUIP:自慰中, 1)
				SELECTCASE GET_COM_RANK()
					;自然な行動
					;%CALLNAME:MASTER%は%CALLNAME%に逆えない…
					CASE "自然な行動"
						SELECTCASE RANDCOM(3, 5)
							CASE 0
								PRINTFORMW 「%RAND_SPLIT("はぁ/んぅ/あん/んっ")%…リリーと一緒だと%RAND_SPLIT("緊張して/恥ずかしくて")%上手にできませんか？　%RAND_SPLIT("ふふっ/へぇ～/えへへっ/えへへぇ")%♪」
								PRINTFORMW 「%RAND_SPLIT("甘えんぼさん/仕方ない子/世話の掛かる子")%なんだから……ほら、優しく\@ TALENT:MASTER:オトコ ? おちんちんを握って # 指を添えて \@みて？……リリーの声に合わせて動かしてみましょうね♪」
							CASE 1
								PRINTFORM 「%RAND_SPLIT("そう…その/あぁ…/えっちな顔…")%%RAND_SPLIT("無理矢理やらされてる/本当は嫌なのに/命令だから仕方ない")%って%RAND_SPLIT("感じ/表情")%が…%RAND_SPLIT("はぁ/んぅ/あん/んっ")%
								CALL HEARTCB
								PRINTFORMW 」
								IF RAND:2 == 0
									PRINTFORM 「%CNAME(MASTER)%をオカズに%RAND_SPLIT("オナニー/ひとりえっち")%するの……%RAND_SPLIT("きもちぃです…/癖になっちゃいます…/いつもより感じちゃう")%
									CALL HEARTCB
