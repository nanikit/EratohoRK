;=====================================================================
;봉사계 반응 COM40 접수대
;=====================================================================
;·조교자에게로의 애무를 포함한 행동으로 볼 수 있다
;·ACT분류:봉사+트리거 이벤트제
;-------------------------------------------------
;─
;COM40, 거부한다
;─
;·REACT분류:거부
;·REACT인상:약간나쁜인상
;·벌포인트 추가
;·의존도 변화(-2, -4)
;-------------------------------------------------
;·REACT파생없음
;=================================================
@KOJO_COM_K15_40
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM40, 거부한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
		;실은 그렇지만도 않다
			;%CALLNAME:MASTER%는 미혹을 보이면서도 요구에는 응하지 않았다…
		;실은 싫어
			;%CALLNAME:MASTER%는 압도되면서도 요구는 삼키지 않았다…
		;무리가 없는 행동
			;%CALLNAME:MASTER%는 %CALLNAME:TARGET%의 요구를 거부했다…
		;자연스러운 행동
			;%CALLNAME:MASTER%는 강한 의지로 요구를 퇴짜놓았다…
	;=================================================
	;ACT분류가 성교
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성교")
		;-------------------------------------------------
		;무리를 한 행동
		;-------------------------------------------------
		IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				PRINTFORMW 「%RAND_SPLIT("안돼/아니")%입니까?   …… 후훗」
				PRINTFORMW 「좋아요……%RAND_SPLIT("기분이 풀릴 때까지 헤매어/그렇게, 좀 더 릴리를 초조하게함이라고")%주세요」
			;실은 싫어
			ELSE
				PRINTFORMW 「므우,%RAND_SPLIT("꽤/상당히")%완고하네요……」
				PRINTFORMW 「작은%RAND_SPLIT("프라이드/자존심")%는 버리게 되는 것이 좋아요?」
				PRINTFORMW 「그렇게 하면, 언제라도 릴리가 범해 주니까요♪」
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「무서워져 버렸습니까?」
				PRINTFORMW 「그렇지 않으면,%RAND_SPLIT("무리하게/억지로")%해 주었으면 할까요…… 후훗」
			CASE 1
				PRINTFORMW 「릴리는 이제(벌써) 준비만단입니다만……%RAND_SPLIT("안돼/아니/무리")%입니까?」
				PRINTFORMW 「그렇게%RAND_SPLIT("혐무늬/거")%와 조금%RAND_SPLIT("쇼크/불끈 해 버리는")%이지만……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가 수치
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "수치")
		;-------------------------------------------------
		;자위·상호 자위
		;-------------------------------------------------
		IF 구상기입체크(100) && IS_NOWACTNAME("자위")
			;조교자도 자위중
			IF GETBIT(TEQUIP:자위중, 1)
				IF RAND:3 == 0
					PRINTFORMW 「이렇게 기분의 것에…%RAND_SPLIT("후~/팥고물//응우")%…」
					PRINTFORMW 「좀 더%RAND_SPLIT("엣치한/")%반찬이 없으면… 할 수 없습니까?」
					PRINTFORM 「그렇다면…%RAND_SPLIT("아//아/응")%… 릴리가 좀 더%RAND_SPLIT("엣치한/굉장하다/엣치/변태인")%오나니―, 보여 주네요…
					CALL HEARTCB
					PRINTFORMW 」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「%RAND_SPLIT("응/아/힝")%…%RAND_SPLIT("후~/응/우우")%♪  오나니─해 주고 없습니까?」
					PRINTFORMW 「릴리 혼자는…… 입적해 야?」
				ELSE
					PRINTFORMW 「이렇게 기분의 것에…%RAND_SPLIT("후~/팥고물//응우")%…」
					PRINTFORMW 「해 주지 않으면…… 그것필요할 수 있는 돕고…」
					PRINTFORM 「그 대신… 릴리의%RAND_SPLIT("오나 2-/혼자 엣치")%, 분명하게 보고 있어 주세요
					CALL HEARTCB
					PRINTFORMW 」
				ENDIF
			;안기 되면서 자위
			ELSEIF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마애널) > 0
				IF RAND:2 == 0
					PRINTFORMW 「%RAND_SPLIT("날름날름/오─해")%하는데 열중해 그럴 곳이 아닌 걸까나?   후후」
					PRINTFORMW 「%RAND_SPLIT("～꾸물거려/이봐요/이봐요/동글동글")%♪  구 찰 수 있어 괴롭습니까?   응, 좀 더 살랑살랑 해 주네요♪」
					PRINTFORM 「자위 해 주지 않으면, 그 만큼%RAND_SPLIT("날름날름/오─해/릴리의 의자로서")%노력하지 않으면 안됩니다
					CALL HEARTCB
					PRINTFORMW 」
				ELSE
					PRINTFORMW 「%RAND_SPLIT("응응/하아, 팥고물/응쿳하아")%♪  이제(벌써), 무슨 말하고 있는지, 읏, 몰라요?」
					PRINTFORM 「이렇게\@ TALENT:MASTER:남자 ? 펄떡펄떡 발기시키고# 보지 적셔\@비치는데, 『%RAND_SPLIT("무리입니다/싫다/할 수 없습니다")%』라고 말하지 않네요
					CALL HEARTCB
					PRINTFORMW 」
				ENDIF
			;자위
			ELSE
				IF RAND:2 == 0
					PRINTFORMW 「역시 스스로 하는 것보다, 릴리에\@ TALENT:MASTER:남자 ? 자지# 보지\@%RAND_SPLIT("스윽스윽/부비부비")%되는 (분)편을 좋아합니까?   후후♪」
					PRINTFORMW 「그렇지만,%RAND_SPLIT("구별하고가 없다/말하는 일 (듣)묻지 않는다")%%RAND_SPLIT("돼지씨/로리콘씨/변태씨/나쁜 아이")%에, 언제까지나 릴리가 상냥하다고 생각하면 큰 실수니까요?」
				ELSE
					PRINTFORMW 「%RAND_SPLIT("이제(벌써)…/저기,")%릴리가%RAND_SPLIT("거들기 해/함께 해 주고/도와 주고")%없으면%RAND_SPLIT("자위도 혼자서/혼자서 자위도")%할 수 없습니까?」
					PRINTFORMW 「릴리도%RAND_SPLIT("돌봐/응석쟁이씨/도와")%는%RAND_SPLIT("싫지 않다/싫지 않는 응")%이지만, 뭐든지 릴리 맡김이 되면%RAND_SPLIT("과연/조금")%곤란해 버려요……」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;수치 무리를 한 행동
		;-------------------------------------------------
		IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				PRINTFORMW 「사실은 릴리에 보여져 기분 좋아지고 있는 것이 아닙니까?」
				PRINTFORMW 「이봐요…… 부끄러워하지 말고 릴리에 보였으면 좋구나♪」
			;실은 싫어
			ELSE
				PRINTFORMW 「부끄러워서%RAND_SPLIT("혐/안돼")%입니까? …… 후훗」
				PRINTFORMW 「그러면 무리하게라도, 시켜 버릴까♪」
			ENDIF
			RETURN 1
		ENDIF
		;---------------------
		;수치 그 외
		;---------------------
		;무리가 없는 행동
		IF TFLAG:REACT부하
			PRINTFORMW 「그러한 것 안됩니다?」
			PRINTFORMW 「이야이야 하지 않고, 분명하게 릴리에 보였으면 좋구나♪」
		;자연스러운 행동
		ELSE
			PRINTFORMW 「그렇게%RAND_SPLIT("말한다/싫어한다/아니")%라면… 어쩔 수 없네요」
			PRINTFORMW 「릴리도 귀신이 아니며…%CNAME(MASTER)%를 위해서(때문에) 좀 더 『즐거운 일』생각해 주네요♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;무리를 한 행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
		;실은 그렇지만도 않다
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 10)
				CASE 0
					PRINTFORMW 「헤매어%RAND_SPLIT("라면/없고")%받아들여 버립시다…♪」
					PRINTFORMW 「그렇지 않으면, 릴리%RAND_SPLIT("누나/")%가%RAND_SPLIT("억지로/무리하게")%해 주지 않으면 싫은 것일까?   후후♪」
				CASE 1
					PRINTFORMW 「저기, 진정한 기분은 어떻습니까?」
					PRINTFORMW 「솔직하지 않은 아이는,%RAND_SPLIT("벌 해/좀 더 괴롭혀")%주지 않으면 안되겠네요」
			ENDSELECT
		;실은 싫어
		ELSE
			SELECTCASE RANDCOM(2, 11)
				CASE 0
					PRINTFORMW 「소리가 떨려 버리고 있어요?」
					PRINTFORMW 「무리하게 할거라고는 말하지 않지만…… 후훗」
					PRINTFORMW 「분명하게 다음에 벌, 하니까요♪」
				CASE 1
					PRINTFORMW 「저기, 싫습니까?   ……」
					PRINTFORMW 「조금%RAND_SPLIT("상냥하고/달콤하고/감이나인가")%너무 해 버린 것입니까……」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	IF RAND:3
		;무리가 없는 행동
		IF TFLAG:REACT부하
			;BASE:7, 초조 파생
			SELECTCASE TENSION(TARGET, 7)
				CASE "높다"
					PRINTFORMW 「저기, 어째서 그렇게 구별이 없습니까?」
					PRINTFORMW 「릴리라도 노력하고 있는데……」
					PRINTFORMW 「조금%RAND_SPLIT("상냥하고/달콤하고/감이나인가")%너무 해 버린 것입니까……」
				CASE "일반"
					PRINTFORMW 「%CNAME(MASTER)%에도 좀 더 협력해 주었으면 합니다만…」
					PRINTFORMW 「～응…… 기분 있고 일에서도, 역시 싫습니까?」
				CASE "낮다"
					PRINTFORMW 「저기,%RAND_SPLIT("그런 일/싫다 라고/안된다니")%말하지 말고?」
					PRINTFORMW 「%RAND_SPLIT("익숙해지면/해 보면")%의외로 기분이 좋은 일일지도 모르고…♪」
			ENDSELECT
		;자연스러운 행동
		ELSE
			;BASE:7, 초조 파생
			SELECTCASE TENSION(TARGET, 7)
				CASE "높다"
					PRINTFORMW 「조금%RAND_SPLIT("상냥하고/달콤하고/감이나인가")%너무 해 버린 것입니까……」
					PRINTFORMW 「릴리가%CNAME(MASTER)%의 주인님이라는 일, 분명하게 철저히 가르치지 않으면 안됩니다」
				CASE "일반"
					PRINTFORMW 「그런 일, 릴리에 말해 버려도 좋습니까?」
					PRINTFORMW 「%RAND_SPLIT("드 M의/로리콘의")%%RAND_SPLIT("변태/돼지")%씨의 주제에…… 후훗♪」
				CASE "낮다"
					PRINTFORMW 「아직%RAND_SPLIT("이렇게 저항한다/기분이 접히지 않는다")%는… 후훗」
					PRINTFORMW 「저기, 자신이 지금 어떤 모습을 하고 있는지 이해하고 있습니까?」
					PRINTFORM 「%RAND_SPLIT("한심하다/에로에로/로리콘의")%변태 노예씨
					CALL HEARTCB
					PRINTFORMW 」
			ENDSELECT
		ENDIF
	ELSE
		IF 0
		ELSEIF (AFFECTION("상사상애") || CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도) && !T_COND("분노")
			PRINTFORMW 「플레이의 일환이라고 알아도, 그 반응은 울컥해 버리네요…」
			PRINTFORMW 「우선…… 구별하고가 없는 아이는 벌이군요♪」
		ELSEIF AFFECTION("몹시 사랑함") && !T_COND("분노")
			PRINTFORMW 「앗…… 마음에 드시지 않았습니까?」
			PRINTFORMW 「기분 좋게 해 주고 싶습니다만… 꽤 어렵네요…」
		ELSEIF AFFECTION("반항") || (!ALI(0) && !RAND:3) || T_COND("분노")
			PRINTFORMW 「전혀 솔직해져 주지 않네요」
			PRINTFORMW 「아픈 것은 좋아하지 않습니다만……」
			PRINTFORMW 「%CNAME(MASTER)%가 그 기분이라면, 릴리도 사고방식을 바꾸지 않으면 안됩니다……」
		ELSE
			PRINTFORMW 「솔직하지 않은 아이는 어떻게 되는지, 잊어 버렸습니까?」
			PRINTFORMW 「몇번이나 아픈 눈 보지 않으면 모를까요……」
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;─
;COM41, 소극적한다
;─
;·REACT분류:소극적 따른다
;·REACT파생 1는 "소극적인의가 마음에 들지 않는다"되어 벌포인트 추가
;·REACT인상:약간좋은인상
;-------------------------------------------------
;·TFLAG:REACT파생 == 0, 소극적이지만 좋은가
;·의존도 변화(-3, +2)
;-------------------------------------------------
;·TFLAG:REACT파생 == 1, 소극적인의가 마음에 들지 않는다
;·의존도 변화(-5, 0)
;=================================================
@KOJO_COM_K15_41
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM41_0 & 1, 소극적한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;빠른 단계로부터 선택 가능하게 될 가능성이 높고, 진전한 뒤는 부하 2로 선택지가 되는 경우도 있거나
;어느 정도 주력 해야 할 개소이도록(듯이) 생각한다. 그렇다고 하는 것으로 조금 성실하게 해 본다
;-------------------------------------------------
IF 구상기입체크(100) && GROUPMATCH(TFLAG:REACT파생, 0, 1) > 0
	;소극적인의가 마음에 들지 않는다
			;%CALLNAME:MASTER%의 소극적인 자세에,%CALLNAME%는 불만인 것 같다…
	;소극적이지만 좋은가
		;실은 그렇지만도 않다
				;%CALLNAME:MASTER%는 자신으로부터 행동하는 용기가 없는 것 같다…
		;실은 싫어
				;%CALLNAME:MASTER%는 분한 듯이 하면서도 거역하려고는 하지 않는다…
		;무리가 없는 행동
			;%CALLNAME:MASTER%는 어떻게든%CALLNAME%가 말하는 대로 하고 있다…
		;자연스러운 행동
			;%CALLNAME:MASTER%는%CALLNAME%에 역네 없다…
	;=================================================
	;ACT분류가 성교
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성교") && SEX_INITIATIVE(MASTER)
		;-------------------------------------------------
		;무리를 한 행동
		;-------------------------------------------------
		IF 구상기입체크(100) && GET_COM_RANK() == "무리를 한 행동"
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				SELECTCASE RANDCOM(3, 0)
					CASE 0
						PRINTFORMW 「이봐요, 릴리에 맞추어%RAND_SPLIT("움직여 봐/허리 흔들어 봐/좀 더 허리 흔들어 볼까/판판이라고 해 봐")%♪」
						PRINTFORMW 「릴리가 분명하게 리드 해 주기 때문에… 스스로%RAND_SPLIT("기분 있고 장소/느끼는 장소")%를 찾아 보자
						CALL HEARTCB
						PRINTFORMW 」
					CASE 1
						PRINTFORM 「%RAND_SPLIT("아/는/응/힝")%
						CALL HEARTCB
						PRINTFORMW   이봐요, 모처럼의 명기인데 사용하지 않으면 아까워요♪」
						PRINTFORMW 「%RAND_SPLIT("혹시/그렇지 않으면")%릴리에 전부 맡길 생각입니까?」
					CASE 2
						PRINTFORMW 「이제(벌써),%RAND_SPLIT("마구로씨상대다/이런 엣치하지")%재미없습니닷」
						PRINTFORMW 「좀 더%RAND_SPLIT("허리를 흔들어/사랑스럽게 허덕여/적극적으로 되어")%릴리를 즐겁게 해 주지 않으면 안됩니다♪」
