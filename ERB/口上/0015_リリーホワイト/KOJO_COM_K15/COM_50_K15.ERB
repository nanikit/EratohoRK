;────────────────────────────────────
;COM50, 自慰し始める
;────────────────────────────────────
;・REACT分類:受け入れる
;・派生1,勝手にオナニーするなの場合はお仕置きポイント追加
;・REACT印象:悪印象小
;・我慢フラグ解消
;-------------------------------------------------
;・トリガーイベント制
;-------------------------------------------------
;・縄で縛られていると駄目
;・すでに自慰してる、性交中、性交奉仕中は弾かれる
;-------------------------------------------------
;TFLAG:REACT派生 == 0, 勝手にオナニーしてるけどいいか, 依存度変化(-1, 0)
;TFLAG:REACT派生 == 1, 勝手にオナニーするな, 依存度変化(-3, +2)
;=================================================
@KOJO_COM_K15_50
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM50, 自慰し始める
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 口上記入チェック(100)
	;=================================================
	;派生：勝手にオナニーするな
	;=================================================
	IF 口上記入チェック(20) && TFLAG:REACT派生 == 1
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「リリーは『オナニーして』なんて言いましたっけ？」
				PRINTFORMW 「我慢出来なくなっちゃったんですか？　ほんと、%RAND_SPLIT("仕方のない子なんですから/お猿さんみたいですね/こんなんじゃただの豚さんですね")%」
			CASE 1
				PRINTFORMW 「リリーの言う事も聞かず、勝手に%RAND_SPLIT("オナニー/ひとりえっち")%しちゃうんですか？」
				PRINTFORMW 「もう…『待て』もできない%RAND_SPLIT("ロリコン豚さん/お猿さん/ヘンタイさん")%にはキツイお仕置きが必要ですかね…ふふっ♪」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;会話系行動から実行
	;=================================================
	IF 口上記入チェック(100) && MENUMATCH(TFLAG:ACT, "会話")
		;調教者自慰
		IF GETBIT(TEQUIP:自慰中, 1)
			PRINTFORMW 「リリーのオナニー見て我慢できなくなっちゃったの？　%RAND_SPLIT("ふふ/くす/あはは")%っ♪」
		ELSEIF TEQUIP:オナホール
			PRINTFORMW 「リリーとのお話よりオナホオナニーの方が楽しいのかな？　%RAND_SPLIT("ふふ/くす/あはは")%っ♪」
		ELSEIF TEQUIP:バイブ || TEQUIP:アナルビーズ || TEQUIP:アナルバイブ
			PRINTFORMW 「ねぇ、リリーに見られながらするバイブオナニーは気持ちいいですか？　%RAND_SPLIT("ふふ/くす/あはは")%っ♪」
		ENDIF
		SELECTCASE RANDCOM(3, 1)
			CASE 0
				PRINTFORMW 「リリーはちゃんとお話したかったんですけど……」
				PRINTFORM 「ヘンタイさん相手じゃお口より身体に聞いた方がいいのかもしれませんね
				CALL HEARTCB
				PRINTFORMW 」
			CASE 1
				PRINTFORM 「『待て』も出来ないなんて……%CNAME(MASTER)%って頭までお猿さんなんですね
				CALL HEARTCB
				PRINTFORMW 」
			CASE 2
				PRINTFORM 「勝手に一人で始めちゃうなんて%RAND_SPLIT("……イケナイ子/、まるでお猿さんみたい")%ですね
				CALL HEARTCB
				PRINTFORMW 」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;アイマスク
	IF TEQUIP:アイマスク
		IF RAND:2 == 0
			PRINTFORM 「目隠ししてこーふんしちゃいました？　%RAND_SPLIT("ふふ/くす/あはは")%っ%RAND_SPLIT("/、本当にヘンタイさんですね/、本当にえっちな子ですね")%
			CALL HEARTCB
			PRINTFORMW 」
		ELSE
			PRINTFORMW 「ねぇ、我慢できなくなっちゃうくらい目隠しオナニーって気持ち良いんですか？」
		ENDIF
	;野外プレイ
	ELSEIF TEQUIP:野外プレイ
			PRINTFORM 「お外に出て興奮し過ぎちゃいましたか？　%RAND_SPLIT("ふふ/くす/あはは")%っ%RAND_SPLIT("/、本当にヘンタイさんですね/、本当にえっちな子ですね")%
			CALL HEARTCB
			PRINTFORMW 」
	;顔面騎乗
	ELSEIF TEQUIP:顔面騎乗 || TEQUIP:顔面騎乗アナル
		PRINTFORMW 「%RAND_SPLIT("ふふ/くす/あはは")%っ♪　お顔ぐりぐりされて%RAND_SPLIT("興奮し/我慢できなくなっ")%ちゃいましたか？」
	;調教者自慰
	ELSEIF GETBIT(TEQUIP:自慰中, 1)
		PRINTFORMW 「リリーのオナニー見て我慢できなくなっちゃったの？　%RAND_SPLIT("ふふ/くす/あはは")%っ♪」
	;バイブ系
	ELSEIF TEQUIP:バイブ || TEQUIP:アナルビーズ || TEQUIP:アナルバイブ
		PRINTFORMW 「ねぇ、リリーに見られながらするバイブオナニーは気持ちいいですか？　%RAND_SPLIT("ふふ/くす/あはは")%っ♪」
	ENDIF
	SELECTCASE RANDCOM(2, 2)
		CASE 0
			PRINTFORM 「『待て』も出来ないなんて……%CNAME(MASTER)%って頭までお猿さんなんですね
			CALL HEARTCB
			PRINTFORMW 」
		CASE 1
			PRINTFORM 「勝手に一人で始めちゃうなんて%RAND_SPLIT("……イケナイ子/、まるでお猿さんみたい")%ですね
			CALL HEARTCB
			PRINTFORMW 」
	ENDSELECT
	RETURN 1
ENDIF

;──────────────────────────────────
;COM51, 道具を外す
;──────────────────────────────────
;・REACT分類:暴れる
