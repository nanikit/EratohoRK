			ENDSELECT
		;실은 싫어
		;%CALLNAME:MASTER%는 들러붙은 것 같은 미소를 돌려주었지만, 눈이 헤엄치고 있다…
		ELSE
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「이봐요, 분명하게 릴리를 봐 주세요」
					PRINTFORMW 「호우호우, 이것은 상당 무리하고 있다고 보이네요…… 후훗♪」
				CASE 1
					PRINTFORMW 「키득♪  이상한 웃는 방법이군요─」
					PRINTFORMW 「이봐요, 다시 한번 웃어 봐 주세요♪」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT 분류가 애무
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "애무")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울적", "몽롱"
				PRINTFORMW 「그렇게 웃는 방법 해도 안됩니다」
				PRINTFORMW 「릴리가 충분히 귀여워해 주니까요♪」
			CASE "분노"
				PRINTFORMW 「이제(벌써), 그렇게 웃는 방법 하지 않아도 괜찮지 않습니까」
				PRINTFORMW 「릴리라도 노력하고 있습니다만……」
			CASE "지루함"
				PRINTFORMW 「도!   -싶게입니닷!」
				PRINTFORMW 「솔직하게 느껴 주세요……」
			CASEELSE
				PRINTFORMW 「미숙한 (분)편이 모에하지 않습니까?」
				PRINTFORMW 「…… 므우, 안 되는 것 같네요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT 분류가 도구
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "도구")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "애널 바이브/애널 비즈"
				PRINTFORMW 「왠지 근사하게 웃고 있습니다만」
				PRINTFORM 「엉덩이로부터 이런 것 기르고 있어서는 엉망이군요
				CALL HEARTCB
				PRINTFORMW 」
			CASE "C애무 도구"
				;클리캡(페니스 없음)
				IF !PENIS(MASTER)
					PRINTFORMW 「응─, 무엇이 재미있겠지요?」
					PRINTFORMW 「콩씨기분 너무 좋아 이상해져 버렸습니까?」
				;오나홀(페니스 있어)
				ELSE
					PRINTFORMW 「그 거 오나홀로 허덕이고 있는 사람이 하는 태도가 아니에요?」
					PRINTFORMW 「키득♪  도, 너무 웃기지 말아 주세욧」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT 분류가 성교
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성교")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			;A성교
			CASE "애널 섹스"
				PRINTFORMW 「릴리에 엉덩이 땅땅 찔리고 있는데 여유군요♪」
				PRINTFORMW 「보통이라면 좀 더 『괴롭다! 아프다!』는 얼굴 한다고 생각합니다만……」
				PRINTFORMW 「이제(벌써) 완전히 엉덩이 엣치하게 익숙해져 버렸다는 일입니까?」
			;질성교
			CASEELSE
				SELECTCASE GET_CONDITION(TARGET)
					CASE "정욕", "울적", "몽롱"
						PRINTFORMW 「아직도 건강한 것 같네요♪」
						PRINTFORM 「벌도 겸해, 안쪽까지 마음껏 찔러 주네요
						CALL HEARTCB
						PRINTFORMW 」
					CASE "분노"
						PRINTFORMW 「후훗, 건방진 아이군요♪」
						PRINTFORMW 「섹스보다, 오나홀 취급이 말해 겠지 우우인가?」
					CASE "지루함"
						PRINTFORMW 「도, 엣치의 한중간에 보이는 얼굴이 아니에요?」
						PRINTFORMW 「좀 더 무드 만들기에 협력해 주었으면 합니다」
					CASEELSE
						PRINTFORMW 「마구로씨의 『생각』입니까?   후후♪」
						PRINTFORM 「어느 쪽으로 하든 질내는 녹진녹진 이고, 오나홀 취급에는 충분합니닷
						CALL HEARTCB
						PRINTFORMW 」
				ENDSELECT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	IF RAND:3
		IF 0
		ELSEIF AFFECTION("상사상애") && PERCENT(60)
			PRINTFORMW 「후훗, 이상한 얼굴♪」
			PRINTFORMW 「좀 더 격렬해서 일입니까?」
		ELSEIF AFFECTION("몹시 사랑함") && PERCENT(60)
			PRINTFORMW 「도, 그런 얼굴 해서는 싫습니닷」
			PRINTFORM 「좀 더 릴리와 즐깁시다
			CALL HEARTCB
			PRINTFORMW 」
		ELSEIF AFFECTION("권애") && PERCENT(60)
			PRINTFORMW 「건방진 느낌입니닷」
			PRINTFORMW 「벌 해 버려요?」
		ELSEIF AFFECTION("공포") || T_COND("분노")
			PRINTFORMW 「그 웃는 방법, 싫습니닷」
			PRINTFORMW 「그렇게 벌해 주었으면 할까요?」
			PRINTFORMW 「어쩔 수 없는 아이군요……」
		ELSE
			PRINTFORMW 「재미있는 웃는 방법이군요…… 후후훗♪」
			PRINTL  
			PRINTFORMDW 릴리는 겁없게 웃어 돌려주어 왔다
		ENDIF
	ELSE
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울적", "몽롱"
				PRINTFORMW 「후훗, 그렇게 재미있는 얼굴 되면……」
				PRINTFORM 「%CNAME(MASTER, TARGET)%를 괴롭히고 싶어져 버립니다
				CALL HEARTCB
				PRINTFORMW 」
			CASE "분노"
				PRINTFORMW 「릴리, 그 웃는 것을 좋아하지 않습니다」
				PRINTFORMW 「그런데도 계속한다면…… 벌이니까요♪」
			CASE "지루함"
				PRINTFORMW 「어차피라면 릴리도 웃겼으면 좋네요─」
				PRINTFORMW 「이봐요, 좀 더 좋은 반응해 봐 주세요♪」
			CASEELSE
				PRINTFORMW 「자신은 아직 굽히지 않다는 일입니까」
				PRINTFORMW 「꽤 만만치 않네요─……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

